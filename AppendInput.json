{
  "input": {
    "type": "fieldset",
    "label": "Select Input Table",
    "repeat": false,
    "fields": {
      "target_table.table_id": {
        "type": "select",
        "label": "Left Table",
        "bind": "TABLES"
      },
      "target_table.remove_input_table_after_processing": {
        "type": "hidden",
        "val": "false"
      },
      "target_table.is_word": {
        "type": "hidden",
        "val": "false"
      },
      "source_table.table_id": {
        "type": "select",
        "label": "Right Table",
        "bind": "TABLES",
        "breakLine": true
      },
      "source_table.remove_input_table_after_processing": {
        "type": "hidden",
        "val": "false"
      },
      "source_table.is_word": {
        "type": "hidden",
        "val": "false"
      },
      "options.cut_off_run": {
        "type": "checkbox",
        "label": "Cut off Input table and Run",
        "val": "false",
        "isOn": "true",
        "isOff": "false",
        "breakLine": true
      },
      "options.input_table_rows_limit": {
        "type": "text",
        "label": "Input table limit(max: 800)",
        "show_if": "options.cut_off_run=='true'",
        "minLength": 1,
        "maxLength": 3,
        "val": "500",
        "maxVal": 800,
        "breakLine": true
      }
    }
  }
}