{
  "options.auto_run": {
    "type": "checkbox",
    "label": "Autorun Task",
    "val": "false",
    "isOn": "true",
    "isOff": "false"
  },
  "trigger_vector_frame": {
    "type": "fieldset",
    "label": "Task Lock",
    "show_if": "options.auto_run=='true'",
    "repeat": false,
    "fields": {
      "options.autorun_tables": {
        "type": "select",
        "label": "Autorun on table change",
        "bind": "TABLES",
        "multiple": true,
        "required": true,
        "breakLine": true
      },
      "trigger_key": {
        "type": "select",
        "label": "Trigger Key",
        "options": {
          "greater": {
            "label": "GREATER THAN"
          },
          "greater_or_equal": {
            "label": "GREATER OR EQUAL"
          },
          "equal": {
            "label": "EQUAL"
          },
          "smaller_or_equal": {
            "label": "SMALLER OR EQUAL"
          },
          "smaller": {
            "label": "SMALLER THAN"
          }
        },
        "val": "larger_or_equal"
      },
      "key_number": {
        "type": "text",
        "label": "Key",
        "val": "1",
        "required": true,
        "breakLine": true
      },  
      "options.names": {
        "type": "fieldset",
        "label": "Task Lock Definition",
        "repeat": true,
        "fields": {
          "name": {
            "type": "text",
            "label": "Name",
            "val": "",
            "required": true
          },
          "value": {
            "type": "text",
            "label": "Value(real number)",
            "val": "",
            "required": true
          },
          "delete": {
            "type": "button",
            "label": "Delete",
            "function": "DELETE_ROW"
          }
        }
      }
    }
  }
}
