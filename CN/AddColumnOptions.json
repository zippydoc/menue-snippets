{
  "options_box": {
    "type": "fieldset", 
    "label": "选项", 
    "repeat": false, 
    "fields": {
      "options.add_columns": {
        "type": "fieldset", 
        "label": "添加新列", 
        "repeat": true, 
        "fields": {
          "new_column_name": {
            "type": "text", 
            "label": "新列名", 
            "val": "", 
            "breakLine": true
          }, 
          "task_type": {
            "type": "select", 
            "label": "任务类型", 
            "required": true, 
            "options": {
              "static_value": {
                "label": "静态值"
              }, 
              "numeric_formula": {
                "label": "数值公式"
              }, 
              "text_formula": {
                "label": "文本公式"
              }, 
              "multi_column_functions": {
                "label": "多列"
              }, 
              "multi_row_functions": {
                "label": "多行"
              }
            }, 
            "val": "multi_column_functions", 
            "breakLine": true
          }, 
          "multi_column_functions__function": {
            "type": "select", 
            "label": "函数", 
            "show_if": "this.task_type=='multi_column_functions'", 
            "required": true, 
            "options": {
              "sum": {
                "label": "总和"
              }, 
              "average": {
                "label": "平均"
              }, 
              "median": {
                "label": "中位数"
              }, 
              "max": {
                "label": "最大"
              }, 
              "min": {
                "label": "最小"
              }, 
              "standard_deviation": {
                "label": "标准差"
              }, 
              "product": {
                "label": "产品"
              }, 
              "quota": {
                "label": "比例"
              }, 
              "absolute_growth": {
                "label": "绝对增长"
              }, 
              "relative_growth": {
                "label": "相对增长"
              }, 
              "compound_growth": {
                "label": "复合增长"
              }
            }, 
            "val": "sum"
          }, 
          "multi_column_functions__sel_columns": {
            "type": "select", 
            "label": "选择列", 
            "show_if": "this.task_type=='multi_column_functions' && (this.multi_column_functions__function=='sum' || this.multi_column_functions__function=='average' || this.multi_column_functions__function=='median' || this.multi_column_functions__function=='max' || this.multi_column_functions__function=='min' || this.multi_column_functions__function=='standard_deviation' || this.multi_column_functions__function=='product')", 
            "table": "input_table", 
            "bind": "COLUMNS", 
            "multiple": true, 
            "required": true
          }, 
          "multi_column_functions__column_a": {
            "type": "select", 
            "label": "选择列 A", 
            "show_if": "this.task_type=='multi_column_functions' && (this.multi_column_functions__function=='quota' || this.multi_column_functions__function=='absolute_growth' || this.multi_column_functions__function=='relative_growth' || this.multi_column_functions__function=='compound_growth')", 
            "table": "input_table", 
            "bind": "COLUMNS", 
            "required": true
          }, 
          "multi_column_functions__column_b": {
            "type": "select", 
            "label": "选择列 B", 
            "show_if": "this.task_type=='multi_column_functions' && (this.multi_column_functions__function=='quota' || this.multi_column_functions__function=='absolute_growth' || this.multi_column_functions__function=='relative_growth' || this.multi_column_functions__function=='compound_growth')", 
            "table": "input_table", 
            "bind": "COLUMNS", 
            "required": true
          }, 
          "multi_column_functions__n_value": {
            "type": "text", 
            "label": "N（正整数）", 
            "show_if": "this.task_type=='multi_column_functions' && this.multi_column_functions__function=='compound_growth'", 
            "val": "1", 
            "required": true
          }, 
          "multi_row_functions__function": {
            "type": "select", 
            "label": "函数", 
            "show_if": "this.task_type=='multi_row_functions'", 
            "required": true, 
            "options": {
              "trailing_row_n_average": {
                "label": "尾随行-N 平均"
              }, 
              "index_from_row_n": {
                "label": "行 N 索引"
              }, 
              "absolute_growth": {
                "label": "绝对增长"
              }, 
              "relative_growth": {
                "label": "相对增长"
              }, 
              "compound_growth": {
                "label": "复合增长"
              }
            }, 
            "val": "trailing_row_n_average"
          }, 
          "multi_row_functions__sel_column": {
            "type": "select", 
            "label": "列", 
            "show_if": "this.task_type=='multi_row_functions' && (this.multi_row_functions__function=='trailing_row_n_average' || this.multi_row_functions__function=='index_from_row_n' || this.multi_row_functions__function=='absolute_growth' || this.multi_row_functions__function=='relative_growth' || this.multi_row_functions__function=='compound_growth')", 
            "table": "input_table", 
            "bind": "COLUMNS", 
            "required": true
          }, 
          "multi_row_functions__n_value": {
            "type": "text", 
            "label": "N（正整数）", 
            "show_if": "this.task_type=='multi_row_functions' && (this.multi_row_functions__function=='trailing_row_n_average' || this.multi_row_functions__function=='index_from_row_n' || this.multi_row_functions__function=='compound_growth')", 
            "val": "1", 
            "required": true, 
            "breakLine": true
          }, 
          "static_value": {
            "type": "text", 
            "label": "输入值", 
            "show_if": "this.task_type=='static_value'", 
            "val": "", 
            "width": "100%"
          }, 
          "numeric_formula": {
            "type": "text", 
            "label": "输入公式，如：[Column1] + 2 * ([Row-1:Column3] - [Column1])", 
            "show_if": "this.task_type=='numeric_formula'", 
            "val": "", 
            "width": "100%"
          }, 
          "text_formula": {
            "type": "text", 
            "label": "输入公式，如：[firstName] + ' ' + [lastName] + ' ' + [Row-1:Column3]", 
            "show_if": "this.task_type=='text_formula'", 
            "val": "", 
            "width": "100%"
          }, 
          "use_multi_row": {
            "type": "checkbox", 
            "label": "多行选项", 
            "show_if": "this.task_type=='numeric_formula' || this.task_type=='text_formula'", 
            "val": "false", 
            "isOn": "true", 
            "isOff": "false"
          }, 
          "if_row_not_exist": {
            "type": "select", 
            "label": "如果引用行不存在", 
            "show_if": "this.use_multi_row=='true' && (this.task_type=='numeric_formula' || this.task_type=='text_formula')", 
            "required": true, 
            "options": {
              "empty": {
                "label": "空或 0"
              }, 
              "closest": {
                "label": "最接近行值"
              }
            }, 
            "val": "empty", 
            "breakLine": true
          }, 
          "format__category": {
            "type": "select", 
            "label": "格式", 
            "required": true, 
            "options": {
              "standard": {
                "label": "标准"
              }, 
              "number": {
                "label": "数字"
              }, 
              "percentage": {
                "label": "百分比"
              }, 
              "text": {
                "label": "文本"
              }, 
              "date": {
                "label": "日期"
              }, 
              "currency": {
                "label": "货币"
              }
            }, 
            "val": "standard"
          }, 
          "format__rounding": {
            "type": "select", 
            "label": "取整", 
            "show_if": "this.format__category=='number' || this.format__category=='percentage' || this.format__category=='currency'", 
            "required": true, 
            "options": {
              "no_rounding": {
                "label": "不取整"
              }, 
              "0": {
                "label": "0 位数"
              }, 
              "1": {
                "label": "1 位数"
              }, 
              "2": {
                "label": "2 位数"
              }, 
              "3": {
                "label": "3 位数"
              }, 
              "4": {
                "label": "4 位数"
              }, 
              "5": {
                "label": "5 位数"
              }, 
              "6": {
                "label": "6 位数"
              }, 
              "7": {
                "label": "7 位数"
              }, 
              "8": {
                "label": "8 位数"
              }, 
              "9": {
                "label": "9 位数"
              }
            }, 
            "val": "2"
          }, 
          "format__use_1000_separator": {
            "type": "checkbox", 
            "label": "使用 1000 分隔符（.）", 
            "show_if": "this.format__category=='number' || this.format__category=='percentage' || this.format__category=='currency'", 
            "val": "true", 
            "isOn": "true", 
            "isOff": "false"
          }, 
          "format__currency_symbol": {
            "type": "select", 
            "label": "符号", 
            "show_if": "this.format__category=='currency'", 
            "required": true, 
            "options": {
              "dollar": {
                "label": "$"
              }, 
              "euro": {
                "label": "€"
              }, 
              "pound": {
                "label": "£"
              }
            }, 
            "val": "dollar"
          }, 
          "format__date_format": {
            "type": "select", 
            "label": "类型", 
            "show_if": "this.format__category=='date'", 
            "required": true, 
            "options": {
              "mm/dd/yyyy": {
                "label": "3/14/2012"
              }, 
              "D,  M dd,  Y": {
                "label": "星期三，3 月 14 日，2012"
              }, 
              "yyyy-mm-dd": {
                "label": "2012-03-14"
              }
            }, 
            "val": "mm/dd/yyyy", 
            "breakLine": true
          }, 
          "group_counter__inc_dec_curly_bracket": {
            "type": "checkbox", 
            "label": "{} 增加/减少", 
            "show_if": "this.task_type=='group_counter'", 
            "val": "false", 
            "isOn": "true", 
            "isOff": "false", 
            "breakLine": true
          }, 
          "group_counter__inc_dec_square_bracket": {
            "type": "checkbox", 
            "label": "[] 增加/减少", 
            "show_if": "this.task_type=='group_counter'", 
            "val": "false", 
            "isOn": "true", 
            "isOff": "false", 
            "breakLine": true
          }, 
          "group_counter__inc_dec_parenthesis": {
            "type": "checkbox", 
            "label": "() 增加/减少", 
            "show_if": "this.task_type=='group_counter'", 
            "val": "false", 
            "isOn": "true", 
            "isOff": "false", 
            "breakLine": true
          }, 
          "group_counter__custom": {
            "type": "checkbox", 
            "label": "自定义群组计数器", 
            "show_if": "this.task_type=='group_counter'", 
            "val": "false", 
            "isOn": "true", 
            "isOff": "false", 
            "breakLine": true
          }, 
          "group_counter__column": {
            "type": "select", 
            "label": "列", 
            "show_if": "this.task_type=='group_counter' && this.group_counter__custom=='true'", 
            "table": "input_table", 
            "bind": "COLUMNS", 
            "required": true
          }, 
          "group_counter__type": {
            "type": "select", 
            "label": "当", 
            "show_if": "this.task_type=='group_counter' && this.group_counter__custom=='true'", 
            "required": true, 
            "options": {
              "starts_with": {
                "label": "开始于"
              }, 
              "ends_with": {
                "label": "结尾是"
              }, 
              "starts_with_regex": {
                "label": "开始于正则表达式"
              }, 
              "ends_with_regex": {
                "label": "结尾是正则表达式"
              }
            }, 
            "val": "starts_with"
          }, 
          "group_counter__value": {
            "type": "text", 
            "label": "输入值", 
            "show_if": "this.task_type=='group_counter' && this.group_counter__custom=='true'", 
            "val": ".", 
            "breakLine": true
          }, 
          "group_counter__label": {
            "type": "text", 
            "label": "输入标签", 
            "show_if": "this.task_type=='group_counter' && this.group_counter__custom=='true'", 
            "val": "sentence", 
            "breakLine": true
          }, 
          "group_counter__if_end_of_string_start_labeling_with_next_record": {
            "type": "checkbox", 
            "label": "如果字符串结尾以后一记录为标签", 
            "show_if": "this.task_type=='group_counter' && this.group_counter__custom=='true' && (this.group_counter__type=='starts_with' || this.group_counter__type=='starts_with_regex')", 
            "val": "true", 
            "isOn": "true", 
            "isOff": "false"
          }, 
          "group_counter__if_begin_of_string_start_labeling_with_previous_record": {
            "type": "checkbox", 
            "label": "如果字符串开始以前一记录为标签", 
            "show_if": "this.task_type=='group_counter' && this.group_counter__custom=='true' && (this.group_counter__type=='ends_with' || this.group_counter__type=='ends_with_regex')", 
            "val": "true", 
            "isOn": "true", 
            "isOff": "false"
          }, 
          "group_counter__always_include": {
            "type": "checkbox", 
            "label": "总是包含", 
            "show_if": "this.task_type=='group_counter' && this.group_counter__custom=='true'", 
            "val": "true", 
            "isOn": "true", 
            "isOff": "false"
          }, 
          "apply_to_group__enabled": {
            "type": "checkbox", 
            "label": "应用至组", 
            "show_if": "this.task_type!='static_value'", 
            "val": "false", 
            "isOn": "true", 
            "isOff": "false"
          }, 
          "apply_to_group__group_columns": {
            "type": "select", 
            "label": "组列", 
            "show_if": "this.apply_to_group__enabled == 'true' && this.task_type!='static_value'", 
            "table": "input_table", 
            "bind": "COLUMNS", 
            "multiple": true, 
            "required": true
          }, 
          "delete": {
            "type": "button", 
            "label": "删除", 
            "function": "DELETE_ROW"
          }
        }
      }, 
      "options.use_filter": {
        "type": "checkbox", 
        "label": "使用筛选", 
        "val": "false", 
        "isOn": "true", 
        "isOff": "false", 
        "style": "margin-top: 10px", 
        "breakLine": true
      }, 
      "filter_box": {
        "type": "fieldset", 
        "label": "筛选选项", 
        "show_if": "options.use_filter=='true'", 
        "repeat": false, 
        "fields": {
          "options.filter": {
            "type": "fieldset", 
            "label": "添加筛选条件", 
            "repeat": true, 
            "fields": {
              "initial_op": {
                "type": "select", 
                "label": "选择逻辑运算符", 
                "required": true, 
                "show_if" :  "$index == 0", 
                "options": {
                  "if": {
                    "label": "IF"
                  }, 
                  "if_not": {
                    "label": "如果不是"
                  }
                }, 
                "val": "if"
              }, 
              "op": {
                "type": "select", 
                "label": "选择逻辑运算符", 
                "required": true, 
                "show_if" :  "$index > 0", 
                "options": {
                  "and": {
                    "label": "AND"
                  }, 
                  "or": {
                    "label": "OR"
                  }, 
                  "and_not": {
                    "label": "AND NOT"
                  }, 
                  "or_not": {
                    "label": "OR NOT"
                  }
                }, 
                "val": "and"
              }, 
              "column": {
                "type": "select", 
                "label": "选择列", 
                "table": "input_table", 
                "bind": "COLUMNS", 
                "required": true
              }, 
              "condition": {
                "type": "select", 
                "label": "选择运算符", 
                "required": true, 
                "options": {
                  "包含": {
                    "label": "包含"
                  }, 
                  "CONTAINED IN STRING": {
                    "label": "包含于"
                  }, 
                  "开始于": {
                    "label": "开始于"
                  }, 
                  "结尾是": {
                    "label": "结尾是"
                  }, 
                  "=": {
                    "label": "相等"
                  }, 
                  ">": {
                    "label": "大于"
                  }, 
                  ">=": {
                    "label": "大于或等于"
                  }, 
                  "<": {
                    "label": "小于"
                  }, 
                  "<=": {
                    "label": "小于或等于"
                  }, 
                  "EMPTY": {
                    "label": "为空"
                  }, 
                  "BLANK": {
                    "label": "为空白"
                  }, 
                  "为 NULL": {
                    "label": "为 NULL"
                  }, 
                  "正则表达式匹配": {
                    "label": "正则表达式匹配"
                  }, 
                  "CONTAINED IN TABLE": {
                    "label": "于表格"
                  }, 
                  "于正则表格": {
                    "label": "于正则表格"
                  }, 
                  "日期": {
                    "label": "日期"
                  }
                }, 
                "val": "包含"
              }, 
              "date_condition": {
                "type": "select", 
                "label": "选择日期运算符", 
                "required": true, 
                "show_if": "this.condition == 'DATE'", 
                "options": {
                  "<": {
                    "label": "PRIOR"
                  }, 
                  "<=": {
                    "label": "先于或等于"
                  }, 
                  "=": {
                    "label": "相等"
                  }, 
                  ">": {
                    "label": "之后"
                  }, 
                  ">=": {
                    "label": "之后或等于"
                  }
                }, 
                "val": "="
              }, 
              "case_sensitive": {
                "type": "select", 
                "label": "选择区分大小写", 
                "required": true, 
                "show_if": "this.condition == 'CONTAINS' || this.condition == 'CONTAINED IN STRING' || this.condition == '=' || this.condition == 'STARTS WITH' || this.condition == 'ENDS WITH' || this.condition == 'REGEX MATCH' || this.condition == 'NOT REGEX MATCH' || this.condition == 'IN REGEX TABLE' || this.condition == 'NOT IN REGEX TABLE'", 
                "options": {
                  "yes": {
                    "label": "区分大小写"
                  }, 
                  "no": {
                    "label": "不区分大小写"
                  }
                }, 
                "val": "yes"
              }, 
              "date_value": {
                "type": "date", 
                "label": "选择日期", 
                "show_if": "this.condition=='DATE'", 
                "val": "1/31/2020", 
                "required": true
              }, 
              "value": {
                "type": "text", 
                "label": "输入值", 
                "show_if": "this.condition != 'DATE' && this.condition != 'EMPTY' && this.condition != 'NOT EMPTY' && this.condition != 'BLANK' && this.condition != 'NOT BLANK' && this.condition != 'IS NULL' && this.condition != 'IS NOT NULL' && this.condition != 'CONTAINED IN TABLE' && this.condition != 'NOT CONTAINED IN TABLE' && this.condition != 'IN REGEX TABLE' && this.condition != 'NOT IN REGEX TABLE'", 
                "val": "", 
                "required": true
              }, 
              "compare_table": {
                "type": "select", 
                "label": "选择表格", 
                "bind": "TABLES", 
                "empty": "", 
                "show_if": "this.condition == 'CONTAINED IN TABLE' || this.condition == 'NOT CONTAINED IN TABLE' || this.condition == 'IN REGEX TABLE' || this.condition == 'NOT IN REGEX TABLE'", 
                "required": true
              }, 
              "compare_column": {
                "type": "select", 
                "label": "选择列", 
                "table": "this.compare_table", 
                "bind": "COLUMNS", 
                "show_if": "this.condition == 'CONTAINED IN TABLE' || this.condition == 'NOT CONTAINED IN TABLE' || this.condition == 'IN REGEX TABLE' || this.condition == 'NOT IN REGEX TABLE'", 
                "required": true
              }, 
              "delete": {
                "type": "button", 
                "label": "删除", 
                "function": "DELETE_ROW"
              }
            }
          }, 
          "options.apply_to_record": {
            "type": "checkbox", 
            "label": "应用至记录", 
            "val": "true", 
            "isOn": "true", 
            "isOff": "false", 
            "style": "margin-top: 10px", 
            "breakLine": true
          }, 
          "apply_to_group_box": {
            "type": "fieldset", 
            "label": "应用至组", 
            "show_if": "options.apply_to_record=='false'", 
            "repeat": false, 
            "fields": {
              "options.apply_to_group_columns": {
                "type": "select", 
                "label": "选择组列", 
                "required": true, 
                "width": "100%", 
                "table": "input_table", 
                "multiple": true, 
                "bind": "COLUMNS", 
                "breakLine": true
              }
            }
          }
        }
      }
    }
  }
}