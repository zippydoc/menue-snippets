{
  "options": {
    "type": "fieldset",
    "标签": "选择选项",
    "repeat": false,
    "fields": {
      "options.position": {
        "type": "select",
        "标签": "选择位置",
        "show_if": "$input_table.numOfLabel >= 1",
        "required": true,
        "options": {
          "add_rows_at_table_end": {
            "标签": "Add row on bottom"
          },
          "add_row_on_top": {
            "标签": "Add row on top"
          },
          "add_rows_above_selected_row": {
            "标签": "Add row above selection"
          },
          "add_rows_below_selected_row": {
            "标签": "Add row below selection"
          },
          "add_row_after_each_group": {
            "标签": "在各组后添加行"
          }
        },
        "val": "add_rows_at_table_end",
        "breakLine": true
      },
      "options.group_column": {
        "type": "select",
        "标签": "组列",
        "show_if": "$input_table.numOfLabel < 1 && options.position=='add_row_after_each_group'",
        "table": "input_table",
        "bind": "COLUMNS",
        "required": true,
        "breakLine": true
      },
      "options.use_filter": {
        "type": "select",
        "标签": "选择类型",
        "show_if": "false",
        "required": true,
        "options": {
          "false": {
            "标签": "为每个表格行添加一行"
          },
          "true": {
            "标签": "为每个指定的行添加一条记录"
          }
        },
        "val": "false",
        "breakLine": true,
        "style": "margin-top: 10px"
      },
      "filter_box": {
        "type": "fieldset",
        "标签": "筛选选项",
        "show_if": "options.use_filter=='true' && $input_table.numOfLabel < 1",
        "repeat": false,
        "fields": {
          "options.filter": {
            "type": "fieldset",
            "标签": "添加筛选条件",
            "repeat": true,
            "fields": {
              "initial_op": {
                "type": "select",
                "标签": "选择逻辑运算符",
                "required": true,
                "show_if": "$index == 0",
                "options": {
                  "if": {
                    "标签": "IF"
                  },
                  "if_not": {
                    "标签": "如果不是"
                  }
                },
                "val": "if"
              },
              "op": {
                "type": "select",
                "标签": "选择逻辑运算符",
                "required": true,
                "show_if": "$index > 0",
                "options": {
                  "and": {
                    "标签": "AND"
                  },
                  "or": {
                    "标签": "OR"
                  },
                  "and_not": {
                    "标签": "AND NOT"
                  },
                  "or_not": {
                    "标签": "OR NOT"
                  }
                },
                "val": "and"
              },
              "column": {
                "type": "select",
                "标签": "选择列",
                "table": "input_table",
                "bind": "COLUMNS",
                "required": true
              },
              "condition": {
                "type": "select",
                "标签": "选择运算符",
                "required": true,
                "options": {
                  "包含": {
                    "标签": "包含"
                  },
                  "CONTAINED IN STRING": {
                    "标签": "包含于"
                  },
                  "开始于": {
                    "标签": "开始于"
                  },
                  "结尾是": {
                    "标签": "结尾是"
                  },
                  "=": {
                    "标签": "相等"
                  },
                  ">": {
                    "标签": "大于"
                  },
                  ">=": {
                    "标签": "大于或等于"
                  },
                  "<": {
                    "标签": "小于"
                  },
                  "<=": {
                    "标签": "小于或等于"
                  },
                  "EMPTY": {
                    "标签": "为空"
                  },
                  "BLANK": {
                    "标签": "为空白"
                  },
                  "为 NULL": {
                    "标签": "为 NULL"
                  },
                  "正则表达式匹配": {
                    "标签": "正则表达式匹配"
                  },
                  "CONTAINED IN TABLE": {
                    "标签": "于表格"
                  },
                  "于正则表格": {
                    "标签": "于正则表格"
                  },
                  "日期": {
                    "标签": "日期"
                  }
                },
                "val": "包含"
              },
              "date_condition": {
                "type": "select",
                "标签": "选择日期运算符",
                "required": true,
                "show_if": "this.condition == 'DATE'",
                "options": {
                  "<": {
                    "标签": "PRIOR"
                  },
                  "<=": {
                    "标签": "先于或等于"
                  },
                  "=": {
                    "标签": "相等"
                  },
                  ">": {
                    "标签": "之后"
                  },
                  ">=": {
                    "标签": "之后或等于"
                  }
                },
                "val": "="
              },
              "case_sensitive": {
                "type": "select",
                "标签": "选择区分大小写",
                "required": true,
                "show_if": "this.condition == 'CONTAINS' || this.condition == 'CONTAINED IN STRING' || this.condition == '=' || this.condition == 'STARTS WITH' || this.condition == 'ENDS WITH' || this.condition == 'REGEX MATCH' || this.condition == 'NOT REGEX MATCH' || this.condition == 'IN REGEX TABLE' || this.condition == 'NOT IN REGEX TABLE'",
                "options": {
                  "yes": {
                    "标签": "区分大小写"
                  },
                  "no": {
                    "标签": "不区分大小写"
                  }
                },
                "val": "yes"
              },
              "date_value": {
                "type": "date",
                "标签": "选择日期",
                "show_if": "this.condition=='DATE'",
                "val": "",
                "required": true
              },
              "value": {
                "type": "text",
                "标签": "输入值",
                "show_if": "this.condition != 'DATE' && this.condition != 'EMPTY' && this.condition != 'NOT EMPTY' && this.condition != 'BLANK' && this.condition != 'NOT BLANK' && this.condition != 'IS NULL' && this.condition != 'IS NOT NULL' && this.condition != 'CONTAINED IN TABLE' && this.condition != 'NOT CONTAINED IN TABLE' && this.condition != 'IN REGEX TABLE' && this.condition != 'NOT IN REGEX TABLE'",
                "val": "",
                "required": true
              },
              "compare_table": {
                "type": "select",
                "标签": "选择表格",
                "bind": "TABLES",
                "empty": "",
                "show_if": "this.condition == 'CONTAINED IN TABLE' || this.condition == 'NOT CONTAINED IN TABLE' || this.condition == 'IN REGEX TABLE' || this.condition == 'NOT IN REGEX TABLE'",
                "required": true
              },
              "compare_column": {
                "type": "select",
                "标签": "选择列",
                "table": "this.compare_table",
                "bind": "COLUMNS",
                "show_if": "this.condition == 'CONTAINED IN TABLE' || this.condition == 'NOT CONTAINED IN TABLE' || this.condition == 'IN REGEX TABLE' || this.condition == 'NOT IN REGEX TABLE'",
                "required": true
              },
              "delete": {
                "type": "button",
                "标签": "删除",
                "function": "DELETE_ROW"
              }
            }
          },
          "options.apply_to_record": {
            "type": "checkbox",
            "标签": "应用至记录",
            "val": "true",
            "isOn": "true",
            "isOff": "false",
            "style": "margin-top: 10px",
            "breakLine": true
          },
          "apply_to_group_box": {
            "type": "fieldset",
            "标签": "应用至组",
            "show_if": "options.apply_to_record=='false'",
            "repeat": false,
            "fields": {
              "options.apply_to_group_columns": {
                "type": "select",
                "标签": "选择组列",
                "required": true,
                "width": "100%",
                "table": "input_table",
                "multiple": true,
                "bind": "COLUMNS",
                "breakLine": true
              }
            }
          }
        }
      },
      "options.add_rows": {
        "type": "fieldset",
        "标签": "新行选项",
        "repeat": true,
        "fields": {
          "new_row_name": {
            "type": "text",
            "标签": "新行名称",
            "show_if": "$input_table.numOfLabel >= 1",
            "val": ""
          },
          "function_type": {
            "type": "select",
            "标签": "任务类型",
            "show_if": "false",
            "required": true,
            "options": {
              "multi_column_functions": {
                "标签": "多列"
              },
              "multi_row_functions": {
                "标签": "多行"
              }
            },
            "val": "multi_row_functions"
          },
          "multi_row_functions__function": {
            "type": "select",
            "标签": "选择功能",
            "show_if": "$input_table.numOfLabel >= 1",
            "required": true,
            "options": {
              "sum": {
                "标签": "总和"
              },
              "average": {
                "标签": "平均"
              },
              "median": {
                "标签": "中位数"
              },
              "standard_deviation": {
                "标签": "标准差"
              },
              "variance": {
                "标签": "方差"
              },
              "product": {
                "标签": "产品"
              },
              "quota": {
                "标签": "比例"
              }
            },
            "val": "sum"
          },
          "multi_row_functions__function_use_filter": {
            "type": "select",
            "标签": "选择功能",
            "show_if": "$input_table.numOfLabel < 1",
            "required": true,
            "options": {
              "sum": {
                "标签": "总和"
              },
              "average": {
                "标签": "平均"
              },
              "median": {
                "标签": "中位数"
              },
              "standard_deviation": {
                "标签": "标准差"
              },
              "variance": {
                "标签": "方差"
              },
              "product": {
                "标签": "产品"
              }
            },
            "val": "sum"
          },
          "multi_row_functions__sel_labels": {
            "type": "select",
            "标签": "选择行(s)",
            "show_if": "$input_table.numOfLabel >= 1 && this.multi_row_functions__function!='quota'",
            "required": true,
            "table": "input_table",
            "multiple": true,
            "bind": "LABELS"
          },
          "multi_row_functions__sel_rows": {
            "type": "select",
            "标签": "选择行",
            "show_if": "false",
            "required": true,
            "options": {
              "row-1": {
                "标签": "行-1"
              },
              "row-2": {
                "标签": "第2行"
              },
              "row-3": {
                "标签": "第3行"
              },
              "row-4": {
                "标签": "第4行"
              },
              "row-5": {
                "标签": "第5行"
              },
              "row-6": {
                "标签": "第6行"
              },
              "row-7": {
                "标签": "第7行"
              },
              "row-8": {
                "标签": "第8行"
              },
              "row-9": {
                "标签": "第9行"
              },
              "row-10": {
                "标签": "第10行"
              },
              "row+1": {
                "标签": "行+1"
              },
              "row+2": {
                "标签": "行+2"
              },
              "row+3": {
                "标签": "行+3"
              },
              "row+4": {
                "标签": "行+4"
              },
              "row+5": {
                "标签": "行+5"
              },
              "row+6": {
                "标签": "行+6"
              },
              "row+7": {
                "标签": "行+7"
              },
              "row+8": {
                "标签": "行+8"
              },
              "row+9": {
                "标签": "行+9"
              },
              "row+10": {
                "标签": "行+10"
              }
            },
            "multiple": true,
            "val": "row-1"
          },
          "multi_row_functions__sel_label_a": {
            "type": "select",
            "标签": "选择标签 A",
            "show_if": "$input_table.numOfLabel >= 1 && this.multi_row_functions__function=='quota'",
            "required": true,
            "table": "input_table",
            "bind": "LABELS"
          },
          "multi_row_functions__sel_label_b": {
            "type": "select",
            "标签": "选择标签 B",
            "show_if": "$input_table.numOfLabel >= 1 && this.multi_row_functions__function=='quota'",
            "required": true,
            "table": "input_table",
            "bind": "LABELS",
            "breakLine": true
          },
          "position": {
            "type": "select",
            "标签": "位置",
            "show_if": "$input_table.numOfLabel >= 1",
            "required": true,
            "options": {
              "bottom": {
                "标签": "底部"
              },
              "top": {
                "标签": "顶部"
              },
              "above": {
                "标签": "之上"
              },
              "below": {
                "标签": "之下"
              }
            },
            "val": "bottom"
          },
          "positioning_row": {
            "type": "select",
            "标签": "定位行",
            "show_if": "$input_table.numOfLabel >= 1 && (this.position=='above' || this.position=='below')",
            "required": true,
            "table": "input_table",
            "bind": "LABELS"
          },
          "format__category": {
            "type": "select",
            "标签": "格式",
            "required": true,
            "options": {
              "standard": {
                "标签": "标准"
              },
              "number": {
                "标签": "数字"
              },
              "percentage": {
                "标签": "百分比"
              },
              "text": {
                "标签": "文本"
              },
              "date": {
                "标签": "日期"
              },
              "currency": {
                "标签": "货币"
              }
            },
            "val": "standard"
          },
          "format__rounding": {
            "type": "select",
            "标签": "取整",
            "show_if": "this.format__category=='number' || this.format__category=='percentage' || this.format__category=='currency'",
            "required": true,
            "options": {
              "no_rounding": {
                "标签": "不取整"
              },
              "0": {
                "标签": "0 位数"
              },
              "1": {
                "标签": "1 位数"
              },
              "2": {
                "标签": "2 位数"
              },
              "3": {
                "标签": "3 位数"
              },
              "4": {
                "标签": "4 位数"
              },
              "5": {
                "标签": "5 位数"
              },
              "6": {
                "标签": "6 位数"
              },
              "7": {
                "标签": "7 位数"
              },
              "8": {
                "标签": "8 位数"
              },
              "9": {
                "标签": "9 位数"
              }
            },
            "val": "no_rounding"
          },
          "format__use_1000_separator": {
            "type": "checkbox",
            "标签": "Use 1000 Separator(.)",
            "show_if": "this.format__category=='number' || this.format__category=='percentage' || this.format__category=='currency'",
            "val": "false",
            "isOn": "true",
            "isOff": "false"
          },
          "format__currency_symbol": {
            "type": "select",
            "标签": "符号",
            "show_if": "this.format__category=='currency'",
            "required": true,
            "options": {
              "dollar": {
                "标签": "$"
              },
              "euro": {
                "标签": "€"
              },
              "pound": {
                "标签": "£"
              }
            },
            "val": "dollar"
          },
          "format__date_format": {
            "type": "select",
            "标签": "类型",
            "show_if": "this.format__category=='date'",
            "required": true,
            "options": {
              "mm/dd/yyyy": {
                "标签": "3/14/2012"
              },
              "D, M dd, Y": {
                "标签": "2012年3月14日，星期三"
              },
              "yyyy-mm-dd": {
                "标签": "2012-03-14"
              }
            },
            "val": "mm/dd/yyyy"
          },
          "delete": {
            "type": "button",
            "标签": "删除",
            "function": "DELETE_ROW"
          }
        }
      },
      "options.add_new_row": {
        "type": "checkbox",
        "标签": "添加标签",
        "show_if": "$input_table.numOfLabel < 1",
        "val": "false",
        "isOn": "true",
        "isOff": "false",
        "style": "margin-top: 10px",
        "breakLine": true
      },
      "options.new_row_label": {
        "type": "text",
        "标签": "Label Name",
        "show_if": "$input_table.numOfLabel < 1 && options.add_new_row == 'true'",
        "val": ""
      },
      "options.new_sel_column": {
        "type": "select",
        "标签": "选择列",
        "show_if": "$input_table.numOfLabel < 1 && options.add_new_row == 'true'",
        "required": true,
        "table": "input_table",
        "multiple": false,
        "bind": "COLUMNS",
        "breakLine": true
      },
      "options.insert_on_top": {
        "type": "checkbox",
        "标签": "Insert on Top",
        "show_if": "$input_table.numOfLabel < 1",
        "val": "false",
        "isOn": "true",
        "isOff": "false",
        "style": "margin-top: 10px",
        "breakLine": true
      }
    }
  }
}