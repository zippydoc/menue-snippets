{
  "options": {
    "type": "fieldset",
    "label": "选项",
    "repeat": false,
    "fields": {
      "compare_set": {
        "type": "fieldset",
        "label": "选择模式",
        "repeat": true,
        "fields": {
          "pattern": {
            "type": "select",
            "label": "表格模式类型",
            "options": {
              "all_col": {
                "label": "所有列标识"
              },
              "two_col": {
                "label": "两列标识"
              }
            },
            "val": "all_col",
            "required": true
          },
          "compare_type": {
            "type": "select",
            "label": "单元格模式类型",
            "options": {
              "equal": {
                "label": "相等"
              },
              "contain": {
                "label": "包含"
              },
              "start_with": {
                "label": "开始于"
              },
              "end_with": {
                "label": "结尾是"
              },
              "reg_exp": {
                "label": "正则表达式"
              }
            },
            "val": "equal",
            "required": true
          },
          "case_sensitive": {
            "type": "select",
            "label": "区分大小写",
            "required": true,
            "options": {
              "yes": {
                "label": "区分大小写"
              },
              "no": {
                "label": "不区分大小写"
              }
            },
            "val": "no"
          },
          "expression": {
            "type": "text",
            "label": "表达式",
            "show_if": "this.compare_type != 'equal'",
            "required": true,
            "val": ""
          },
          "delete": {
            "show_if": "this.field_name != ''",
            "type": "button",
            "label": "删除",
            "function": "DELETE_ROW"
          }
        },
        "breakLine": true
      },
      "target": {
        "type": "select",
        "label": "目标",
        "options": {
          "to_table": {
            "label": "应用至表格"
          },
          "to_rows": {
            "label": "逐行"
          }
        },
        "val": "to_table",
        "required": true
      },
      "match_type": {
        "type": "select",
        "show_if": "target != 'to_table'",
        "label": "匹配类型",
        "options": {
          "match": {
            "label": "仅匹配"
          },
          "nonmatch": {
            "label": "仅非匹配"
          },
          "all": {
            "label": "所有"
          }
        },
        "val": "match",
        "required": true,
        "breakLine": true
      },
      "create_sql": {
        "type": "checkbox",
        "label": "显示 SQL-详细信息",
        "val": "false",
        "isOn": "true",
        "isOff": "false"
      }
    }
  }
}