{
  "options": {
    "type": "fieldset",
    "label": "选择选项",
    "repeat": false,
    "fields": {
      "options.convert_type": {
        "type": "select",
        "label": "转换类型",
        "required": true,
        "options": {
          "complete_date": {
            "label": "完成日期"
          },
          "expand_date": {
            "label": "展开日期"
          },
          "convert_date": {
            "label": "转换日期"
          },
          "convert_date_range": {
            "label": "转换日期范围"
          }
        },
        "val": "complete_date"
      },
      "options.date_convert_type": {
        "type": "select",
        "label": "转换为",
        "show_if": "options.convert_type=='convert_date'",
        "required": true,
        "options": {
          "begin_of_month": {
            "label": "月初"
          },
          "end_of_month": {
            "label": "月末"
          },
          "hy_year": {
            "label": "半年"
          },
          "qrt_year": {
            "label": "季度年"
          },
          "month_year": {
            "label": "月 年"
          },
          "week_year": {
            "label": "周 年"
          }
        },
        "val": "begin_of_month"
      },
      "options.date_range_convert_type": {
        "type": "select",
        "label": "转换为",
        "show_if": "options.convert_type=='convert_date_range'",
        "required": true,
        "options": {
          "begin_of_month": {
            "label": "月初"
          },
          "end_of_month": {
            "label": "月末"
          },
          "begin_of_range": {
            "label": "范围开始"
          },
          "end_of_range": {
            "label": "范围结束"
          },
          "begin_of_quarter": {
            "label": "季初"
          },
          "end_of_quarter": {
            "label": "季末"
          }
        },
        "val": "begin_of_month",
        "breakLine": true
      },
      "options.output_in_new_column": {
        "type": "checkbox",
        "label": "在新列中输出",
        "val": "false",
        "isOn": "true",
        "isOff": "false"
      },
      "options.new_field_name": {
        "type": "text",
        "label": "新列名",
        "show_if": "options.output_in_new_column=='true'",
        "val": "GeneratedDate",
        "required": true,
        "minLength": 1,
        "maxLength": 255,
        "breakLine": true
      }
    }
  }
}