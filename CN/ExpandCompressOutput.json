{
  "output": {
    "type": "fieldset",
    "label": "定义输出表格",
    "show_if": "options.create_new_table=='true'",
    "repeat": false,
    "fields": {
      "output.custom_text": {
        "type": "text",
        "label": "输出表格名称",
        "show_if": "output.custom_options=='false' || (output.custom_options=='true' && output.compose_table_name=='false')",
        "minLength": 1,
        "val": "",
        "width": "100%",
        "breakLine": true
      },
      "output.custom_options": {
        "type": "checkbox",
        "label": "输出选项",
        "val": "false",
        "isOn": "true",
        "isOff": "false",
        "breakLine": true
      },
      "output.compose_table_name": {
        "type": "checkbox",
        "label": "编写表格名称",
        "show_if": "output.custom_options=='true'",
        "val": "false",
        "isOn": "true",
        "isOff": "false",
        "breakLine": true
      },
      "compose_table_name_frame": {
        "type": "fieldset",
        "label": "编写输出表格名称",
        "show_if": "output.custom_options=='true' && output.compose_table_name=='true'",
        "repeat": false,
        "fields": {
          "output.prefix_rule": {
            "type": "select",
            "label": "前缀",
            "show_if": "output.compose_table_name=='true'",
            "val": "empty",
            "options": {
              "empty": {
                "label": "-"
              },
              "script_name": {
                "label": "任务名称"
              },
              "date_string_type1": {
                "label": "Date: dd_mm_yyyy"
              },
              "date_string_type2": {
                "label": "Date: yyyy_mm_dd"
              },
              "time_string": {
                "label": "时间：h_min_sec"
              },
              "datetime_string": {
                "label": "Date_Time"
              },
              "custom": {
                "label": "自定义"
              }
            }
          },
          "output.prefix_text": {
            "type": "text",
            "label": "前缀文本",
            "show_if": "output.compose_table_name=='true' && output.prefix_rule=='custom'",
            "minLength": 1,
            "maxLength": 31,
            "val": ""
          },
          "output.core_delimiter": {
            "type": "text",
            "label": "分隔符",
            "show_if": "output.compose_table_name=='true' && output.prefix_rule!='empty'",
            "val": "_",
            "width": "50px"
          },
          "output.core_rule": {
            "type": "select",
            "label": "表格名称",
            "show_if": "output.compose_table_name=='true'",
            "val": "input_table_name",
            "options": {
              "input_table_name": {
                "label": "输入表格名称"
              },
              "script_name": {
                "label": "任务名称"
              },
              "custom": {
                "label": "自定义"
              }
            }
          },
          "output.core_text": {
            "type": "text",
            "label": "主表格名称",
            "show_if": "output.compose_table_name=='true' && output.core_rule=='custom'",
            "required": true,
            "minLength": 1,
            "maxLength": 31,
            "val": ""
          },
          "output.suffix_delimiter": {
            "type": "text",
            "label": "分隔符",
            "show_if": "output.compose_table_name=='true'",
            "val": "_",
            "width": "50px"
          },
          "output.suffix_rule": {
            "type": "select",
            "label": "后缀",
            "show_if": "output.compose_table_name=='true'",
            "val": "script_name",
            "options": {
              "empty": {
                "label": "-"
              },
              "script_name": {
                "label": "任务名称"
              },
              "date_string_type1": {
                "label": "Date: dd_mm_yyyy"
              },
              "date_string_type2": {
                "label": "Date: yyyy_mm_dd"
              },
              "time_string": {
                "label": "时间：h_min_sec"
              },
              "datetime_string": {
                "label": "Date_Time"
              },
              "custom": {
                "label": "自定义"
              }
            }
          },
          "output.suffix_text": {
            "type": "text",
            "label": "后缀文本",
            "show_if": "output.compose_table_name=='true' && output.suffix_rule=='custom'",
            "minLength": 1,
            "maxLength": 31,
            "val": "",
            "breakLine": true
          },
