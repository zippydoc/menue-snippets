{
  "options": {
    "type": "fieldset",
    "label": "选择选项",
    "repeat": false,
    "fields": {
      "options.to": {
        "type": "select",
        "label": "类型",
        "options": {
          "database": {
            "label": "导出至数据库"
          },
          "file": {
            "label": "导出至文件"
          },
          "mytask": {
            "label": "导出 MyTask"
          }
        },
        "val": "database",
        "required": true
      },
      "options.export_series": {
        "type": "checkbox",
        "label": "导出系列",
        "show_if": "options.to=='file'",
        "val": "false",
        "isOn": "true",
        "isOff": "false",
        "breakLine": true
      },
      "options.file_name_column": {
        "type": "select",
        "label": "选择文件名列",
        "show_if": "options.to=='file' && options.export_series=='true'",
        "table": "input_table",
        "bind": "COLUMNS",
        "required": true,
        "breakLine": true
      },
      "postgresql_db_credential_box": {
        "type": "fieldset",
        "label": "数据库连接凭据",
        "show_if": "options.to=='database'",
        "repeat": false,
        "fields": {
          "options.dbms": {
            "type": "select",
            "label": "选择 DBMS",
            "options": {
              "postgresql": {
                "label": "PostgreSQL"
              },
              "mysql": {
                "label": "MySQL"
              },
              "sql_server": {
                "label": "MS SQL Server"
              },
              "oracle": {
                "label": "Oracle"
              }
            },
            "val": "postgresql",
            "required": true,
            "breakLine": true
          },
          "options.postgresql_credential.user": {
            "type": "text",
            "label": "用户",
            "show_if": "options.dbms=='postgresql'",
            "required": true,
            "width": "100%",
            "breakLine": true
          },
          "options.postgresql_credential.password": {
            "type": "text",
            "label": "密码",
            "show_if": "options.dbms=='postgresql'",
            "required": true,
            "width": "100%",
            "breakLine": true
          },
          "options.postgresql_credential.host": {
            "type": "text",
            "label": "主机",
            "show_if": "options.dbms=='postgresql'",
            "required": true,
            "width": "100%",
            "breakLine": true
          },
          "options.postgresql_credential.database": {
            "type": "text",
            "label": "数据库",
            "show_if": "options.dbms=='postgresql'",
            "required": true,
            "width": "100%",
            "breakLine": true
          }
        }
      },
      "options.schema_name_to_export": {
        "type": "text",
        "label": "需导出的 Schema 名称",
        "show_if": "options.to=='database' && options.dbms=='postgresql'",
        "required": true,
        "val": "public",
        "width": "100%",
        "breakLine": true
      },
      "options.webservice": {
        "type": "select",
        "label": "选择 Web 服务",
        "show_if": "options.to=='file' || options.to=='mytask'",
        "options": {
          "github": {
            "label": "Github"
          }
        },
        "val": "github",
        "required": true
      },
      "options.export_format": {
        "type": "select",
        "label": "导出格式",
        "show_if": "options.to=='file'",
        "options": {
          "csv_file": {
            "label": "CSV 文件"
          },
          "word_by_word": {
            "label": "逐字"
          },
          "line_by_line": {
            "label": "逐行"
          }
        },
        "val": "csv_file",
        "required": true,
        "breakLine": true
      },
      "options.github_token": {
        "type": "text",
        "label": "指定拥有写入权限的 Github 个人访问 Token：",
        "show_if": "options.to=='file' || options.to=='mytask'",
        "required": true,
        "width": "100%",
        "breakLine": true
      },
      "options.github_url": {
        "type": "text",
        "label": "输入链接，如：https://github.com/zippydoc/test",
        "show_if": "options.to=='file' || options.to=='mytask'",
        "val": "https://github.com/zippydoc/public-tables",
        "required": true,
        "width": "100%",
        "breakLine": true
      },
      "options.if_duplicated": {
        "type": "select",
        "label": "如果重复",
        "val": "drop_if_exist",
        "options": {
          "drop_if_exist": {
            "label": "创建或删除（如存在）"
          },
          "error_if_exist": {
            "label": "新建（如存在则抛出错误）"
          },
          "error_if_not_exist": {
            "label": "删除（如不存在则抛出错误）"
          },
          "append": {
            "label": "追加"
          }
        }
      },
      "options.export_task_name": {
        "type": "text",
        "label": "输入任务名称",
        "show_if": "options.to=='mytask'",
        "required": true,
        "val": "Flow6_Task1_AddColumns",
        "width": "100%",
        "breakLine": true
      }
    }
  }
}