{
  "output": {
    "type": "fieldset",
    "标签": "定义输出表格",
    "show_if": "options.export_series=='false'",
    "repeat": false,
    "fields": {
      "output.custom_text": {
        "type": "text",
        "标签": "输出表格名称",
        "show_if": "output.custom_options=='false' || (output.custom_options=='true' && output.compose_table_name=='false')",
        "minLength": 1,
        "val": "",
        "width": "100%",
        "breakLine": true
      },
      "output.custom_options": {
        "type": "checkbox",
        "标签": "输出选项",
        "val": "false",
        "isOn": "true",
        "isOff": "false",
        "breakLine": true
      },
      "output.compose_table_name": {
        "type": "checkbox",
        "标签": "编写表格名称",
        "show_if": "output.custom_options=='true'",
        "val": "false",
        "isOn": "true",
        "isOff": "false",
        "breakLine": true
      },
      "compose_table_name_frame": {
        "type": "fieldset",
        "标签": "编写输出表格名称",
        "show_if": "output.custom_options=='true' && output.compose_table_name=='true'",
        "repeat": false,
        "fields": {
          "output.prefix_rule": {
            "type": "select",
            "标签": "前缀",
            "show_if": "output.compose_table_name=='true'",
            "val": "empty",
            "options": {
              "empty": {
                "标签": "-"
              },
              "script_name": {
                "标签": "任务名称"
              },
              "date_string_type1": {
                "标签": "Date: dd_mm_yyyy"
              },
              "date_string_type2": {
                "标签": "Date: yyyy_mm_dd"
              },
              "time_string": {
                "标签": "时间：h_min_sec"
              },
              "datetime_string": {
                "标签": "Date_Time"
              },
              "custom": {
                "标签": "自定义"
              }
            }
          },
          "output.prefix_text": {
            "type": "text",
            "标签": "前缀文本",
            "show_if": "output.compose_table_name=='true' && output.prefix_rule=='custom'",
            "minLength": 1,
            "maxLength": 31,
            "val": ""
          },
          "output.core_delimiter": {
            "type": "text",
            "标签": "分隔符",
            "show_if": "output.compose_table_name=='true' && output.prefix_rule!='empty'",
            "val": "_",
            "width": "50px"
          },
          "output.core_rule": {
            "type": "select",
            "标签": "表格名称",
            "show_if": "output.compose_table_name=='true'",
            "val": "input_table_name",
            "options": {
              "input_table_name": {
                "标签": "输入表格名称"
              },
              "script_name": {
                "标签": "任务名称"
              },
              "custom": {
                "标签": "自定义"
              }
            }
          },
          "output.core_text": {
            "type": "text",
            "标签": "主表格名称",
            "show_if": "output.compose_table_name=='true' && output.core_rule=='custom'",
            "required": true,
            "minLength": 1,
            "maxLength": 31,
            "val": ""
          },
          "output.suffix_delimiter": {
            "type": "text",
            "标签": "分隔符",
            "show_if": "output.compose_table_name=='true'",
            "val": "_",
            "width": "50px"
          },
          "output.suffix_rule": {
            "type": "select",
            "标签": "后缀",
            "show_if": "output.compose_table_name=='true'",
            "val": "script_name",
            "options": {
              "empty": {
                "标签": "-"
              },
              "script_name": {
                "标签": "任务名称"
              },
              "date_string_type1": {
                "标签": "Date: dd_mm_yyyy"
              },
              "date_string_type2": {
                "标签": "Date: yyyy_mm_dd"
              },
              "time_string": {
                "标签": "时间：h_min_sec"
              },
              "datetime_string": {
                "标签": "Date_Time"
              },
              "custom": {
                "标签": "自定义"
              }
            }
          },
          "output.suffix_text": {
            "type": "text",
            "标签": "后缀文本",
            "show_if": "output.compose_table_name=='true' && output.suffix_rule=='custom'",
            "minLength": 1,
            "maxLength": 31,
            "val": "",
            "breakLine": true
          },
          "output.timezone": {
            "type": "select",
            "标签": "时区",
            "show_if": "output.compose_table_name=='true' && (output.prefix_rule=='date_string_type1' || output.prefix_rule=='date_string_type2' || output.prefix_rule=='time_string' || output.prefix_rule=='datetime_string' || output.suffix_rule=='date_string_type1' || output.suffix_rule=='date_string_type2' || output.suffix_rule=='time_string' || output.suffix_rule=='datetime_string')",
            "val": "欧洲/柏林",
            "required": true,
            "options": {
              "America/Adak": {
                "标签": "阿达克、甘比尔"
              },
              "Australia/Adelaide": {
                "标签": "Adelaide, Broken_Hill, Darwin"
              },
