{
  "options": {
    "type": "fieldset", 
    "label": "选择过滤器选项", 
    "repeat": false, 
    "fields": {
      "filter": {
        "type": "fieldset", 
        "label": "添加过滤条件", 
        "repeat": true, 
        "fields": {
          "initial_op": {
            "type": "select", 
            "label": "选择逻辑运算符", 
            "required": true, 
            "show_if" :  "$index == 0", 
            "options": {
              "if": {
                "label": "IF"
              }, 
              "if_not": {
                "label": "IF NOT"
              }
            }, 
            "val": "if"
          }, 
          "op": {
            "type": "select", 
            "label": "选择逻辑运算符", 
            "required": true, 
            "show_if" :  "$index > 0", 
            "options": {
              "and": {
                "label": "AND"
              }, 
              "or": {
                "label": "或"
              }, 
              "and_not": {
                "label": "而不是"
              }, 
              "or_not": {
                "label": "或不"
              }
            }, 
            "val": "and"
          }, 
          "column": {
            "type": "select", 
            "label": "选择列", 
            "table": "input_table", 
            "bind": "COLUMNS", 
            "required": true
          }, 
          "condition": {
            "type": "select", 
            "label": "选择操作员", 
            "required": true, 
            "options": {
              "含有": {
                "label": "含有"
              }, 
              "CONTAINED IN STRING": {
                "label": "包含在"
              }, 
              "开始于": {
                "label": "开始于"
              }, 
              "ENDS WITH": {
                "label": "ENDS WITH"
              }, 
              "=": {
                "label": "EQUAL"
              }, 
              ">": {
                "label": "大于"
              }, 
              ">=": {
                "label": "大于或等于"
              }, 
              "<": {
                "label": "小于"
              }, 
              "<=": {
                "label": "较小或相等"
              }, 
              "EMPTY": {
                "label": "是空的"
              }, 
              "BLANK": {
                "label": "是空白的"
              }, 
              "IS NULL": {
                "label": "IS NULL"
              }, 
              "REGEX MATCH": {
                "label": "REGEX MATCH"
              }, 
              "CONTAINED IN TABLE": {
                "label": "表中"
              }, 
              "在REGEX表中": {
                "label": "在REGEX表中"
              }, 
              "日期": {
                "label": "日期"
              }
            }, 
            "val": "含有"
          }, 
          "date_condition": {
            "type": "select", 
            "label": "选择日期运算符", 
            "required": true, 
            "show_if": "this.condition == 'DATE'", 
            "options": {
              "<": {
                "label": "以前"
              }, 
              "<=": {
                "label": "优先或平等"
              }, 
              "=": {
                "label": "EQUAL"
              }, 
              ">": {
                "label": "后"
              }, 
              ">=": {
                "label": "后或等于"
              }
            }, 
            "val": "="
          }, 
          "case_sensitive": {
            "type": "select", 
            "label": "选择区分大小写", 
            "required": true, 
            "show_if": "this.condition == 'CONTAINS' || this.condition == 'CONTAINED IN STRING' || this.condition == '=' || this.condition == 'STARTS WITH' || this.condition == 'ENDS WITH' || this.condition == 'REGEX MATCH' || this.condition == 'NOT REGEX MATCH' || this.condition == 'IN REGEX TABLE' || this.condition == 'NOT IN REGEX TABLE'", 
            "options": {
              "yes": {
                "label": "大小写敏感"
              }, 
              "no": {
                "label": "不区分大小写"
              }
            }, 
            "val": "yes"
          }, 
          "date_value": {
            "type": "date", 
            "label": "选择日期", 
            "show_if": "this.condition=='DATE'", 
            "val": "1/31/2020", 
            "required": true
          }, 
          "value": {
            "type": "text", 
            "label": "输入值", 
            "show_if": "this.condition != 'DATE' && this.condition != 'EMPTY' && this.condition != 'NOT EMPTY' && this.condition != 'BLANK' && this.condition != 'NOT BLANK' && this.condition != 'IS NULL' && this.condition != 'IS NOT NULL' && this.condition != 'CONTAINED IN TABLE' && this.condition != 'NOT CONTAINED IN TABLE' && this.condition != 'IN REGEX TABLE' && this.condition != 'NOT IN REGEX TABLE'", 
            "val": "", 
            "required": true
          }, 
          "compare_table": {
            "type": "select", 
            "label": "选择表格", 
            "bind": "TABLES", 
            "empty": "", 
            "show_if": "this.condition == 'CONTAINED IN TABLE' || this.condition == 'NOT CONTAINED IN TABLE' || this.condition == 'IN REGEX TABLE' || this.condition == 'NOT IN REGEX TABLE'", 
            "required": true
          }, 
          "compare_column": {
            "type": "select", 
            "label": "选择列", 
            "table": "this.compare_table", 
            "bind": "COLUMNS", 
            "show_if": "this.condition == 'CONTAINED IN TABLE' || this.condition == 'NOT CONTAINED IN TABLE' || this.condition == 'IN REGEX TABLE' || this.condition == 'NOT IN REGEX TABLE'", 
            "required": true
          }, 
          "delete": {
            "type": "button", 
            "label": "删除", 
            "function": "DELETE_ROW"
          }
        }
      }, 
      "apply_to_record": {
        "type": "checkbox", 
        "label": "申请记录", 
        "val": "true", 
        "isOn": "true", 
        "isOff": "false", 
        "style": "margin-top: 10px", 
        "breakLine": true
      }, 
      "apply_to_group_box": {
        "type": "fieldset", 
        "label": "申请加入集团", 
        "show_if": "apply_to_record=='false'", 
        "repeat": false, 
        "fields": {
          "apply_to_group_columns": {
            "type": "select", 
            "label": "选择组列", 
            "required": true, 
            "width": "100%", 
            "table": "input_table", 
            "multiple": true, 
            "bind": "COLUMNS", 
            "breakLine": true
          }
        }
      }
    }
  }
}
