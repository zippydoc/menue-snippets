            "label": "输入表格应用至组", 
            "show_if": "options.input_table.apply_to_record=='false'", 
            "repeat": false, 
            "fields": {
              "options.input_table.apply_to_group_columns": {
                "type": "select", 
                "label": "选择组列", 
                "required": true, 
                "width": "100%", 
                "table": "input_table", 
                "multiple": true, 
                "bind": "COLUMNS", 
                "breakLine": true
              }
            }
          }
        }
