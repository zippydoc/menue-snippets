        "repeat": false, 
        "fields": {
          "options.input_table.filter": {
            "type": "fieldset", 
            "label": "为输入表格添加筛选条件", 
            "repeat": true, 
            "fields": {
              "initial_op": {
                "type": "select", 
                "label": "选择逻辑运算符", 
                "required": true, 
                "show_if": "$index == 0", 
                "options": {
                  "if": {
                    "label": "IF"
                  }, 
                  "if_not": {
                    "label": "如果不是"
                  }
                }, 
                "val": "if"
              }, 
              "op": {
                "type": "select", 
                "label": "选择逻辑运算符", 
                "required": true, 
                "show_if": "$index > 0", 
                "options": {
                  "and": {
                    "label": "AND"
                  }, 
                  "or": {
                    "label": "OR"
                  }, 
                  "and_not": {
                    "label": "AND NOT"
                  }, 
                  "or_not": {
                    "label": "OR NOT"
                  }
                }, 
                "val": "and"
              }, 
              "column": {
                "type": "select", 
                "label": "选择列", 
                "table": "input_table", 
                "bind": "COLUMNS", 
                "required": true
              }, 
              "condition": {
                "type": "select", 
                "label": "选择运算符", 
                "required": true, 
                "options": {
                  "包含": {
                    "label": "包含"
                  }, 
                  "CONTAINED IN STRING": {
                    "label": "包含于"
                  }, 
                  "开始于": {
                    "label": "开始于"
                  }, 
                  "结尾是": {
                    "label": "结尾是"
                  }, 
                  "=": {
                    "label": "相等"
                  }, 
                  ">": {
                    "label": "大于"
                  }, 
                  ">=": {
                    "label": "大于或等于"
                  }, 
                  "<": {
                    "label": "小于"
                  }, 
                  "<=": {
                    "label": "小于或等于"
                  }, 
                  "EMPTY": {
                    "label": "为空"
                  }, 
                  "BLANK": {
                    "label": "为空白"
                  }, 
                  "为 NULL": {
                    "label": "为 NULL"
                  }, 
                  "正则表达式匹配": {
                    "label": "正则表达式匹配"
                  }, 
                  "CONTAINED IN TABLE": {
                    "label": "于表格"
                  }, 
                  "于正则表格": {
                    "label": "于正则表格"
                  }, 
                  "日期": {
                    "label": "日期"
                  }
                }, 
                "val": "包含"
              }, 
              "date_condition": {
                "type": "select", 
                "label": "选择日期运算符", 
                "required": true, 
                "show_if": "this.condition == 'DATE'", 
                "options": {
                  "<": {
                    "label": "PRIOR"
                  }, 
                  "<=": {
                    "label": "先于或等于"
                  }, 
                  "=": {
                    "label": "相等"
                  }, 
                  ">": {
                    "label": "之后"
                  }, 
                  ">=": {
                    "label": "之后或等于"
                  }
                }, 
                "val": "="
              }, 
              "case_sensitive": {
                "type": "select", 
                "label": "选择区分大小写", 
                "required": true, 
                "show_if": "this.condition == 'CONTAINS' || this.condition == 'CONTAINED IN STRING' || this.condition == '=' || this.condition == 'STARTS WITH' || this.condition == 'ENDS WITH' || this.condition == 'REGEX MATCH' || this.condition == 'NOT REGEX MATCH' || this.condition == 'IN REGEX TABLE' || this.condition == 'NOT IN REGEX TABLE'", 
                "options": {
                  "yes": {
                    "label": "区分大小写"
                  }, 
                  "no": {
                    "label": "不区分大小写"
                  }
                }, 
                "val": "yes"
              }, 
              "date_value": {
                "type": "date", 
                "label": "选择日期", 
                "show_if": "this.condition=='DATE'", 
                "val": "1/31/2020", 
                "required": true
              }, 
              "value": {
                "type": "text", 
                "label": "输入值", 
                "show_if": "this.condition != 'DATE' && this.condition != 'EMPTY' && this.condition != 'NOT EMPTY' && this.condition != 'BLANK' && this.condition != 'NOT BLANK' && this.condition != 'IS NULL' && this.condition != 'IS NOT NULL' && this.condition != 'CONTAINED IN TABLE' && this.condition != 'NOT CONTAINED IN TABLE' && this.condition != 'IN REGEX TABLE' && this.condition != 'NOT IN REGEX TABLE'", 
                "val": "", 
                "required": true
              }, 
              "compare_table": {
                "type": "select", 
                "label": "选择表格", 
                "bind": "TABLES", 
                "empty": "", 
                "show_if": "this.condition == 'CONTAINED IN TABLE' || this.condition == 'NOT CONTAINED IN TABLE' || this.condition == 'IN REGEX TABLE' || this.condition == 'NOT IN REGEX TABLE'", 
                "required": true
              }, 
              "compare_column": {
                "type": "select", 
                "label": "选择列", 
                "table": "this.compare_table", 
                "bind": "COLUMNS", 
                "show_if": "this.condition == 'CONTAINED IN TABLE' || this.condition == 'NOT CONTAINED IN TABLE' || this.condition == 'IN REGEX TABLE' || this.condition == 'NOT IN REGEX TABLE'", 
                "required": true
              }, 
              "delete": {
                "type": "button", 
                "label": "删除", 
                "function": "DELETE_ROW"
              }
            }
          }, 
          "options.input_table.apply_to_record": {
            "type": "checkbox", 
            "label": "为输入表格筛选应用至记录", 
            "val": "true", 
            "isOn": "true", 
            "isOff": "false", 
            "style": "margin-top: 10px", 
            "breakLine": true
