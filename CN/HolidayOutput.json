{
  "output": {
    "type": "fieldset",
    "标签": "定义输出表格",
    "repeat": false,
    "fields": {
      "output.custom_text": {
        "type": "text",
        "标签": "输出表格名称",
        "show_if": "output.custom_options=='false' || (output.custom_options=='true' && output.compose_table_name=='false')",
        "minLength": 1,
        "val": "",
        "width": "100%",
        "breakLine": true
      },
      "output.custom_options": {
        "type": "checkbox",
        "标签": "输出选项",
        "val": "false",
        "isOn": "true",
        "isOff": "false",
        "breakLine": true
      },
      "output.select_output_columns": {
        "type": "checkbox",
        "标签": "选择输出列",
        "show_if": "options.is_generate_holiday_table=='add_column' && output.custom_options=='true'",
        "val": "false",
        "isOn": "true",
        "isOff": "false"
      },
      "output_columns": {
        "type": "fieldset",
        "标签": "Select OutputColumns",
        "show_if": "options.is_generate_holiday_table=='add_column' && output.custom_options=='true' && output.select_output_columns=='true'",
        "repeat": true,
        "fields": {
          "field_name": {
            "type": "select",
            "标签": "选择列",
            "table": "input_table",
            "bind": "COLUMNS",
            "required": true
          },
          "order": {
            "type": "select",
            "标签": "顺序",
            "options": {
              "-": {
                "标签": "-"
              },
              "asc": {
                "标签": "ASC"
              },
              "desc": {
                "标签": "DESC"
              }
            },
            "val": "-"
          },
          "is_rename": {
            "type": "checkbox",
            "标签": "重命名",
            "val": "false",
            "isOn": "true",
            "isOff": "false"
          },
          "rename": {
            "show_if": "this.is_rename == 'true'",
            "type": "text",
            "required": true,
            "minLength": 1,
            "maxLength": 255,
            "标签": "新列名",
            "style": "margin-left: 10px"
          },
          "format__category": {
            "type": "select",
            "标签": "格式",
            "required": true,
            "options": {
              "standard": {
                "标签": "标准"
              },
              "number": {
                "标签": "数字"
              },
              "percentage": {
                "标签": "百分比"
              },
              "text": {
                "标签": "文本"
              },
              "date": {
                "标签": "日期"
              },
              "currency": {
                "标签": "货币"
              }
            },
            "val": "standard"
          },
          "format__rounding": {
            "type": "select",
            "标签": "取整",
            "show_if": "this.format__category=='number' || this.format__category=='percentage' || this.format__category=='currency'",
            "required": true,
            "options": {
              "no_rounding": {
                "标签": "不取整"
              },
              "0": {
                "标签": "0 位数"
              },
              "1": {
                "标签": "1 位数"
              },
              "2": {
                "标签": "2 位数"
              },
              "3": {
                "标签": "3 位数"
              },
              "4": {
                "标签": "4 位数"
              },
              "5": {
                "标签": "5 位数"
              },
              "6": {
                "标签": "6 位数"
              },
              "7": {
                "标签": "7 位数"
              },
              "8": {
                "标签": "8 位数"
              },
              "9": {
                "标签": "9 位数"
              }
            },
            "val": "2"
          },
          "format__use_1000_separator": {
            "type": "checkbox",
            "标签": "Use 1000 Separator(.)",
            "show_if": "this.format__category=='number' || this.format__category=='percentage' || this.format__category=='currency'",
            "val": "true",
            "isOn": "true",
            "isOff": "false"
          },
          "format__currency_symbol": {
            "type": "select",
            "标签": "符号",
            "show_if": "this.format__category=='currency'",
            "required": true,
            "options": {
              "dollar": {
                "标签": "$"
              },
              "euro": {
                "标签": "€"
              },
              "pound": {
                "标签": "£"
              }
            },
            "val": "dollar"
          },
          "format__date_format": {
            "type": "select",
            "标签": "类型",
            "show_if": "this.format__category=='date'",
            "required": true,
            "options": {
              "mm/dd/yyyy": {
                "标签": "3/14/2012"
              },
              "D, M dd, Y": {
                "标签": "2012年3月14日，星期三"
              },
              "yyyy-mm-dd": {
                "标签": "2012-03-14"
              }
            },
            "val": "mm/dd/yyyy",
            "breakLine": true
          },
          "delete": {
            "type": "button",
            "标签": "删除",
            "function": "DELETE_ROW"
          }
        },
        "breakLine": true,
        "style": "margin-bottom: 20px"
      },
      "output.compose_table_name": {
        "type": "checkbox",
        "标签": "编写表格名称",
        "show_if": "output.custom_options=='true'",
        "val": "false",
        "isOn": "true",
        "isOff": "false",
        "breakLine": true
      },
      "compose_table_name_frame": {
        "type": "fieldset",
        "标签": "编写输出表格名称",
        "show_if": "output.custom_options=='true' && output.compose_table_name=='true'",
        "repeat": false,
        "fields": {
          "output.prefix_rule": {
            "type": "select",
            "标签": "前缀",
            "show_if": "output.compose_table_name=='true'",
            "val": "empty",
            "options": {
              "empty": {
                "标签": "-"
              },
              "script_name": {
                "标签": "任务名称"
              },
              "date_string_type1": {
                "标签": "Date: dd_mm_yyyy"
              },
              "date_string_type2": {
                "标签": "Date: yyyy_mm_dd"
              },
              "time_string": {
                "标签": "时间：h_min_sec"
              },
              "datetime_string": {
                "标签": "Date_Time"
              },
              "custom": {
                "标签": "自定义"
              }
            }
          },
          "output.prefix_text": {
            "type": "text",
            "标签": "前缀文本",
            "show_if": "output.compose_table_name=='true' && output.prefix_rule=='custom'",
            "minLength": 1,
            "maxLength": 31,
            "val": ""
          },
          "output.core_delimiter": {
            "type": "text",
            "标签": "分隔符",
            "show_if": "output.compose_table_name=='true' && output.prefix_rule!='empty'",
            "val": "_",
            "width": "50px"
          },
          "output.generate_core_rule": {
            "type": "select",
            "标签": "表格名称",
            "show_if": "output.compose_table_name=='true' && options.is_generate_holiday_table=='generate_table'",
            "val": "script_name",
            "options": {
              "script_name": {
                "标签": "任务名称"
              },
              "custom": {
                "标签": "自定义"
              }
            }
          },
          "output.core_rule": {
            "type": "select",
            "标签": "表格名称",
            "show_if": "output.compose_table_name=='true' && options.is_generate_holiday_table=='add_column'",
            "val": "input_table_name",
            "options": {
              "input_table_name": {
                "标签": "输入表格名称"
              },
              "script_name": {
                "标签": "任务名称"
              },
              "custom": {
                "标签": "自定义"
              }
            }
          },
          "output.core_text": {
            "type": "text",
            "标签": "主表格名称",
            "show_if": "output.compose_table_name=='true' && output.core_rule=='custom'",
            "required": true,
            "minLength": 1,
            "maxLength": 31,
            "val": ""
          },
          "output.suffix_delimiter": {
            "type": "text",
            "标签": "分隔符",
            "show_if": "output.compose_table_name=='true'",
            "val": "_",
            "width": "50px"
          },
          "output.suffix_rule": {
            "type": "select",
            "标签": "后缀",
            "show_if": "output.compose_table_name=='true'",
            "val": "script_name",
            "options": {
              "empty": {
                "标签": "-"
              },
              "script_name": {
                "标签": "任务名称"
              },
              "date_string_type1": {
                "标签": "Date: dd_mm_yyyy"
              },
              "date_string_type2": {
                "标签": "Date: yyyy_mm_dd"
              },
              "time_string": {
                "标签": "时间：h_min_sec"
              },
              "datetime_string": {
                "标签": "Date_Time"
              },
              "custom": {
                "标签": "自定义"
              }
            }
          },
          "output.suffix_text": {
            "type": "text",
            "标签": "后缀文本",
            "show_if": "output.compose_table_name=='true' && output.suffix_rule=='custom'",
            "minLength": 1,
            "maxLength": 31,
            "val": "",
            "breakLine": true
          },
          "output.timezone": {
            "type": "select",
            "标签": "时区",
            "show_if": "output.compose_table_name=='true' && (output.prefix_rule=='date_string_type1' || output.prefix_rule=='date_string_type2' || output.prefix_rule=='time_string' || output.prefix_rule=='datetime_string' || output.suffix_rule=='date_string_type1' || output.suffix_rule=='date_string_type2' || output.suffix_rule=='time_string' || output.suffix_rule=='datetime_string')",
            "val": "欧洲/柏林",
            "required": true,
            "options": {
              "America/Adak": {
                "标签": "阿达克、甘比尔"
              },
              "Australia/Adelaide": {
                "标签": "Adelaide, Broken_Hill, Darwin"
              },
              "America/Anchorage": {
                "标签": "安克雷奇、朱诺、西特卡"
              },
              "Pacific/Apia": {
                "标签": "阿皮亚、阿绍、穆利法努阿、阿费加、卢卢莫加"
              },
              "Pacific/Enderbury": {
                "标签": "阿皮亚、斐济、汤加塔普"
              },
              "Antarctica/McMurdo": {
                "标签": "奥克兰、凯西、戴维斯"
              },
              "Pacific/Auckland": {
                "标签": "奥克兰、惠灵顿、基督城"
              },
              "Asia/Bangkok": {
                "标签": "曼谷、雅加达、新西伯利亚"
              },
              "欧洲/柏林": {
                "标签": "柏林、巴黎、阿姆斯特丹"
              },
              "Australia/Perth": {
                "标签": "文莱、新加坡、珀斯"
              },
              "Atlantic/Cape_Verde": {
                "标签": "Cape_Verde,Praia, Mindelo, Santa Maria"
              },
              "America/Belize": {
                "标签": "Costa_Rica, El_Salvador, Denver"
              },
              "Antarctica/Vostok": {
                "标签": "达卡、比什凯克、鄂木斯克"
              },
              "Europe/Astrakhan": {
                "标签": "迪拜、巴库、第比利斯"
              },
              "Australia/Eucla": {
                "标签": "欧克拉"
              },
              "Pacific/Honolulu": {
                "标签": "火奴鲁鲁、拉罗汤加、大溪地"
              },
              "Asia/Kabul": {
                "标签": "喀布尔、坎大哈、马扎尔-谢里夫、赫拉特、贾拉巴德
              },
              "Asia/Aqtau": {
                "标签": "卡拉奇、塔什干、马尔代夫"
              },
              "Asia/Kathmandu": {
                "标签": "Kathmandu,Pokhara, Pātan, Biratnagar"
              },
              "Pacific/Kiritimati": {
                "标签": "Kiritimati"
              },
              "Asia/Colombo": {
                "标签": "Kolkata, Colombo,Jaffna, Negombo"
              },
              "Europe/Guernsey": {
                "标签": "London. Dublin, Lisbon"
              },
              "Australia/Lord_Howe": {
                "标签": "Lord_Howe"
              },
              "America/Creston": {
                "标签": "Los_Angeles, Vancouver, Hermosillo"
              },
              "Pacific/Marquesas": {
                "标签": "Marquesas,Taiohae"
              },
              "America/Cayman": {
                "标签": "Mexico_City, Panama, Chicago"
              },
              "Pacific/Niue": {
                "标签": "Midway, Niue, Pago_Pago"
              },
              "Europe/Mariehamn": {
                "标签": "莫斯科、伊斯坦布尔、卡塔尔"
              },
              "Asia/Kolkata": {
                "标签": "孟买、德里、班加罗尔、加尔各答、钦奈"
              },
              "America/Miquelon": {
                "标签": "诺罗尼亚、努克、米克朗"
              },
              "America/Danmarkshavn": {
                "标签": "雷克雅未克、达喀尔、卡萨布兰卡"
              },
              "Pacific/Bougainville": {
                "标签": "萨哈林岛、诺福克、布干维尔"
              },
              "America/Araguaina": {
                "标签": "Sao_Paulo, Buenos_Aires, Montevideo"
              },
              "America/St_Johns": {
                "标签": "St_Johns,Mount Pearl, Corner Brook"
              },
              "Asia/Tehran": {
                "标签": "德黑兰、马什哈德、伊斯法罕、卡拉吉、大不里士
              },
              "Asia/Tokyo": {
                "标签": "东京、首尔、查亚普拉"
              },
              "America/New_York": {
                "标签": "Toronto, Santiago, New_York"
              },
              "Australia/Sydney": {
                "标签": "符拉迪沃斯托克、悉尼、布里斯班"
              },
              "Asia/Yangon": {
                "标签": "Yangon, Cocos,Mandalay, Nay Pyi Taw, Mawlamyine"
              },
              "Indian/Cocos": {
                "标签": "Yangon, Cocos,West Island"
              }
            },
            "breakLine": true
          }
        }
      }
    }
  }
}