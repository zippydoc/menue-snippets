        "type": "select", 
        "label": "选择左表格", 
        "bind": "TABLES"
      }, 
      "left_table.remove_input_table_after_processing": {
        "type": "hidden", 
        "val": "false"
      }, 
      "left_table.is_word": {
        "type": "hidden", 
        "val": "false", 
        "breakLine": true
      }, 
      "right_table.table_id": {
        "type": "select", 
        "label": "选择右表格", 
        "bind": "TABLES"
      }, 
      "right_table.remove_input_table_after_processing": {
        "type": "hidden", 
        "val": "false"
      }, 
      "right_table.is_word": {
        "type": "hidden", 
        "val": "false"
      }
    }
  }
}