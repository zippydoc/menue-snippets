{
  "output.output_table_types": {
    "type": "select", 
    "label": "输出表格类型", 
    "required": true, 
    "options": {
      "inner": {
        "label": "内连接表格"
      }, 
      "left": {
        "label": "左连接表格"
      }, 
      "right": {
        "label": "右连接表格"
      }
    }, 
    "multiple": true, 
    "val": ["inner"]
  }, 
  "output.standard_ending": {
    "type": "checkbox", 
    "label": "标准后缀", 
    "val": "true", 
    "isOn": "true", 
    "isOff": "false", 
    "breakLine": true
  }, 
  "output.inner_ending": {
    "show_if": "output.standard_ending == 'false'", 
    "type": "text", 
    "label": "内连接后缀", 
    "val": "_j", 
    "style": "margin-left: 10px"
  }, 
  "output.left_ending": {
    "show_if": "output.standard_ending == 'false'", 
    "type": "text", 
    "label": "左连接后缀", 
    "val": "_l", 
    "style": "margin-left: 10px"
  }, 
  "output.right_ending": {
    "show_if": "output.standard_ending == 'false'", 
    "type": "text", 
    "label": "右连接后缀", 
    "val": "_r", 
    "style": "margin-left: 10px", 
    "breakLine": true
  }
}