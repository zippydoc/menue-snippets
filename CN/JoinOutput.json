{
  "output.output_table_types": {
    "type": "select",
    "标签": "输出表格类型",
    "required": true,
    "options": {
      "inner": {
        "标签": "内连接表格"
      },
      "left": {
        "标签": "左连接表格"
      },
      "right": {
        "标签": "右连接表格"
      }
    },
    "multiple": true,
    "val": ["inner"]
  },
  "output.standard_ending": {
    "type": "checkbox",
    "标签": "标准后缀",
    "val": "true",
    "isOn": "true",
    "isOff": "false",
    "breakLine": true
  },
  "output.inner_ending": {
    "show_if": "output.standard_ending == 'false'",
    "type": "text",
    "标签": "内连接后缀",
    "val": "_j",
    "style": "margin-left: 10px"
  },
  "output.left_ending": {
    "show_if": "output.standard_ending == 'false'",
    "type": "text",
    "标签": "左连接后缀",
    "val": "_l",
    "style": "margin-left: 10px"
  },
  "output.right_ending": {
    "show_if": "output.standard_ending == 'false'",
    "type": "text",
    "标签": "右连接后缀",
    "val": "_r",
    "style": "margin-left: 10px",
    "breakLine": true
  }
}