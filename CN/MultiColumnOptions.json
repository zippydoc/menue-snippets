{
  "options_box": {
    "type": "fieldset",
    "标签": "选项",
    "repeat": false,
    "fields": {
      "options.task": {
        "type": "hidden",
        "val": "multi_column"
      },
      "options.add_columns": {
        "type": "fieldset",
        "标签": "添加新列",
        "repeat": true,
        "fields": {
          "new_column_name": {
            "type": "text",
            "标签": "新列名",
            "val": "",
            "breakLine": true
          },
          "multi_column_functions__function": {
            "type": "select",
            "标签": "函数",
            "required": true,
            "options": {
              "sum": {
                "标签": "总和"
              },
              "average": {
                "标签": "平均"
              },
              "median": {
                "标签": "中位数"
              },
              "max": {
                "标签": "最大"
              },
              "min": {
                "标签": "最小"
              },
              "standard_deviation": {
                "标签": "标准差"
              },
              "product": {
                "标签": "产品"
              },
              "quota": {
                "标签": "比例"
              },
              "absolute_growth": {
                "标签": "绝对增长"
              },
              "relative_growth": {
                "标签": "相对增长"
              },
              "compound_growth": {
                "标签": "复合增长"
              },
              "static_value": {
                "标签": "静态值"
              },
              "numeric_formula": {
                "标签": "数值公式"
              },
              "text_formula": {
                "标签": "文本公式"
              }
            },
            "val": "sum"
          },
          "multi_column_functions__sel_columns": {
            "type": "select",
            "标签": "选择列",
            "show_if": "this.multi_column_functions__function=='sum' || this.multi_column_functions__function=='average' || this.multi_column_functions__function=='median' || this.multi_column_functions__function=='max' || this.multi_column_functions__function=='min' || this.multi_column_functions__function=='standard_deviation' || this.multi_column_functions__function=='product'",
            "table": "input_table",
            "bind": "COLUMNS",
            "multiple": true,
            "required": true
          },
          "multi_column_functions__column_a": {
            "type": "select",
            "标签": "选择列 A",
            "show_if": "this.multi_column_functions__function=='quota' || this.multi_column_functions__function=='absolute_growth' || this.multi_column_functions__function=='relative_growth' || this.multi_column_functions__function=='compound_growth'",
            "table": "input_table",
            "bind": "COLUMNS",
            "required": true
          },
          "multi_column_functions__column_b": {
            "type": "select",
            "标签": "选择列 B",
            "show_if": "this.multi_column_functions__function=='quota' || this.multi_column_functions__function=='absolute_growth' || this.multi_column_functions__function=='relative_growth' || this.multi_column_functions__function=='compound_growth'",
            "table": "input_table",
            "bind": "COLUMNS",
            "required": true
          },
          "multi_column_functions__n_value": {
            "type": "text",
            "标签": "N（正整数）",
            "show_if": "this.multi_column_functions__function=='compound_growth'",
            "val": "1",
            "required": true
          },
          "static_value": {
            "type": "text",
            "标签": "输入值",
            "show_if": "this.multi_column_functions__function=='static_value'",
            "val": "",
            "width": "100%"
          },
          "numeric_formula": {
            "type": "text",
            "标签": "输入公式，例如：[Column1] + 2 * ([Row-1:Column3] - [Column1])",
            "show_if": "this.multi_column_functions__function=='numeric_formula'",
            "val": "",
            "width": "100%"
          },
          "text_formula": {
            "type": "text",
            "标签": "输入公式，例如：[名]+''+[姓]+''+[行-1:列3]",
            "show_if": "this.multi_column_functions__function=='text_formula'",
            "val": "",
            "width": "100%"
          },
          "use_multi_row": {
            "type": "checkbox",
            "标签": "多行选项",
            "show_if": "this.multi_column_functions__function=='numeric_formula' || this.multi_column_functions__function=='text_formula'",
            "val": "false",
            "isOn": "true",
            "isOff": "false"
          },
          "if_row_not_exist": {
            "type": "select",
            "标签": "如果引用行不存在",
            "show_if": "this.use_multi_row=='true' && (this.multi_column_functions__function=='numeric_formula' || this.multi_column_functions__function=='text_formula')",
            "required": true,
            "options": {
              "empty": {
                "标签": "空或 0"
              },
              "closest": {
                "标签": "最接近行值"
              }
            },
            "val": "empty",
            "breakLine": true
          },
          "format__category": {
            "type": "select",
            "标签": "格式",
            "required": true,
            "options": {
              "standard": {
                "标签": "标准"
              },
              "number": {
                "标签": "数字"
              },
              "percentage": {
                "标签": "百分比"
              },
              "text": {
                "标签": "文本"
              },
              "date": {
                "标签": "日期"
              },
              "currency": {
                "标签": "货币"
              }
            },
            "val": "standard"
          },
          "format__rounding": {
            "type": "select",
            "标签": "取整",
            "show_if": "this.format__category=='number' || this.format__category=='percentage' || this.format__category=='currency'",
            "required": true,
            "options": {
              "no_rounding": {
                "标签": "不取整"
              },
              "0": {
                "标签": "0 位数"
              },
              "1": {
                "标签": "1 位数"
              },
              "2": {
                "标签": "2 位数"
              },
              "3": {
                "标签": "3 位数"
              },
              "4": {
                "标签": "4 位数"
              },
              "5": {
                "标签": "5 位数"
              },
              "6": {
                "标签": "6 位数"
              },
              "7": {
                "标签": "7 位数"
              },
              "8": {
                "标签": "8 位数"
              },
              "9": {
                "标签": "9 位数"
              }
            },
            "val": "2"
          },
          "format__use_1000_separator": {
            "type": "checkbox",
            "标签": "Use 1000 Separator(.)",
            "show_if": "this.format__category=='number' || this.format__category=='percentage' || this.format__category=='currency'",
            "val": "true",
            "isOn": "true",
            "isOff": "false"
          },
          "format__currency_symbol": {
            "type": "select",
            "标签": "符号",
            "show_if": "this.format__category=='currency'",
            "required": true,
            "options": {
              "dollar": {
                "标签": "$"
              },
              "euro": {
                "标签": "€"
              },
              "pound": {
                "标签": "£"
              }
            },
            "val": "dollar"
          },
          "format__date_format": {
            "type": "select",
            "标签": "类型",
            "show_if": "this.format__category=='date'",
            "required": true,
            "options": {
              "mm/dd/yyyy": {
                "标签": "3/14/2012"
              },
              "D, M dd, Y": {
                "标签": "2012年3月14日，星期三"
              },
              "yyyy-mm-dd": {
                "标签": "2012-03-14"
              }
            },
            "val": "mm/dd/yyyy",
            "breakLine": true
          },
          "group_counter__inc_dec_curly_bracket": {
            "type": "checkbox",
            "标签": "{} 增加/减少",
            "show_if": "this.task_type=='group_counter'",
            "val": "false",
            "isOn": "true",
            "isOff": "false",
            "breakLine": true
          },
          "group_counter__inc_dec_square_bracket": {
            "type": "checkbox",
            "标签": "[] 增加/减少",
            "show_if": "this.task_type=='group_counter'",
            "val": "false",
            "isOn": "true",
            "isOff": "false",
            "breakLine": true
          },
          "group_counter__inc_dec_parenthesis": {
            "type": "checkbox",
            "标签": "() 增加/减少",
            "show_if": "this.task_type=='group_counter'",
            "val": "false",
            "isOn": "true",
            "isOff": "false",
            "breakLine": true
          },
          "group_counter__custom": {
            "type": "checkbox",
            "标签": "自定义群组计数器",
            "show_if": "this.task_type=='group_counter'",
            "val": "false",
            "isOn": "true",
            "isOff": "false",
            "breakLine": true
          },
          "group_counter__column": {
            "type": "select",
            "标签": "列",
            "show_if": "this.task_type=='group_counter' && this.group_counter__custom=='true'",
            "table": "input_table",
            "bind": "COLUMNS",
            "required": true
          },
          "group_counter__type": {
            "type": "select",
            "标签": "当",
            "show_if": "this.task_type=='group_counter' && this.group_counter__custom=='true'",
            "required": true,
            "options": {
              "starts_with": {
                "标签": "开始于"
              },
              "ends_with": {
                "标签": "结尾是"
              },
              "starts_with_regex": {
                "标签": "开始于正则表达式"
              },
              "ends_with_regex": {
                "标签": "结尾是正则表达式"
              }
            },
            "val": "starts_with"
          },
          "group_counter__value": {
            "type": "text",
            "标签": "输入值",
            "show_if": "this.task_type=='group_counter' && this.group_counter__custom=='true'",
            "val": ".",
            "breakLine": true
          },
          "group_counter__label": {
            "type": "text",
            "标签": "输入标签",
            "show_if": "this.task_type=='group_counter' && this.group_counter__custom=='true'",
            "val": "sentence",
            "breakLine": true
          },
          "group_counter__if_end_of_string_start_labeling_with_next_record": {
            "type": "checkbox",
            "标签": "如果字符串结尾以后一记录为标签",
            "show_if": "this.task_type=='group_counter' && this.group_counter__custom=='true' && (this.group_counter__type=='starts_with' || this.group_counter__type=='starts_with_regex')",
            "val": "true",
            "isOn": "true",
            "isOff": "false"
          },
          "group_counter__if_begin_of_string_start_labeling_with_previous_record": {
            "type": "checkbox",
            "标签": "如果字符串开始以前一记录为标签",
            "show_if": "this.task_type=='group_counter' && this.group_counter__custom=='true' && (this.group_counter__type=='ends_with' || this.group_counter__type=='ends_with_regex')",
            "val": "true",
            "isOn": "true",
            "isOff": "false"
          },
          "group_counter__always_include": {
            "type": "checkbox",
            "标签": "总是包含",
            "show_if": "this.task_type=='group_counter' && this.group_counter__custom=='true'",
            "val": "true",
            "isOn": "true",
            "isOff": "false"
          },
          "apply_to_group__enabled": {
            "type": "checkbox",
            "标签": "应用至组",
            "show_if": "this.multi_column_functions__function!='static_value'",
            "val": "false",
            "isOn": "true",
            "isOff": "false"
          },
          "apply_to_group__group_columns": {
            "type": "select",
            "标签": "组列",
            "show_if": "this.apply_to_group__enabled == 'true' && this.multi_column_functions__function!='static_value'",
            "table": "input_table",
            "bind": "COLUMNS",
            "multiple": true,
            "required": true
          },
          "delete": {
            "type": "button",
            "标签": "删除",
            "function": "DELETE_ROW"
          }
        }
      },
      "options.use_filter": {
        "type": "checkbox",
        "标签": "使用筛选",
        "val": "false",
        "isOn": "true",
        "isOff": "false",
        "style": "margin-top: 10px",
        "breakLine": true
      },
      "filter_box": {
        "type": "fieldset",
        "标签": "筛选选项",
        "show_if": "options.use_filter=='true'",
        "repeat": false,
        "fields": {
          "options.filter": {
            "type": "fieldset",
            "标签": "添加筛选条件",
            "repeat": true,
            "fields": {
              "initial_op": {
                "type": "select",
                "标签": "选择逻辑运算符",
                "required": true,
                "show_if" :  "$index == 0",
                "options": {
                  "if": {
                    "标签": "IF"
                  },
                  "if_not": {
                    "标签": "如果不是"
                  }
                },
                "val": "if"
              },
              "op": {
                "type": "select",
                "标签": "选择逻辑运算符",
                "required": true,
                "show_if" :  "$index > 0",
                "options": {
                  "and": {
                    "标签": "AND"
                  },
                  "or": {
                    "标签": "OR"
                  },
                  "and_not": {
                    "标签": "AND NOT"
                  },
                  "or_not": {
                    "标签": "OR NOT"
                  }
                },
                "val": "and"
              },
              "column": {
                "type": "select",
                "标签": "选择列",
                "table": "input_table",
                "bind": "COLUMNS",
                "required": true
              },
              "condition": {
                "type": "select",
                "标签": "选择运算符",
                "required": true,
                "options": {
                  "包含": {
                    "标签": "包含"
                  },
                  "CONTAINED IN STRING": {
                    "标签": "包含于"
                  },
                  "开始于": {
                    "标签": "开始于"
                  },
                  "结尾是": {
                    "标签": "结尾是"
                  },
                  "=": {
                    "标签": "相等"
                  },
                  ">": {
                    "标签": "大于"
                  },
                  ">=": {
                    "标签": "大于或等于"
                  },
                  "<": {
                    "标签": "小于"
                  },
                  "<=": {
                    "标签": "小于或等于"
                  },
                  "EMPTY": {
                    "标签": "为空"
                  },
                  "BLANK": {
                    "标签": "为空白"
                  },
                  "为 NULL": {
                    "标签": "为 NULL"
                  },
                  "正则表达式匹配": {
                    "标签": "正则表达式匹配"
                  },
                  "CONTAINED IN TABLE": {
                    "标签": "于表格"
                  },
                  "于正则表格": {
                    "标签": "于正则表格"
                  },
                  "日期": {
                    "标签": "日期"
                  }
                },
                "val": "包含"
              },
              "date_condition": {
                "type": "select",
                "标签": "选择日期运算符",
                "required": true,
                "show_if": "this.condition == 'DATE'",
                "options": {
                  "<": {
                    "标签": "PRIOR"
                  },
                  "<=": {
                    "标签": "先于或等于"
                  },
                  "=": {
                    "标签": "相等"
                  },
                  ">": {
                    "标签": "之后"
                  },
                  ">=": {
                    "标签": "之后或等于"
                  }
                },
                "val": "="
              },
              "case_sensitive": {
                "type": "select",
                "标签": "选择区分大小写",
                "required": true,
                "show_if": "this.condition == 'CONTAINS' || this.condition == 'CONTAINED IN STRING' || this.condition == '=' || this.condition == 'STARTS WITH' || this.condition == 'ENDS WITH' || this.condition == 'REGEX MATCH' || this.condition == 'NOT REGEX MATCH' || this.condition == 'IN REGEX TABLE' || this.condition == 'NOT IN REGEX TABLE'",
                "options": {
                  "yes": {
                    "标签": "区分大小写"
                  },
                  "no": {
                    "标签": "不区分大小写"
                  }
                },
                "val": "yes"
              },
              "date_value": {
                "type": "date",
                "标签": "选择日期",
                "show_if": "this.condition=='DATE'",
                "val": "",
                "required": true
              },
              "value": {
                "type": "text",
                "标签": "输入值",
                "show_if": "this.condition != 'DATE' && this.condition != 'EMPTY' && this.condition != 'NOT EMPTY' && this.condition != 'BLANK' && this.condition != 'NOT BLANK' && this.condition != 'IS NULL' && this.condition != 'IS NOT NULL' && this.condition != 'CONTAINED IN TABLE' && this.condition != 'NOT CONTAINED IN TABLE' && this.condition != 'IN REGEX TABLE' && this.condition != 'NOT IN REGEX TABLE'",
                "val": "",
                "required": true
              },
              "compare_table": {
                "type": "select",
                "标签": "选择表格",
                "bind": "TABLES",
                "empty": "",
                "show_if": "this.condition == 'CONTAINED IN TABLE' || this.condition == 'NOT CONTAINED IN TABLE' || this.condition == 'IN REGEX TABLE' || this.condition == 'NOT IN REGEX TABLE'",
                "required": true
              },
              "compare_column": {
                "type": "select",
                "标签": "选择列",
                "table": "this.compare_table",
                "bind": "COLUMNS",
                "show_if": "this.condition == 'CONTAINED IN TABLE' || this.condition == 'NOT CONTAINED IN TABLE' || this.condition == 'IN REGEX TABLE' || this.condition == 'NOT IN REGEX TABLE'",
                "required": true
              },
              "delete": {
                "type": "button",
                "标签": "删除",
                "function": "DELETE_ROW"
              }
            }
          },
          "options.apply_to_record": {
            "type": "checkbox",
            "标签": "应用至记录",
            "val": "true",
            "isOn": "true",
            "isOff": "false",
            "style": "margin-top: 10px",
            "breakLine": true
          },
          "apply_to_group_box": {
            "type": "fieldset",
            "标签": "应用至组",
            "show_if": "options.apply_to_record=='false'",
            "repeat": false,
            "fields": {
              "options.apply_to_group_columns": {
                "type": "select",
                "标签": "选择组列",
                "required": true,
                "width": "100%",
                "table": "input_table",
                "multiple": true,
                "bind": "COLUMNS",
                "breakLine": true
              }
            }
          }
        }
      }
    }
  }
}