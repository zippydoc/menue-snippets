{
  "actions": {
    "type": "fieldset",
    "label": "添加操作",
    "repeat": true,
    "fields": {
      "column": {
        "type": "select",
        "label": "列",
        "table": "input_table",
		"assign_selected_value_to":"this.output_column_name",
        "bind": "COLUMNS"
      },
      "action": {
        "type": "select",
        "label": "操作",
        "options": {
          "group_by": {
            "label": "分组方式："
          },
          "count": {
            "label": "计数"
          },
          "sum": {
            "label": "总和"
          },
          "first": {
            "label": "首行"
          },
          "last": {
            "label": "后"
          },
          "shortest": {
            "label": "最短"
          },
          "longest": {
            "label": "最长"
          },
          "concatenate": {
            "label": "连接"
          },
          "mathematics": {
            "label": "数学"
          }
        }
      },
      "count_action_category": {
        "type": "select",
        "label": "操作类型",
        "show_if": "this.action=='count'",
        "options": {
          "all": {
            "label": "所有"
          },
          "distinct": {
            "label": "独特"
          },
          "empty_or_zero": {
            "label": "空或 0"
          },
          "non_empty_or_zero": {
            "label": "非空或 0"
          }
        },
        "val": "all"
      },
      "action_type1": {
        "type": "select",
        "label": "操作子类型",
        "show_if": "this.action=='count' && (this.count_action_category=='all' || this.count_action_category=='distinct')",
        "options": {
          "-": {
            "label": "-"
          },
          "non_zero": {
            "label": "非 0"
          },
          "non_empty": {
            "label": "非空"
          },
          "non_null": {
            "label": "非 Null"
          },
          "non_blank": {
            "label": "非空白"
          }
        },
        "val": "-"
      },
      "action_type2": {
        "type": "select",
        "label": "操作子类型",
        "show_if": "this.action=='count' && this.count_action_category=='empty_or_zero'",
        "options": {
          "zero": {
            "label": "0"
          },
          "empty": {
            "label": "空（Null & Blank）"
          },
          "null": {
            "label": "Null"
          },
          "blank": {
            "label": "空白"
          }
        },
        "val": "zero"
      },
      "action_type3": {
        "type": "select",
        "label": "操作子类型",
        "show_if": "this.action=='count' && this.count_action_category=='non_empty_or_zero'",
        "options": {
          "non_zero": {
            "label": "非 0"
          },
          "non_empty": {
            "label": "非空"
          },
          "non_null": {
            "label": "非 Null"
          },
          "non_blank": {
            "label": "非空白"
          }
        },
        "val": "non_zero"
      },
      "mathematics_action_category": {
        "type": "select",
        "label": "操作类型",
        "show_if": "this.action=='mathematics'",
        "options": {
          "min": {
            "label": "最小"
          },
          "max": {
            "label": "最大"
          },
          "average": {
            "label": "平均"
          },
          "median": {
            "label": "中位数"
          },
          "standard_deviation": {
            "label": "标准差"
          },
          "variance": {
            "label": "方差"
          }
        },
        "val": "min"
      },
      "action_type4": {
        "type": "select",
        "label": "操作子类型",
        "show_if": "this.action=='mathematics'",
        "options": {
          "include_zero": {
            "label": "包括 0"
          },
          "exclude_zero": {
            "label": "排除 0"
          }
        },
        "val": "include_zero"
      },
      "concatenate_separator": {
        "type": "text",
        "label": "分隔符",
        "show_if": "this.action=='concatenate'",
        "val": ","
      },
      "output_column_name": {
        "type": "text",
        "label": "输出列名",
        "val": ""
      },
      "delete": {
        "type": "button",
        "label": "删除",
        "function": "DELETE_ROW"
      }
    }
  }
}