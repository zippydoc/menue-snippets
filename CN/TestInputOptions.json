{
  "options": {
    "type": "fieldset",
    "标签": "表头和标签结果",
    "repeat": false,
    "fields": {
      "has_header": {
        "type": "button",
        "标签": "有标题！",
        "show_if": "$input_table.numOfHeader >= 1"
      },
      "has_label": {
        "type": "button",
        "标签": "有标签！",
        "show_if": "$input_table.numOfLabel >= 1"
      },
      "has_both": {
        "type": "button",
        "标签": "都有！",
        "show_if": "$input_table.numOfHeader >= 1 && $input_table.numOfLabel >= 1"
      },
      "has_both2": {
        "type": "text",
        "标签": "都有2！",
        "show_if": "$input_table.numOfHeader>=1 && $input_table.numOfLabel>=1",
        "val": "both"
      },
      "header_only": {
        "type": "button",
        "标签": "只有标题！",
        "show_if": "$input_table.numOfHeader >= 1 && $input_table.numOfLabel < 1"
      },
      "label_only": {
        "type": "button",
        "标签": "只有标签！",
        "show_if": "$input_table.numOfHeader < 1 && $input_table.numOfLabel >= 1"
      },
      "no_header": {
        "type": "button",
        "标签": "没有标头！",
        "show_if": "$input_table.numOfHeader < 1"
      },
      "no_label": {
        "type": "button",
        "标签": "没有标签！",
        "show_if": "$input_table.numOfLabel < 1"
      }
    }
  }
}