{
  "output.output_table_types": {
    "type": "select", 
    "label": "输出表格类型", 
    "required": true, 
    "options": {
      "unique": {
        "label": "唯一表格"
      }, 
      "duplicates": {
        "label": "复制表格"
      }
    }, 
    "multiple": true, 
    "val": ["unique"]
  }, 
  "output.standard_ending": {
    "type": "checkbox", 
    "label": "标准后缀", 
    "val": "true", 
    "isOn": "true", 
    "isOff": "false", 
    "breakLine": true
  }, 
  "output.unique_ending": {
    "show_if": "output.standard_ending == 'false'", 
    "type": "text", 
    "label": "唯一输出后缀", 
    "val": "_u", 
    "style": "width: 160px; margin-left: 10px"
  }, 
  "output.duplicates_ending": {
    "show_if": "output.standard_ending == 'false'", 
    "type": "text", 
    "label": "重复输出后缀", 
    "val": "_d", 
    "style": "width: 160px; margin-left: 10px", 
    "breakLine": true
  }
}