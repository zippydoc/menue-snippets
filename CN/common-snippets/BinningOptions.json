{
  "options": {
    "type": "fieldset",
    "label": "选择分箱选项",
    "repeat": false,
    "fields": {
      "number_of_tiles": {
        "type": "number",
        "label": "分箱数",
        "val": "2",
        "required": true,
        "breakLine": true
      },
      "binning_type": {
        "type": "select",
        "label": "分箱类型",
        "options": {
          "equal_records": {
            "label": "简单分箱"
          },
          "equal_intervals": {
            "label": "等间隔分箱"
          }
        },
        "val": "equal_records"
      },
      "equal_records_order": {
        "type": "select",
        "label": "顺序",
        "show_if": "binning_type=='equal_records'",
        "options": {
          "keep_current": {
            "label": "保持现有"
          },
          "asc": {
            "label": "升序"
          },
          "desc": {
            "label": "降序"
          }
        },
        "val": "keep_current"
      },
      "equal_intervals_order": {
        "type": "select",
        "label": "顺序",
        "show_if": "binning_type=='equal_intervals'",
        "options": {
          "asc": {
            "label": "升序"
          },
          "desc": {
            "label": "降序"
          }
        },
        "val": "asc",
        "breakLine": true
      }
    },
    "breakLine": true
  }
}