{
  "options": {
    "type": "fieldset",
    "label": "选择选项",
    "repeat": false,
    "fields": {
      "transpose_type": {
        "type": "hidden",
        "val": "crosstab"
      },
      "crosstab.group_data_by_these_values": {
        "type": "fieldset",
        "label": "Rows",
        "repeat": true,
        "fields": {
          "row": {
            "type": "select",
            "label": "Row",
            "table": "table_id",
            "bind": "COLUMNS",
            "required": true
          },
          "delete": {
            "type": "button",
            "label": "删除",
            "function": "DELETE_ROW"
          }
        }
      },
      "crosstab.change_column_headers": {
        "type": "fieldset",
        "label": "列",
        "repeat": true,
        "fields": {
          "column": {
            "type": "select",
            "label": "列",
            "table": "table_id",
            "bind": "COLUMNS",
            "required": true
          },
          "delete": {
            "type": "button",
            "label": "删除",
            "function": "DELETE_ROW"
          }
        }
      },
      "crosstab.values_for_new_columns": {
        "type": "select",
        "label": "Values",
        "table": "table_id",
        "bind": "COLUMNS",
        "required": true
      },
      "crosstab.values_set_format": {
        "type": "checkbox",
        "label": "Reformat Values",
        "val": "false",
        "isOn": "true",
        "isOff": "false",
        "breakLine": true
      },
      "crosstab_values_format_frame": {
        "type": "fieldset",
        "label": "Values Formatting",
        "show_if": "crosstab.values_set_format=='true'",
        "repeat": false,
        "fields": {
          "crosstab.values.format__category": {
            "type": "select",
            "label": "显示格式",
            "required": true,
            "options": {
              "standard": {
                "label": "标准"
              },
              "number": {
                "label": "数字"
              },
              "percentage": {
                "label": "百分比"
              },
              "text": {
                "label": "文本"
              },
              "date": {
                "label": "日期"
              },
              "currency": {
                "label": "货币"
              }
            },
            "val": "standard"
          },
          "crosstab.values.format__rounding": {
            "type": "select",
            "label": "显示的小数位数",
            "show_if": "crosstab.values.format__category=='number' || crosstab.values.format__category=='percentage' || crosstab.values.format__category=='currency'",
            "required": true,
            "options": {
              "no_rounding": {
                "label": "不取整"
              },
              "0": {
                "label": "0 位数"
              },
              "1": {
                "label": "1 位数"
              },
              "2": {
                "label": "2 位数"
              },
              "3": {
                "label": "3 位数"
              },
              "4": {
                "label": "4 位数"
              },
              "5": {
                "label": "5 位数"
              },
              "6": {
                "label": "6 位数"
              },
              "7": {
                "label": "7 位数"
              },
              "8": {
                "label": "8 位数"
              },
              "9": {
                "label": "9 位数"
              }
            },
            "val": "2"
          },
          "crosstab.values.format__use_1000_separator": {
            "type": "checkbox",
            "label": "使用 1000 分隔符（.）",
            "show_if": "crosstab.values.format__category=='number' || crosstab.values.format__category=='percentage' || crosstab.values.format__category=='currency'",
            "val": "true",
            "isOn": "true",
            "isOff": "false"
          },
          "crosstab.values.format__currency_symbol": {
            "type": "select",
            "label": "符号",
            "show_if": "crosstab.values.format__category=='currency'",
            "required": true,
            "options": {
              "dollar": {
                "label": "$"
              },
              "euro": {
                "label": "€"
              },
              "pound": {
                "label": "£"
              },
              "yen": {
                "label": "¥ (Yen, currency of Japan)"
              },
              "colon": {
                "label": "₡ (Colón, currency of Costa Rica)"
              },
              "naira": {
                "label": "₦ (Naira, currency of Nigeria)"
              },
              "rupee": {
                "label": "₨ (Rupee, currency of India)"
              },
              "rupie": {
                "label": "₹ (Rupee, currency of India)"
              },
              "won": {
                "label": "₩ (Won, currency in South Korea and North Korea)"
              },
              "hebrew": {
                "label": "₪ (Currency of Israel)"
              },
              "dong": {
                "label": "₫ (Currency of Vietnam)"
              },
              "kip": {
                "label": "₭ (Kip, currency of Laos)"
              },
              "mongolia": {
                "label": "₮ (Currency of Mongolia)"
              },
              "peso": {
                "label": "₱ (Peso, currency of the Philippines)"
              },
              "guarani": {
                "label": "₲ (Guaraní, currency of Paraguay)"
              },
              "hryvnia": {
                "label": "₴ (Hryvnia, currency of Ukraine)"
              },
              "tenge": {
                "label": "₸ (Tenge, currency of Kazakhstan)"
              },
              "lira": {
                "label": "₺ (Lira, currency of Turkey)"
              },
              "manat": {
                "label": "₼ (Manat, currency of Azerbaijan)"
              },
              "rubles": {
                "label": "₽ (Rubles, currency of Russia)"
              },
              "lari": {
                "label": "₾ (Lari, currency of Georgia)"
              },
              "bitcoin": {
                "label": "₿ (Bitcoin, crypto currency)"
              }
            },
            "val": "dollar"
          },
          "crosstab.values.format__date_format": {
            "type": "select",
            "label": "类型",
            "show_if": "crosstab.values.format__category=='date'",
            "required": true,
            "options": {
              "mm/dd/yyyy": {
                "label": "3/14/2012"
              },
              "D, M dd, Y": {
                "label": "2012年3月14日，星期三"
              },
              "yyyy-mm-dd": {
                "label": "2012-03-14"
              },
              "m.dd.yyyy": {
                "label": "3.14.2012"
              },
              "m.dd.yy": {
                "label": "3.14.12"
              },
              "dd.m.yyyy": {
                "label": "14.3.2012"
              },
              "dd.m.yy": {
                "label": "14.3.12"
              },
              "M dd, Y": {
                "label": "March 14, 2012"
              },
              "dd-mm-yyyy": {
                "label": "14-03-2012"
              },
              "mm-dd-yyyy": {
                "label": "03-14-2012"
              },
              "dd-mm-yy": {
                "label": "14-03-12"
              },
              "mm-dd-yy": {
                "label": "03-14-12"
              }
            },
            "val": "mm/dd/yyyy",
            "breakLine": true
          }
        }
      },
      "crosstab.method_for_aggregating_values": {
        "type": "select",
        "label": "聚合值方法",
        "options": {
          "Concatenate": {
            "label": "连接"
          },
          "First": {
            "label": "首行"
          },
          "Last": {
            "label": "后"
          },
          "Sum": {
            "label": "总和"
          },
          "Average": {
            "label": "平均"
          },
          "Count": {
            "label": "计数"
          },
          "User Defined": {
            "label": "已定义用户"
          }
        },
        "val": "Concatenate",
        "required": true
      },
      "crosstab.user_defined_str": {
        "type": "text",
        "label": "用户定义值",
        "show_if": "crosstab.method_for_aggregating_values=='User Defined'",
        "val": "x",
        "trim": "false",
        "breakLine": true
      },
      "crosstab.separator": {
        "type": "text",
        "label": "分隔符",
        "show_if": "method_for_aggregating_values=='Concatenate'",
        "val": ",",
        "trim": "false",
        "required": true,
        "breakLine": true
      }
    }
  }
}