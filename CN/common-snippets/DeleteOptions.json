{
  "placeholder": {
    "breakLine": true
  },
  "delete_options": {
    "type": "select",
    "label": "删除选项",
    "required": true,
    "options": {
      "del_table": {
        "label": "删除表"
      },
      "del_columns": {
        "label": "删除选定的列"
      },
      "del_rows": {
        "label": "删除选定的行"
      }
    },
    "val": "del_table"
  },
  "apply_columns": {
    "type": "select",
    "label": "选择要删除的列",
    "show_if": "delete_options=='del_columns'",
    "required": true,
    "table": "table_id",
    "multiple": true,
    "bind": "COLUMNS"
  },
  "apply_rows": {
    "type": "select",
    "label": "选择要删除的行",
    "show_if": "delete_options=='del_rows'",
    "required": true,
    "table": "table_id",
    "multiple": true,
    "bind": "LABELS"
  }
}