{
  "placeholder": {
    "breakLine": true
  }, 
  "apply_to_all_columns": {
    "type": "checkbox", 
    "label": "所有栏目", 
    "val": "true", 
    "isOn": "true", 
    "isOff": "false"
  }, 
  "apply_columns": {
    "type": "select", 
    "label": "选择要应用的列", 
    "show_if": "apply_to_all_columns=='false'", 
    "required": true, 
    "table": "input_table", 
    "multiple": true, 
    "bind": "COLUMNS", 
    "breakLine": true
  }, 
  "apply_to_all_rows": {
    "type": "checkbox", 
    "label": "All Rows", 
    "val": "true", 
    "isOn": "true", 
    "isOff": "false"
  }, 
  "apply_rows": {
    "type": "select", 
    "label": "选择要应用的行", 
    "show_if": "apply_to_all_rows=='false'", 
    "required": true, 
    "table": "input_table", 
    "multiple": true, 
    "bind": "LABELS", 
    "breakLine": true
  }, 
  "intersection": {
    "type": "select", 
    "label": "选择", 
    "show_if": "apply_to_all_columns=='false' && apply_to_all_rows=='false'", 
    "options": {
      "all": {
        "label": "所有选定的行和列"
      }, 
      "intersection": {
        "label": "选定行和列的交叉点"
      }
    }, 
    "val": "intersection"
  }
}
