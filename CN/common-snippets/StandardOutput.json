{
  "output": {
    "type": "fieldset", 
    "label": "定义输出表", 
    "repeat": false, 
    "fields": {
      "output.type": {
        "type": "select", 
        "label": "表名", 
        "val": "auto", 
        "options": {
          "auto": {
            "label": "自动"
          }, 
          "custom": {
            "label": "自定义"
          }, 
          "compose": {
            "label": "撰写"
          }
        }
      }, 
      "output.custom_text": {
        "type": "text", 
        "label": "输出表名称", 
        "show_if": "output.type=='custom'", 
        "required": true, 
        "minLength": 1, 
        "val": "", 
        "breakLine": true
      }, 
      "output.prefix_rule": {
        "type": "select", 
        "label": "前缀", 
        "show_if": "output.type=='compose'", 
        "val": "empty", 
        "options": {
          "empty": {
            "label": "-"
          }, 
          "script_name": {
            "label": "任务名称"
          }, 
          "date_string_type1": {
            "label": "日期：日_月_年"
          }, 
          "date_string_type2": {
            "label": "日期：yyy_mm_dd"
          }, 
          "time_string": {
            "label": "时间：h_min_sec"
          }, 
          "datetime_string": {
            "label": "日期_时间"
          }, 
          "custom": {
            "label": "自定义"
          }
        }
      }, 
      "output.prefix_text": {
        "type": "text", 
        "label": "前缀文字", 
        "show_if": "output.type=='compose' && output.prefix_rule=='custom'", 
        "minLength": 1, 
        "maxLength": 31, 
        "val": ""
      }, 
      "output.core_delimiter": {
        "type": "text", 
        "label": "定界符", 
        "show_if": "output.type=='compose' && output.prefix_rule!='empty'", 
        "val": "_", 
        "width": "50px"
      }, 
      "output.core_rule": {
        "type": "select", 
        "label": "表名", 
        "show_if": "output.type=='compose'", 
        "val": "input_table_name", 
        "options": {
          "input_table_name": {
            "label": "输入表名称"
          }, 
          "script_name": {
            "label": "任务名称"
          }, 
          "custom": {
            "label": "自定义"
          }
        }
      }, 
      "output.core_text": {
        "type": "text", 
        "label": "主表名称", 
        "show_if": "output.type=='compose' && output.core_rule=='custom'", 
        "required": true, 
        "minLength": 1, 
        "maxLength": 31, 
        "val": ""
      }, 
      "output.suffix_delimiter": {
        "type": "text", 
        "label": "定界符", 
        "show_if": "output.type=='compose'", 
        "val": "_", 
        "width": "50px"
      }, 
      "output.suffix_rule": {
        "type": "select", 
        "label": "后缀", 
        "show_if": "output.type=='compose'", 
        "val": "script_name", 
        "options": {
          "empty": {
            "label": "-"
          }, 
          "script_name": {
            "label": "任务名称"
          }, 
          "date_string_type1": {
            "label": "日期：日_月_年"
          }, 
          "date_string_type2": {
            "label": "日期：yyy_mm_dd"
          }, 
          "time_string": {
            "label": "时间：h_min_sec"
          }, 
          "datetime_string": {
            "label": "日期_时间"
          }, 
          "custom": {
            "label": "自定义"
          }
        }
      }, 
      "output.suffix_text": {
        "type": "text", 
        "label": "后缀文本", 
        "show_if": "output.type=='compose' && output.suffix_rule=='custom'", 
        "minLength": 1, 
        "maxLength": 31, 
        "val": "", 
        "breakLine": true
      }, 
      "output.timezone": {
        "type": "select", 
        "label": "时区", 
        "show_if": "output.type=='compose' && (output.prefix_rule=='date_string_type1' || output.prefix_rule=='date_string_type2' || output.prefix_rule=='time_string' || output.prefix_rule=='datetime_string' || output.suffix_rule=='date_string_type1' || output.suffix_rule=='date_string_type2' || output.suffix_rule=='time_string' || output.suffix_rule=='datetime_string')", 
        "val": "欧洲/柏林", 
        "required": true, 
        "options": {
          "欧洲/阿姆斯特丹": {
            "label": "欧洲/阿姆斯特丹"
          }, 
          "欧洲/柏林": {
            "label": "欧洲/柏林"
          }, 
          "欧洲/赫尔辛基": {
            "label": "欧洲/赫尔辛基"
          }, 
          "欧洲/伦敦": {
            "label": "欧洲/伦敦"
          }, 
          "欧洲/卢森堡": {
            "label": "欧洲/卢森堡"
          }, 
          "欧洲/奥斯陆": {
            "label": "欧洲/奥斯陆"
          }, 
          "欧洲/巴黎": {
            "label": "欧洲/巴黎"
          }, 
          "欧洲/罗马": {
            "label": "欧洲/罗马"
          }, 
          "美国/阿拉斯加": {
            "label": "美国/阿拉斯加"
          }, 
          "美国/中部": {
            "label": "美国/中部"
          }, 
          "美国/东部": {
            "label": "美国/东部"
          }, 
          "美国/太平洋": {
            "label": "美国/太平洋"
          }
        }, 
        "breakLine": true
      }, 
      "output.set_options": {
        "type": "checkbox", 
        "label": "输出选项", 
        "val": "false", 
        "isOn": "true", 
        "isOff": "false"
      }, 
      "output_options_box": {
        "type": "fieldset", 
        "label": "输出选项", 
        "show_if": "output.set_options=='true'", 
        "repeat": false, 
        "fields": {
          "output.options.location": {
            "type": "text", 
            "label": "指定地点", 
            "val": "A1", 
            "breakLine": true
          }, 
          "output.options.table_mode": {
            "type": "select", 
            "label": "粘贴", 
            "val": "content_only", 
            "options": {
              "content_only": {
                "label": "仅限内容"
              }, 
              "fully_formatted": {
                "label": "完全格式化"
              }
            }
          }, 
          "output.options.formatted_as_table": {
            "type": "checkbox", 
            "label": "格式为表", 
            "val": "true", 
            "isOn": "true", 
            "isOff": "false", 
            "breakLine": true
          }, 
          "output.add_tag": {
            "type": "checkbox", 
            "label": "添加标签", 
            "val": "false", 
            "isOn": "true", 
            "isOff": "false"
          }, 
          "output.tag_name": {
            "type": "text", 
            "label": "标签名称", 
            "show_if": "output.add_tag=='true'", 
            "required": true, 
            "minLength": 1, 
            "maxLength": 255, 
            "val": "", 
            "breakLine": true, 
            "style": "margin-left: 15px"
          }, 
          "output.options.visibility": {
            "type": "select", 
            "label": "可见度", 
            "val": "current_flow", 
            "options": {
              "current_flow": {
                "label": "当前流量"
              }, 
              "all_flow": {
                "label": "所有流量"
              }
            }
          }
        }, 
        "breakLine": true
      }
    }
  }
}
