{
  "options": {
    "type": "fieldset",
    "label": "合并选项",
    "repeat": false,
    "fields": {
      "options.union_type": {
        "type": "select",
        "label": "选择合并类型",
        "val": "auto_config_by_name",
        "options": {
          "auto_config_by_name": {
            "label": "自动配置（按名称）"
          },
          "auto_config_by_position": {
            "label": "自动配置（按位置）"
          }
        }
      },
      "options.field_output_type": {
        "type": "select",
        "label": "列输出类型",
        "show_if": "options.union_type=='auto_config_by_name' || options.union_type=='auto_config_by_position'",
        "val": "output_all_fields",
        "options": {
          "output_all_fields": {
            "label": "输出所有列"
          },
          "output_common_fields": {
            "label": "输出公共列"
          }
        },
        "breakLine": true
      },
      "options.add_table_name_column": {
        "type": "checkbox",
        "label": "添加表名列",
        "val": "false",
        "isOn": "true",
        "isOff": "false",
        "breakLine": true
      }
    }
  }
}