{
  "options": {
    "type": "fieldset",
    "label": "Optionen auswählen",
    "repeat": false,
    "fields": {
      "options.position": {
        "type": "select",
        "label": "Position wählen",
        "required": true,
        "show_if": "table_type==''header_only''",
        "options": {
          "add_rows_below_selected_row": {
            "label": "Zeilen unten hinzufügen"
          },
          "add_rows_above_selected_row": {
            "label": "Zeilen oben hinzufügen"
          },
          "add_rows_at_table_end": {
            "label": "Zeilen am Tabellenende hinzufügen"
          },
          "add_single_row_at_table_end": {
            "label": "Einzelne Zeile am Tabellenende hinzufügen"
          },
          "add_row_after_each_group": {
            "label": "Zeile nach jeder Gruppe hinzufügen"
          }
        },
        "val": "add_rows_below_selected_row",
        "breakLine": true
      },
      "options.group_column": {
        "type": "select",
        "label": "Gruppenspalte",
        "show_if": "table_type==''header_only'' && options.position==''add_row_after_each_group''",
        "table": "input_table",
        "bind": "COLUMNS",
        "required": true,
        "breakLine": true
      },
      "options.use_filter": {
        "type": "checkbox",
        "label": "Filter verwenden",
        "show_if": "table_type==''header_only'' && (options.position==''add_rows_below_selected_row'' || options.position==''add_rows_above_selected_row'' || options.position==''add_rows_at_table_end'')",
        "val": "false",
        "isOn": "true",
        "isOff": "false",
        "breakLine": true,
        "style": "margin-top: 10px"
      },
      "filter_box": {
        "type": "fieldset",
        "label": "Filter-Optionen",
        "show_if": "table_type==''header_only'' && options.use_filter==''true'' && (options.position==''add_rows_below_selected_row'' || options.position==''add_rows_above_selected_row'' || options.position==''add_rows_at_table_end'')",
        "repeat": false,
        "fields": {
          "options.filter": {
            "type": "fieldset",
            "label": "Filterbedingungen hinzufügen",
            "repeat": true,
            "fields": {
              "initial_op": {
                "type": "select",
                "label": "Logik-Operator auswählen",
                "required": true,
                "show_if": "$index == 0",
                "options": {
                  "if": {
                    "label": "IF"
                  },
                  "if_not": {
                    "label": "WENN NICHT"
                  }
                },
                "val": "if"
              },
              "op": {
                "type": "select",
                "label": "Logik-Operator auswählen",
                "required": true,
                "show_if": "$index > 0",
                "options": {
                  "and": {
                    "label": "UND"
                  },
                  "or": {
                    "label": "OR"
                  },
                  "and_not": {
                    "label": "UND NICHT"
                  },
                  "or_not": {
                    "label": "ODER NICHT"
                  }
                },
                "val": "and"
              },
              "column": {
                "type": "select",
                "label": "Spalte auswählen",
                "table": "input_table",
                "bind": "COLUMNS",
                "required": true
              },
              "condition": {
                "type": "select",
                "label": "Bediener auswählen",
                "required": true,
                "options": {
                  "ENTHÄLT": {
                    "label": "ENTHÄLT"
                  },
                  "CONTAINED IN STRING": {
                    "label": "ENTHALTEN IN"
                  },
                  "STARTET MIT": {
                    "label": "STARTET MIT"
                  },
                  "ENDET MIT": {
                    "label": "ENDET MIT"
                  },
                  "=": {
                    "label": "EQUAL"
                  },
                  ">": {
                    "label": "GRÖSSER ALS"
                  },
                  ">=": {
                    "label": "GRÖSSER ODER GLEICH"
                  },
                  "<": {
                    "label": "KLEINER ALS"
                  },
                  "<=": {
                    "label": "KLEINER ODER GLEICH"
                  },
                  "EMPTY": {
                    "label": "IST LEER"
                  },
                  "BLANK": {
                    "label": "IST BLANK"
                  },
                  "IS NULL": {
                    "label": "IS NULL"
                  },
                  "REGEX-MATCH": {
                    "label": "REGEX-MATCH"
                  },
                  "CONTAINED IN TABLE": {
                    "label": "IN TABELLE"
                  },
                  "IN REGEX-TABELLE": {
                    "label": "IN REGEX-TABELLE"
                  },
                  "DATUM": {
                    "label": "DATUM"
                  }
                },
                "val": "ENTHÄLT"
              },
              "date_condition": {
                "type": "select",
                "label": "Datumsoperator auswählen",
                "required": true,
                "show_if": "this.condition == ''DATE''",
                "options": {
                  "<": {
                    "label": "PRIOR"
                  },
                  "<=": {
                    "label": "PRIOR ODER GLEICH"
                  },
                  "=": {
                    "label": "EQUAL"
                  },
                  ">": {
                    "label": "NACH"
                  },
                  ">=": {
                    "label": "NACHHER ODER GLEICH"
                  }
                },
                "val": "="
              },
              "case_sensitive": {
                "type": "select",
                "label": "Wählen Sie Case Sensitive",
                "required": true,
                "show_if": "this.condition == ''CONTAINS'' || this.condition == ''CONTAINED IN STRING'' || this.condition == ''='' || this.condition == ''STARTS WITH'' || this.condition == ''ENDS WITH'' || this.condition == ''REGEX MATCH'' || this.condition == ''NOT REGEX MATCH'' || this.condition == ''IN REGEX TABLE'' || this.condition == ''NOT IN REGEX TABLE''",
                "options": {
                  "yes": {
                    "label": "Gehäuse empfindlich"
                  },
                  "no": {
                    "label": "Groß-/Kleinschreibung nicht beachten"
                  }
                },
                "val": "yes"
              },
              "date_value": {
                "type": "date",
                "label": "Datum auswählen",
                "show_if": "this.condition==''DATE''",
                "val": "1/31/2020",
                "required": true
              },
              "value": {
                "type": "text",
                "label": "Eingangswert",
                "show_if": "this.condition != ''DATE'' && this.condition != ''EMPTY'' && this.condition != ''NOT EMPTY'' && this.condition != ''BLANK'' && this.condition != ''NOT BLANK'' && this.condition != ''IS NULL'' && this.condition != ''IS NOT NULL'' && this.condition != ''CONTAINED IN TABLE'' && this.condition != ''NOT CONTAINED IN TABLE'' && this.condition != ''IN REGEX TABLE'' && this.condition != ''NOT IN REGEX TABLE''",
                "val": "",
                "required": true
              },
              "compare_table": {
                "type": "select",
                "label": "Tabelle auswählen",
                "bind": "TABLES",
                "empty": "",
                "show_if": "this.condition == ''CONTAINED IN TABLE'' || this.condition == ''NOT CONTAINED IN TABLE'' || this.condition == ''IN REGEX TABLE'' || this.condition == ''NOT IN REGEX TABLE''",
                "required": true
              },
              "compare_column": {
                "type": "select",
                "label": "Spalte auswählen",
                "table": "this.compare_table",
                "bind": "COLUMNS",
                "show_if": "this.condition == ''CONTAINED IN TABLE'' || this.condition == ''NOT CONTAINED IN TABLE'' || this.condition == ''IN REGEX TABLE'' || this.condition == ''NOT IN REGEX TABLE''",
                "required": true
              },
              "delete": {
                "type": "button",
                "label": "Lösche",
                "function": "DELETE_ROW"
              }
            }
          },
          "options.apply_to_record": {
            "type": "checkbox",
            "label": "Auf den Datensatz anwenden",
            "val": "true",
            "isOn": "true",
            "isOff": "false",
