{
  "options": {
    "type": "fieldset",
    "Beschriftung": "Binning-Optionen auswählen",
    "repeat": false,
    "fields": {
      "number_of_tiles": {
        "type": "text",
        "Beschriftung": "Anzahl von Behältern",
        "val": "2",
        "required": true,
        "breakLine": true
      },
      "binning_type": {
        "type": "select",
        "Beschriftung": "Binning Typ",
        "options": {
          "equal_records": {
            "Beschriftung": "Einfaches Sortieren"
          },
          "equal_intervals": {
            "Beschriftung": "Binning mit gleichem Intervall"
          }
        },
        "val": "equal_records"
      },
      "equal_records_order": {
        "type": "select",
        "Beschriftung": "Bestellung",
        "show_if": "binning_type=='equal_records'",
        "options": {
          "keep_current": {
            "Beschriftung": "Strom halten"
          },
          "asc": {
            "Beschriftung": "Aufsteigend"
          },
          "desc": {
            "Beschriftung": "Absteigend"
          }
        },
        "val": "keep_current"
      },
      "equal_intervals_order": {
        "type": "select",
        "Beschriftung": "Bestellung",
        "show_if": "binning_type=='equal_intervals'",
        "options": {
          "asc": {
            "Beschriftung": "Aufsteigend"
          },
          "desc": {
            "Beschriftung": "Absteigend"
          }
        },
        "val": "asc"
      }
    },
    "breakLine": true
  }
}