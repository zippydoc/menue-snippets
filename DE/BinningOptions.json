{
  "options": {
    "type": "fieldset",
    "label": "Binning-Optionen auswählen",
    "repeat": false,
    "fields": {
      "number_of_tiles": {
        "type": "number",
        "label": "Anzahl von Behältern",
        "val": "2",
        "required": true,
        "breakLine": true
      },
      "binning_type": {
        "type": "select",
        "label": "Binning Typ",
        "options": {
          "equal_records": {
            "label": "Einfaches Sortieren"
          },
          "equal_intervals": {
            "label": "Binning mit gleichem Intervall"
          }
        },
        "val": "equal_records"
      },
      "equal_records_order": {
        "type": "select",
        "label": "Bestellung",
        "show_if": "binning_type=='equal_records'",
        "options": {
          "keep_current": {
            "label": "Strom halten"
          },
          "asc": {
            "label": "Aufsteigend"
          },
          "desc": {
            "label": "Absteigend"
          }
        },
        "val": "keep_current"
      },
      "equal_intervals_order": {
        "type": "select",
        "label": "Bestellung",
        "show_if": "binning_type=='equal_intervals'",
        "options": {
          "asc": {
            "label": "Aufsteigend"
          },
          "desc": {
            "label": "Absteigend"
          }
        },
        "val": "asc",
        "breakLine": true
      }
    },
    "breakLine": true
  }
}