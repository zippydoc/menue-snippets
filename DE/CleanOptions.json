{
  "options": {
    "type": "fieldset",
    "label": "Reinigungsoptionen auswählen",
    "repeat": false,
    "fields": {
      "replace_null": {
        "type": "fieldset",
        "label": "Leere Datensätze",
        "repeat": false,
        "fields": {
          "replace_null": {
            "type": "checkbox",
            "label": "Leere Datensätze",
            "val": "false",
            "isOn": "true",
            "isOff": "false",
            "breakLine": true
          },
          "replace_null_target": {
            "type": "select",
            "label": "Ersetzen Sie",
            "show_if": "replace_null=='true'",
            "options": {
              "empty": {
                "label": "Leer (NULL oder Leer)"
              },
              "blank": {
                "label": "Leer"
              },
              "null": {
                "label": "NULL"
              }
            },
            "val": "empty",
            "breakLine": true
          },
          "replace_null_by": {
            "type": "radio",
            "show_if": "replace_null=='true'",
            "val": "by_blank",
            "values": {
              "by_blank": "By Blank",
              "by_zero": "By Zero"
            },
            "breakLine": true
          }
        }
      },
      "remove_whitespace": {
        "type": "fieldset",
        "label": "Leerzeichen u.ä. entfernen",
        "repeat": false,
        "fields": {
          "remove_characters.all_whitespace": {
            "type": "checkbox",
            "label": "Nicht sichtbare Zeichen",
            "val": "false",
            "isOn": "true",
            "isOff": "false",
            "breakLine": true
          },
          "remove_characters.leading_whitespace": {
            "type": "checkbox",
            "label": "Führende Leerzeichen u.ä.",
            "show_if": "remove_characters.all_whitespace=='false'",
            "val": "false",
            "isOn": "true",
            "isOff": "false",
            "breakLine": true
          },
          "remove_characters.trailing_whitespace": {
            "type": "checkbox",
            "label": "Nachlaufende Leerzeichen",
            "show_if": "remove_characters.all_whitespace=='false'",
            "val": "false",
            "isOn": "true",
            "isOff": "false",
            "breakLine": true
          },
          "remove_characters.whitespace": {
            "type": "checkbox",
            "label": "Leerzeichen",
            "show_if": "remove_characters.all_whitespace=='false'",
            "val": "false",
            "isOn": "true",
            "isOff": "false",
            "breakLine": true
          },
          "remove_characters.tabs": {
            "type": "checkbox",
            "label": "Tabulator",
            "show_if": "remove_characters.all_whitespace=='false'",
            "val": "false",
            "isOn": "true",
            "isOff": "false",
            "breakLine": true
          },
          "remove_characters.line_breaks": {
            "type": "checkbox",
            "label": "Zeilenumbrüche",
            "show_if": "remove_characters.all_whitespace=='false'",
            "val": "false",
            "isOn": "true",
            "isOff": "false",
            "breakLine": true
          },
          "remove_characters.duplicate_whitespace": {
            "type": "checkbox",
            "label": "Duplizierte Leerzeichen u.ä.",
            "show_if": "remove_characters.all_whitespace=='false'",
            "val": "false",
            "isOn": "true",
            "isOff": "false",
            "breakLine": true
          }
        }
      },
      "remove_duplicates": {
        "type": "fieldset",
        "label": "Duplikate entfernen",
        "repeat": false,
        "fields": {
          "remove_duplicates.cell": {
            "type": "checkbox",
            "label": "Duplikate in Zelle",
            "val": "false",
            "isOn": "true",
            "isOff": "false",
            "breakLine": true
          },
          "remove_duplicates.custom_delimiter": {
            "type": "checkbox",
            "label": "Benutzerdefinierte Begrenzer",
            "show_if": "remove_duplicates.cell=='true'",
            "val": "false",
            "isOn": "true",
            "isOff": "false"
          },
          "remove_duplicates.delimiter": {
            "type": "text",
            "label": "Benutzerdefinierte Begrenzer",
            "show_if": "remove_duplicates.custom_delimiter=='true'",
            "val": ";",
            "required": true
          },
          "remove_duplicates.include_whitespace": {
            "type": "checkbox",
            "label": "(Leerzeichen u.ä. einschließen)",
            "show_if": "remove_duplicates.cell=='true'",
            "val": "true",
            "isOn": "true",
            "isOff": "false"
          },
          "remove_duplicates.rows": {
            "type": "checkbox",
            "label": "Zeilen Duplikate",
            "val": "false",
            "isOn": "true",
            "isOff": "false",
            "breakLine": true
          }
        }
      },
      "remove_characters": {
        "type": "fieldset",
        "label": "Zeichen entfernen",
        "repeat": false,
        "fields": {
          "remove_characters.letters": {
            "type": "checkbox",
            "label": "Buchstaben",
            "val": "false",
            "isOn": "true",
            "isOff": "false",
            "breakLine": true
          },
          "remove_characters.numbers": {
            "type": "checkbox",
            "label": "Zahlen",
            "val": "false",
            "isOn": "true",
            "isOff": "false",
            "breakLine": true
          },
          "remove_characters.punctuation": {
            "type": "checkbox",
            "label": "Zeichensetzung",
            "val": "false",
            "isOn": "true",
            "isOff": "false",
            "breakLine": true
          }
        }
      },
      "modify_case": {
        "type": "fieldset",
        "label": "Groß- / Kleinschreibung",
        "repeat": false,
        "fields": {
          "modify_case": {
            "type": "checkbox",
            "label": "Groß- / Kleinschreibung",
            "val": "false",
            "isOn": "true",
            "isOff": "false",
            "breakLine": true
          },
          "modify_case_by": {
            "type": "radio",
            "show_if": "modify_case=='true'",
            "val": "upper",
            "values": {
              "upper": "to Upper Case",
              "lower": "to Lower Case",
              "title": "to Title Case"
            },
            "breakLine": true
          }
        }
      }
    }
  }
}