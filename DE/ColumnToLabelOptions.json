{
  "options": {
    "type": "fieldset",
    "label": "Optionen auswählen",
    "repeat": false,
    "fields": {
      "transpose_type": {
        "type": "hidden",
        "val": "column_name_to_label"
      },
      "mirror_matrix.convert_header_to_label": {
        "type": "checkbox",
        "label": "Kopfzeile in Etikett umwandeln",
        "show_if": "transpose_type=='mirror_matrix'",
        "val": "true",
        "isOn": "true",
        "isOff": "false",
        "breakLine": true
      },
      "column_name_to_label__columns_to_label_frame": {
        "type": "fieldset",
        "label": "Select columns to convert into Labels",
        "show_if": "transpose_type=='column_name_to_label'",
        "repeat": false,
        "fields": {
          "column_name_to_label.reverse_col_selection": {
            "type": "checkbox",
            "label": "All but selected",
            "val": "false",
            "isOn": "true",
            "isOff": "false",
            "breakLine": true
          },
          "column_name_to_label.columns_to_label": {
            "type": "select",
            "label": "Select columns",
            "table": "table_id",
            "multiple": true,
            "bind": "COLUMNS",
            "width": "100%",
            "breakLine": true
          }
        }
      },
      "column_name_to_label.label_column_name": {
        "type": "text",
        "label": "Spalte Name der Etikettenspalte",
        "show_if": "transpose_type=='column_name_to_label'",
        "required": true,
        "val": "Label",
        "width": "100%",
        "breakLine": true
      },
      "column_name_to_label.value_column_name": {
        "type": "text",
        "label": "Spalte Name der Wertespalte",
        "show_if": "transpose_type=='column_name_to_label'",
        "required": true,
        "val": "Value",
        "width": "100%",
        "breakLine": true
      },
      "column_name_to_label.is_one_label": {
        "type": "checkbox",
        "label": "Eine Etikettenspalte",
        "show_if": "transpose_type=='column_name_to_label'",
        "val": "false",
        "isOn": "true",
        "isOff": "false"
      },
      "crosstab.group_data_by_these_values": {
        "type": "select",
        "label": "Daten nach diesen Werten gruppieren",
        "show_if": "transpose_type=='crosstab'",
        "table": "table_id",
        "multiple": true,
        "bind": "COLUMNS",
        "required": true,
        "width": "100%",
        "breakLine": true
      },
      "crosstab.change_column_headers": {
        "type": "select",
        "label": "Spaltenüberschrift ändern",
        "show_if": "transpose_type=='crosstab'",
        "table": "table_id",
        "bind": "COLUMNS",
        "required": true,
        "width": "100%",
        "breakLine": true
      },
      "crosstab.values_for_new_columns": {
        "type": "select",
        "label": "Werte für neue Spalten",
        "show_if": "transpose_type=='crosstab'",
        "table": "table_id",
        "bind": "COLUMNS",
        "required": true,
        "width": "100%",
        "breakLine": true
      },
      "crosstab.method_for_aggregating_values": {
        "type": "select",
        "label": "Methode zum Aggregieren von Werten",
        "show_if": "transpose_type=='crosstab'",
        "options": {
          "Concatenate": {
            "label": "Verketten"
          },
          "First": {
            "label": "Erste"
          },
          "Last": {
            "label": "Zuletzt"
          },
          "Sum": {
            "label": "Summe"
          },
          "Average": {
            "label": "Durchschnitt"
          },
          "Count": {
            "label": "Zählen Sie"
          },
          "User Defined": {
            "label": "Benutzerdefiniert"
          }
        },
        "val": "Concatenate",
        "required": true
      },
      "crosstab.user_defined_str": {
        "type": "text",
        "label": "Benutzerdefinierter Wert",
        "show_if": "transpose_type=='crosstab' && crosstab.method_for_aggregating_values=='User Defined'",
        "val": "x",
        "trim": "false",
        "breakLine": true
      },
      "crosstab.separator": {
        "type": "text",
        "label": "Separator",
        "show_if": "transpose_type=='crosstab' && method_for_aggregating_values=='Concatenate'",
        "val": ",",
        "trim": "false",
        "required": true,
        "breakLine": true
      }
    }
  }
}