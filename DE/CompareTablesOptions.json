{
  "options": {
    "type": "fieldset",
    "label": "Optionen",
    "repeat": false,
    "fields": {
      "compare_set": {
        "type": "fieldset",
        "label": "Muster auswählen",
        "repeat": true,
        "fields": {
          "pattern": {
            "type": "select",
            "label": "Tabellenmustertyp",
            "options": {
              "all_col": {
                "label": "Alle Spaltenidentität"
              },
              "two_col": {
                "label": "Zweispaltige Identität"
              }
            },
            "val": "all_col",
            "required": true
          },
          "compare_type": {
            "type": "select",
            "label": "Zellenmustertyp",
            "options": {
              "equal": {
                "label": "Gleich"
              },
              "contain": {
                "label": "Enthält"
              },
              "start_with": {
                "label": "Beginnt mit"
              },
              "end_with": {
                "label": "Endet mit"
              },
              "reg_exp": {
                "label": "Regulärer Ausdruck"
              }
            },
            "val": "equal",
            "required": true
          },
          "case_sensitive": {
            "type": "select",
            "label": "Groß-/Kleinschreibungsunterscheidung",
            "required": true,
            "options": {
              "yes": {
                "label": "Gehäuse empfindlich"
              },
              "no": {
                "label": "Groß-/Kleinschreibung nicht beachten"
              }
            },
            "val": "no"
          },
          "expression": {
            "type": "text",
            "label": "Ausdruck",
            "show_if": "this.compare_type != 'equal'",
            "required": true,
            "val": ""
          },
          "delete": {
            "show_if": "this.field_name != ''",
            "type": "button",
            "label": "Lösche",
            "function": "DELETE_ROW"
          }
        },
        "breakLine": true
      },
      "target": {
        "type": "select",
        "label": "Ziel",
        "options": {
          "to_table": {
            "label": "Auf Tabelle anwenden"
          },
          "to_rows": {
            "label": "Zeile für Zeile"
          }
        },
        "val": "to_table",
        "required": true
      },
      "match_type": {
        "type": "select",
        "show_if": "target != 'to_table'",
        "label": "Übereinstimmungs-Typ",
        "options": {
          "match": {
            "label": "Nur Übereinstimmungen"
          },
          "nonmatch": {
            "label": "Nur Nichtübereinstimmungen"
          },
          "all": {
            "label": "Alle"
          }
        },
        "val": "match",
        "required": true,
        "breakLine": true
      },
      "create_sql": {
        "type": "checkbox",
        "label": "SQL-Details anzeigen",
        "val": "false",
        "isOn": "true",
        "isOff": "false"
      }
    }
  }
}