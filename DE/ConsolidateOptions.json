{
  "input": {
    "type": "fieldset",
    "label": "Optionen konsolidieren",
    "repeat": true,
    "fields": {
      "new_position": {
        "type": "text",
        "label": "Neue Position",
        "val": "",
        "placeholder": "Text",
        "required": true,
        "breakLine": true
      },
      "table_id": {
        "type": "select",
        "label": "Tabelle auswählen",
        "bind": "TABLES",
        "empty": "",
        "required": true
      },
      "rows": {
        "type": "select",
        "label": "Reihenauswahl",
        "required": true,
        "table": "this.table_id",
        "multiple": true,
        "bind": "LABELS"
      },
      "multiplier": {
        "type": "select",
        "label": "Multiplikator",
        "required": true,
        "options": {
          "positive": {
            "label": "Schild behalten"
          },
          "negative": {
            "label": "Zeichen wechseln"
          }
        },
        "val": "positive",
        "breakLine": true
      },
      "format__category": {
        "type": "select",
        "label": "Anzeigeformat",
        "required": true,
        "options": {
          "standard": {
            "label": "Standard"
          },
          "number": {
            "label": "Nummer"
          },
          "percentage": {
            "label": "Prozentsatz"
          },
          "text": {
            "label": "Text"
          },
          "date": {
            "label": "Datum"
          },
          "currency": {
            "label": "Währung"
          }
        },
        "val": "standard"
      },
      "format__rounding": {
        "type": "select",
        "label": "Dezimalstellen für die Anzeige",
        "show_if": "this.format__category=='number' || this.format__category=='percentage' || this.format__category=='currency'",
        "required": true,
        "options": {
          "no_rounding": {
            "label": "Keine Rundung"
          },
          "0": {
            "label": "0 Ziffern"
          },
          "1": {
            "label": "1 Ziffern"
          },
          "2": {
            "label": "2 Ziffern"
          },
          "3": {
            "label": "3 Ziffern"
          },
          "4": {
            "label": "4 Ziffern"
          },
          "5": {
            "label": "5 Ziffern"
          },
          "6": {
            "label": "6 Ziffern"
          },
          "7": {
            "label": "7 Ziffern"
          },
          "8": {
            "label": "8 Ziffern"
          },
          "9": {
            "label": "9 Ziffern"
          }
        },
        "val": "2"
      },
      "format__use_1000_separator": {
        "type": "checkbox",
        "label": "1000 Trennzeichen(.) verwenden",
        "show_if": "this.format__category=='number' || this.format__category=='percentage' || this.format__category=='currency'",
        "val": "true",
        "isOn": "true",
        "isOff": "false"
      },
      "format__currency_symbol": {
        "type": "select",
        "label": "Symbol",
        "show_if": "this.format__category=='currency'",
        "required": true,
        "options": {
          "dollar": {
            "label": "$"
          },
          "euro": {
            "label": "€"
          },
          "pound": {
            "label": "£"
          }
        },
        "val": "dollar"
      },
      "format__date_format": {
        "type": "select",
        "label": "Typ",
        "show_if": "this.format__category=='date'",
        "required": true,
        "options": {
          "mm/dd/yyyy": {
            "label": "3/14/2012"
          },
          "D, M dd, Y": {
            "label": "Mittwoch, 14. März 2012"
          },
          "yyyy-mm-dd": {
            "label": "2012-03-14"
          },
          "m.dd.yyyy": {
            "label": "3.14.2012"
          },
          "m.dd.yy": {
            "label": "3.14.12"
          },
          "dd.m.yyyy": {
            "label": "14.3.2012"
          },
          "dd.m.yy": {
            "label": "14.3.12"
          },
          "M dd, Y": {
            "label": "14. März 2012"
          },
          "dd-mm-yyyy": {
            "label": "14-03-2012"
          },
          "mm-dd-yyyy": {
            "label": "03-14-2012"
          },
          "dd-mm-yy": {
            "label": "14-03-12"
          },
          "mm-dd-yy": {
            "label": "03-14-12"
          }
        },
        "val": "mm/dd/yyyy",
        "breakLine": true
      },
      "delete": {
        "type": "button",
        "label": "Lösche",
        "function": "DELETE_ROW"
      }
    }
  }
}