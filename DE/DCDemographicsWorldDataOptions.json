{
  "options_frame": {
    "type": "fieldset",
    "label": "Optionen",
    "repeat": false,
    "fields": {
      "options.group_by": {
        "type": "select",
        "label": "Gruppe von",
        "required": true,
        "options": {
          "Region": {
            "label": "Region"
          },
          "Income Group": {
            "label": "Einkommensgruppe"
          }
        },
        "val": "Region"
      },
      "options.stat_var_name": {
        "type": "select",
        "label": "Statistische Daten",
        "required": true,
        "options": {
          "Count_Person": {
            "label": "Bevölkerung"
          },
          "GrowthRate_Count_Person": {
            "label": "Wachstumsrate der Bevölkerung"
          },
          "Count_Person_PerArea": {
            "label": "Bevölkerungsdichte"
          },
          "Count_Person_IsInternetUser_PerCapita": {
            "label": "Prozentsatz der Internetnutzer"
          },
          "Count_Person_ResidingLessThan5MetersAboveSeaLevel_AsFractionOf_Count_Person": {
            "label": "Prozentsatz der Bevölkerung, der in Gebieten unter 5 Metern Höhe lebt"
          },
          "Count_Person_Rural": {
            "label": "Ländliche Bevölkerung"
          },
          "Count_Person_Urban": {
            "label": "Städtische Bevölkerung"
          }
        },
        "val": "Count_Person"
      },
      "options.per_capita": {
        "type": "checkbox",
        "label": "Pro-Kopf",
        "show_if": "false",
        "val": "false",
        "isOn": "true",
        "isOff": "false",
        "breakLine": true
      },
      "options.all_countries": {
        "type": "checkbox",
        "label": "Alle Länder",
        "val": "true",
        "isOn": "true",
        "isOff": "false",
        "breakLine": true
      },
      "options.output_as_chart": {
        "type": "checkbox",
        "label": "Graph ausgeben",
        "val": "true",
        "isOn": "true",
        "isOff": "false"
      },
      "options.graph_type": {
        "type": "select",
        "label": "Diagrammtyp",
        "show_if": "options.output_as_chart=='true'",
        "required": true,
        "options": {
          "zoomable_sunburst": {
            "label": "Sunburst"
          },
          "tidy_tree": {
            "label": "Baum"
          },
          "treemap": {
            "label": "Baumkarte"
          }
        },
        "val": "zoomable_sunburst",
        "breakLine": true
      }
    }
  }
}