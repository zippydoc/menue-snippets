{
  "options_frame": {
    "type": "fieldset",
    "label": "Optionen",
    "repeat": false,
    "fields": {
      "options.group_by": {
        "type": "select",
        "label": "Gruppe von",
        "required": true,
        "options": {
          "Region": {
            "label": "Region"
          },
          "Income Group": {
            "label": "Einkommensgruppe"
          }
        },
        "val": "Region",
        "breakLine": true
      },
      "options.stat_var_name": {
        "type": "select",
        "label": "Statistische Daten",
        "required": true,
        "options": {
          "UnemploymentRate_Person": {
            "label": "Arbeitslosenquote"
          },
          "Count_Person_InLaborForce": {
            "label": "Erwerbstätige Bevölkerung"
          },
          "Count_Person_15To64Years_InLaborForce_AsFractionOf_Count_Person_15To64Years": {
            "label": "15 - 64 Jahre, in Arbeitskräften Bevölkerung pro Kopf 15 bis 64 Jahre"
          },
          "Count_Person_15To64Years_Female_InLaborForce_AsFractionOf_Count_Person_15To64Years_Female": {
            "label": "15 - 64 Jahre, in Arbeitskräften, weibliche Bevölkerung pro Kopf 15 bis 64 Jahre weiblich"
          },
          "Count_Person_15To64Years_Male_InLaborForce_AsFractionOf_Count_Person_15To64Years_Male": {
            "label": "15 - 64 Jahre, in Arbeitskräften, männliche Bevölkerung pro Kopf 15 bis 64 Jahre männlich"
          },
          "Count_Person_15OrMoreYears_InLaborForce_Female_AsFractionOf_Count_Person_InLaborForce": {
            "label": "15 Jahre oder mehr, erwerbstätig, weibliche Bevölkerung pro Kopf der Erwerbsbevölkerung"
          }
        },
        "val": "UnemploymentRate_Person"
      },
      "options.per_capita": {
        "type": "checkbox",
        "label": "Pro-Kopf",
        "show_if": "false",
        "val": "false",
        "isOn": "true",
        "isOff": "false",
        "breakLine": true
      },
      "options.all_countries": {
        "type": "checkbox",
        "label": "Alle Länder",
        "val": "true",
        "isOn": "true",
        "isOff": "false",
        "breakLine": true
      },
      "options.output_as_chart": {
        "type": "checkbox",
        "label": "Graph ausgeben",
        "val": "true",
        "isOn": "true",
        "isOff": "false"
      },
      "options.graph_type": {
        "type": "select",
        "label": "Diagrammtyp",
        "show_if": "options.output_as_chart=='true'",
        "required": true,
        "options": {
          "zoomable_sunburst": {
            "label": "Sunburst"
          },
          "treemap": {
            "label": "Baumkarte"
          }
        },
        "val": "zoomable_sunburst",
        "breakLine": true
      }
    }
  }
}