{
  "options_frame": {
    "type": "fieldset",
    "label": "Optionen",
    "repeat": false,
    "fields": {
      "options.all_countries": {
        "type": "hidden",
        "val": "false"
      },
      "options.stat_vars": {
        "type": "fieldset",
        "label": "Statistics data category(s)",
        "repeat": true,
        "fields": {
          "stat_var_name": {
            "type": "select",
            "label": "Statistische Daten",
            "required": true,
            "options": {
              "FertilityRate_Person_Female": {
                "label": "Fruchtbarkeitsrate"
              },
              "LifeExpectancy_Person": {
                "label": "Lebenserwartung"
              },
              "Count_BirthEvent_LiveBirth_AsFractionOf_Count_Person": {
                "label": "Anzahl der Lebendgeburten während des Jahres, pro Kopf"
              },
              "Count_Person_20To79Years_Diabetes_AsFractionOf_Count_Person_20To79Years": {
                "label": "Prozentualer Anteil der Personen im Alter von 20-79 Jahren, die an Diabetes Typ 1 oder Typ 2 leiden"
              },
              "Count_Death": {
                "label": "Zählung der Sterbefälle"
              },
              "Count_Person_15OrMoreYears_Smoking_AsFractionOf_Count_Person_15OrMoreYears": {
                "label": "15 Jahre oder mehr, rauchende Bevölkerung pro Kopf 15 oder mehr Jahre"
              },
              "Count_Person_Upto4Years_Overweight_AsFractionOf_Count_Person_Upto4Years": {
                "label": "4 Jahre oder weniger, übergewichtige Bevölkerung pro Kopf bis zu 4 Jahren"
              }
            }
          },
          "per_capita": {
            "type": "checkbox",
            "label": "Pro-Kopf",
            "show_if": "false",
            "val": "false",
            "isOn": "true",
            "isOff": "false",
            "breakLine": true
          },
          "delete": {
            "type": "button",
            "label": "Lösche",
            "function": "DELETE_ROW"
          }
        }
      },
      "options.output_as_chart": {
        "type": "checkbox",
        "label": "Graph ausgeben",
        "val": "true",
        "isOn": "true",
        "isOff": "false",
        "breakLine": true
      }
    }
  }
}