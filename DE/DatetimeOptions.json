{
  "options": {
    "type": "fieldset",
    "label": "Optionen auswählen",
    "repeat": false,
    "fields": {
      "options.convert_type": {
        "type": "select",
        "label": "Typ konvertieren",
        "required": true,
        "options": {
          "complete_date": {
            "label": "Vollständiges Datum"
          },
          "expand_date": {
            "label": "Datum erweitern"
          },
          "convert_date": {
            "label": "Datum umrechnen"
          },
          "convert_date_range": {
            "label": "Datumsbereich umrechnen"
          }
        },
        "val": "complete_date"
      },
      "options.date_convert_type": {
        "type": "select",
        "label": "Konvertieren in",
        "show_if": "options.convert_type=='convert_date'",
        "required": true,
        "options": {
          "begin_of_month": {
            "label": "Beginn des Monats"
          },
          "end_of_month": {
            "label": "Ende des Monats"
          },
          "hy_year": {
            "label": "HY Jahr"
          },
          "qrt_year": {
            "label": "Qrt Jahr"
          },
          "month_year": {
            "label": "Monat Jahr"
          },
          "week_year": {
            "label": "Woche Jahr"
          }
        },
        "val": "begin_of_month"
      },
      "options.date_range_convert_type": {
        "type": "select",
        "label": "Konvertieren in",
        "show_if": "options.convert_type=='convert_date_range'",
        "required": true,
        "options": {
          "begin_of_month": {
            "label": "Beginn des Monats"
          },
          "end_of_month": {
            "label": "Ende des Monats"
          },
          "begin_of_range": {
            "label": "Beginn des Bereichs"
          },
          "end_of_range": {
            "label": "Ende des Bereichs"
          },
          "begin_of_quarter": {
            "label": "Beginn des Quartals"
          },
          "end_of_quarter": {
            "label": "Ende des Quartals"
          }
        },
        "val": "begin_of_month",
        "breakLine": true
      },
      "options.output_in_new_column": {
        "type": "checkbox",
        "label": "Ausgabe in neuer Spalte",
        "val": "false",
        "isOn": "true",
        "isOff": "false"
      },
      "options.new_field_name": {
        "type": "text",
        "label": "Neuer Spaltenname",
        "show_if": "options.output_in_new_column=='true'",
        "val": "GeneratedDate",
        "required": true,
        "minLength": 1,
        "maxLength": 255,
        "breakLine": true
      }
    }
  }
}