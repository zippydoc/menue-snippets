{
  "output": {
    "type": "fieldset",
    "label": "Ausgabetabelle definieren",
    "show_if": "options.create_new_table=='true'",
    "repeat": false,
    "fields": {
      "output.custom_text": {
        "type": "text",
        "label": "Name der Ausgabetabelle",
        "show_if": "(output.set_options=='true' && output.compose_table_name=='false') || output.set_options=='false'",
        "minLength": 1,
        "val": "",
        "width": "100%",
        "breakLine": true
      },
      "output.select_output_columns": {
        "type": "checkbox",
        "label": "Ausgabespalten auswählen",
        "val": "false",
        "isOn": "true",
        "isOff": "false"
      },
      "output_columns": {
        "type": "fieldset",
        "label": "Select OutputColumns",
        "show_if": "output.select_output_columns=='true'",
        "repeat": true,
        "fields": {
          "field_name": {
            "type": "select",
            "label": "Spalte auswählen",
            "table": "input_table",
            "bind": "COLUMNS",
            "required": true
          },
          "order": {
            "type": "select",
            "label": "Bestellung",
            "options": {
              "-": {
                "label": "-"
              },
              "asc": {
                "label": "Asc"
              },
              "desc": {
                "label": "Desc"
              }
            },
            "val": "-"
          },
          "is_rename": {
            "type": "checkbox",
            "label": "Umbenennen",
            "val": "false",
            "isOn": "true",
            "isOff": "false"
          },
          "rename": {
            "show_if": "this.is_rename == 'true'",
            "type": "text",
            "required": true,
            "minLength": 1,
            "maxLength": 255,
            "label": "Neuer Spaltenname",
            "style": "margin-left: 10px"
          },
          "format__category": {
            "type": "select",
            "label": "Format",
            "required": true,
            "options": {
              "standard": {
                "label": "Standard"
              },
              "number": {
                "label": "Nummer"
              },
              "percentage": {
                "label": "Prozentsatz"
              },
              "text": {
                "label": "Text"
              },
              "date": {
                "label": "Datum"
              },
              "currency": {
                "label": "Währung"
              }
            },
            "val": "standard"
          },
          "format__rounding": {
            "type": "select",
            "label": "Abrunden",
            "show_if": "this.format__category=='number' || this.format__category=='percentage' || this.format__category=='currency'",
            "required": true,
            "options": {
              "no_rounding": {
                "label": "Keine Rundung"
              },
              "0": {
                "label": "0 Ziffern"
              },
              "1": {
                "label": "1 Ziffern"
              },
              "2": {
                "label": "2 Ziffern"
              },
              "3": {
                "label": "3 Ziffern"
              },
              "4": {
                "label": "4 Ziffern"
              },
              "5": {
                "label": "5 Ziffern"
              },
              "6": {
                "label": "6 Ziffern"
              },
              "7": {
                "label": "7 Ziffern"
              },
              "8": {
                "label": "8 Ziffern"
              },
              "9": {
                "label": "9 Ziffern"
              }
            },
            "val": "2"
          },
          "format__use_1000_separator": {
            "type": "checkbox",
            "label": "1000 Trennzeichen(.) verwenden",
            "show_if": "this.format__category=='number' || this.format__category=='percentage' || this.format__category=='currency'",
            "val": "true",
            "isOn": "true",
            "isOff": "false"
          },
          "format__currency_symbol": {
            "type": "select",
            "label": "Symbol",
            "show_if": "this.format__category=='currency'",
            "required": true,
            "options": {
              "dollar": {
                "label": "$"
              },
              "euro": {
                "label": "€"
              },
              "pound": {
                "label": "£"
              }
            },
            "val": "dollar"
          },
          "format__date_format": {
            "type": "select",
            "label": "Typ",
            "show_if": "this.format__category=='date'",
            "required": true,
            "options": {
              "mm/dd/yyyy": {
                "label": "3/14/2012"
              },
              "D, M dd, Y": {
                "label": "Wednesday, March 14, 2012"
              },
              "yyyy-mm-dd": {
                "label": "2012-03-14"
              }
            },
            "val": "mm/dd/yyyy",
            "breakLine": true
          },
          "delete": {
            "type": "button",
            "label": "Lösche",
            "function": "DELETE_ROW"
          }
        },
        "breakLine": true,
        "style": "margin-bottom: 20px"
      },
      "output.set_options": {
        "type": "checkbox",
        "label": "Ausgabe-Optionen",
        "val": "false",
        "isOn": "true",
        "isOff": "false"
      },
      "output_options_box": {
        "type": "fieldset",
        "label": "Ausgabe-Optionen",
        "show_if": "output.set_options=='true'",
        "repeat": false,
        "fields": {
          "output.compose_table_name": {
            "type": "checkbox",
            "label": "Tabellenname zusammenstellen",
            "val": "false",
            "isOn": "true",
            "isOff": "false",
            "breakLine": true
          },
          "compose_table_name_frame": {
            "type": "fieldset",
            "label": "Name der Ausgabetabelle zusammenstellen",
            "show_if": "output.compose_table_name=='true'",
            "repeat": false,
            "fields": {
              "output.prefix_rule": {
                "type": "select",
                "label": "Präfix",
                "show_if": "output.compose_table_name=='true'",
                "val": "empty",
                "options": {
                  "empty": {
                    "label": "-"
                  },
                  "script_name": {
                    "label": "Aufgabe Name"
                  },
                  "date_string_type1": {
                    "label": "Datum: tt_mm_jjjj"
                  },
                  "date_string_type2": {
                    "label": "Datum: jjjj_mm_tt"
                  },
                  "time_string": {
                    "label": "Zeit: h_min_sec"
                  },
                  "datetime_string": {
                    "label": "Datum_Zeit"
                  },
                  "custom": {
                    "label": "Benutzerdefiniert"
                  }
                }
              },
              "output.prefix_text": {
                "type": "text",
                "label": "Präfix Text",
                "show_if": "output.compose_table_name=='true' && output.prefix_rule=='custom'",
                "minLength": 1,
                "maxLength": 31,
                "val": ""
              },
              "output.core_delimiter": {
                "type": "text",
                "label": "Begrenzer",
                "show_if": "output.compose_table_name=='true' && output.prefix_rule!='empty'",
                "val": "_",
                "width": "50px"
              },
              "output.core_rule": {
                "type": "select",
                "label": "Tabelle Name",
                "show_if": "output.compose_table_name=='true'",
                "val": "input_table_name",
                "options": {
                  "input_table_name": {
                    "label": "Name der Eingangstabelle"
                  },
                  "script_name": {
                    "label": "Aufgabe Name"
                  },
                  "custom": {
                    "label": "Benutzerdefiniert"
                  }
                }
              },
              "output.core_text": {
                "type": "text",
                "label": "Haupttabelle Name",
                "show_if": "output.compose_table_name=='true' && output.core_rule=='custom'",
                "required": true,
                "minLength": 1,
                "maxLength": 31,
                "val": ""
              },
              "output.suffix_delimiter": {
                "type": "text",
                "label": "Begrenzer",
                "show_if": "output.compose_table_name=='true'",
                "val": "_",
                "width": "50px"
              },
              "output.suffix_rule": {
                "type": "select",
                "label": "Suffix",
                "show_if": "output.compose_table_name=='true'",
                "val": "script_name",
                "options": {
                  "empty": {
                    "label": "-"
                  },
                  "script_name": {
                    "label": "Aufgabe Name"
                  },
                  "date_string_type1": {
                    "label": "Datum: tt_mm_jjjj"
                  },
                  "date_string_type2": {
                    "label": "Datum: jjjj_mm_tt"
                  },
                  "time_string": {
                    "label": "Zeit: h_min_sec"
                  },
                  "datetime_string": {
                    "label": "Datum_Zeit"
                  },
                  "custom": {
                    "label": "Benutzerdefiniert"
                  }
                }
              },
              "output.suffix_text": {
                "type": "text",
                "label": "Suffix Text",
                "show_if": "output.compose_table_name=='true' && output.suffix_rule=='custom'",
                "minLength": 1,
                "maxLength": 31,
                "val": "",
                "breakLine": true
              },
              "output.timezone": {
                "type": "select",
                "label": "Zeitzone",
                "show_if": "output.compose_table_name=='true' && (output.prefix_rule=='date_string_type1' || output.prefix_rule=='date_string_type2' || output.prefix_rule=='time_string' || output.prefix_rule=='datetime_string' || output.suffix_rule=='date_string_type1' || output.suffix_rule=='date_string_type2' || output.suffix_rule=='time_string' || output.suffix_rule=='datetime_string')",
                "val": "CET",
                "required": true,
                "options": {
                  "NZDT": {
                    "label": "NZDT(Neuseeländische Sommerzeit)"
                  },
                  "IDLE": {
                    "label": "IDLE(International Date Line, East)"
                  },
                  "NZST": {
                    "label": "NZST(Neuseeländische Standardzeit)"
                  },
                  "NZT": {
                    "label": "NZT(Neuseeländische Zeit)"
                  },
