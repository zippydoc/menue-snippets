                "required": true,
                "options": {
                  "ENTHÄLT": {
                    "label": "ENTHÄLT"
                  },
                  "CONTAINED IN STRING": {
                    "label": "ENTHALTEN IN"
                  },
                  "STARTET MIT": {
                    "label": "STARTET MIT"
                  },
                  "ENDET MIT": {
                    "label": "ENDET MIT"
                  },
                  "=": {
                    "label": "EQUAL"
                  },
                  ">": {
                    "label": "GRÖSSER ALS"
                  },
                  ">=": {
                    "label": "GRÖSSER ODER GLEICH"
                  },
                  "<": {
                    "label": "KLEINER ALS"
                  },
                  "<=": {
                    "label": "KLEINER ODER GLEICH"
                  },
                  "EMPTY": {
                    "label": "IST LEER"
                  },
                  "BLANK": {
                    "label": "IST BLANK"
                  },
                  "IS NULL": {
                    "label": "IS NULL"
