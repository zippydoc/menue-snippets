                  },
                  "or": {
                    "label": "OR"
                  },
                  "and_not": {
                    "label": "UND NICHT"
                  },
                  "or_not": {
                    "label": "ODER NICHT"
                  }
                },
                "val": "and"
              },
              "column": {
                "type": "select",
                "label": "Spalte auswählen",
                "table": "options.lookup_table_id",
                "bind": "COLUMNS",
                "required": true
              },
              "condition": {
