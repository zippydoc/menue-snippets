{
  "options": {
    "type": "fieldset",
    "label": "Optionen auswählen",
    "repeat": false,
    "fields": {
      "options.task": {
        "type": "hidden",
        "val": "find_duplicates"
      },
      "options.task_type": {
        "type": "hidden",
        "val": "label_duplicates"
      },
      "options.pattern_type": {
        "type": "select",
        "label": "Muster Typ",
        "show_if": "options.task_type==''generate_lookup_table'' || options.task_type==''apply_lookup_table'' || options.task_type==''apply_lookup_table_to_cells''",
        "required": true,
        "options": {
          "duplicate_pattern": {
            "label": "Wort-Muster"
          },
          "number_range": {
            "label": "Bereiche (z. B. 10 < x <= 100)"
          }
        },
        "val": "duplicate_pattern"
      },
      "duplicate_option_box": {
        "type": "fieldset",
        "label": "Option Duplizieren von Mustern",
        "show_if": "options.task_type==''label_duplicates'' || (options.task_type==''generate_lookup_table'' && options.pattern_type==''duplicate_pattern'')",
        "repeat": false,
        "fields": {
          "options.minimum_number_of_matching_characters": {
            "type": "text",
            "label": "Mindestanzahl übereinstimmender Zeichen",
            "val": "3",
            "required": true,
            "style": "min-width: 240px"
          },
          "options.minimum_number_of_duplicates": {
            "type": "text",
            "label": "Minimale Anzahl von Duplikaten",
            "val": "2",
            "required": true,
            "style": "min-width: 240px",
            "breakLine": true
          }
        }
      },
      "number_range_option_box": {
        "type": "fieldset",
        "label": "Option Nummernkreise",
        "show_if": "options.task_type==''generate_lookup_table'' && options.pattern_type==''number_range''",
        "repeat": false,
        "fields": {
          "options.number_range.range_type": {
            "type": "select",
            "label": "Bereich Typ",
            "required": true,
            "options": {
              "equal_interval": {
                "label": "N Gleiche Intervalle"
              },
              "equal_range": {
                "label": "Gleicher Bereich"
              },
              "custom_range": {
                "label": "Benutzerdefinierter Bereich"
              }
            },
            "val": "equal_interval"
          },
          "options.number_range.n_value": {
            "type": "text",
            "label": "N",
            "show_if": "options.number_range.range_type==''equal_interval''",
            "val": "3",
            "required": true
          },
          "options.number_range.range_value": {
            "type": "text",
            "label": "Bereich",
            "show_if": "options.number_range.range_type==''equal_range''",
            "val": "10",
            "required": true
          },
          "options.number_range.custom_ranges": {
            "type": "fieldset",
            "label": "Benutzerdefiniertes Bereichsmuster hinzufügen",
            "show_if": "options.number_range.range_type==''custom_range''",
            "repeat": true,
            "fields": {
              "pattern": {
                "type": "text",
                "label": "Muster",
                "val": "0<=x<10",
                "required": true
              },
              "label": {
                "type": "text",
                "label": "Beschrifte",
                "val": "positive10",
                "required": true
              },
              "delete": {
                "type": "button",
                "label": "Lösche",
                "function": "DELETE_ROW"
              }
            }
          }
        },
        "breakLine": true
      },
      "options.lookup_table_id": {
        "type": "select",
        "label": "Nachschlagetabelle",
        "show_if": "options.task_type==''apply_lookup_table'' || options.task_type==''apply_regex_table'' || options.task_type==''apply_lookup_table_to_cells'' || options.task_type==''apply_regex_table_to_cells''",
        "bind": "TABLES",
        "empty": "",
        "required": true
      },
      "options.partial_match": {
        "type": "checkbox",
        "label": "Teilweise Übereinstimmung",
        "show_if": "options.pattern_type==''duplicate_pattern'' && (options.task_type==''apply_lookup_table'' || options.task_type==''apply_lookup_table_to_cells'')",
        "val": "true",
        "isOn": "true",
        "isOff": "false"
      },
      "options.case_sensitive": {
        "type": "checkbox",
        "label": "Gehäuse empfindlich",
        "show_if": "options.pattern_type==''duplicate_pattern'' && (options.task_type==''apply_lookup_table'' || options.task_type==''apply_lookup_table_to_cells'')",
        "val": "false",
        "isOn": "true",
        "isOff": "false",
        "breakLine": true
      },
      "options.use_delimiter": {
        "type": "checkbox",
        "label": "Benutzerdefinierte Begrenzer",
        "show_if": "options.task_type==''apply_lookup_table'' || options.task_type==''apply_lookup_table_to_cells''",
        "val": "false",
        "isOn": "true",
        "isOff": "false"
      },
      "options.delimiter": {
        "type": "text",
        "label": "Eingabe-Begrenzer",
        "show_if": "options.use_delimiter==''true'' && (options.task_type==''apply_lookup_table'' || options.task_type==''apply_lookup_table_to_cells'')",
        "val": ";",
        "required": true,
        "breakLine": true
      },
      "options.input_table.use_filter": {
        "type": "checkbox",
        "label": "Filter für Eingangstabelle verwenden",
        "show_if": "options.task_type==''apply_lookup_table'' || options.task_type==''apply_regex_table'' || options.task_type==''apply_lookup_table_to_cells'' || options.task_type==''apply_regex_table_to_cells''",
        "val": "false",
        "isOn": "true",
        "isOff": "false",
        "breakLine": true
      },
      "input_table_filter_box": {
        "type": "fieldset",
        "label": "Filteroptionen der Eingabetabelle",
        "show_if": "options.input_table.use_filter==''true'' && (options.task_type==''apply_lookup_table'' || options.task_type==''apply_regex_table'' || options.task_type==''apply_lookup_table_to_cells'' || options.task_type==''apply_regex_table_to_cells'')",
        "repeat": false,
        "fields": {
          "options.input_table.filter": {
            "type": "fieldset",
            "label": "Filterbedingungen für Eingangstabelle hinzufügen",
            "repeat": true,
            "fields": {
              "initial_op": {
                "type": "select",
                "label": "Logik-Operator auswählen",
                "required": true,
                "show_if": "$index == 0",
                "options": {
                  "if": {
                    "label": "IF"
                  },
                  "if_not": {
                    "label": "WENN NICHT"
                  }
                },
                "val": "if"
              },
              "op": {
                "type": "select",
                "label": "Logik-Operator auswählen",
                "required": true,
                "show_if": "$index > 0",
                "options": {
                  "and": {
                    "label": "UND"
                  },
                  "or": {
                    "label": "OR"
                  },
                  "and_not": {
                    "label": "UND NICHT"
                  },
                  "or_not": {
                    "label": "ODER NICHT"
                  }
                },
                "val": "and"
              },
              "column": {
                "type": "select",
                "label": "Spalte auswählen",
                "table": "input_table",
                "bind": "COLUMNS",
                "required": true
              },
              "condition": {
                "type": "select",
                "label": "Bediener auswählen",
                "required": true,
                "options": {
                  "ENTHÄLT": {
                    "label": "ENTHÄLT"
                  },
                  "CONTAINED IN STRING": {
                    "label": "ENTHALTEN IN"
                  },
                  "STARTET MIT": {
                    "label": "STARTET MIT"
                  },
                  "ENDET MIT": {
                    "label": "ENDET MIT"
                  },
                  "=": {
                    "label": "EQUAL"
                  },
                  ">": {
                    "label": "GRÖSSER ALS"
                  },
                  ">=": {
                    "label": "GRÖSSER ODER GLEICH"
                  },
                  "<": {
                    "label": "KLEINER ALS"
                  },
                  "<=": {
                    "label": "KLEINER ODER GLEICH"
                  },
                  "EMPTY": {
                    "label": "IST LEER"
                  },
                  "BLANK": {
                    "label": "IST BLANK"
                  },
                  "IS NULL": {
                    "label": "IS NULL"
                  },
                  "REGEX-MATCH": {
                    "label": "REGEX-MATCH"
                  },
                  "CONTAINED IN TABLE": {
                    "label": "IN TABELLE"
                  },
                  "IN REGEX-TABELLE": {
                    "label": "IN REGEX-TABELLE"
                  },
                  "DATUM": {
                    "label": "DATUM"
                  }
                },
                "val": "ENTHÄLT"
              },
              "date_condition": {
                "type": "select",
                "label": "Datumsoperator auswählen",
                "required": true,
                "show_if": "this.condition == ''DATE''",
                "options": {
                  "<": {
                    "label": "PRIOR"
                  },
                  "<=": {
                    "label": "PRIOR ODER GLEICH"
                  },
                  "=": {
                    "label": "EQUAL"
                  },
                  ">": {
                    "label": "NACH"
                  },
                  ">=": {
                    "label": "NACHHER ODER GLEICH"
                  }
                },
                "val": "="
              },
              "case_sensitive": {
                "type": "select",
                "label": "Wählen Sie Case Sensitive",
                "required": true,
                "show_if": "this.condition == ''CONTAINS'' || this.condition == ''CONTAINED IN STRING'' || this.condition == ''='' || this.condition == ''STARTS WITH'' || this.condition == ''ENDS WITH'' || this.condition == ''REGEX MATCH'' || this.condition == ''NOT REGEX MATCH'' || this.condition == ''IN REGEX TABLE'' || this.condition == ''NOT IN REGEX TABLE''",
                "options": {
                  "yes": {
                    "label": "Gehäuse empfindlich"
                  },
                  "no": {
                    "label": "Groß-/Kleinschreibung nicht beachten"
                  }
                },
                "val": "yes"
              },
              "date_value": {
                "type": "date",
                "label": "Datum auswählen",
                "show_if": "this.condition==''DATE''",
                "val": "1/31/2020",
                "required": true
              },
              "value": {
                "type": "text",
                "label": "Eingangswert",
                "show_if": "this.condition != ''DATE'' && this.condition != ''EMPTY'' && this.condition != ''NOT EMPTY'' && this.condition != ''BLANK'' && this.condition != ''NOT BLANK'' && this.condition != ''IS NULL'' && this.condition != ''IS NOT NULL'' && this.condition != ''CONTAINED IN TABLE'' && this.condition != ''NOT CONTAINED IN TABLE'' && this.condition != ''IN REGEX TABLE'' && this.condition != ''NOT IN REGEX TABLE''",
                "val": "",
                "required": true
              },
              "compare_table": {
                "type": "select",
                "label": "Tabelle auswählen",
                "bind": "TABLES",
                "empty": "",
                "show_if": "this.condition == ''CONTAINED IN TABLE'' || this.condition == ''NOT CONTAINED IN TABLE'' || this.condition == ''IN REGEX TABLE'' || this.condition == ''NOT IN REGEX TABLE''",
                "required": true
              },
              "compare_column": {
                "type": "select",
                "label": "Spalte auswählen",
                "table": "this.compare_table",
                "bind": "COLUMNS",
                "show_if": "this.condition == ''CONTAINED IN TABLE'' || this.condition == ''NOT CONTAINED IN TABLE'' || this.condition == ''IN REGEX TABLE'' || this.condition == ''NOT IN REGEX TABLE''",
                "required": true
              },
              "delete": {
                "type": "button",
                "label": "Lösche",
                "function": "DELETE_ROW"
              }
            }
          },
          "options.input_table.apply_to_record": {
            "type": "checkbox",
            "label": "Anwenden auf Datensatz für Eingangstabellenfilter",
            "val": "true",
            "isOn": "true",
            "isOff": "false",
            "style": "margin-top: 10px",
            "breakLine": true
          },
          "input_table_apply_to_group_box": {
            "type": "fieldset",
            "label": "Auf die Gruppe der Eingangstabelle anwenden",
            "show_if": "options.input_table.apply_to_record==''false''",
            "repeat": false,
            "fields": {
              "options.input_table.apply_to_group_columns": {
                "type": "select",
                "label": "Gruppenspalten auswählen",
                "required": true,
                "width": "100%",
                "table": "input_table",
                "multiple": true,
                "bind": "COLUMNS",
                "breakLine": true
              }
            }
          }
        }
      },
      "options.lookup_table.use_filter": {
        "type": "checkbox",
        "label": "Filter für Lookup-Tabelle verwenden",
        "show_if": "options.task_type==''apply_lookup_table'' || options.task_type==''apply_regex_table'' || options.task_type==''apply_lookup_table_to_cells'' || options.task_type==''apply_regex_table_to_cells''",
        "val": "false",
        "isOn": "true",
        "isOff": "false",
        "style": "margin-top: 10px",
        "breakLine": true
      },
