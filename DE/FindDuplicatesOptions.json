      "options.output_type": {
        "type": "select",
        "label": "Output Type",
        "required": true,
        "options": {
          "list_all_duplicates_once": {
            "label": "List all duplicates once"
          },
          "list_found_duplicates_per_item": {
            "label": "List found duplicates per item"
          }
        },
        "val": "list_all_duplicates_once"
      }
