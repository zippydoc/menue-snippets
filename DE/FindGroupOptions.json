{
  "options": {
    "type": "fieldset",
    "label": "Find Group Options",
    "repeat": false,
    "fields": {
      "group_column": {
        "type": "select",
        "label": "Wähle Gruppen-Spalte",
        "required": true,
        "table": "input_table",
        "bind": "COLUMNS",
        "multiple": false,
        "breakLine": true
      },
      "item_column": {
        "type": "select",
        "label": "Wähle Gruppenelements-Spalte",
        "required": true,
        "table": "input_table",
        "bind": "COLUMNS",
        "multiple": false,
        "breakLine": true
      },
      "min_group_count": {
        "type": "number",
        "label": "Min Group Count",
        "required": true,
        "val": "3",
        "breakLine": true
      },
      "min_group_size": {
        "type": "number",
        "label": "Min Group Size",
        "required": true,
        "val": "1",
        "breakLine": true
      },
      "ordered": {
        "type": "select",
        "label": "Orderd / Unordered",
        "required": true,
        "options": {
          "ordered": {
            "label": "Ordered"
          },
          "unordered": {
            "label": "Unordered"
          }
        },
        "val": "unordered",
        "breakLine": true
      },
      "output_type": {
        "type": "select",
        "label": "Ausgabetyp",
        "required": true,
        "options": {
          "item_group_list": {
            "label": "Liste alle Gruppen"
          },
          "item_group_column": {
            "label": "Ordne Gruppen zu"
          }
        },
        "val": "item_group_list"
      }
    }
  }
}