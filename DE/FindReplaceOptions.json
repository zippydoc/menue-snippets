{
  "options": {
    "type": "fieldset",
    "label": "Optionen spezifizieren",
    "repeat": false,
    "fields": {
      "incoming_value_to_replace.type": {
        "type": "select",
        "label": "Eingehender Wert",
        "options": {
          "user_specified_value": {
            "label": "Entspricht"
          },
          "begins_with": {
            "label": "Beginnt mit"
          },
          "ends_with": {
            "label": "Endet mit"
          },
          "empty": {
            "label": "Leer (NULL oder Leer)"
          },
          "regex": {
            "label": "Regulärer Ausdruck"
          }
        },
        "val": "user_specified_value",
        "style": "min-width: 190px"
      },
      "incoming_value_to_replace.value": {
        "type": "text",
        "label": "Eingehender Wert zum Ersetzen",
        "required": true,
        "show_if": "incoming_value_to_replace.type=='user_specified_value' || incoming_value_to_replace.type=='begins_with' || incoming_value_to_replace.type=='ends_with'",
        "trim": "false",
        "val": "",
        "style": "min-width: 170px"
      },
      "incoming_value_to_replace.regex": {
        "type": "text",
        "label": "Regulärer Ausdruck",
        "required": true,
        "show_if": "incoming_value_to_replace.type=='regex'",
        "val": "(.*),(.*),(.*)\\s(\\d{5})(-\\d{4})?",
        "style": "min-width: 200px"
      },
      "replace_table.table_id": {
        "type": "select",
        "label": "Ersatztabelle",
        "show_if": "incoming_value_to_replace.type=='replacement_table'",
        "bind": "TABLES",
        "required": true
      },
      "replace_table.find_column": {
        "type": "select",
        "label": "Wählen Sie Spalte suchen",
        "show_if": "incoming_value_to_replace.type=='replacement_table'",
        "table": "replace_table.table_id",
        "bind": "COLUMNS",
        "required": true
      },
      "replace_table.replace_with_column": {
        "type": "select",
        "label": "Wählen Sie Ersetzen durch Spalte",
        "show_if": "incoming_value_to_replace.type=='replacement_table'",
        "table": "replace_table.table_id",
        "bind": "COLUMNS",
        "required": true
      },
      "incoming_value_to_replace.partial_match": {
        "type": "select",
        "label": "Teilweise Übereinstimmung",
        "show_if": "incoming_value_to_replace.type=='user_specified_value' || incoming_value_to_replace.type=='replacement_table'",
        "options": {
          "full_match": {
            "label": "Vollständige Übereinstimmung"
          },
          "full_and_partial_match": {
            "label": "Vollständige und teilweise Übereinstimmung"
          },
          "partial_match_only": {
            "label": "Nur partielle Übereinstimmung"
          }
        },
        "val": "full_and_partial_match"
      },
      "replace_with.value_type": {
        "type": "select",
        "label": "Ersetzen durch",
        "show_if": "incoming_value_to_replace.type!='regex' && incoming_value_to_replace.type!='replacement_table'",
        "options": {
          "user_specified_value": {
            "label": "Benutzerdefinierter Wert"
          },
          "average": {
            "label": "Spaltendurchschnitt"
          },
          "median": {
            "label": "Spalte Median"
          },
          "mode": {
            "label": "Spaltenmodus"
          },
          "sum": {
            "label": "Spaltensumme"
          },
          "table": {
            "label": "Tabellenspalte"
          }
        },
        "val": "user_specified_value",
        "style": "min-width: 190px"
      },
      "replace_with.value": {
        "type": "text",
        "label": "Ersetzen durch Wert",
        "show_if": "incoming_value_to_replace.type!='regex' && replace_with.value_type=='user_specified_value' && incoming_value_to_replace.type!='replacement_table'",
        "trim": "false",
        "val": ""
      },
      "replace_with.regex_value": {
        "type": "text",
        "label": "Ersetzen durch Wert",
        "show_if": "incoming_value_to_replace.type=='regex' && incoming_value_to_replace.type!='replacement_table'",
        "val": "\\4: \\2, \\3"
      },
      "replace_with.table_id": {
        "type": "select",
        "label": "Tabelle auswählen",
        "show_if": "incoming_value_to_replace.type!='regex' && replace_with.value_type=='table' && incoming_value_to_replace.type!='replacement_table'",
        "bind": "TABLES",
        "required": true
      },
      "replace_with.column": {
        "type": "select",
        "label": "Spalte auswählen",
        "show_if": "incoming_value_to_replace.type!='regex' && replace_with.value_type=='table' && incoming_value_to_replace.type!='replacement_table'",
        "table": "replace_with.table_id",
        "bind": "COLUMNS",
        "required": true,
        "breakLine": true
      },
      "replacement_table_notice": {
        "type": "text",
        "label": "",
        "show_if": "incoming_value_to_replace.type=='replacement_table'",
        "val": "Line-break will be ignored in replacement table",
        "disabled": true,
        "width": "100%"
      },
      "use_filter": {
        "type": "checkbox",
        "label": "Filter verwenden",
        "val": "false",
        "isOn": "true",
        "isOff": "false",
        "breakLine": true
      },
      "filter_options_box": {
        "type": "fieldset",
        "label": "Filter-Optionen",
        "show_if": "use_filter=='true'",
        "repeat": false,
        "fields": {
          "filter": {
            "type": "fieldset",
            "label": "Filterbedingungen hinzufügen",
            "show_if": "use_filter=='true'",
            "repeat": true,
            "fields": {
              "initial_op": {
                "type": "select",
                "label": "Logik-Operator auswählen",
                "required": true,
                "show_if" :  "$index == 0",
                "options": {
                  "if": {
                    "label": "IF"
                  },
                  "if_not": {
                    "label": "WENN NICHT"
                  }
                },
                "val": "if"
              },
              "op": {
                "type": "select",
                "label": "Logik-Operator auswählen",
                "required": true,
                "show_if" :  "$index > 0",
                "options": {
                  "and": {
                    "label": "UND"
                  },
                  "or": {
                    "label": "OR"
                  },
                  "and_not": {
                    "label": "UND NICHT"
                  },
                  "or_not": {
                    "label": "ODER NICHT"
                  }
                },
                "val": "and"
              },
              "column": {
                "type": "select",
                "label": "Spalte auswählen",
                "table": "table_id",
                "bind": "COLUMNS",
                "required": true
              },
              "condition": {
                "type": "select",
                "label": "Bediener auswählen",
                "required": true,
                "options": {
                  "CONTAINS": {
                    "label": "ENTHÄLT"
                  },
                  "CONTAINED IN STRING": {
                    "label": "ENTHALTEN IN"
                  },
                  "STARTS WITH": {
                    "label": "STARTET MIT"
                  },
                  "ENDS WITH": {
                    "label": "ENDET MIT"
                  },
                  "=": {
                    "label": "EQUAL"
                  },
                  ">": {
                    "label": "GRÖSSER ALS"
                  },
                  ">=": {
                    "label": "GRÖSSER ODER GLEICH"
                  },
                  "<": {
                    "label": "KLEINER ALS"
                  },
                  "<=": {
                    "label": "KLEINER ODER GLEICH"
                  },
                  "EMPTY": {
                    "label": "IST LEER"
                  },
                  "BLANK": {
                    "label": "IST BLANK"
                  },
                  "IS NULL": {
                    "label": "IS NULL"
                  },
                  "IS TRUE": {
                    "label": "IST WAHR"
                  },
                  "IS FALSE": {
                    "label": "IST FALSCH"
                  },
                  "REGEX MATCH": {
                    "label": "REGEX-MATCH"
                  },
                  "CONTAINED IN TABLE": {
                    "label": "IN TABELLE"
                  },
                  "IN REGEX TABLE": {
                    "label": "IN REGEX-TABELLE"
                  },
                  "DATE": {
                    "label": "DATUM"
                  }
                },
                "val": "CONTAINS"
              },
              "date_condition": {
                "type": "select",
                "label": "Datumsoperator auswählen",
                "required": true,
                "show_if": "this.condition == 'DATE'",
                "options": {
                  "<": {
                    "label": "PRIOR"
                  },
                  "<=": {
                    "label": "PRIOR ODER GLEICH"
                  },
                  "=": {
                    "label": "EQUAL"
                  },
                  ">": {
                    "label": "NACH"
                  },
                  ">=": {
                    "label": "NACHHER ODER GLEICH"
                  }
                },
                "val": "="
              },
              "compare_with": {
                "type": "select",
                "label": "Compare with",
                "required": true,
                "show_if": "this.condition=='CONTAINS' || this.condition=='CONTAINED IN STRING' || this.condition=='STARTS WITH' || this.condition=='ENDS WITH' || this.condition=='=' || this.condition=='>' || this.condition=='>=' || this.condition=='<' || this.condition=='<='",
                "options": {
                  "custom_value": {
                    "label": "Custom value"
                  },
                  "column_value": {
                    "label": "Column value"
                  }
                },
                "val": "custom_value"
              },
              "self_compare_column": {
                "type": "select",
                "label": "Select Compare Column",
                "table": "table_id",
                "bind": "COLUMNS",
                "show_if": "this.compare_with=='column_value' && (this.condition=='CONTAINS' || this.condition=='CONTAINED IN STRING' || this.condition=='STARTS WITH' || this.condition=='ENDS WITH' || this.condition=='=' || this.condition=='>' || this.condition=='>=' || this.condition=='<' || this.condition=='<=')",
                "required": true
              },
              "date_value": {
                "type": "date",
                "label": "Datum auswählen",
                "show_if": "this.condition=='DATE'",
                "val": "",
                "required": true
              },
              "value": {
                "type": "text",
                "label": "Eingangswert",
                "show_if": "this.compare_with=='custom_value' && this.condition != 'DATE' && this.condition != 'EMPTY' && this.condition != 'NOT EMPTY' && this.condition != 'BLANK' && this.condition != 'NOT BLANK' && this.condition != 'IS NULL' && this.condition != 'IS NOT NULL' && this.condition != 'IS TRUE' && this.condition != 'IS FALSE' && this.condition != 'CONTAINED IN TABLE' && this.condition != 'NOT CONTAINED IN TABLE' && this.condition != 'IN REGEX TABLE' && this.condition != 'NOT IN REGEX TABLE'",
                "val": "",
                "required": true
              },
              "case_sensitive": {
                "type": "select",
                "label": "Wählen Sie Case Sensitive",
                "required": true,
                "show_if": "this.condition == 'CONTAINS' || this.condition == 'CONTAINED IN STRING' || this.condition == '=' || this.condition == 'STARTS WITH' || this.condition == 'ENDS WITH' || this.condition == 'REGEX MATCH' || this.condition == 'NOT REGEX MATCH' || this.condition == 'IN REGEX TABLE' || this.condition == 'NOT IN REGEX TABLE'",
                "options": {
                  "yes": {
                    "label": "Gehäuse empfindlich"
                  },
                  "no": {
                    "label": "Groß-/Kleinschreibung nicht beachten"
                  }
                },
                "val": "yes"
              },
              "compare_table_id": {
                "type": "select",
                "label": "Tabelle auswählen",
                "bind": "TABLES",
                "empty": "",
                "show_if": "this.condition == 'CONTAINED IN TABLE' || this.condition == 'NOT CONTAINED IN TABLE' || this.condition == 'IN REGEX TABLE' || this.condition == 'NOT IN REGEX TABLE'",
                "required": true
              },
              "compare_column": {
                "type": "select",
                "label": "Spalte auswählen",
                "table": "this.compare_table_id",
                "bind": "COLUMNS",
                "show_if": "this.condition == 'CONTAINED IN TABLE' || this.condition == 'NOT CONTAINED IN TABLE' || this.condition == 'IN REGEX TABLE' || this.condition == 'NOT IN REGEX TABLE'",
                "required": true
              },
              "delete": {
                "type": "button",
                "label": "Lösche",
                "function": "DELETE_ROW"
              }
            }
          },
          "apply_to_record": {
            "type": "checkbox",
            "label": "Auf den Datensatz anwenden",
            "val": "true",
            "isOn": "true",
            "isOff": "false",
            "style": "margin-top: 10px",
            "breakLine": true
          },
          "apply_to_group_box": {
            "type": "fieldset",
            "label": "Anwenden auf Gruppe",
            "show_if": "apply_to_record=='false'",
            "repeat": false,
            "fields": {
              "apply_to_group_columns": {
                "type": "select",
                "label": "Gruppenspalten auswählen",
                "required": true,
                "width": "100%",
                "table": "table_id",
                "multiple": true,
                "bind": "COLUMNS",
                "breakLine": true
              }
            }
          }
        }
      },
      "use_replace_table_filter": {
        "type": "checkbox",
        "label": "Use Filter on \"Replace With\" Table",
        "show_if": "incoming_value_to_replace.type=='replacement_table' || (incoming_value_to_replace.type!='regex' && replace_with.value_type=='table')",
        "val": "false",
        "isOn": "true",
        "isOff": "false",
        "style": "margin-top: 10px",
        "breakLine": true
      },
      "replace_table_filter_option_box": {
        "type": "fieldset",
        "label": "\"Replace With\" Table Filter Options",
        "show_if": "use_replace_table_filter=='true' && (incoming_value_to_replace.type=='replacement_table' || (incoming_value_to_replace.type!='regex' && replace_with.value_type=='table'))",
        "repeat": false,
        "fields": {
          "replace_table_filter_option.filter": {
            "type": "fieldset",
            "label": "Add Filter Conditions on \"Replace With\" Table",
            "repeat": true,
            "fields": {
              "initial_op": {
                "type": "select",
                "label": "Logik-Operator auswählen",
                "required": true,
                "show_if" :  "$index == 0",
                "options": {
                  "if": {
                    "label": "IF"
                  },
                  "if_not": {
                    "label": "WENN NICHT"
                  }
                },
                "val": "if"
              },
              "op": {
                "type": "select",
                "label": "Logik-Operator auswählen",
                "required": true,
                "show_if" :  "$index > 0",
                "options": {
                  "and": {
                    "label": "UND"
                  },
                  "or": {
                    "label": "OR"
                  },
                  "and_not": {
                    "label": "UND NICHT"
                  },
                  "or_not": {
                    "label": "ODER NICHT"
                  }
                },
                "val": "and"
              },
              "column": {
                "type": "select",
                "label": "Spalte auswählen",
                "table": "replace_with.table_id",
                "bind": "COLUMNS",
                "required": true
              },
              "condition": {
                "type": "select",
                "label": "Bediener auswählen",
                "required": true,
                "options": {
                  "CONTAINS": {
                    "label": "ENTHÄLT"
                  },
                  "CONTAINED IN STRING": {
                    "label": "ENTHALTEN IN"
                  },
                  "STARTS WITH": {
                    "label": "STARTET MIT"
                  },
                  "ENDS WITH": {
                    "label": "ENDET MIT"
                  },
                  "=": {
                    "label": "EQUAL"
                  },
                  ">": {
                    "label": "GRÖSSER ALS"
                  },
                  ">=": {
                    "label": "GRÖSSER ODER GLEICH"
                  },
                  "<": {
                    "label": "KLEINER ALS"
                  },
                  "<=": {
                    "label": "KLEINER ODER GLEICH"
                  },
                  "EMPTY": {
                    "label": "IST LEER"
                  },
                  "BLANK": {
                    "label": "IST BLANK"
                  },
                  "IS NULL": {
                    "label": "IS NULL"
                  },
                  "IS TRUE": {
                    "label": "IST WAHR"
                  },
                  "IS FALSE": {
                    "label": "IST FALSCH"
                  },
                  "REGEX MATCH": {
                    "label": "REGEX-MATCH"
                  },
                  "CONTAINED IN TABLE": {
                    "label": "IN TABELLE"
                  },
                  "IN REGEX TABLE": {
                    "label": "IN REGEX-TABELLE"
                  },
                  "DATE": {
                    "label": "DATUM"
                  }
                },
                "val": "CONTAINS"
              },
              "date_condition": {
                "type": "select",
                "label": "Datumsoperator auswählen",
                "required": true,
                "show_if": "this.condition == 'DATE'",
                "options": {
                  "<": {
                    "label": "PRIOR"
                  },
                  "<=": {
                    "label": "PRIOR ODER GLEICH"
                  },
                  "=": {
                    "label": "EQUAL"
                  },
                  ">": {
                    "label": "NACH"
                  },
                  ">=": {
                    "label": "NACHHER ODER GLEICH"
                  }
                },
                "val": "="
              },
              "compare_with": {
                "type": "select",
                "label": "Compare with",
                "required": true,
                "show_if": "this.condition=='CONTAINS' || this.condition=='CONTAINED IN STRING' || this.condition=='STARTS WITH' || this.condition=='ENDS WITH' || this.condition=='=' || this.condition=='>' || this.condition=='>=' || this.condition=='<' || this.condition=='<='",
                "options": {
                  "custom_value": {
                    "label": "Custom value"
                  },
                  "column_value": {
                    "label": "Column value"
                  }
                },
                "val": "custom_value"
              },
              "self_compare_column": {
                "type": "select",
                "label": "Select Compare Column",
                "table": "table_id",
                "bind": "COLUMNS",
                "show_if": "this.compare_with=='column_value' && (this.condition=='CONTAINS' || this.condition=='CONTAINED IN STRING' || this.condition=='STARTS WITH' || this.condition=='ENDS WITH' || this.condition=='=' || this.condition=='>' || this.condition=='>=' || this.condition=='<' || this.condition=='<=')",
                "required": true
              },
              "date_value": {
                "type": "date",
                "label": "Datum auswählen",
                "show_if": "this.condition=='DATE'",
                "val": "",
                "required": true
              },
              "value": {
                "type": "text",
                "label": "Eingangswert",
                "show_if": "this.compare_with=='custom_value' && this.condition != 'DATE' && this.condition != 'EMPTY' && this.condition != 'NOT EMPTY' && this.condition != 'BLANK' && this.condition != 'NOT BLANK' && this.condition != 'IS NULL' && this.condition != 'IS NOT NULL' && this.condition != 'IS TRUE' && this.condition != 'IS FALSE' && this.condition != 'CONTAINED IN TABLE' && this.condition != 'NOT CONTAINED IN TABLE' && this.condition != 'IN REGEX TABLE' && this.condition != 'NOT IN REGEX TABLE'",
                "val": "",
                "required": true
              },
              "case_sensitive": {
                "type": "select",
                "label": "Wählen Sie Case Sensitive",
                "required": true,
                "show_if": "this.condition == 'CONTAINS' || this.condition == 'CONTAINED IN STRING' || this.condition == '=' || this.condition == 'STARTS WITH' || this.condition == 'ENDS WITH' || this.condition == 'REGEX MATCH' || this.condition == 'NOT REGEX MATCH' || this.condition == 'IN REGEX TABLE' || this.condition == 'NOT IN REGEX TABLE'",
                "options": {
                  "yes": {
                    "label": "Gehäuse empfindlich"
                  },
                  "no": {
                    "label": "Groß-/Kleinschreibung nicht beachten"
                  }
                },
                "val": "yes"
              },
              "compare_table_id": {
                "type": "select",
                "label": "Tabelle auswählen",
                "bind": "TABLES",
