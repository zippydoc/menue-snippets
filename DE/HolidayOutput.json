              "Asia/Tehran": {
                "label": "Teheran, Mashhad, Isfahan, Karaj, Tabriz"
              },
              "Asia/Tokyo": {
                "label": "Tokio, Seoul, Jayapura"
              },
              "America/New_York": {
                "label": "Toronto, Santiago, New_York"
              },
              "Australia/Sydney": {
                "label": "Wladiwostok, Sydney, Brisbane"
              },
              "Asia/Yangon": {
                "label": "Yangon, Cocos, Mandalay, Nay Pyi Taw, Mawlamyine"
              },
              "Indian/Cocos": {
                "label": "Yangon, Cocos, Westinsel"
              }
            },
            "breakLine": true
          }
        }
      }
    }
  }
}