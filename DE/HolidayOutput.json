{
  "output": {
    "type": "fieldset",
    "label": "Ausgabetabelle definieren",
    "repeat": false,
    "fields": {
      "output.custom_text": {
        "type": "text",
        "label": "Name der Ausgabetabelle",
        "show_if": "output.custom_options=='false' || (output.custom_options=='true' && output.compose_table_name=='false')",
        "minLength": 1,
        "val": "",
        "width": "100%",
        "breakLine": true
      },
      "output.custom_options": {
        "type": "checkbox",
        "label": "Ausgabe-Optionen",
        "val": "false",
        "isOn": "true",
        "isOff": "false",
        "breakLine": true
      },
      "output.select_output_columns": {
        "type": "checkbox",
        "label": "Ausgabe-Spalten Auswählen",
        "show_if": "options.is_generate_holiday_table=='add_column' && output.custom_options=='true'",
        "val": "false",
        "isOn": "true",
        "isOff": "false"
      },
      "output_columns": {
        "type": "fieldset",
        "label": "Select OutputColumns",
        "show_if": "options.is_generate_holiday_table=='add_column' && output.custom_options=='true' && output.select_output_columns=='true'",
        "repeat": true,
        "fields": {
          "field_name": {
            "type": "select",
            "label": "Spalte auswählen",
            "table": "table_id",
            "bind": "COLUMNS",
            "required": true
          },
          "order": {
            "type": "select",
            "label": "Bestellung",
            "options": {
              "-": {
                "label": "-"
              },
              "asc": {
                "label": "Asc"
              },
              "desc": {
                "label": "Desc"
              }
            },
            "val": "-"
          },
          "is_rename": {
            "type": "checkbox",
            "label": "Umbenennen",
            "val": "false",
            "isOn": "true",
            "isOff": "false"
          },
          "rename": {
            "show_if": "this.is_rename == 'true'",
            "type": "text",
            "required": true,
            "minLength": 1,
            "maxLength": 255,
            "label": "Neuer Spaltenname",
            "style": "margin-left: 10px"
          },
          "format__category": {
            "type": "select",
            "label": "Anzeigeformat",
            "required": true,
            "options": {
              "standard": {
                "label": "Standard"
              },
              "number": {
                "label": "Nummer"
              },
              "percentage": {
                "label": "Prozentsatz"
              },
              "text": {
                "label": "Text"
              },
              "date": {
                "label": "Datum"
              },
              "currency": {
                "label": "Währung"
              }
            },
            "val": "standard"
          },
          "format__rounding": {
            "type": "select",
            "label": "Dezimalstellen für die Anzeige",
            "show_if": "this.format__category=='number' || this.format__category=='percentage' || this.format__category=='currency'",
            "required": true,
            "options": {
              "no_rounding": {
                "label": "Keine Rundung"
              },
              "0": {
                "label": "0 Ziffern"
              },
              "1": {
                "label": "1 Ziffern"
              },
              "2": {
                "label": "2 Ziffern"
              },
              "3": {
                "label": "3 Ziffern"
              },
              "4": {
                "label": "4 Ziffern"
              },
              "5": {
                "label": "5 Ziffern"
              },
              "6": {
                "label": "6 Ziffern"
              },
              "7": {
                "label": "7 Ziffern"
              },
              "8": {
                "label": "8 Ziffern"
              },
              "9": {
                "label": "9 Ziffern"
              }
            },
            "val": "2"
          },
          "format__use_1000_separator": {
            "type": "checkbox",
            "label": "1000 Trennzeichen(.) verwenden",
            "show_if": "this.format__category=='number' || this.format__category=='percentage' || this.format__category=='currency'",
            "val": "true",
            "isOn": "true",
            "isOff": "false"
          },
          "format__currency_symbol": {
            "type": "select",
            "label": "Symbol",
            "show_if": "this.format__category=='currency'",
            "required": true,
            "options": {
              "dollar": {
                "label": "$"
              },
              "euro": {
                "label": "€"
              },
              "pound": {
                "label": "£"
              },
              "yen": {
                "label": "¥ (Yen, currency of Japan)"
              },
              "colon": {
                "label": "₡ (Colón, currency of Costa Rica)"
              },
              "naira": {
                "label": "₦ (Naira, currency of Nigeria)"
              },
              "rupee": {
                "label": "₨ (Rupee, currency of India)"
              },
              "rupie": {
                "label": "₹ (Rupee, currency of India)"
              },
              "won": {
                "label": "₩ (Won, currency in South Korea and North Korea)"
              },
              "hebrew": {
                "label": "₪ (Currency of Israel)"
              },
              "dong": {
                "label": "₫ (Currency of Vietnam)"
              },
              "kip": {
                "label": "₭ (Kip, currency of Laos)"
              },
              "mongolia": {
                "label": "₮ (Currency of Mongolia)"
              },
              "peso": {
                "label": "₱ (Peso, currency of the Philippines)"
              },
              "guarani": {
                "label": "₲ (Guaraní, currency of Paraguay)"
              },
              "hryvnia": {
                "label": "₴ (Hryvnia, currency of Ukraine)"
              },
              "tenge": {
                "label": "₸ (Tenge, currency of Kazakhstan)"
              },
              "lira": {
                "label": "₺ (Lira, currency of Turkey)"
              },
              "manat": {
                "label": "₼ (Manat, currency of Azerbaijan)"
              },
              "rubles": {
                "label": "₽ (Rubles, currency of Russia)"
              },
              "lari": {
                "label": "₾ (Lari, currency of Georgia)"
              },
              "bitcoin": {
                "label": "₿ (Bitcoin, crypto currency)"
              }
            },
            "val": "dollar"
          },
          "format__date_format": {
            "type": "select",
            "label": "Typ",
            "show_if": "this.format__category=='date'",
            "required": true,
            "options": {
              "mm/dd/yyyy": {
                "label": "3/14/2012"
              },
              "D, M dd, Y": {
                "label": "Mittwoch, 14. März 2012"
              },
              "yyyy-mm-dd": {
                "label": "2012-03-14"
              },
              "m.dd.yyyy": {
                "label": "3.14.2012"
              },
              "m.dd.yy": {
                "label": "3.14.12"
              },
              "dd.m.yyyy": {
                "label": "14.3.2012"
              },
              "dd.m.yy": {
                "label": "14.3.12"
              },
              "M dd, Y": {
                "label": "14. März 2012"
              },
              "dd-mm-yyyy": {
                "label": "14-03-2012"
              },
              "mm-dd-yyyy": {
                "label": "03-14-2012"
              },
              "dd-mm-yy": {
                "label": "14-03-12"
              },
              "mm-dd-yy": {
                "label": "03-14-12"
              }
            },
            "val": "mm/dd/yyyy",
            "breakLine": true
          },
          "delete": {
            "type": "button",
            "label": "Lösche",
            "function": "DELETE_ROW"
          }
        },
        "breakLine": true,
        "style": "margin-bottom: 20px"
      },
      "output.compose_table_name": {
        "type": "checkbox",
        "label": "Tabellenname zusammenstellen",
        "show_if": "output.custom_options=='true'",
        "val": "false",
        "isOn": "true",
        "isOff": "false",
        "breakLine": true
      },
      "compose_table_name_frame": {
        "type": "fieldset",
        "label": "Name der Ausgabetabelle zusammenstellen",
        "show_if": "output.custom_options=='true' && output.compose_table_name=='true'",
        "repeat": false,
        "fields": {
          "output.prefix_rule": {
            "type": "select",
            "label": "Präfix",
            "show_if": "output.compose_table_name=='true'",
            "val": "empty",
            "options": {
              "empty": {
                "label": "-"
              },
              "script_name": {
                "label": "Aufgabe Name"
              },
              "date_string_type1": {
                "label": "Datum: tt_mm_jjjj"
              },
              "date_string_type2": {
                "label": "Datum: jjjj_mm_tt"
              },
              "time_string": {
                "label": "Zeit: h_min_sec"
              },
              "datetime_string": {
                "label": "Datum_Zeit"
              },
              "custom": {
                "label": "Benutzerdefiniert"
              }
            }
          },
          "output.prefix_text": {
            "type": "text",
            "label": "Präfix Text",
            "show_if": "output.compose_table_name=='true' && output.prefix_rule=='custom'",
            "minLength": 1,
            "maxLength": 31,
            "val": ""
          },
          "output.core_delimiter": {
            "type": "text",
            "label": "Begrenzer",
            "show_if": "output.compose_table_name=='true' && output.prefix_rule!='empty'",
            "val": "_",
            "width": "50px"
          },
          "output.generate_core_rule": {
            "type": "select",
            "label": "Tabelle Name",
            "show_if": "output.compose_table_name=='true' && options.is_generate_holiday_table=='generate_table'",
            "val": "script_name",
            "options": {
              "script_name": {
                "label": "Aufgabe Name"
              },
              "custom": {
                "label": "Benutzerdefiniert"
              }
            }
          },
          "output.core_rule": {
            "type": "select",
            "label": "Tabelle Name",
            "show_if": "output.compose_table_name=='true' && options.is_generate_holiday_table=='add_column'",
            "val": "input_table_name",
            "options": {
              "input_table_name": {
                "label": "Name der Eingangstabelle"
              },
              "script_name": {
                "label": "Aufgabe Name"
              },
              "custom": {
                "label": "Benutzerdefiniert"
              }
            }
          },
          "output.core_text": {
            "type": "text",
            "label": "Haupttabelle Name",
            "show_if": "output.compose_table_name=='true' && output.core_rule=='custom'",
            "required": true,
            "minLength": 1,
            "maxLength": 31,
            "val": ""
          },
          "output.suffix_delimiter": {
            "type": "text",
            "label": "Begrenzer",
            "show_if": "output.compose_table_name=='true'",
            "val": "_",
            "width": "50px"
          },
          "output.suffix_rule": {
            "type": "select",
            "label": "Suffix",
            "show_if": "output.compose_table_name=='true'",
            "val": "script_name",
            "options": {
              "empty": {
                "label": "-"
              },
              "script_name": {
                "label": "Aufgabe Name"
              },
              "date_string_type1": {
                "label": "Datum: tt_mm_jjjj"
              },
              "date_string_type2": {
                "label": "Datum: jjjj_mm_tt"
              },
              "time_string": {
                "label": "Zeit: h_min_sec"
              },
              "datetime_string": {
                "label": "Datum_Zeit"
              },
              "custom": {
                "label": "Benutzerdefiniert"
              }
            }
          },
          "output.suffix_text": {
            "type": "text",
            "label": "Suffix Text",
            "show_if": "output.compose_table_name=='true' && output.suffix_rule=='custom'",
            "minLength": 1,
            "maxLength": 31,
            "val": "",
            "breakLine": true
          },
          "output.timezone": {
            "type": "select",
            "label": "Zeitzone",
            "show_if": "output.compose_table_name=='true' && (output.prefix_rule=='date_string_type1' || output.prefix_rule=='date_string_type2' || output.prefix_rule=='time_string' || output.prefix_rule=='datetime_string' || output.suffix_rule=='date_string_type1' || output.suffix_rule=='date_string_type2' || output.suffix_rule=='time_string' || output.suffix_rule=='datetime_string')",
            "val": "Europe/Berlin",
            "required": true,
            "options": {
              "America/Adak": {
                "label": "Adak, Gambier"
              },
              "Australia/Adelaide": {
                "label": "Adelaide, Broken_Hill, Darwin"
              },
              "America/Anchorage": {
                "label": "Anchorage, Juneau, Sitka"
              },
              "Pacific/Apia": {
                "label": "Apia, Asau, Mulifanua, Afega, Leulumoega"
              },
              "Pacific/Enderbury": {
                "label": "Apia, Fiji, Tongatapu"
              },
              "Antarctica/McMurdo": {
                "label": "Auckland, Casey, Davis"
              },
              "Pacific/Auckland": {
                "label": "Auckland, Wellington, Christchurch"
              },
              "Asia/Bangkok": {
                "label": "Bangkok, Jakarta, Nowosibirsk"
              },
              "Europe/Berlin": {
                "label": "Berlin, Paris, Amsterdam"
              },
              "Australia/Perth": {
                "label": "Brunei, Singapur, Perth"
              },
              "Atlantic/Cape_Verde": {
                "label": "Kap_Verde,Praia, Mindelo, Santa Maria"
              },
              "America/Belize": {
                "label": "Costa_Rica, El_Salvador, Denver"
              },
              "Antarctica/Vostok": {
                "label": "Dhaka, Bishkek, Omsk"
              },
              "Europe/Astrakhan": {
                "label": "Dubai, Baku, Tiflis"
              },
              "Australia/Eucla": {
                "label": "Eucla"
              },
              "Pacific/Honolulu": {
                "label": "Honolulu, Rarotonga, Tahiti"
              },
              "Asia/Kabul": {
                "label": "Kabul, Kandahār, Mazār-e Sharīf, Herāt, Jalālābād"
              },
              "Asia/Aqtau": {
                "label": "Karatschi, Taschkent, Malediven"
              },
              "Asia/Kathmandu": {
                "label": "Kathmandu, Pokhara, Pātan, Biratnagar"
              },
              "Pacific/Kiritimati": {
                "label": "Kiritimati"
              },
              "Asia/Colombo": {
                "label": "Kalkutta, Colombo, Jaffna, Negombo"
              },
              "Europe/Guernsey": {
                "label": "London. Dublin, Lissabon"
              },
              "Australia/Lord_Howe": {
                "label": "Lord_Howe"
              },
              "America/Creston": {
                "label": "Los_Angeles, Vancouver, Hermosillo"
              },
              "Pacific/Marquesas": {
                "label": "Marquesas, Taiohae"
              },
              "America/Cayman": {
                "label": "Mexiko_Stadt, Panama, Chicago"
              },
              "Pacific/Niue": {
                "label": "Midway, Niue, Pago_Pago"
              },
              "Europe/Mariehamn": {
                "label": "Moskau, Istanbul, Katar"
              },
              "Asia/Kolkata": {
                "label": "Mumbai, Delhi, Bengaluru, Kolkata, Chennai"
              },
              "America/Miquelon": {
                "label": "Noronha, Nuuk, Miquelon"
              },
              "America/Danmarkshavn": {
                "label": "Reykjavik, Dakar, Casablanca"
              },
              "Pacific/Bougainville": {
                "label": "Sakhalin, Norfolk, Bougainville"
              },
              "America/Araguaina": {
                "label": "Sao Paulo, Buenos Aires, Montevideo"
              },
              "America/St_Johns": {
                "label": "St_Johns, Mount Pearl, Corner Brook"
