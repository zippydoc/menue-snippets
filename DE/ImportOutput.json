{
  "output": {
    "type": "fieldset",
    "Beschriftung": "Ausgabetabelle definieren",
    "show_if": "options.from=='database' || (options.from=='web' && options.output_format=='file_list') || (options.from=='web' && options.output_format!='file_list' && options.is_cloud_dir=='true') || (options.from=='web' && options.output_format!='file_list' && options.is_cloud_dir=='false' && options.use_filename_as_output_table_name=='false')",
    "repeat": false,
    "fields": {
      "output.custom_text": {
        "type": "text",
        "Beschriftung": "Name der Ausgabetabelle",
        "show_if": "output.custom_options=='false' || (output.custom_options=='true' && output.compose_table_name=='false')",
        "minLength": 1,
        "val": "",
        "width": "100%",
        "breakLine": true
      },
      "output.custom_options": {
        "type": "checkbox",
        "Beschriftung": "Ausgabe-Optionen",
        "val": "false",
        "isOn": "true",
        "isOff": "false",
        "breakLine": true
      },
      "output.compose_table_name": {
        "type": "checkbox",
        "Beschriftung": "Tabellenname zusammenstellen",
        "show_if": "output.custom_options=='true'",
        "val": "false",
        "isOn": "true",
        "isOff": "false",
        "breakLine": true
      },
      "compose_table_name_frame": {
        "type": "fieldset",
        "Beschriftung": "Name der Ausgabetabelle zusammenstellen",
        "show_if": "output.custom_options=='true' && output.compose_table_name=='true'",
        "repeat": false,
        "fields": {
          "output.prefix_rule": {
            "type": "select",
            "Beschriftung": "Präfix",
            "show_if": "output.compose_table_name=='true'",
            "val": "empty",
            "options": {
              "empty": {
                "Beschriftung": "-"
              },
              "script_name": {
                "Beschriftung": "Aufgabe Name"
              },
              "date_string_type1": {
                "Beschriftung": "Datum: tt_mm_jjjj"
              },
              "date_string_type2": {
                "Beschriftung": "Datum: jjjj_mm_tt"
              },
              "time_string": {
                "Beschriftung": "Zeit: h_min_sec"
              },
              "datetime_string": {
                "Beschriftung": "Datum_Zeit"
              },
              "custom": {
                "Beschriftung": "Benutzerdefiniert"
              }
            }
          },
          "output.prefix_text": {
            "type": "text",
            "Beschriftung": "Präfix Text",
            "show_if": "output.compose_table_name=='true' && output.prefix_rule=='custom'",
            "minLength": 1,
            "maxLength": 31,
            "val": ""
          },
          "output.core_delimiter": {
            "type": "text",
            "Beschriftung": "Begrenzer",
            "show_if": "output.compose_table_name=='true' && output.prefix_rule!='empty'",
            "val": "_",
            "width": "50px"
          },
          "output.core_rule": {
            "type": "select",
            "Beschriftung": "Tabelle Name",
            "show_if": "output.compose_table_name=='true'",
            "val": "script_name",
            "options": {
              "script_name": {
                "Beschriftung": "Aufgabe Name"
              },
              "custom": {
                "Beschriftung": "Benutzerdefiniert"
              }
            }
          },
          "output.core_text": {
            "type": "text",
            "Beschriftung": "Haupttabelle Name",
            "show_if": "output.compose_table_name=='true' && output.core_rule=='custom'",
            "required": true,
            "minLength": 1,
            "maxLength": 31,
            "val": ""
          },
          "output.suffix_delimiter": {
            "type": "text",
            "Beschriftung": "Begrenzer",
            "show_if": "output.compose_table_name=='true'",
            "val": "_",
            "width": "50px"
          },
          "output.suffix_rule": {
            "type": "select",
            "Beschriftung": "Suffix",
            "show_if": "output.compose_table_name=='true'",
            "val": "script_name",
            "options": {
              "empty": {
                "Beschriftung": "-"
              },
              "script_name": {
                "Beschriftung": "Aufgabe Name"
              },
              "date_string_type1": {
                "Beschriftung": "Datum: tt_mm_jjjj"
              },
              "date_string_type2": {
                "Beschriftung": "Datum: jjjj_mm_tt"
              },
              "time_string": {
                "Beschriftung": "Zeit: h_min_sec"
              },
              "datetime_string": {
                "Beschriftung": "Datum_Zeit"
              },
              "custom": {
                "Beschriftung": "Benutzerdefiniert"
              }
            }
          },
          "output.suffix_text": {
            "type": "text",
            "Beschriftung": "Suffix Text",
            "show_if": "output.compose_table_name=='true' && output.suffix_rule=='custom'",
            "minLength": 1,
            "maxLength": 31,
            "val": "",
            "breakLine": true
          },
          "output.timezone": {
