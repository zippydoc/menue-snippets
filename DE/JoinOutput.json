{
  "output.output_table_types": {
    "type": "select",
    "label": "Tabellentyp(en) ausgeben",
    "required": true,
    "options": {
      "inner": {
        "label": "Inner Join Tabelle"
      },
      "left": {
        "label": "Linke Join-Tabelle"
      },
      "right": {
        "label": "Tabelle rechts verbinden"
      }
    },
    "multiple": true,
    "val": ["inner"],
    "breakLine": true
  },
  "output.unify_output_table": {
    "type": "checkbox",
    "label": "Unify output tables",
    "val": "false",
    "isOn": "true",
    "isOff": "false",
    "breakLine": true
  },
  "output.standard_ending": {
    "type": "checkbox",
    "label": "Standard-Ende",
    "show_if": "output.unify_output_table == 'false'",
    "val": "true",
    "isOn": "true",
    "isOff": "false",
    "breakLine": true
  },
  "output.inner_ending": {
    "show_if": "output.unify_output_table == 'false' && output.standard_ending == 'false'",
    "type": "text",
    "label": "Inneres Ende",
    "val": "_j",
    "style": "margin-left: 10px"
  },
  "output.left_ending": {
    "show_if": "output.unify_output_table == 'false' && output.standard_ending == 'false'",
    "type": "text",
    "label": "Links endend",
    "val": "_l",
    "style": "margin-left: 10px"
  },
  "output.right_ending": {
    "show_if": "output.unify_output_table == 'false' && output.standard_ending == 'false'",
    "type": "text",
    "label": "Rechts endend",
    "val": "_r",
    "style": "margin-left: 10px",
    "breakLine": true
  }
}