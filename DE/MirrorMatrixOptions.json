{
  "options": {
    "type": "fieldset",
    "label": "Optionen auswählen",
    "repeat": false,
    "fields": {
      "transpose_type": {
        "type": "hidden",
        "val": "mirror_matrix"
      },
      "mirror_matrix.convert_header_to_label": {
        "type": "checkbox",
        "label": "Kopfzeile in Etikett umwandeln",
        "show_if": "transpose_type=='mirror_matrix'",
        "val": "true",
        "isOn": "true",
        "isOff": "false",
        "breakLine": true
      },
      "column_name_to_label.columns_to_label": {
        "type": "select",
        "label": "Spalten zum Konvertieren in Etiketten auswählen",
        "show_if": "transpose_type=='column_name_to_label'",
        "table": "input_table",
        "multiple": true,
        "bind": "COLUMNS",
        "required": true,
        "width": "100%",
        "breakLine": true
      },
      "column_name_to_label.label_column_name": {
        "type": "text",
        "label": "Spalte Name der Etikettenspalte",
        "show_if": "transpose_type=='column_name_to_label'",
        "required": true,
        "val": "Etikett",
        "width": "100%",
        "breakLine": true
      },
      "column_name_to_label.value_column_name": {
        "type": "text",
        "label": "Spalte Name der Wertespalte",
        "show_if": "transpose_type=='column_name_to_label'",
        "required": true,
        "val": "Wert",
        "width": "100%",
        "breakLine": true
      },
      "crosstab.group_data_by_these_values": {
        "type": "select",
        "label": "Daten nach diesen Werten gruppieren",
        "show_if": "transpose_type=='crosstab'",
        "table": "input_table",
        "multiple": true,
        "bind": "COLUMNS",
        "required": true,
        "width": "100%",
        "breakLine": true
      },
      "crosstab.change_column_headers": {
        "type": "select",
        "label": "Spaltenüberschrift ändern",
        "show_if": "transpose_type=='crosstab'",
        "table": "input_table",
        "bind": "COLUMNS",
        "required": true,
        "width": "100%",
        "breakLine": true
      },
      "crosstab.values_for_new_columns": {
        "type": "select",
        "label": "Werte für neue Spalten",
        "show_if": "transpose_type=='crosstab'",
        "table": "input_table",
        "bind": "COLUMNS",
        "required": true,
        "width": "100%",
        "breakLine": true
      },
      "crosstab.method_for_aggregating_values": {
        "type": "select",
        "label": "Methode zum Aggregieren von Werten",
        "show_if": "transpose_type=='crosstab'",
        "options": {
          "Verketten": {
            "label": "Verketten"
          },
          "Erste": {
            "label": "Erste"
          },
          "Zuletzt": {
            "label": "Zuletzt"
          },
          "Summe": {
            "label": "Summe"
          },
          "Durchschnitt": {
            "label": "Durchschnitt"
          },
          "Zählen Sie": {
            "label": "Zählen Sie"
          },
          "Benutzerdefiniert": {
            "label": "Benutzerdefiniert"
          }
        },
        "val": "Verketten",
        "required": true
      },
      "crosstab.user_defined_str": {
        "type": "text",
        "label": "Benutzerdefinierter Wert",
        "show_if": "transpose_type=='crosstab' && crosstab.method_for_aggregating_values=='User Defined'",
        "val": "x",
        "trim": "false",
        "breakLine": true
      },
      "crosstab.separator": {
        "type": "text",
        "label": "Separator",
        "show_if": "transpose_type=='crosstab' && method_for_aggregating_values=='Concatenate'",
        "val": ",",
        "trim": "false",
        "required": true,
        "breakLine": true
      }
    }
  }
}