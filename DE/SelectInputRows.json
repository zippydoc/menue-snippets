{
  "options.cut_off_run": {
    "type": "checkbox",
    "Beschriftung": "Abschneiden der Eingabetabelle",
    "val": "false",
    "isOn": "true",
    "isOff": "false"
  },
  "options.cut_off.type": {
    "type": "select",
    "Beschriftung": "Typ der Abschaltung",
    "show_if": "options.cut_off_run=='true'",
    "required": true,
    "options": {
      "from_first_row": {
        "Beschriftung": "Von erster Zeile"
      },
      "range": {
        "Beschriftung": "Bereich"
      }
    },
    "val": "from_first_row"
  },
  "options.cut_off.start_row_num": {
    "type": "text",
    "Beschriftung": "Start Zeile #",
    "show_if": "options.cut_off_run=='true' && options.cut_off.type=='range'",
    "required": true,
    "minLength": 1,
    "maxLength": 7,
    "val": "1"
  },
  "options.cut_off.row_count": {
    "type": "text",
    "Beschriftung": "Zeilenzahl",
    "show_if": "options.cut_off_run=='true'",
    "required": true,
    "minLength": 1,
    "maxLength": 7,
    "val": "100"
  }
}