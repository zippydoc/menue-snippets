{
  "options": {
    "type": "fieldset",
    "label": "Optionen auswählen",
    "repeat": false,
    "fields": {
      "type": {
        "type": "select",
        "label": "Typ auswählen",
        "val": "first_n",
        "options": {
          "first_n": {
            "label": "Erste N"
          },
          "last_n": {
            "label": "Letzte N"
          },
          "custom_selection": {
            "label": "Benutzerdefinierte Auswahl"
          },
          "skip_first_n": {
            "label": "Erste überspringen N"
          },
          "skip_last_n": {
            "label": "Letztes N überspringen"
          },
          "first_n_percent": {
            "label": "Erste N%"
          },
          "last_n_percent": {
            "label": "Letzte N%"
          },
          "random_n_records": {
            "label": "Zufällig N"
          },
          "random_n_percent": {
            "label": "Zufällig N%"
          },
          "random_choice_1_n": {
            "label": "Zufallsauswahl 1:N"
          },
          "every_nth_record": {
            "label": "Jeder N-te Datensatz"
          }
        },
        "required": true,
        "width": "100%",
        "breakLine": true
      },
      "n": {
        "type": "text",
        "label": "Eingang N",
        "show_if": "type!='custom_selection'",
        "val": "1",
        "required": true,
        "width": "100%",
        "breakLine": true
      },
      "ranges": {
        "type": "text",
        "label": "Eingangsbereiche",
        "show_if": "type=='custom_selection'",
        "val": "-2; 4-6; 9; 12+",
        "required": true,
        "width": "100%",
        "breakLine": true
      },
      "set_offset": {
        "type": "checkbox",
        "label": "Versetzt",
        "show_if": "type=='every_nth_record'",
        "val": "false",
        "isOn": "true",
        "isOff": "false"
      },
      "offset": {
        "type": "text",
        "label": "Versetzt",
        "show_if": "type=='every_nth_record' && set_offset=='true'",
        "val": "1",
        "required": true,
        "breakLine": true
      },
      "group_by_column": {
        "type": "checkbox",
        "label": "Gruppieren nach Spalte",
        "val": "false",
        "isOn": "true",
        "isOff": "false",
        "breakLine": true
      },
      "columns_to_group_box": {
        "type": "fieldset",
        "label": "Gruppieren nach Spalte",
        "show_if": "group_by_column=='true'",
        "repeat": false,
        "fields": {
          "columns_to_group": {
            "type": "select",
            "label": "Gruppenspalten auswählen",
            "required": true,
            "width": "100%",
            "table": "table_id",
            "multiple": true,
            "bind": "COLUMNS",
            "breakLine": true
          }
        }
      }
    }
  }
}