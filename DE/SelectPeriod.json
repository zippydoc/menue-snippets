{
  "output.add_period": {
    "type": "checkbox",
    "label": "Periodizität hinzufügen",
    "show_if": "options.type=='process' || options.type=='custom'",
    "val": "false",
    "isOn": "true",
    "isOff": "false",
    "style": "margin-top: 10px",
    "breakLine": true
  },
  "output.period": {
    "type": "select",
    "label": "Periodizität",
    "show_if": "(options.type=='process' || options.type=='custom') && output.add_period=='true'",
    "multiple": true,
    "options": {
      "year": {
        "label": "Jahr"
      },
      "half-year": {
        "label": "Halbjahresbericht"
      },
      "quarter": {
        "label": "Viertel"
      },
      "month": {
        "label": "Monat"
      },
      "week": {
        "label": "Woche"
      }
    },
    "val": "",
    "required": true
  },
  "output.period_concatenate": {
    "type": "checkbox",
    "label": "Verketten",
    "show_if": "(options.type=='process' || options.type=='custom') && output.add_period=='true'",
    "val": "false",
    "isOn": "true",
    "isOff": "false",
    "breakLine": true
  }
}