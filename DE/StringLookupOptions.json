{
  "options": {
    "type": "fieldset",
    "label": "Optionen auswählen",
    "repeat": false,
    "fields": {
      "options.task_type": {
        "type": "select",
        "label": "Aufgaben-Typ",
        "required": true,
        "options": {
          "generate_lookup_table": {
            "label": "Lookup-Tabelle generieren"
          },
          "apply_lookup_table": {
            "label": "Lookup-Tabelle anwenden"
          },
          "apply_regex_table": {
            "label": "Regex-Tabelle anwenden"
          },
          "apply_lookup_table_to_cells": {
            "label": "Lookup-Tabelle auf Zellen anwenden"
          },
          "apply_regex_table_to_cells": {
            "label": "Regex-Tabelle auf Zellen anwenden"
          }
        },
        "val": "generate_lookup_table"
      },
      "options.pattern_type": {
        "type": "hidden",
        "val": "duplicate_pattern"
      },
      "duplicate_option_box": {
        "type": "fieldset",
        "label": "Option Duplizieren von Mustern",
        "show_if": "options.task_type=='label_duplicates' || (options.task_type=='generate_lookup_table' && options.pattern_type=='duplicate_pattern')",
        "repeat": false,
        "fields": {
          "options.minimum_number_of_matching_characters": {
            "type": "text",
            "label": "Mindestanzahl übereinstimmender Zeichen",
            "val": "3",
            "required": true,
            "style": "min-width: 240px"
          },
          "options.minimum_number_of_duplicates": {
            "type": "text",
            "label": "Minimale Anzahl von Duplikaten",
            "val": "2",
            "required": true,
            "style": "min-width: 240px",
            "breakLine": true
          }
        }
      },
      "number_range_option_box": {
        "type": "fieldset",
        "label": "Option Nummernkreise",
        "show_if": "options.task_type=='generate_lookup_table' && options.pattern_type=='number_range'",
        "repeat": false,
        "fields": {
          "options.number_range.range_type": {
            "type": "select",
            "label": "Bereich Typ",
            "required": true,
            "options": {
              "equal_interval": {
                "label": "N Gleiche Intervalle"
              },
              "equal_range": {
                "label": "Gleicher Bereich"
              },
              "custom_range": {
                "label": "Benutzerdefinierter Bereich"
              }
            },
            "val": "equal_interval"
          },
          "options.number_range.n_value": {
            "type": "text",
            "label": "N",
            "show_if": "options.number_range.range_type=='equal_interval'",
            "val": "3",
            "required": true
          },
          "options.number_range.range_value": {
            "type": "text",
            "label": "Bereich",
            "show_if": "options.number_range.range_type=='equal_range'",
            "val": "10",
            "required": true
          },
          "options.number_range.custom_ranges": {
            "type": "fieldset",
            "label": "Benutzerdefiniertes Bereichsmuster hinzufügen",
            "show_if": "options.number_range.range_type=='custom_range'",
            "repeat": true,
            "fields": {
              "pattern": {
                "type": "text",
                "label": "Muster",
                "val": "0<=x<10",
                "required": true
              },
              "label": {
                "type": "text",
                "label": "Etikett",
                "val": "positive10",
                "required": true
              },
              "delete": {
                "type": "button",
                "label": "Lösche",
                "function": "DELETE_ROW"
              }
            }
          }
        }
      },
      "options.is_custom_lookup_table": {
        "type": "checkbox",
        "label": "Benutzerdefinierte Nachschlagetabelle",
        "show_if": "options.task_type=='apply_lookup_table' || options.task_type=='apply_regex_table' || options.task_type=='apply_lookup_table_to_cells' || options.task_type=='apply_regex_table_to_cells'",
        "val": "true",
        "isOn": "true",
        "isOff": "false",
        "breakLine": true
      },
      "lookup_table_frame": {
        "type": "fieldset",
        "label": "Lookup-Tabelle auswählen",
        "show_if": "options.task_type=='apply_lookup_table' || options.task_type=='apply_regex_table' || options.task_type=='apply_lookup_table_to_cells' || options.task_type=='apply_regex_table_to_cells'",
        "repeat": false,
        "fields": {
          "options.lookup_table_id": {
            "type": "select",
            "label": "Nachschlagetabelle",
            "show_if": "options.is_custom_lookup_table=='false'",
            "bind": "TABLES",
            "empty": "",
            "required": true,
            "breakLine": true
          },
          "options.custom_lookup_table": {
            "type": "fieldset",
            "label": "Benutzerdefinierte Nachschlagetabelle",
            "show_if": "options.is_custom_lookup_table=='true'",
            "repeat": true,
            "fields": {
              "lookup": {
                "type": "text",
                "label": "Schlage nach",
                "val": "",
                "required": true
              },
              "label": {
                "type": "text",
                "label": "Etikett",
                "val": "",
                "required": true
              },
              "delete": {
                "type": "button",
                "label": "Lösche",
                "function": "DELETE_ROW"
              }
            }
          },
          "options.manual_select": {
            "type": "checkbox",
            "label": "Select manually",
            "show_if": "options.pattern_type=='duplicate_pattern' && options.is_custom_lookup_table=='false' && (options.task_type=='apply_lookup_table' || options.task_type=='apply_regex_table')",
            "val": "false",
            "isOn": "true",
            "isOff": "false",
            "breakLine": true
          },
          "options.apply_lookup_table__lookup_table_pattern_label_columns": {
            "type": "fieldset",
            "label": "Add Pattern column / Label column pairs",
            "show_if": "options.pattern_type=='duplicate_pattern' && options.is_custom_lookup_table=='false' && options.manual_select=='true' && options.task_type=='apply_lookup_table'",
            "repeat": true,
            "fields": {
              "pattern_column": {
                "type": "select",
                "label": "Pattern Column",
                "table": "options.lookup_table_id",
                "bind": "COLUMNS",
                "required": true
              },
              "label_column": {
                "type": "select",
                "label": "Label Column",
                "table": "options.lookup_table_id",
                "bind": "COLUMNS",
                "required": true
              },
              "delete": {
                "type": "button",
                "label": "Lösche",
                "function": "DELETE_ROW"
              }
            }
          },
          "options.apply_regex_table__lookup_table_pattern_column": {
            "type": "select",
            "label": "Pattern Column",
            "show_if": "options.pattern_type=='duplicate_pattern' && options.is_custom_lookup_table=='false' && options.manual_select=='true' && options.task_type=='apply_regex_table'",
            "table": "options.lookup_table_id",
            "bind": "COLUMNS",
            "required": true
          },
          "options.apply_regex_table__lookup_table_label_columns": {
            "type": "select",
            "label": "Label Column(s)",
            "show_if": "options.pattern_type=='duplicate_pattern' && options.task_type=='apply_regex_table' && options.manual_select=='true'",
            "table": "options.lookup_table_id",
            "bind": "COLUMNS",
            "multiple": true,
            "required": true
          },
          "options.apply_regex_table__match_column_name": {
            "type": "text",
            "label": "Match Column Name",
            "show_if": "options.pattern_type=='duplicate_pattern' && options.task_type=='apply_regex_table' && options.manual_select=='true'",
            "val": "Match",
            "required": true
          },
          "options.apply_regex_table__full_match_column_name": {
            "type": "text",
            "label": "Full Match Column Name",
            "show_if": "options.pattern_type=='duplicate_pattern' && options.task_type=='apply_regex_table' && options.manual_select=='true'",
            "val": "Full-Match",
            "required": true
          },
          "options.partial_match": {
            "type": "checkbox",
            "label": "Teilweise Übereinstimmung",
            "show_if": "options.pattern_type=='duplicate_pattern' && (options.task_type=='apply_lookup_table' || options.task_type=='apply_lookup_table_to_cells')",
            "val": "true",
            "isOn": "true",
            "isOff": "false",
            "style": "margin-top: 10px",
            "breakLine": true
          },
          "options.case_sensitive": {
            "type": "checkbox",
            "label": "Gehäuse empfindlich",
            "show_if": "options.pattern_type=='duplicate_pattern' && (options.task_type=='apply_lookup_table' || options.task_type=='apply_lookup_table_to_cells' || options.task_type=='apply_regex_table' || options.task_type=='apply_regex_table_to_cells')",
            "val": "false",
            "isOn": "true",
            "isOff": "false",
            "breakLine": true
          },
          "options.use_delimiter": {
            "type": "checkbox",
            "label": "Benutzerdefinierte Begrenzer",
            "val": "false",
            "isOn": "true",
            "isOff": "false",
            "style": "margin-top: 10px"
          },
          "options.delimiter": {
            "type": "text",
            "label": "Eingabe-Begrenzer",
            "show_if": "options.use_delimiter=='true'",
            "val": ";",
            "required": true,
            "breakLine": true
          }
        }
      },
      "help_1": {
        "type": "textarea",
        "label": "",
        "show_if": "options.task_type=='apply_regex_table' || options.task_type=='apply_regex_table_to_cells'",
        "val": "To compile a regular expression you may e.g. use  regex101.com",
        "disabled": true,
        "style": "width: 100%"
      },
      "options.input_table.use_filter": {
        "type": "checkbox",
        "label": "Filter für Eingangstabelle verwenden",
        "show_if": "options.task_type=='apply_lookup_table' || options.task_type=='apply_regex_table' || options.task_type=='apply_lookup_table_to_cells' || options.task_type=='apply_regex_table_to_cells'",
        "val": "false",
        "isOn": "true",
        "isOff": "false",
        "style": "margin-top: 10px",
        "breakLine": true
      },
      "input_table_filter_box": {
        "type": "fieldset",
        "label": "Filteroptionen der Eingabetabelle",
        "show_if": "options.input_table.use_filter=='true' && (options.task_type=='apply_lookup_table' || options.task_type=='apply_regex_table' || options.task_type=='apply_lookup_table_to_cells' || options.task_type=='apply_regex_table_to_cells')",
        "repeat": false,
        "fields": {
          "options.input_table.filter": {
            "type": "fieldset",
            "label": "Filterbedingungen für Eingangstabelle hinzufügen",
            "repeat": true,
            "fields": {
              "initial_op": {
                "type": "select",
                "label": "Logik-Operator auswählen",
                "required": true,
                "show_if": "$index == 0",
                "options": {
                  "if": {
                    "label": "IF"
                  },
                  "if_not": {
                    "label": "WENN NICHT"
                  }
                },
                "val": "if"
              },
              "op": {
                "type": "select",
                "label": "Logik-Operator auswählen",
                "required": true,
                "show_if": "$index > 0",
                "options": {
                  "and": {
                    "label": "UND"
                  },
                  "or": {
                    "label": "OR"
                  },
                  "and_not": {
                    "label": "UND NICHT"
                  },
                  "or_not": {
                    "label": "ODER NICHT"
                  }
                },
                "val": "and"
              },
              "column": {
                "type": "select",
                "label": "Spalte auswählen",
                "table": "input_table",
                "bind": "COLUMNS",
                "required": true
              },
              "condition": {
                "type": "select",
                "label": "Bediener auswählen",
                "required": true,
                "options": {
                  "CONTAINS": {
                    "label": "ENTHÄLT"
                  },
                  "CONTAINED IN STRING": {
                    "label": "ENTHALTEN IN"
                  },
                  "STARTS WITH": {
                    "label": "STARTET MIT"
                  },
                  "ENDS WITH": {
                    "label": "ENDET MIT"
                  },
                  "=": {
                    "label": "EQUAL"
                  },
                  ">": {
                    "label": "GRÖSSER ALS"
                  },
                  ">=": {
                    "label": "GRÖSSER ODER GLEICH"
                  },
                  "<": {
                    "label": "KLEINER ALS"
                  },
                  "<=": {
                    "label": "KLEINER ODER GLEICH"
                  },
                  "EMPTY": {
                    "label": "IST LEER"
                  },
                  "BLANK": {
                    "label": "IST BLANK"
                  },
                  "IS NULL": {
                    "label": "IS NULL"
                  },
                  "IS TRUE": {
                    "label": "IST WAHR"
                  },
                  "IS FALSE": {
                    "label": "IST FALSCH"
                  },
                  "REGEX MATCH": {
                    "label": "REGEX-MATCH"
                  },
                  "CONTAINED IN TABLE": {
                    "label": "IN TABELLE"
                  },
                  "IN REGEX TABLE": {
                    "label": "IN REGEX-TABELLE"
                  },
                  "DATE": {
                    "label": "DATUM"
                  }
                },
                "val": "CONTAINS"
              },
              "date_condition": {
                "type": "select",
                "label": "Datumsoperator auswählen",
                "required": true,
                "show_if": "this.condition == 'DATE'",
                "options": {
                  "<": {
                    "label": "PRIOR"
                  },
                  "<=": {
                    "label": "PRIOR ODER GLEICH"
                  },
                  "=": {
                    "label": "EQUAL"
                  },
                  ">": {
                    "label": "NACH"
                  },
                  ">=": {
                    "label": "NACHHER ODER GLEICH"
                  }
                },
                "val": "="
              },
              "compare_with": {
                "type": "select",
                "label": "Compare with",
                "required": true,
                "show_if": "this.condition=='CONTAINS' || this.condition=='CONTAINED IN STRING' || this.condition=='STARTS WITH' || this.condition=='ENDS WITH' || this.condition=='=' || this.condition=='>' || this.condition=='>=' || this.condition=='<' || this.condition=='<='",
                "options": {
                  "custom_value": {
                    "label": "Custom value"
                  },
                  "column_value": {
                    "label": "Column value"
                  }
                },
                "val": "custom_value"
              },
              "self_compare_column": {
                "type": "select",
                "label": "Select Compare Column",
                "table": "table_id",
                "bind": "COLUMNS",
                "show_if": "this.compare_with=='column_value' && (this.condition=='CONTAINS' || this.condition=='CONTAINED IN STRING' || this.condition=='STARTS WITH' || this.condition=='ENDS WITH' || this.condition=='=' || this.condition=='>' || this.condition=='>=' || this.condition=='<' || this.condition=='<=')",
                "required": true
              },
              "date_value": {
                "type": "date",
                "label": "Datum auswählen",
                "show_if": "this.condition=='DATE'",
                "val": "",
                "required": true
              },
              "value": {
                "type": "text",
                "label": "Eingangswert",
                "show_if": "this.compare_with=='custom_value' && this.condition != 'DATE' && this.condition != 'EMPTY' && this.condition != 'NOT EMPTY' && this.condition != 'BLANK' && this.condition != 'NOT BLANK' && this.condition != 'IS NULL' && this.condition != 'IS NOT NULL' && this.condition != 'IS TRUE' && this.condition != 'IS FALSE' && this.condition != 'CONTAINED IN TABLE' && this.condition != 'NOT CONTAINED IN TABLE' && this.condition != 'IN REGEX TABLE' && this.condition != 'NOT IN REGEX TABLE'",
                "val": "",
                "required": true
              },
              "case_sensitive": {
                "type": "select",
                "label": "Wählen Sie Case Sensitive",
                "required": true,
                "show_if": "this.condition == 'CONTAINS' || this.condition == 'CONTAINED IN STRING' || this.condition == '=' || this.condition == 'STARTS WITH' || this.condition == 'ENDS WITH' || this.condition == 'REGEX MATCH' || this.condition == 'NOT REGEX MATCH' || this.condition == 'IN REGEX TABLE' || this.condition == 'NOT IN REGEX TABLE'",
                "options": {
                  "yes": {
                    "label": "Gehäuse empfindlich"
                  },
                  "no": {
                    "label": "Groß-/Kleinschreibung nicht beachten"
                  }
                },
                "val": "yes"
              },
              "compare_table_id": {
                "type": "select",
                "label": "Tabelle auswählen",
                "bind": "TABLES",
                "empty": "",
                "show_if": "this.condition == 'CONTAINED IN TABLE' || this.condition == 'NOT CONTAINED IN TABLE' || this.condition == 'IN REGEX TABLE' || this.condition == 'NOT IN REGEX TABLE'",
                "required": true
              },
              "compare_column": {
                "type": "select",
                "label": "Spalte auswählen",
                "table": "this.compare_table_id",
                "bind": "COLUMNS",
                "show_if": "this.condition == 'CONTAINED IN TABLE' || this.condition == 'NOT CONTAINED IN TABLE' || this.condition == 'IN REGEX TABLE' || this.condition == 'NOT IN REGEX TABLE'",
                "required": true
              },
              "delete": {
                "type": "button",
                "label": "Lösche",
                "function": "DELETE_ROW"
              }
            }
          },
          "options.input_table.apply_to_record": {
            "type": "checkbox",
            "label": "Anwenden auf Datensatz für Eingangstabellenfilter",
            "val": "true",
            "isOn": "true",
            "isOff": "false",
            "style": "margin-top: 10px",
            "breakLine": true
          },
          "input_table_apply_to_group_box": {
            "type": "fieldset",
            "label": "Auf die Gruppe der Eingangstabelle anwenden",
            "show_if": "options.input_table.apply_to_record=='false'",
            "repeat": false,
            "fields": {
              "options.input_table.apply_to_group_columns": {
                "type": "select",
                "label": "Gruppenspalten auswählen",
                "required": true,
                "width": "100%",
                "table": "input_table",
                "multiple": true,
                "bind": "COLUMNS",
                "breakLine": true
              }
            }
          }
        }
      },
