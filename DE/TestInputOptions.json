{
  "options": {
    "type": "fieldset",
    "Beschriftung": "Tabellenkopf & Beschriftung Ergebnis",
    "repeat": false,
    "fields": {
      "has_header": {
        "type": "button",
        "Beschriftung": "Hat Kopfzeile!",
        "show_if": "$input_table.numOfHeader >= 1"
      },
      "has_label": {
        "type": "button",
        "Beschriftung": "Hat Label!",
        "show_if": "$input_table.numOfLabel >= 1"
      },
      "has_both": {
        "type": "button",
        "Beschriftung": "Hat beides!",
        "show_if": "$input_table.numOfHeader >= 1 && $input_table.numOfLabel >= 1"
      },
      "has_both2": {
        "type": "text",
        "Beschriftung": "Hat beides2!",
        "show_if": "$input_table.numOfHeader>=1 && $input_table.numOfLabel>=1",
        "val": "both"
      },
      "header_only": {
        "type": "button",
        "Beschriftung": "Nur Kopfzeile!",
        "show_if": "$input_table.numOfHeader >= 1 && $input_table.numOfLabel < 1"
      },
      "label_only": {
        "type": "button",
        "Beschriftung": "Nur Etikett!",
        "show_if": "$input_table.numOfHeader < 1 && $input_table.numOfLabel >= 1"
      },
      "no_header": {
        "type": "button",
        "Beschriftung": "Keine Kopfzeile!",
        "show_if": "$input_table.numOfHeader < 1"
      },
      "no_label": {
        "type": "button",
        "Beschriftung": "Kein Etikett!",
        "show_if": "$input_table.numOfLabel < 1"
      }
    }
  }
}