{
  "output.output_table_types": {
    "type": "select",
    "label": "Tabellentyp(en) ausgeben",
    "required": true,
    "options": {
      "unique": {
        "label": "Eindeutige Tabelle"
      },
      "duplicates": {
        "label": "Duplikate-Tabelle"
      }
    },
    "multiple": true,
    "val": ["unique"]
  },
  "output.standard_ending": {
    "type": "checkbox",
    "label": "Standard-Ende",
    "val": "true",
    "isOn": "true",
    "isOff": "false",
    "breakLine": true
  },
  "output.unique_ending": {
    "show_if": "output.standard_ending == 'false'",
    "type": "text",
    "label": "Eindeutiges Ende des Ausgangs",
    "val": "_u",
    "style": "width: 160px; margin-left: 10px"
  },
  "output.duplicates_ending": {
    "show_if": "output.standard_ending == 'false'",
    "type": "text",
    "label": "Duplikate Ausgabe-Ende",
    "val": "_d",
    "style": "width: 160px; margin-left: 10px",
    "breakLine": true
  }
}