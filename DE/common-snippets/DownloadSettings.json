{
  "download_setting_frame": {
    "type": "fieldset",
    "label": "Einstellungen herunterladen",
    "show_if": "output.custom_options=='true'",
    "repeat": false,
    "fields": {
      "output.options.location": {
        "type": "text",
        "label": "Standort angeben",
        "val": "A1",
        "breakLine": true
      },
      "output.options.formatted_as_table": {
        "type": "checkbox",
        "label": "Als Tabelle formatiert",
        "val": "true",
        "isOn": "true",
        "isOff": "false",
        "breakLine": true
      },
      "output.options.visibility": {
        "type": "select",
        "label": "Sichtbarkeit",
        "val": "current_flow",
        "options": {
          "current_flow": {
            "label": "Aktueller Flow"
          },
          "all_flow": {
            "label": "Alle Flows"
          }
        },
        "breakLine": true
      },
      "output.options.table_mode": {
        "type": "select",
        "label": "Einf√ºgen",
        "val": "content_only",
        "options": {
          "content_only": {
            "label": "Nur Inhalt"
          },
          "fully_formatted": {
            "label": "Formatiert"
          }
        }
      }
    },
    "breakLine": true,
    "style": "margin-bottom: 20px"
  }
}