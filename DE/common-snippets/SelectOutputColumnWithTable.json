{
  "output.select_output_columns": {
    "type": "checkbox",
    "label": "Ausgabe-Spalten Auswählen",
    "show_if": "output.custom_options=='true'",
    "val": "false",
    "isOn": "true",
    "isOff": "false",
    "style": "margin-top: 10px"
  },
  "output_columns": {
    "type": "fieldset",
    "label": "Ausgabe-Spalten Auswählen",
    "show_if": "output.custom_options=='true' && output.select_output_columns=='true'",
    "repeat": true,
    "fields": {
      "table_id": {
        "type": "select",
        "label": "Tabelle auswählen",
        "bind": "TABLES",
        "required": true
      },
      "field_name": {
        "type": "select",
        "label": "Spalte auswählen",
        "table": "this.table_id",
        "bind": "COLUMNS",
        "required": true
      },
      "order": {
        "type": "select",
        "label": "Reihenfolge",
        "options": {
          "-": {
            "label": "-"
          },
          "asc": {
            "label": "Asc"
          },
          "desc": {
            "label": "Desc"
          }
        },
        "val": "-"
      },
      "is_rename": {
        "type": "checkbox",
        "label": "Umbenennen",
        "val": "false",
        "isOn": "true",
        "isOff": "false"
      },
      "rename": {
        "show_if": "this.is_rename == 'true'",
        "type": "text",
        "required": true,
        "minLength": 1,
        "maxLength": 255,
        "label": "Neuer Spaltenname",
        "style": "margin-left: 10px"
      },
      "format__category": {
        "type": "select",
        "label": "Format",
        "required": true,
        "options": {
          "standard": {
            "label": "Standard"
          },
          "number": {
            "label": "Nummer"
          },
          "percentage": {
            "label": "Prozentsatz"
          },
          "text": {
            "label": "Text"
          },
          "date": {
            "label": "Datum"
          },
          "currency": {
            "label": "Währung"
          }
        },
        "val": "standard"
      },
      "format__rounding": {
        "type": "select",
        "label": "Abrunden",
        "show_if": "this.format__category=='number' || this.format__category=='percentage' || this.format__category=='currency'",
        "required": true,
        "options": {
          "no_rounding": {
            "label": "Keine Rundung"
          },
          "0": {
            "label": "0 Ziffern"
          },
          "1": {
            "label": "1 Ziffern"
          },
          "2": {
            "label": "2 Ziffern"
          },
          "3": {
            "label": "3 Ziffern"
          },
          "4": {
            "label": "4 Ziffern"
          },
          "5": {
            "label": "5 Ziffern"
          },
          "6": {
            "label": "6 Ziffern"
          },
          "7": {
            "label": "7 Ziffern"
          },
          "8": {
            "label": "8 Ziffern"
          },
          "9": {
            "label": "9 Ziffern"
          }
        },
        "val": "2"
      },
      "format__use_1000_separator": {
        "type": "checkbox",
        "label": "Use 1000 Separator(.)",
        "show_if": "this.format__category=='number' || this.format__category=='percentage' || this.format__category=='currency'",
        "val": "true",
        "isOn": "true",
        "isOff": "false"
      },
      "format__currency_symbol": {
        "type": "select",
        "label": "Symbol",
        "show_if": "this.format__category=='currency'",
        "required": true,
        "options": {
          "dollar": {
            "label": "$"
          },
          "euro": {
            "label": "€"
          },
          "pound": {
            "label": "£"
          },
          "yen": {
            "label": "¥ (Yen, currency of Japan)"
          },
          "colon": {
            "label": "₡ (Colón, currency of Costa Rica)"
          },
          "naira": {
            "label": "₦ (Naira, currency of Nigeria)"
          },
          "rupee": {
            "label": "₨ (Rupee, currency of India)"
          },
          "rupie": {
            "label": "₹ (Rupee, currency of India)"
          },
          "won": {
            "label": "₩ (Won, currency in South Korea and North Korea)"
          },
          "hebrew": {
            "label": "₪ (Currency of Israel)"
          },
          "dong": {
            "label": "₫ (Currency of Vietnam)"
          },
          "kip": {
            "label": "₭ (Kip, currency of Laos)"
          },
          "mongolia": {
            "label": "₮ (Currency of Mongolia)"
          },
          "peso": {
            "label": "₱ (Peso, currency of the Philippines)"
          },
          "guarani": {
            "label": "₲ (Guaraní, currency of Paraguay)"
          },
          "hryvnia": {
            "label": "₴ (Hryvnia, currency of Ukraine)"
          },
          "tenge": {
            "label": "₸ (Tenge, currency of Kazakhstan)"
          },
          "lira": {
            "label": "₺ (Lira, currency of Turkey)"
          },
          "manat": {
            "label": "₼ (Manat, currency of Azerbaijan)"
          },
          "rubles": {
            "label": "₽ (Rubles, currency of Russia)"
          },
          "lari": {
            "label": "₾ (Lari, currency of Georgia)"
          },
          "bitcoin": {
            "label": "₿ (Bitcoin, crypto currency)"
          }
        },
        "val": "dollar"
      },
      "format__date_format": {
        "type": "select",
        "label": "Typ",
        "show_if": "this.format__category=='date'",
        "required": true,
        "options": {
          "mm/dd/yyyy": {
            "label": "3/14/2012"
          },
          "D, M dd, Y": {
            "label": "Mittwoch, 14. März 2012"
          },
          "yyyy-mm-dd": {
            "label": "2012-03-14"
          }
        },
        "val": "mm/dd/yyyy",
        "breakLine": true
      },
      "delete": {
        "type": "button",
        "label": "Lösche",
        "function": "DELETE_ROW"
      }
    },
    "breakLine": true,
    "style": "margin-bottom: 20px"
  }
}