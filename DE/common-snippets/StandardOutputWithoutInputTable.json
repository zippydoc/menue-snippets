{
  "output": {
    "type": "fieldset",
    "label": "Ausgabetabelle definieren",
    "repeat": false,
    "fields": {
      "output.custom_text": {
        "type": "text",
        "label": "Name der Ausgabetabelle",
        "show_if": "output.custom_options=='false' || (output.custom_options=='true' && output.compose_table_name=='false')",
        "minLength": 1,
        "val": "",
        "width": "100%",
        "breakLine": true
      },
      "output.custom_options": {
        "type": "checkbox",
        "label": "Ausgabe-Optionen",
        "val": "false",
        "isOn": "true",
        "isOff": "false",
        "breakLine": true
      },
      "output.compose_table_name": {
        "type": "checkbox",
        "label": "Tabellenname zusammenstellen",
        "show_if": "output.custom_options=='true'",
        "val": "false",
        "isOn": "true",
        "isOff": "false",
        "breakLine": true
      },
      "compose_table_name_frame": {
        "type": "fieldset",
        "label": "Name der Ausgabetabelle zusammenstellen",
        "show_if": "output.custom_options=='true' && output.compose_table_name=='true'",
        "repeat": false,
        "fields": {
          "output.prefix_rule": {
            "type": "select",
            "label": "Präfix",
            "show_if": "output.compose_table_name=='true'",
            "val": "empty",
            "options": {
              "empty": {
                "label": "-"
              },
              "script_name": {
                "label": "Aufgabe Name"
              },
              "date_string_type1": {
                "label": "Datum: tt_mm_jjjj"
              },
              "date_string_type2": {
                "label": "Datum: jjjj_mm_tt"
              },
              "time_string": {
                "label": "Zeit: h_min_sec"
              },
              "datetime_string": {
                "label": "Datum_Zeit"
              },
              "custom": {
                "label": "Benutzerdefiniert"
              }
            }
          },
          "output.prefix_text": {
            "type": "text",
            "label": "Präfix Text",
            "show_if": "output.compose_table_name=='true' && output.prefix_rule=='custom'",
            "minLength": 1,
            "maxLength": 31,
            "val": ""
          },
          "output.core_delimiter": {
            "type": "text",
            "label": "Begrenzer",
            "show_if": "output.compose_table_name=='true' && output.prefix_rule!='empty'",
            "val": "_",
            "width": "50px"
          },
          "output.core_rule": {
            "type": "select",
            "label": "Tabelle Name",
            "show_if": "output.compose_table_name=='true'",
            "val": "script_name",
            "options": {
              "script_name": {
                "label": "Aufgabe Name"
              },
              "custom": {
                "label": "Benutzerdefiniert"
              }
            }
          },
          "output.core_text": {
            "type": "text",
            "label": "Name der Ausgabetabelle",
            "show_if": "output.compose_table_name=='true' && output.core_rule=='custom'",
            "required": true,
            "minLength": 1,
            "maxLength": 31,
            "val": ""
          },
          "output.suffix_delimiter": {
            "type": "text",
            "label": "Begrenzer",
            "show_if": "output.compose_table_name=='true'",
            "val": "_",
            "width": "50px"
          },
          "output.suffix_rule": {
            "type": "select",
            "label": "Suffix",
