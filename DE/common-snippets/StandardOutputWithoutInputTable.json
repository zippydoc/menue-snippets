{
  "output": {
    "type": "fieldset", 
    "label": "Ausgabetabelle definieren", 
    "repeat": false, 
    "fields": {
      "output.type": {
        "type": "select", 
        "label": "Tabelle Name", 
        "val": "auto", 
        "options": {
          "auto": {
            "label": "Auto"
          }, 
          "custom": {
            "label": "Benutzerdefiniert"
          }, 
          "compose": {
            "label": "Zusammenstellen"
          }
        }
      }, 
      "output.custom_text": {
        "type": "text", 
        "label": "Name der Ausgabetabelle", 
        "show_if": "output.type=='custom'", 
        "required": true, 
        "minLength": 1, 
        "maxLength": 31, 
        "val": "", 
        "breakLine": true
      }, 
      "output.prefix_rule": {
        "type": "select", 
        "label": "Präfix", 
        "show_if": "output.type=='compose'", 
        "val": "empty", 
        "options": {
          "empty": {
            "label": "-"
          }, 
          "script_name": {
            "label": "Aufgabe Name"
          }, 
          "date_string_type1": {
            "label": "Datum: tt_mm_jjjj"
          }, 
          "date_string_type2": {
            "label": "Datum: jjjj_mm_tt"
          }, 
          "time_string": {
            "label": "Zeit: h_min_sec"
          }, 
          "datetime_string": {
            "label": "Datum_Zeit"
          }, 
          "custom": {
            "label": "Benutzerdefiniert"
          }
        }
      }, 
      "output.prefix_text": {
        "type": "text", 
        "label": "Präfix Text", 
        "show_if": "output.type=='compose' && output.prefix_rule=='custom'", 
        "minLength": 1, 
        "maxLength": 31, 
        "val": ""
      }, 
      "output.core_delimiter": {
        "type": "text", 
        "label": "Begrenzer", 
        "show_if": "output.type=='compose' && output.prefix_rule!='empty'", 
        "val": "_", 
        "width": "50px"
      }, 
      "output.core_rule": {
        "type": "select", 
        "label": "Tabelle Name", 
        "show_if": "output.type=='compose'", 
        "val": "script_name", 
        "options": {
          "script_name": {
            "label": "Aufgabe Name"
          }, 
          "custom": {
            "label": "Benutzerdefiniert"
          }
        }
      }, 
      "output.core_text": {
        "type": "text", 
        "label": "Name der Ausgabetabelle", 
        "show_if": "output.type=='compose' && output.core_rule=='custom'", 
        "required": true, 
        "minLength": 1, 
        "maxLength": 31, 
        "val": ""
      }, 
      "output.suffix_delimiter": {
        "type": "text", 
        "label": "Begrenzer", 
        "show_if": "output.type=='compose'", 
        "val": "_", 
        "width": "50px"
      }, 
      "output.suffix_rule": {
        "type": "select", 
        "label": "Suffix", 
        "show_if": "output.type=='compose'", 
        "val": "script_name", 
        "options": {
          "empty": {
            "label": "-"
          }, 
          "script_name": {
            "label": "Aufgabe Name"
          }, 
          "date_string_type1": {
            "label": "Datum: tt_mm_jjjj"
          }, 
          "date_string_type2": {
            "label": "Datum: jjjj_mm_tt"
          }, 
          "time_string": {
            "label": "Zeit: h_min_sec"
          }, 
          "datetime_string": {
            "label": "Datum_Zeit"
          }, 
          "custom": {
            "label": "Benutzerdefiniert"
          }
        }
      }, 
      "output.suffix_text": {
        "type": "text", 
        "label": "Suffix Text", 
        "show_if": "output.type=='compose' && output.suffix_rule=='custom'", 
        "minLength": 1, 
        "maxLength": 31, 
        "val": "", 
        "breakLine": true
      }, 
      "output.timezone": {
        "type": "select", 
        "label": "Zeitzone", 
        "show_if": "output.type=='compose' && (output.prefix_rule=='date_string_type1' || output.prefix_rule=='date_string_type2' || output.prefix_rule=='time_string' || output.prefix_rule=='datetime_string' || output.core_rule=='date_string_type1' || output.core_rule=='date_string_type2' || output.core_rule=='time_string' || output.core_rule=='datetime_string' || output.suffix_rule=='date_string_type1' || output.suffix_rule=='date_string_type2' || output.suffix_rule=='time_string' || output.suffix_rule=='datetime_string')", 
        "val": "Europa/Berlin", 
        "required": true, 
        "options": {
          "Europa/Amsterdam": {
            "label": "Europa/Amsterdam"
          }, 
          "Europa/Berlin": {
            "label": "Europa/Berlin"
          }, 
          "Europa/Helsinki": {
            "label": "Europa/Helsinki"
          }, 
          "Europa/London": {
            "label": "Europa/London"
          }, 
          "Europa/Luxemburg": {
            "label": "Europa/Luxemburg"
          }, 
          "Europa/Oslo": {
            "label": "Europa/Oslo"
          }, 
          "Europa/Paris": {
            "label": "Europa/Paris"
          }, 
          "Europa/Rom": {
            "label": "Europa/Rom"
          }, 
          "US/Alaska": {
            "label": "US/Alaska"
          }, 
          "US/Zentral": {
            "label": "US/Zentral"
          }, 
          "US/Ost": {
            "label": "US/Ost"
          }, 
          "US/Pazifik": {
            "label": "US/Pazifik"
          }
        }, 
        "breakLine": true
      }, 
      "output.set_options": {
        "type": "checkbox", 
        "label": "Ausgabe-Optionen", 
        "val": "false", 
        "isOn": "true", 
        "isOff": "false"
      }, 
      "output_options_box": {
        "type": "fieldset", 
        "label": "Ausgabe-Optionen", 
        "show_if": "output.set_options=='true'", 
        "repeat": false, 
        "fields": {
          "output.options.location": {
            "type": "text", 
            "label": "Standort angeben", 
            "val": "A1", 
            "breakLine": true
          }, 
          "output.options.table_mode": {
            "type": "select", 
            "label": "Einfügen", 
            "val": "content_only", 
            "options": {
              "content_only": {
                "label": "Nur Inhalt"
              }, 
              "fully_formatted": {
                "label": "Vollständig formatiert"
              }
            }
          }, 
          "output.options.formatted_as_table": {
            "type": "checkbox", 
            "label": "Als Tabelle formatiert", 
            "val": "true", 
            "isOn": "true", 
            "isOff": "false", 
            "breakLine": true
          }, 
          "output.add_tag": {
            "type": "checkbox", 
            "label": "Tag hinzufügen", 
            "val": "false", 
            "isOn": "true", 
            "isOff": "false"
          }, 
          "output.tag_name": {
            "type": "text", 
            "label": "Tag Name", 
            "show_if": "output.add_tag=='true'", 
            "required": true, 
            "minLength": 1, 
            "maxLength": 255, 
            "val": "", 
            "breakLine": true, 
            "style": "margin-left: 15px"
          }, 
          "output.options.visibility": {
            "type": "select", 
            "label": "Sichtbarkeit", 
            "val": "current_flow", 
            "options": {
              "current_flow": {
                "label": "Stromfluss"
              }, 
              "all_flow": {
                "label": "Alle Flows"
              }
            }
          }
        }, 
        "breakLine": true
      }
    }
  }
}
