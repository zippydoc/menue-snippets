              "Revenues": {
                "label": "Revenues"
              },
              "Cost of Goods Sold": {
                "label": "Cost of Goods Sold"
              },
              "Marketing Expenses": {
                "label": "Marketing Expenses"
              },
              "Operating Expenses": {
                "label": "Operating Expenses"
              },
              "General and Administration Expenses": {
                "label": "General and Administration Expenses"
              },
              "Personnel Expenses": {
                "label": "Personnel Expenses"
              },
              "Tax": {
                "label": "Tax"
              },
              "Extraordinary Items": {
                "label": "Extraordinary Items"
              },
              "Capital Expenditure": {
                "label": "Capital Expenditure"
              },
              "Depreciation": {
                "label": "Depreciation"
              },
              "Amortization": {
                "label": "Amortization"
              },
              "Depretiation & Amortisation": {
                "label": "Depretiation & Amortisation"
              }
            },
            "val": "Revenues",
            "required": true
          },
          "revenues__sub_position": {
            "type": "select",
            "label": "Sub Position",
            "show_if": "this.position=='Revenues'",
            "options": {
              "Sales": {
                "label": "Sales"
              },
              "Increase(+)/Decrease(-) Inventories": {
                "label": "Increase(+)/Decrease(-) Inventories"
              },
              "Other Revenues": {
                "label": "Other Revenues"
              }
            },
            "val": "Sales",
            "required": true
          },
          "cost_of_goods_sold__sub_position": {
            "type": "select",
            "label": "Sub Position",
            "show_if": "this.position=='Cost of Goods Sold'",
            "options": {
              "Material Costs": {
                "label": "Material Costs"
              },
              "Purchased Services": {
                "label": "Purchased Services"
              }
            },
            "val": "Material Costs",
            "required": true
          },
          "general_administration_expenses__sub_position": {
            "type": "select",
            "label": "Sub Position",
            "show_if": "this.position=='General and Administration Expenses'",
            "options": {
              "Rental Expenses": {
                "label": "Rental Expenses"
              },
              "Cleaning": {
                "label": "Cleaning"
              },
              "Office Items": {
                "label": "Office Items"
              },
              "Consulting": {
                "label": "Consulting"
              },
              "Recruiting": {
                "label": "Recruiting"
              }
            },
            "val": "Rental Expenses",
            "required": true
          },
          "personnel_expenses__sub_position": {
            "type": "select",
            "label": "Sub Position",
            "show_if": "this.position=='Personnel Expenses'",
            "options": {
              "Fix Salary": {
                "label": "Fix Salary"
              },
              "Bonus": {
                "label": "Bonus"
              },
              "Other Compensation": {
                "label": "Other Compensation"
              }
            },
            "val": "Fix Salary",
            "required": true
          },
          "capital_expenditure__sub_position": {
            "type": "select",
            "label": "Sub Position",
            "show_if": "this.position=='Capital Expenditure'",
            "options": {
              "Maintenance Capex": {
                "label": "Maintenance Capex"
              },
              "Expansion Capex": {
                "label": "Expansion Capex"
              }
            },
            "val": "Maintenance Capex",
            "required": true
          },
          "extraordinary_items__sub_position": {
            "type": "select",
            "label": "Sub Position",
            "show_if": "this.position=='Extraordinary Items'",
            "options": {
              "Extraordinary Income": {
                "label": "Extraordinary Income"
              },
              "Extraordinary Expenses": {
                "label": "Extraordinary Expenses"
              }
            },
            "val": "Extraordinary Income",
            "required": true
          },
          "tax__sub_position": {
            "type": "select",
            "label": "Sub Position",
            "show_if": "this.position=='Tax'",
            "options": {
              "Value Added Tax": {
                "label": "Value Added Tax"
              },
              "Corporate Tax": {
                "label": "Corporate Tax"
              },
              "Trade Tax": {
                "label": "Trade Tax"
              }
            },
            "val": "Value Added Tax",
            "required": true
          },
          "interest__sub_position": {
            "type": "select",
            "label": "Sub Position",
            "show_if": "this.position=='Interest'",
            "options": {
              "Interest Income": {
                "label": "Interest Income"
              },
              "Interest Expenses": {
                "label": "Interest Expenses"
              }
            },
            "val": "Interest Income",
            "required": true
          },
          "depretiation_amortisation__sub_position": {
            "type": "select",
            "label": "Sub Position",
            "show_if": "this.position=='Depretiation & Amortisation'",
            "options": {
              "Depretiation": {
                "label": "Depretiation"
              },
              "Amortisation": {
                "label": "Amortisation"
              }
            },
            "val": "Depretiation",
            "required": true
          },
          "item": {
            "type": "text",
            "label": "Item",
            "val": ""
          },
          "amount": {
            "type": "number",
            "label": "Amount",
            "val": "",
            "required": true 
          },
          "payment_days": {
            "type": "number",
            "label": "Payment Days",
            "show_if": "this.no_payment=='false'",
            "val": "0"
          },
          "no_payment": {
            "type": "checkbox",
            "label": "No Payment",
            "val": "false",
            "isOn": "true",
            "isOff": "false",
            "breakLine": true
          },
          "initial_date": {
            "type": "date",
            "label": "(Initial) Date",
            "val": "",
            "required": true
          },
          "final_date": {
            "type": "date",
            "label": "Final Date",
            "show_if": "this.periodicy!='One-Off'",
            "val": "",
            "breakLine": true
          },
          "periodicy": {
            "type": "select",
            "label": "Repetition (of Position)",
            "required": true,
            "options": {
              "One-Off": {
                "label": "One-Off"
              },
              "Daily": {
                "label": "Daily"
              },
              "Weekly": {
                "label": "Weekly"
              },
              "Monthly": {
                "label": "Monthly"
              },
              "Quarterly": {
                "label": "Quarterly"
              },
              "Yearly": {
                "label": "Yearly"
              }
            },
            "val": "One-Off"
          },
          "growth": {
            "type": "number",
            "label": "Growth in %...",
            "show_if": "this.periodicy!='One-Off'",
            "val": ""
          },
          "growth_period": {
            "type": "select",
            "label": "...applied on Period",
            "show_if": "this.periodicy!='One-Off'",
            "options": {
              "selected_period": {
                "label": "Equal to Periodicy"
              },
              "Weekly": {
                "label": "Week on Week"
              },
              "Monthly": {
                "label": "Month on Month"
              },
              "Quarterly": {
                "label": "Quarter on Quarter"
              },
              "Yearly": {
                "label": "Year on Year"
              }
            },
            "val": "selected_period"
          },
          "growth_pattern": {
            "type": "select",
            "label": "Growth curve (within a period)",
            "show_if": "this.periodicy!='One-Off' && this.growth_period!='selected_period'",
            "options": {
              "Linear": {
                "label": "Linear"
              },
              "Compounding": {
                "label": "Compounding"
              },
              "Jump-Fix Linear": {
                "label": "Jump-Fix Linear"
              },
              "Jump-Fix Compounding": {
                "label": "Jump-Fix Compounding"
              }
            },
            "val": "Jump-Fix Linear",
            "breakLine": true
          },
          "delete": {
            "type": "button",
            "label": "Delete",
            "function": "DELETE_ROW"
          }
        }
      }
    }
  },
