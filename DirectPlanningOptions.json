{
  "options": {
    "type": "fieldset",
    "label": "Select Options",
    "repeat": false,
    "fields": {
      "options.type": {
        "type": "hidden",
        "val": "custom"
      },
      "options.custom.table": {
        "type": "fieldset",
        "label": "Planning Position",
        "repeat": true,
        "fields": {
          "position": {
            "type": "select",
            "label": "Position",
            "options": {
              "Sales Revenues": {
                "label": "Sales Revenues"
              },
              "Other Revenues": {
                "label": "Other Revenues"
              },
              "Cost of Goods Sold": {
                "label": "Cost of Goods Sold"
              },
              "Marketing Expenses": {
                "label": "Marketing Expenses"
              },
              "Operating Expenses": {
                "label": "Operating Expenses"
              },
              "General and Administration Expenses": {
                "label": "General and Administration Expenses"
              },
              "Rental Expenses": {
                "label": "Rental Expenses"
              },
              "Personnel Expenses": {
                "label": "Personnel Expenses"
              },
              "Tax": {
                "label": "Tax"
              },
              "Interest Income": {
                "label": "Interest Income"
              },
              "Interest Expenses": {
                "label": "Interest Expenses"
              },
              "Capital Expenditure": {
                "label": "Capital Expenditure"
              },
              "Maintenance Capex": {
                "label": "Maintenance Capex"
              },
              "Expansion Capex": {
                "label": "Expansion Capex,"
              },
              "Depreciation": {
                "label": "Depreciation"
              },
              "Amortization": {
                "label": "Amortization"
              },
              "Extraordinary Income": {
                "label": "Extraordinary Income"
              },
              "Extraordinary Expenses": {
                "label": "Extraordinary Expenses"
              }
            },
            "val": "Sales Revenues",
            "required": true
          },
          "sub_position": {
            "type": "select",
            "label": "Sub Position",
            "show_if": "this.position=='Personnel Expenses'",
            "options": {
              "Fix Salary": {
                "label": "Fix Salary"
              },
              "Bonus": {
                "label": "Bonus"
              },
              "Other Compensation": {
                "label": "Other Compensation"
              }
            },
            "val": "Fix Salary",
            "required": true,
            "breakLine": true
          },
          "item": {
            "type": "text",
            "label": "Item",
            "val": "",
            "required": true
          },
          "amount": {
            "type": "number",
            "label": "Amount",
            "val": "",
            "required": true
          },
          "initial_date": {
            "type": "date",
            "label": "Initial Date",
            "val": "",
            "required": true
          },
          "periodicy": {
            "type": "select",
            "label": "Periodicy",
            "required": true,
            "options": {
              "One-Off": {
                "label": "One-Off"
              },
              "Daily": {
                "label": "Daily"
              },
              "Work-Daily": {
                "label": "Work-Daily"
              },
              "Weekly": {
                "label": "Weekly"
              },
              "Monthly": {
                "label": "Monthly"
              },
              "Quarterly": {
                "label": "Quarterly"
              },
              "Yearly": {
                "label": "Yearly"
              }
            },
            "val": "One-Off"
          },
          "growth": {
            "type": "number",
            "label": "Growth",
            "show_if": "this.periodicy!='One-Off'",
            "val": "",
            "required": true
          },
          "growth_period": {
            "type": "select",
            "label": "Growth Period",
            "show_if": "this.periodicy!='One-Off'",
            "options": {
              "Weekly": {
                "label": "Weekly"
              },
              "Monthly": {
                "label": "Month on Month"
              },
              "Quarterly": {
                "label": "Quarter on Quarter"
              },
              "Yearly": {
                "label": "Year on Year"
              }
            },
            "val": "Weekly"
          },
          "growth_pattern": {
            "type": "select",
            "label": "Growth Pattern",
            "show_if": "this.periodicy!='One-Off'",
            "options": {
              "Compounding": {
                "label": "Compounding"
              },
              "Linear": {
                "label": "Linear"
              },
              "Jump-Fix": {
                "label": "Jump-Fix"
              }
            },
            "val": "Compounding"
          },
          "final_date": {
            "type": "date",
            "label": "Final Date",
            "show_if": "this.periodicy!='One-Off'",
            "val": ""
          },
          "delete": {
            "type": "button",
            "label": "Delete",
            "function": "DELETE_ROW"
          }
        }
      },
      "planning_configuration_frame": {
        "type": "fieldset",
        "label": "Planning Configuration",
        "repeat": false,
        "fields": {
          "options.end_date": {
            "type": "date",
            "label": "End Date of Table",
            "show_if": "options.type=='process' || options.type=='custom'",
            "val": "",
            "required": true,
            "style": "width: 300px"
          },
          "options.categories": {
            "type": "fieldset",
            "label": "Add Categories",
            "repeat": true,
            "fields": {
              "category": {
                "type": "text",
                "label": "Category",
                "val": "",
                "required": true
              },
              "value": {
                "type": "text",
                "label": "Value",
                "val": "",
                "required": true
              },
              "delete": {
                "type": "button",
                "label": "Delete",
                "function": "DELETE_ROW"
              }
            }
          }
        }
      }
    }
  }
}