{
  "planing_position_frame": {
    "type": "fieldset",
    "label": "Configure planning position",
    "repeat": false,
    "fields": {
      "options.task_symbol": {
        "type": "hidden",
        "val": "direct_planning"
      },
      "options.direct_planning__configure_by_table": {
        "type": "checkbox",
        "label": "Configure by table",
        "val": "false",
        "isOn": "true",
        "isOff": "false",
        "breakLine": true
      },
      "table_id": {
        "type": "select",
        "label": "Select Table",
        "show_if": "options.direct_planning__configure_by_table=='true'",
        "bind": "TABLES",
        "empty": "",
        "required": true,
        "breakLine": true
      },
      "options.configure_by_table_help": {
        "type": "textarea",
        "label": "",
        "show_if": "options.direct_planning__configure_by_table=='true' && (options.output_type=='profit_loss' || options.output_type=='cash_flow')",
        "val": "Table Structure ( Initial Date | Periodicy | Final Date | Amount | Growth | Growth Period | Growth Pattern | Payment Days )",
        "disabled": true,
        "md_no_resize": false,
        "width": "100%"
      },
      "options.configure_by_table_help2": {
        "type": "textarea",
        "label": "",
        "show_if": "options.direct_planning__configure_by_table=='true' && options.output_type=='assumptions' && (options.assumptions_type=='planning_assumptions' || options.assumptions_type=='transaction_list_details')",
        "val": "Table Structure ( Initial Date | Periodicy | Final Date | Amount | Growth | Growth Period | Growth Pattern | Payment Days )",
        "disabled": true,
        "md_no_resize": false,
        "width": "100%"
      },
      "options.configure_by_table_help3": {
        "type": "textarea",
        "label": "",
        "show_if": "options.direct_planning__configure_by_table=='true' && options.output_type=='assumptions' && options.assumptions_type=='balance_sheet_details'",
        "val": "Table Structure ( Position | Sub Position | Item | Initial Date | Periodicy | Final Date | Amount | Growth | Growth Period | Growth Pattern | Payment Days )",
        "disabled": true,
        "md_no_resize": false,
        "width": "100%"
      },
      "options.configure_by_table_help4": {
        "type": "textarea",
        "label": "",
        "show_if": "options.direct_planning__configure_by_table=='true' && options.output_type=='balance_sheet'",
        "val": "Table Structure ( Position | Sub Position | Item | Initial Date | Periodicy | Final Date | Amount | Growth | Growth Period | Growth Pattern | Payment Days )",
        "disabled": true,
        "md_no_resize": false,
        "width": "100%"
      },
      "options.custom.table": {
        "type": "fieldset",
