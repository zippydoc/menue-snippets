{
  "options_box": {
    "type": "fieldset",
    "Etiqueta": "Opciones",
    "repeat": false,
    "fields": {
      "options.add_columns": {
        "type": "fieldset",
        "Etiqueta": "Agregar Nuevas Columnas",
        "repeat": true,
        "fields": {
          "new_column_name": {
            "type": "text",
            "Etiqueta": "Nuevo Nombre de Columna",
            "val": "",
            "breakLine": true
          },
          "task_type": {
            "type": "select",
            "Etiqueta": "Tipo de Tarea",
            "required": true,
            "options": {
              "static_value": {
                "Etiqueta": "Valor Estático"
              },
              "numeric_formula": {
                "Etiqueta": "Formula Numérica"
              },
              "text_formula": {
                "Etiqueta": "Formula del Texto"
              },
              "multi_column_functions": {
                "Etiqueta": "Múltiples Columnas"
              },
              "multi_row_functions": {
                "Etiqueta": "Múltiples Filas"
              }
            },
            "val": "multi_column_functions",
            "breakLine": true
          },
          "multi_column_functions__function": {
            "type": "select",
            "Etiqueta": "Función",
            "show_if": "this.task_type=='multi_column_functions'",
            "required": true,
            "options": {
              "sum": {
                "Etiqueta": "Suma"
              },
              "average": {
                "Etiqueta": "Promedio"
              },
              "median": {
                "Etiqueta": "Mediana"
              },
              "max": {
                "Etiqueta": "Máx"
              },
              "min": {
                "Etiqueta": "Mín"
              },
              "standard_deviation": {
                "Etiqueta": "Desviación Estándar"
              },
              "product": {
                "Etiqueta": "Producto"
              },
              "quota": {
                "Etiqueta": "Cuota"
              },
              "absolute_growth": {
                "Etiqueta": "Crecimiento Absoluto"
              },
              "relative_growth": {
                "Etiqueta": "Crecimiento Relativo"
              },
              "compound_growth": {
                "Etiqueta": "Crecimiento Compuesto"
              }
            },
            "val": "sum"
          },
          "multi_column_functions__sel_columns": {
            "type": "select",
            "Etiqueta": "Seleccionar Columna(s)",
            "show_if": "this.task_type=='multi_column_functions' && (this.multi_column_functions__function=='sum' || this.multi_column_functions__function=='average' || this.multi_column_functions__function=='median' || this.multi_column_functions__function=='max' || this.multi_column_functions__function=='min' || this.multi_column_functions__function=='standard_deviation' || this.multi_column_functions__function=='product')",
            "table": "input_table",
            "bind": "COLUMNS",
            "multiple": true,
            "required": true
          },
          "multi_column_functions__column_a": {
            "type": "select",
            "Etiqueta": "Seleccionar Columna A",
            "show_if": "this.task_type=='multi_column_functions' && (this.multi_column_functions__function=='quota' || this.multi_column_functions__function=='absolute_growth' || this.multi_column_functions__function=='relative_growth' || this.multi_column_functions__function=='compound_growth')",
            "table": "input_table",
            "bind": "COLUMNS",
            "required": true
          },
          "multi_column_functions__column_b": {
            "type": "select",
            "Etiqueta": "Seleccionar Columna B",
            "show_if": "this.task_type=='multi_column_functions' && (this.multi_column_functions__function=='quota' || this.multi_column_functions__function=='absolute_growth' || this.multi_column_functions__function=='relative_growth' || this.multi_column_functions__function=='compound_growth')",
            "table": "input_table",
            "bind": "COLUMNS",
            "required": true
          },
          "multi_column_functions__n_value": {
            "type": "text",
            "Etiqueta": "N (entero positivo)",
            "show_if": "this.task_type=='multi_column_functions' && this.multi_column_functions__function=='compound_growth'",
            "val": "1",
            "required": true
          },
          "multi_row_functions__function": {
            "type": "select",
            "Etiqueta": "Función",
            "show_if": "this.task_type=='multi_row_functions'",
            "required": true,
            "options": {
              "trailing_row_n_average": {
                "Etiqueta": "Promedio Móvil de la Fila N"
              },
              "index_from_row_n": {
                "Etiqueta": "Índice de la Fila N"
              },
              "absolute_growth": {
                "Etiqueta": "Crecimiento Absoluto"
              },
              "relative_growth": {
                "Etiqueta": "Crecimiento Relativo"
              },
              "compound_growth": {
                "Etiqueta": "Crecimiento Compuesto"
              }
            },
            "val": "trailing_row_n_average"
          },
          "multi_row_functions__sel_column": {
            "type": "select",
            "Etiqueta": "Columna",
            "show_if": "this.task_type=='multi_row_functions' && (this.multi_row_functions__function=='trailing_row_n_average' || this.multi_row_functions__function=='index_from_row_n' || this.multi_row_functions__function=='absolute_growth' || this.multi_row_functions__function=='relative_growth' || this.multi_row_functions__function=='compound_growth')",
            "table": "input_table",
            "bind": "COLUMNS",
            "required": true
          },
          "multi_row_functions__n_value": {
            "type": "text",
            "Etiqueta": "N (entero positivo)",
            "show_if": "this.task_type=='multi_row_functions' && (this.multi_row_functions__function=='trailing_row_n_average' || this.multi_row_functions__function=='index_from_row_n' || this.multi_row_functions__function=='compound_growth')",
            "val": "1",
            "required": true,
            "breakLine": true
          },
          "static_value": {
            "type": "text",
            "Etiqueta": "Ingresar Valor",
            "show_if": "this.task_type=='static_value'",
            "val": "",
            "width": "100%"
          },
          "numeric_formula": {
            "type": "text",
            "Etiqueta": "Fórmula de entrada, por ejemplo, [Columna1] + 2 * ([Fila-1:Columna3] - [Columna1])",
            "show_if": "this.task_type=='numeric_formula'",
            "val": "",
            "width": "100%"
          },
          "text_formula": {
            "type": "text",
            "Etiqueta": "Fórmula de entrada, por ejemplo, [firstName] + ' + [lastName] + ' ' + [Row-1:Column3]",
            "show_if": "this.task_type=='text_formula'",
            "val": "",
            "width": "100%"
          },
          "use_multi_row": {
            "type": "checkbox",
            "Etiqueta": "Opciones de Múltiples Filas",
            "show_if": "this.task_type=='numeric_formula' || this.task_type=='text_formula'",
            "val": "false",
            "isOn": "true",
            "isOff": "false"
          },
          "if_row_not_exist": {
            "type": "select",
            "Etiqueta": "Si la fila referida no existe",
            "show_if": "this.use_multi_row=='true' && (this.task_type=='numeric_formula' || this.task_type=='text_formula')",
            "required": true,
            "options": {
              "empty": {
                "Etiqueta": "Vacío o 0"
              },
              "closest": {
                "Etiqueta": "Valor Más Cercano de la Fila"
              }
            },
            "val": "empty",
            "breakLine": true
          },
          "format__category": {
            "type": "select",
            "Etiqueta": "Formato",
            "required": true,
            "options": {
              "standard": {
                "Etiqueta": "Estándar"
              },
              "number": {
                "Etiqueta": "Número"
              },
              "percentage": {
                "Etiqueta": "Porcentaje"
              },
              "text": {
                "Etiqueta": "Texto"
              },
              "date": {
                "Etiqueta": "Fecha"
              },
              "currency": {
                "Etiqueta": "Moneda"
              }
            },
            "val": "standard"
          },
          "format__rounding": {
            "type": "select",
            "Etiqueta": "Redondeo",
            "show_if": "this.format__category=='number' || this.format__category=='percentage' || this.format__category=='currency'",
            "required": true,
            "options": {
              "no_rounding": {
                "Etiqueta": "Redondeo de números"
              },
              "0": {
                "Etiqueta": "0 dígitos"
              },
              "1": {
                "Etiqueta": "1 dígitos"
              },
              "2": {
                "Etiqueta": "2 dígitos"
              },
              "3": {
                "Etiqueta": "3 dígitos"
              },
              "4": {
                "Etiqueta": "4 dígitos"
              },
              "5": {
                "Etiqueta": "5 dígitos"
              },
              "6": {
                "Etiqueta": "6 dígitos"
              },
              "7": {
                "Etiqueta": "7 dígitos"
              },
              "8": {
                "Etiqueta": "8 dígitos"
              },
              "9": {
                "Etiqueta": "9 dígitos"
              }
            },
            "val": "2"
          },
          "format__use_1000_separator": {
            "type": "checkbox",
            "Etiqueta": "Usar el Separador de 1000(.)",
            "show_if": "this.format__category=='number' || this.format__category=='percentage' || this.format__category=='currency'",
            "val": "true",
            "isOn": "true",
            "isOff": "false"
          },
          "format__currency_symbol": {
            "type": "select",
            "Etiqueta": "Símbolo",
            "show_if": "this.format__category=='currency'",
            "required": true,
            "options": {
              "dollar": {
                "Etiqueta": "$"
              },
              "euro": {
                "Etiqueta": "€"
              },
              "pound": {
                "Etiqueta": "£"
              }
            },
            "val": "dollar"
          },
          "format__date_format": {
            "type": "select",
            "Etiqueta": "Tipo",
            "show_if": "this.format__category=='date'",
            "required": true,
            "options": {
              "mm/dd/yyyy": {
                "Etiqueta": "3/14/2012"
              },
              "D, M dd, Y": {
                "Etiqueta": "Miércoles, 14 de marzo de 2012"
              },
              "yyyy-mm-dd": {
                "Etiqueta": "40982"
              }
            },
            "val": "mm/dd/yyyy",
            "breakLine": true
          },
          "group_counter__inc_dec_curly_bracket": {
            "type": "checkbox",
            "Etiqueta": "Aumentar/Disminuir con {}",
            "show_if": "this.task_type=='group_counter'",
            "val": "false",
            "isOn": "true",
            "isOff": "false",
            "breakLine": true
          },
          "group_counter__inc_dec_square_bracket": {
            "type": "checkbox",
            "Etiqueta": "Aumentar/Disminuir con []",
            "show_if": "this.task_type=='group_counter'",
            "val": "false",
            "isOn": "true",
            "isOff": "false",
            "breakLine": true
          },
          "group_counter__inc_dec_parenthesis": {
            "type": "checkbox",
            "Etiqueta": "Aumentar/Disminuir con ()",
            "show_if": "this.task_type=='group_counter'",
            "val": "false",
            "isOn": "true",
            "isOff": "false",
            "breakLine": true
          },
          "group_counter__custom": {
            "type": "checkbox",
            "Etiqueta": "Contador de Grupo Personalizado",
            "show_if": "this.task_type=='group_counter'",
            "val": "false",
            "isOn": "true",
            "isOff": "false",
            "breakLine": true
          },
          "group_counter__column": {
            "type": "select",
            "Etiqueta": "Columna",
            "show_if": "this.task_type=='group_counter' && this.group_counter__custom=='true'",
            "table": "input_table",
            "bind": "COLUMNS",
            "required": true
          },
          "group_counter__type": {
            "type": "select",
            "Etiqueta": "Cuando",
            "show_if": "this.task_type=='group_counter' && this.group_counter__custom=='true'",
            "required": true,
            "options": {
              "starts_with": {
                "Etiqueta": "EMPEZAR CON"
              },
              "ends_with": {
                "Etiqueta": "TERMINA CON"
              },
              "starts_with_regex": {
                "Etiqueta": "EMPEZAR CON EXPRESIÓN REGULAR"
              },
              "ends_with_regex": {
                "Etiqueta": "TERMINA CON EXPRESIÓN REGULAR"
              }
            },
            "val": "starts_with"
          },
          "group_counter__value": {
            "type": "text",
            "Etiqueta": "Ingresar Valor",
            "show_if": "this.task_type=='group_counter' && this.group_counter__custom=='true'",
            "val": ".",
            "breakLine": true
          },
          "group_counter__label": {
            "type": "text",
            "Etiqueta": "Ingresar Etiqueta",
            "show_if": "this.task_type=='group_counter' && this.group_counter__custom=='true'",
            "val": "sentence",
            "breakLine": true
          },
          "group_counter__if_end_of_string_start_labeling_with_next_record": {
            "type": "checkbox",
            "Etiqueta": "Si el Final de la Cadena de Caracteres empieza el etiquetado con el registro siguiente",
            "show_if": "this.task_type=='group_counter' && this.group_counter__custom=='true' && (this.group_counter__type=='starts_with' || this.group_counter__type=='starts_with_regex')",
            "val": "true",
            "isOn": "true",
            "isOff": "false"
          },
          "group_counter__if_begin_of_string_start_labeling_with_previous_record": {
            "type": "checkbox",
            "Etiqueta": "Si el Comienzo de la Cadena de Caracteres empieza el etiquetado con el registro previo",
            "show_if": "this.task_type=='group_counter' && this.group_counter__custom=='true' && (this.group_counter__type=='ends_with' || this.group_counter__type=='ends_with_regex')",
            "val": "true",
            "isOn": "true",
            "isOff": "false"
          },
          "group_counter__always_include": {
            "type": "checkbox",
            "Etiqueta": "Incluir siempre",
            "show_if": "this.task_type=='group_counter' && this.group_counter__custom=='true'",
            "val": "true",
            "isOn": "true",
            "isOff": "false"
          },
          "apply_to_group__enabled": {
            "type": "checkbox",
            "Etiqueta": "Aplicar al Grupo",
            "show_if": "this.task_type!='static_value'",
            "val": "false",
            "isOn": "true",
            "isOff": "false"
          },
          "apply_to_group__group_columns": {
            "type": "select",
            "Etiqueta": "Columna(s) del Grupo",
            "show_if": "this.apply_to_group__enabled == 'true' && this.task_type!='static_value'",
            "table": "input_table",
            "bind": "COLUMNS",
            "multiple": true,
            "required": true
          },
          "delete": {
            "type": "button",
            "Etiqueta": "Borrar",
            "function": "DELETE_ROW"
          }
        }
      },
      "options.use_filter": {
        "type": "checkbox",
        "Etiqueta": "Usar Filtro",
        "val": "false",
        "isOn": "true",
        "isOff": "false",
        "style": "margin-top: 10px",
        "breakLine": true
      },
      "filter_box": {
        "type": "fieldset",
        "Etiqueta": "Opciones del Filtro",
        "show_if": "options.use_filter=='true'",
        "repeat": false,
        "fields": {
          "options.filter": {
            "type": "fieldset",
            "Etiqueta": "Agregar Condiciones del Filtro",
            "repeat": true,
            "fields": {
              "initial_op": {
                "type": "select",
                "Etiqueta": "Seleccionar Operador Lógico",
                "required": true,
                "show_if" :  "$index == 0",
                "options": {
                  "if": {
                    "Etiqueta": "SI"
                  },
                  "if_not": {
                    "Etiqueta": "SI NO"
                  }
                },
                "val": "if"
              },
              "op": {
                "type": "select",
                "Etiqueta": "Seleccionar Operador Lógico",
                "required": true,
                "show_if" :  "$index > 0",
                "options": {
                  "and": {
                    "Etiqueta": "AND"
                  },
                  "or": {
                    "Etiqueta": "OR"
                  },
                  "and_not": {
                    "Etiqueta": "AND NOT"
                  },
                  "or_not": {
                    "Etiqueta": "OR NOT"
                  }
                },
                "val": "and"
              },
              "column": {
                "type": "select",
                "Etiqueta": "Seleccionar Columna",
                "table": "input_table",
                "bind": "COLUMNS",
                "required": true
              },
              "condition": {
                "type": "select",
                "Etiqueta": "Seleccionar Operador",
                "required": true,
                "options": {
                  "CONTIENE": {
                    "Etiqueta": "CONTIENE"
                  },
                  "CONTAINED IN STRING": {
                    "Etiqueta": "CONTENIDO EN"
                  },
                  "EMPEZAR CON": {
                    "Etiqueta": "EMPEZAR CON"
                  },
                  "TERMINA CON": {
                    "Etiqueta": "TERMINA CON"
                  },
                  "=": {
                    "Etiqueta": "IGUAL"
                  },
                  ">": {
                    "Etiqueta": "MAYOR QUE"
                  },
                  ">=": {
                    "Etiqueta": "MAYOR O IGUAL"
                  },
                  "<": {
                    "Etiqueta": "MENOR QUE"
                  },
                  "<=": {
                    "Etiqueta": "MENOR O IGUAL"
                  },
                  "EMPTY": {
                    "Etiqueta": "ESTÁ VACÍO"
                  },
                  "BLANK": {
                    "Etiqueta": "ESTÁ EN BLANCO"
                  },
                  "IS NULL": {
                    "Etiqueta": "IS NULL"
                  },
                  "COINCIDENCIA CON LA EXPRESIÓN REGULAR": {
                    "Etiqueta": "COINCIDENCIA CON LA EXPRESIÓN REGULAR"
                  },
                  "CONTAINED IN TABLE": {
                    "Etiqueta": "EN TABLA"
                  },
                  "EN LA TABLA DE LAS EXPRESIONES REGULARES": {
                    "Etiqueta": "EN LA TABLA DE LAS EXPRESIONES REGULARES"
                  },
                  "FECHA": {
                    "Etiqueta": "FECHA"
                  }
                },
                "val": "CONTIENE"
              },
              "date_condition": {
                "type": "select",
                "Etiqueta": "Seleccionar Operador de la Fecha",
                "required": true,
                "show_if": "this.condition == 'DATE'",
                "options": {
                  "<": {
                    "Etiqueta": "PREVIO"
                  },
                  "<=": {
                    "Etiqueta": "PREVIO O IGUAL"
                  },
                  "=": {
                    "Etiqueta": "IGUAL"
                  },
                  ">": {
                    "Etiqueta": "DESPUÉS"
                  },
                  ">=": {
                    "Etiqueta": "DESPUÉS O IGUAL"
                  }
                },
                "val": "="
              },
              "case_sensitive": {
                "type": "select",
                "Etiqueta": "Seleccionar Sensible a Mayúsculas y Minúsculas",
                "required": true,
                "show_if": "this.condition == 'CONTAINS' || this.condition == 'CONTAINED IN STRING' || this.condition == '=' || this.condition == 'STARTS WITH' || this.condition == 'ENDS WITH' || this.condition == 'REGEX MATCH' || this.condition == 'NOT REGEX MATCH' || this.condition == 'IN REGEX TABLE' || this.condition == 'NOT IN REGEX TABLE'",
                "options": {
                  "yes": {
                    "Etiqueta": "Sensible a Mayúsculas y Minúsculas"
                  },
                  "no": {
                    "Etiqueta": "Insensible a Mayúsculas y Minúsculas"
                  }
                },
                "val": "yes"
              },
              "date_value": {
                "type": "date",
                "Etiqueta": "Seleccionar Fecha",
                "show_if": "this.condition=='DATE'",
                "val": "1/31/2020",
                "required": true
              },
              "value": {
                "type": "text",
                "Etiqueta": "Ingresar Valor",
                "show_if": "this.condition != 'DATE' && this.condition != 'EMPTY' && this.condition != 'NOT EMPTY' && this.condition != 'BLANK' && this.condition != 'NOT BLANK' && this.condition != 'IS NULL' && this.condition != 'IS NOT NULL' && this.condition != 'CONTAINED IN TABLE' && this.condition != 'NOT CONTAINED IN TABLE' && this.condition != 'IN REGEX TABLE' && this.condition != 'NOT IN REGEX TABLE'",
                "val": "",
                "required": true
              },
              "compare_table": {
                "type": "select",
                "Etiqueta": "Seleccionar Tabla",
                "bind": "TABLES",
                "empty": "",
                "show_if": "this.condition == 'CONTAINED IN TABLE' || this.condition == 'NOT CONTAINED IN TABLE' || this.condition == 'IN REGEX TABLE' || this.condition == 'NOT IN REGEX TABLE'",
                "required": true
              },
              "compare_column": {
                "type": "select",
                "Etiqueta": "Seleccionar Columna",
                "table": "this.compare_table",
                "bind": "COLUMNS",
                "show_if": "this.condition == 'CONTAINED IN TABLE' || this.condition == 'NOT CONTAINED IN TABLE' || this.condition == 'IN REGEX TABLE' || this.condition == 'NOT IN REGEX TABLE'",
                "required": true
              },
              "delete": {
                "type": "button",
                "Etiqueta": "Borrar",
                "function": "DELETE_ROW"
              }
            }
          },
          "options.apply_to_record": {
            "type": "checkbox",
            "Etiqueta": "Aplicar al Registro",
            "val": "true",
            "isOn": "true",
            "isOff": "false",
            "style": "margin-top: 10px",
            "breakLine": true
          },
          "apply_to_group_box": {
            "type": "fieldset",
            "Etiqueta": "Aplicar al Grupo",
            "show_if": "options.apply_to_record=='false'",
            "repeat": false,
            "fields": {
              "options.apply_to_group_columns": {
                "type": "select",
                "Etiqueta": "Seleccionar Columnas del Grupo",
                "required": true,
                "width": "100%",
                "table": "input_table",
                "multiple": true,
                "bind": "COLUMNS",
                "breakLine": true
              }
            }
          }
        }
      }
    }
  }
}