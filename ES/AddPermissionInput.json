{
  "input": {
    "type": "fieldset",
    "Etiqueta": "Seleccionar Tabla de Entrada",
    "repeat": false,
    "fields": {
      "input.table_or_flow": {
        "type": "select",
        "Etiqueta": "Tipo",
        "required": true,
        "options": {
          "set_table_permissions": {
            "Etiqueta": "Establecer los permisos de la tabla"
          },
          "set_flow_permissions": {
            "Etiqueta": "Establecer permisos de flujo"
          }
        },
        "val": "set_table_permissions"
      },
      "input.table_id": {
        "type": "select",
        "Etiqueta": "Seleccionar Tabla",
        "show_if": "input.table_or_flow=='set_table_permissions'",
        "bind": "TABLES",
        "empty": "",
        "required": true
      }
    }
  }
}