{
  "options": {
    "type": "fieldset",
    "label": "Seleccionar Opciones",
    "repeat": false,
    "fields": {
      "options.position": {
        "type": "select",
        "label": "Seleccionar Posición",
        "show_if": "$input_table.numOfLabel >= 1",
        "required": true,
        "options": {
          "add_rows_at_table_end": {
            "label": "Add row on bottom"
          },
          "add_row_on_top": {
            "label": "Add row on top"
          },
          "add_rows_above_selected_row": {
            "label": "Add row above selection"
          },
          "add_rows_below_selected_row": {
            "label": "Add row below selection"
          },
          "add_row_after_each_group": {
            "label": "Agregar fila después de cada grupo"
          }
        },
        "val": "add_rows_at_table_end",
        "breakLine": true
      },
      "options.group_column": {
        "type": "select",
        "label": "Columna del Grupo",
        "show_if": "$input_table.numOfLabel < 1 && options.position=='add_row_after_each_group'",
        "table": "input_table",
        "bind": "COLUMNS",
        "required": true,
        "breakLine": true
      },
      "options.use_filter": {
        "type": "select",
        "label": "Seleccionar Tipo",
        "show_if": "false",
        "required": true,
        "options": {
          "false": {
            "label": "Añadir una fila por cada fila de la tabla"
          },
          "true": {
            "label": "Añadir una fila por cada fila especificada"
          }
        },
        "val": "false",
        "breakLine": true,
        "style": "margin-top: 10px"
      },
      "filter_box": {
        "type": "fieldset",
        "label": "Opciones del Filtro",
        "show_if": "options.use_filter=='true' && $input_table.numOfLabel < 1",
        "repeat": false,
        "fields": {
          "options.filter": {
            "type": "fieldset",
            "label": "Agregar Condiciones del Filtro",
            "repeat": true,
            "fields": {
              "initial_op": {
                "type": "select",
                "label": "Seleccionar Operador Lógico",
                "required": true,
                "show_if": "$index == 0",
                "options": {
                  "if": {
                    "label": "SI"
                  },
                  "if_not": {
                    "label": "SI NO"
                  }
                },
                "val": "if"
              },
              "op": {
                "type": "select",
                "label": "Seleccionar Operador Lógico",
                "required": true,
                "show_if": "$index > 0",
                "options": {
                  "and": {
                    "label": "AND"
                  },
                  "or": {
                    "label": "OR"
                  },
                  "and_not": {
                    "label": "AND NOT"
                  },
                  "or_not": {
                    "label": "OR NOT"
                  }
                },
                "val": "and"
              },
              "column": {
                "type": "select",
                "label": "Seleccionar Columna",
                "table": "input_table",
                "bind": "COLUMNS",
                "required": true
              },
              "condition": {
                "type": "select",
                "label": "Seleccionar Operador",
                "required": true,
                "options": {
                  "CONTIENE": {
                    "label": "CONTIENE"
                  },
                  "CONTAINED IN STRING": {
                    "label": "CONTENIDO EN"
                  },
                  "EMPEZAR CON": {
                    "label": "EMPEZAR CON"
                  },
                  "TERMINA CON": {
                    "label": "TERMINA CON"
                  },
                  "=": {
                    "label": "IGUAL"
                  },
                  ">": {
                    "label": "MAYOR QUE"
                  },
                  ">=": {
                    "label": "MAYOR O IGUAL"
                  },
                  "<": {
                    "label": "MENOR QUE"
                  },
                  "<=": {
                    "label": "MENOR O IGUAL"
                  },
                  "EMPTY": {
                    "label": "ESTÁ VACÍO"
                  },
                  "BLANK": {
                    "label": "ESTÁ EN BLANCO"
                  },
                  "IS NULL": {
                    "label": "IS NULL"
                  },
                  "COINCIDENCIA CON LA EXPRESIÓN REGULAR": {
                    "label": "COINCIDENCIA CON LA EXPRESIÓN REGULAR"
                  },
                  "CONTAINED IN TABLE": {
                    "label": "EN TABLA"
                  },
                  "EN LA TABLA DE LAS EXPRESIONES REGULARES": {
                    "label": "EN LA TABLA DE LAS EXPRESIONES REGULARES"
                  },
                  "FECHA": {
                    "label": "FECHA"
                  }
                },
                "val": "CONTIENE"
              },
              "date_condition": {
                "type": "select",
                "label": "Seleccionar Operador de la Fecha",
                "required": true,
                "show_if": "this.condition == 'DATE'",
                "options": {
                  "<": {
                    "label": "PREVIO"
                  },
                  "<=": {
                    "label": "PREVIO O IGUAL"
                  },
                  "=": {
                    "label": "IGUAL"
                  },
                  ">": {
                    "label": "DESPUÉS"
                  },
                  ">=": {
                    "label": "DESPUÉS O IGUAL"
                  }
                },
                "val": "="
              },
              "case_sensitive": {
                "type": "select",
                "label": "Seleccionar Sensible a Mayúsculas y Minúsculas",
                "required": true,
                "show_if": "this.condition == 'CONTAINS' || this.condition == 'CONTAINED IN STRING' || this.condition == '=' || this.condition == 'STARTS WITH' || this.condition == 'ENDS WITH' || this.condition == 'REGEX MATCH' || this.condition == 'NOT REGEX MATCH' || this.condition == 'IN REGEX TABLE' || this.condition == 'NOT IN REGEX TABLE'",
                "options": {
                  "yes": {
                    "label": "Sensible a Mayúsculas y Minúsculas"
                  },
                  "no": {
                    "label": "Insensible a Mayúsculas y Minúsculas"
                  }
                },
                "val": "yes"
              },
              "date_value": {
                "type": "date",
                "label": "Seleccionar Fecha",
                "show_if": "this.condition=='DATE'",
                "val": "",
                "required": true
              },
              "value": {
                "type": "text",
                "label": "Ingresar Valor",
                "show_if": "this.condition != 'DATE' && this.condition != 'EMPTY' && this.condition != 'NOT EMPTY' && this.condition != 'BLANK' && this.condition != 'NOT BLANK' && this.condition != 'IS NULL' && this.condition != 'IS NOT NULL' && this.condition != 'CONTAINED IN TABLE' && this.condition != 'NOT CONTAINED IN TABLE' && this.condition != 'IN REGEX TABLE' && this.condition != 'NOT IN REGEX TABLE'",
                "val": "",
                "required": true
              },
              "compare_table": {
                "type": "select",
                "label": "Seleccionar Tabla",
                "bind": "TABLES",
                "empty": "",
                "show_if": "this.condition == 'CONTAINED IN TABLE' || this.condition == 'NOT CONTAINED IN TABLE' || this.condition == 'IN REGEX TABLE' || this.condition == 'NOT IN REGEX TABLE'",
                "required": true
              },
              "compare_column": {
                "type": "select",
                "label": "Seleccionar Columna",
                "table": "this.compare_table",
                "bind": "COLUMNS",
                "show_if": "this.condition == 'CONTAINED IN TABLE' || this.condition == 'NOT CONTAINED IN TABLE' || this.condition == 'IN REGEX TABLE' || this.condition == 'NOT IN REGEX TABLE'",
                "required": true
              },
              "delete": {
                "type": "button",
                "label": "Borrar",
                "function": "DELETE_ROW"
              }
            }
          },
          "options.apply_to_record": {
            "type": "checkbox",
            "label": "Aplicar al Registro",
            "val": "true",
            "isOn": "true",
            "isOff": "false",
            "style": "margin-top: 10px",
            "breakLine": true
          },
          "apply_to_group_box": {
            "type": "fieldset",
            "label": "Aplicar al Grupo",
            "show_if": "options.apply_to_record=='false'",
            "repeat": false,
            "fields": {
              "options.apply_to_group_columns": {
                "type": "select",
                "label": "Seleccionar Columnas del Grupo",
                "required": true,
                "width": "100%",
                "table": "input_table",
                "multiple": true,
                "bind": "COLUMNS",
                "breakLine": true
              }
            }
          }
        }
      },
      "options.add_rows": {
        "type": "fieldset",
        "label": "Nueva Opción de la Fila",
        "repeat": true,
        "fields": {
          "new_row_name": {
            "type": "text",
            "label": "Nombre de la Nueva Fila",
            "show_if": "$input_table.numOfLabel >= 1",
            "val": ""
          },
          "function_type": {
            "type": "select",
            "label": "Tipo de Tarea",
            "show_if": "false",
            "required": true,
            "options": {
              "multi_column_functions": {
                "label": "Múltiples Columnas"
              },
              "multi_row_functions": {
                "label": "Múltiples Filas"
              }
            },
            "val": "multi_row_functions"
          },
          "multi_row_functions__function": {
            "type": "select",
            "label": "Seleccionar funciones",
            "show_if": "$input_table.numOfLabel >= 1",
            "required": true,
            "options": {
              "sum": {
                "label": "Suma"
              },
              "average": {
                "label": "Promedio"
              },
              "median": {
                "label": "Mediana"
              },
              "standard_deviation": {
                "label": "Desviación Estándar"
              },
              "variance": {
                "label": "Varianza"
              },
              "product": {
                "label": "Producto"
              },
              "quota": {
                "label": "Cuota"
              }
            },
            "val": "sum"
          },
          "multi_row_functions__function_use_filter": {
            "type": "select",
            "label": "Seleccionar funciones",
            "show_if": "$input_table.numOfLabel < 1",
            "required": true,
            "options": {
              "sum": {
                "label": "Suma"
              },
              "average": {
                "label": "Promedio"
              },
              "median": {
                "label": "Mediana"
              },
              "standard_deviation": {
                "label": "Desviación Estándar"
              },
              "variance": {
                "label": "Varianza"
              },
              "product": {
                "label": "Producto"
              }
            },
            "val": "sum"
          },
          "multi_row_functions__sel_labels": {
            "type": "select",
            "label": "Seleccionar filas",
            "show_if": "$input_table.numOfLabel >= 1 && this.multi_row_functions__function!='quota'",
            "required": true,
            "table": "input_table",
            "multiple": true,
            "bind": "LABELS"
          },
          "multi_row_functions__sel_rows": {
            "type": "select",
            "label": "Seleccionar filas",
            "show_if": "false",
            "required": true,
            "options": {
              "row-1": {
                "label": "Fila-1"
              },
              "row-2": {
                "label": "Fila-2"
              },
              "row-3": {
                "label": "Fila-3"
              },
              "row-4": {
                "label": "Fila-4"
              },
              "row-5": {
                "label": "Fila-5"
              },
              "row-6": {
                "label": "Fila-6"
              },
              "row-7": {
                "label": "Fila-7"
              },
              "row-8": {
                "label": "Fila-8"
              },
              "row-9": {
                "label": "Fila-9"
              },
              "row-10": {
                "label": "Fila-10"
              },
              "row+1": {
                "label": "Fila+1"
              },
              "row+2": {
                "label": "Fila+2"
              },
              "row+3": {
                "label": "Fila+3"
              },
              "row+4": {
                "label": "Fila+4"
              },
              "row+5": {
                "label": "Fila+5"
              },
              "row+6": {
                "label": "Fila+6"
              },
              "row+7": {
                "label": "Fila+7"
              },
              "row+8": {
                "label": "Fila+8"
              },
              "row+9": {
                "label": "Fila+9"
              },
              "row+10": {
                "label": "Fila+10"
              }
            },
            "multiple": true,
            "val": "row-1"
          },
          "multi_row_functions__sel_label_a": {
            "type": "select",
            "label": "Seleccionar Etiqueta A",
            "show_if": "$input_table.numOfLabel >= 1 && this.multi_row_functions__function=='quota'",
            "required": true,
            "table": "input_table",
            "bind": "LABELS"
          },
          "multi_row_functions__sel_label_b": {
            "type": "select",
            "label": "Seleccionar Etiqueta B",
            "show_if": "$input_table.numOfLabel >= 1 && this.multi_row_functions__function=='quota'",
            "required": true,
            "table": "input_table",
            "bind": "LABELS",
            "breakLine": true
          },
          "position": {
            "type": "select",
            "label": "Posición",
            "show_if": "$input_table.numOfLabel >= 1",
            "required": true,
            "options": {
              "bottom": {
                "label": "FONDO"
              },
              "top": {
                "label": "TOP"
              },
              "above": {
                "label": "ARRIBA"
              },
              "below": {
                "label": "DEBAJO"
              }
            },
            "val": "bottom"
          },
          "positioning_row": {
            "type": "select",
            "label": "Posicionar fila",
            "show_if": "$input_table.numOfLabel >= 1 && (this.position=='above' || this.position=='below')",
            "required": true,
            "table": "input_table",
            "bind": "LABELS"
          },
          "format__category": {
            "type": "select",
            "label": "Formato",
            "required": true,
            "options": {
              "standard": {
                "label": "Estándar"
              },
              "number": {
                "label": "Número"
              },
              "percentage": {
                "label": "Porcentaje"
              },
              "text": {
                "label": "Texto"
              },
              "date": {
                "label": "Fecha"
              },
              "currency": {
                "label": "Moneda"
              }
            },
            "val": "standard"
          },
          "format__rounding": {
            "type": "select",
            "label": "Redondeo",
            "show_if": "this.format__category=='number' || this.format__category=='percentage' || this.format__category=='currency'",
            "required": true,
            "options": {
              "no_rounding": {
                "label": "Redondeo de números"
              },
              "0": {
                "label": "0 dígitos"
              },
              "1": {
                "label": "1 dígitos"
              },
              "2": {
                "label": "2 dígitos"
              },
              "3": {
                "label": "3 dígitos"
              },
              "4": {
                "label": "4 dígitos"
              },
              "5": {
                "label": "5 dígitos"
              },
              "6": {
                "label": "6 dígitos"
              },
              "7": {
                "label": "7 dígitos"
              },
              "8": {
                "label": "8 dígitos"
              },
              "9": {
                "label": "9 dígitos"
              }
            },
            "val": "no_rounding"
          },
          "format__use_1000_separator": {
            "type": "checkbox",
            "label": "Use 1000 Separator(.)",
            "show_if": "this.format__category=='number' || this.format__category=='percentage' || this.format__category=='currency'",
            "val": "false",
            "isOn": "true",
            "isOff": "false"
          },
          "format__currency_symbol": {
            "type": "select",
            "label": "Símbolo",
            "show_if": "this.format__category=='currency'",
            "required": true,
            "options": {
              "dollar": {
                "label": "$"
              },
              "euro": {
                "label": "€"
              },
              "pound": {
                "label": "£"
              }
            },
            "val": "dollar"
          },
          "format__date_format": {
            "type": "select",
            "label": "Tipo",
            "show_if": "this.format__category=='date'",
            "required": true,
            "options": {
              "mm/dd/yyyy": {
                "label": "3/14/2012"
              },
              "D, M dd, Y": {
                "label": "Miércoles, 14 de marzo de 2012"
              },
              "yyyy-mm-dd": {
                "label": "40982"
              }
            },
            "val": "mm/dd/yyyy"
          },
          "delete": {
            "type": "button",
            "label": "Borrar",
            "function": "DELETE_ROW"
          }
        }
      },
      "options.add_new_row": {
        "type": "checkbox",
        "label": "Agregar Etiqueta",
        "show_if": "$input_table.numOfLabel < 1",
        "val": "false",
        "isOn": "true",
        "isOff": "false",
        "style": "margin-top: 10px",
        "breakLine": true
      },
      "options.new_row_label": {
        "type": "text",
        "label": "Label Name",
        "show_if": "$input_table.numOfLabel < 1 && options.add_new_row == 'true'",
        "val": ""
      },
      "options.new_sel_column": {
        "type": "select",
        "label": "Seleccionar Columna",
        "show_if": "$input_table.numOfLabel < 1 && options.add_new_row == 'true'",
        "required": true,
        "table": "input_table",
        "multiple": false,
        "bind": "COLUMNS",
        "breakLine": true
      },
      "options.insert_on_top": {
        "type": "checkbox",
        "label": "Insert on Top",
        "show_if": "$input_table.numOfLabel < 1",
        "val": "false",
        "isOn": "true",
        "isOff": "false",
        "style": "margin-top: 10px",
        "breakLine": true
      }
    }
  }
}