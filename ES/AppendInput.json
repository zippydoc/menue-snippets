{
  "input": {
    "type": "fieldset",
    "label": "Seleccionar Tabla de Entrada",
    "repeat": false,
    "fields": {
      "target_table.table": {
        "type": "select",
        "label": "Tabla Izquierda",
        "bind": "TABLES"
      },
      "target_table.remove_input_table_after_processing": {
        "type": "hidden",
        "val": "false"
      },
      "target_table.is_word": {
        "type": "hidden",
        "val": "false"
      },
      "source_table.table": {
        "type": "select",
        "label": "Tabla Derecha",
        "bind": "TABLES",
        "breakLine": true
      },
      "source_table.remove_input_table_after_processing": {
        "type": "hidden",
        "val": "false"
      },
      "source_table.is_word": {
        "type": "hidden",
        "val": "false"
      },
      "options.cut_off_run": {
        "type": "checkbox",
        "label": "Cortar tabla de entrada y ejecutar",
        "val": "false",
        "isOn": "true",
        "isOff": "false"
      },
      "options.input_table_rows_limit": {
        "type": "text",
        "label": "Límite de la tabla de entrada (máximo: 800)",
        "show_if": "options.cut_off_run=='true'",
        "minLength": 1,
        "maxLength": 3,
        "val": "500",
        "maxVal": 800
      }
    }
  }
}