{
  "options": {
    "type": "fieldset",
    "label": "Seleccionar Opciones",
    "repeat": false,
    "fields": {
      "target_table.table_id": {
        "type": "select",
        "label": "Tabla Objetivo",
        "bind": "TABLES",
        "required": true
      },
      "options.value_columns": {
        "type": "select",
        "label": "Columnas de Valores",
        "table": "target_table.table_id",
        "bind": "COLUMNS",
        "multiple": true,
        "required": true
      },
      "options.mapping_column": {
        "type": "select",
        "label": "Columna del Mapeo",
        "table": "target_table.table_id",
        "bind": "COLUMNS",
        "breakLine": true
      },
      "options.autofill_empty_cells": {
        "type": "checkbox",
        "label": "Rellenar Automáticamente las Celdas Vacías",
        "val": "true",
        "isOn": "true",
        "isOff": "false"
      },
      "options.operation": {
        "type": "select",
        "label": "Operación",
        "show_if": "options.autofill_empty_cells=='false'",
        "required": true,
        "options": {
          "replace": {
            "label": "Reemplazar Contenido Existente"
          },
          "add": {
            "label": "Agregar al Contenido Existente"
          },
          "average": {
            "label": "Construir Promedio con el Contenido Existente"
          }
        },
        "val": "replace",
        "breakLine": true
      },
      "options.use_filter": {
        "type": "checkbox",
        "label": "Usar Filtro",
        "val": "false",
        "isOn": "true",
        "isOff": "false",
        "breakLine": true
      },
      "filter_box": {
        "type": "fieldset",
        "label": "Opciones del Filtro",
        "show_if": "options.use_filter=='true'",
        "repeat": false,
        "fields": {
          "options.filter": {
            "type": "fieldset",
            "label": "Agregar Condiciones del Filtro",
            "repeat": true,
            "fields": {
              "initial_op": {
                "type": "select",
                "label": "Seleccionar Operador Lógico",
                "required": true,
                "show_if": "$index == 0",
                "options": {
                  "if": {
                    "label": "SI"
                  },
                  "if_not": {
                    "label": "SI NO"
                  }
                },
                "val": "if"
              },
              "op": {
                "type": "select",
                "label": "Seleccionar Operador Lógico",
                "required": true,
                "show_if": "$index > 0",
                "options": {
                  "and": {
                    "label": "AND"
                  },
                  "or": {
                    "label": "OR"
                  },
                  "and_not": {
                    "label": "AND NOT"
                  },
                  "or_not": {
                    "label": "OR NOT"
                  }
                },
                "val": "and"
              },
              "column": {
                "type": "select",
                "label": "Seleccionar Columna",
                "table": "target_table.table_id",
                "bind": "COLUMNS",
                "required": true
              },
              "condition": {
                "type": "select",
                "label": "Seleccionar Operador",
                "required": true,
                "options": {
                  "CONTAINS": {
                    "label": "CONTIENE"
                  },
                  "CONTAINED IN STRING": {
                    "label": "CONTENIDO EN"
                  },
                  "STARTS WITH": {
                    "label": "EMPEZAR CON"
                  },
                  "ENDS WITH": {
                    "label": "TERMINA CON"
                  },
                  "=": {
                    "label": "IGUAL"
                  },
                  ">": {
                    "label": "MAYOR QUE"
                  },
                  ">=": {
                    "label": "MAYOR O IGUAL"
                  },
                  "<": {
                    "label": "MENOR QUE"
                  },
                  "<=": {
                    "label": "MENOR O IGUAL"
                  },
                  "EMPTY": {
                    "label": "ESTÁ VACÍO"
                  },
                  "BLANK": {
                    "label": "ESTÁ EN BLANCO"
                  },
                  "IS NULL": {
                    "label": "IS NULL"
                  },
                  "IS TRUE": {
                    "label": "ES VERDAD"
                  },
                  "IS FALSE": {
                    "label": "ES FALSO"
                  },
                  "REGEX MATCH": {
                    "label": "COINCIDENCIA CON LA EXPRESIÓN REGULAR"
                  },
                  "CONTAINED IN TABLE": {
                    "label": "EN TABLA"
                  },
                  "IN REGEX TABLE": {
                    "label": "EN LA TABLA DE LAS EXPRESIONES REGULARES"
                  },
                  "DATE": {
                    "label": "FECHA"
                  }
                },
                "val": "CONTAINS"
              },
              "date_condition": {
                "type": "select",
                "label": "Seleccionar Operador de la Fecha",
                "required": true,
                "show_if": "this.condition == 'DATE'",
                "options": {
                  "<": {
                    "label": "PREVIO"
                  },
                  "<=": {
                    "label": "PREVIO O IGUAL"
                  },
                  "=": {
                    "label": "IGUAL"
                  },
                  ">": {
                    "label": "DESPUÉS"
                  },
                  ">=": {
                    "label": "DESPUÉS O IGUAL"
                  }
                },
                "val": "="
              },
              "compare_with": {
                "type": "select",
                "label": "Compare with",
                "required": true,
                "show_if": "this.condition=='CONTAINS' || this.condition=='CONTAINED IN STRING' || this.condition=='STARTS WITH' || this.condition=='ENDS WITH' || this.condition=='=' || this.condition=='>' || this.condition=='>=' || this.condition=='<' || this.condition=='<='",
                "options": {
                  "custom_value": {
                    "label": "Custom value"
                  },
                  "column_value": {
                    "label": "Column value"
                  }
                },
                "val": "custom_value"
              },
              "self_compare_column": {
                "type": "select",
                "label": "Select Compare Column",
                "table": "table_id",
                "bind": "COLUMNS",
                "show_if": "this.compare_with=='column_value' && (this.condition=='CONTAINS' || this.condition=='CONTAINED IN STRING' || this.condition=='STARTS WITH' || this.condition=='ENDS WITH' || this.condition=='=' || this.condition=='>' || this.condition=='>=' || this.condition=='<' || this.condition=='<=')",
                "required": true
              },
              "date_value": {
                "type": "date",
                "label": "Seleccionar Fecha",
                "show_if": "this.condition=='DATE'",
                "val": "",
                "required": true
              },
              "value": {
                "type": "text",
                "label": "Ingresar Valor",
                "show_if": "this.compare_with=='custom_value' && this.condition != 'DATE' && this.condition != 'EMPTY' && this.condition != 'NOT EMPTY' && this.condition != 'BLANK' && this.condition != 'NOT BLANK' && this.condition != 'IS NULL' && this.condition != 'IS NOT NULL' && this.condition != 'IS TRUE' && this.condition != 'IS FALSE' && this.condition != 'CONTAINED IN TABLE' && this.condition != 'NOT CONTAINED IN TABLE' && this.condition != 'IN REGEX TABLE' && this.condition != 'NOT IN REGEX TABLE'",
                "val": "",
                "required": true
              },
              "case_sensitive": {
                "type": "select",
                "label": "Seleccionar Sensible a Mayúsculas y Minúsculas",
                "required": true,
                "show_if": "this.condition == 'CONTAINS' || this.condition == 'CONTAINED IN STRING' || this.condition == '=' || this.condition == 'STARTS WITH' || this.condition == 'ENDS WITH' || this.condition == 'REGEX MATCH' || this.condition == 'NOT REGEX MATCH' || this.condition == 'IN REGEX TABLE' || this.condition == 'NOT IN REGEX TABLE'",
                "options": {
                  "yes": {
                    "label": "Sensible a Mayúsculas y Minúsculas"
                  },
                  "no": {
                    "label": "Insensible a Mayúsculas y Minúsculas"
                  }
                },
                "val": "yes"
              },
              "compare_table": {
                "type": "select",
                "label": "Seleccionar Tabla",
                "bind": "TABLES",
                "empty": "",
                "show_if": "this.condition == 'CONTAINED IN TABLE' || this.condition == 'NOT CONTAINED IN TABLE' || this.condition == 'IN REGEX TABLE' || this.condition == 'NOT IN REGEX TABLE'",
                "required": true
              },
              "compare_column": {
                "type": "select",
                "label": "Seleccionar Columna",
                "table": "this.compare_table",
                "bind": "COLUMNS",
                "show_if": "this.condition == 'CONTAINED IN TABLE' || this.condition == 'NOT CONTAINED IN TABLE' || this.condition == 'IN REGEX TABLE' || this.condition == 'NOT IN REGEX TABLE'",
                "required": true
              },
              "delete": {
                "type": "button",
                "label": "Borrar",
                "function": "DELETE_ROW"
              }
            }
          },
          "options.apply_to_record": {
            "type": "checkbox",
            "label": "Aplicar al Registro",
            "val": "true",
            "isOn": "true",
            "isOff": "false",
            "style": "margin-top: 10px",
            "breakLine": true
          },
          "apply_to_group_box": {
            "type": "fieldset",
            "label": "Aplicar al Grupo",
            "show_if": "options.apply_to_record=='false'",
            "repeat": false,
            "fields": {
              "options.apply_to_group_columns": {
                "type": "select",
                "label": "Seleccionar Columnas del Grupo",
                "required": true,
                "width": "100%",
                "table": "target_table.table_id",
                "multiple": true,
                "bind": "COLUMNS",
                "breakLine": true
              }
            }
          }
        }
      }
    }
  }
}