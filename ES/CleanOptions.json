{
  "options": {
    "type": "fieldset",
    "label": "Seleccionar Opciones de Limpieza",
    "repeat": false,
    "fields": {
      "replace_null": {
        "type": "fieldset",
        "label": "Registros Vacíos",
        "repeat": false,
        "fields": {
          "replace_null": {
            "type": "checkbox",
            "label": "Registros Vacíos",
            "val": "false",
            "isOn": "true",
            "isOff": "false",
            "breakLine": true
          },
          "replace_null_target": {
            "type": "select",
            "label": "Reemplazar",
            "show_if": "replace_null=='true'",
            "options": {
              "empty": {
                "label": "Vacío"
              },
              "blank": {
                "label": "En Blanco"
              },
              "null": {
                "label": "NULL"
              }
            },
            "val": "empty",
            "breakLine": true
          },
          "replace_null_by": {
            "type": "radio",
            "show_if": "replace_null=='true'",
            "val": "by_blank",
            "values": {
              "by_blank": "By Blank",
              "by_zero": "By Zero"
            },
            "breakLine": true
          }
        }
      },
      "remove_whitespace": {
        "type": "fieldset",
        "label": "Eliminar Espacio en Blanco",
        "repeat": false,
        "fields": {
          "remove_characters.all_whitespace": {
            "type": "checkbox",
            "label": "Todos los Espacios en Blanco",
            "val": "false",
            "isOn": "true",
            "isOff": "false",
            "breakLine": true
          },
          "remove_characters.leading_whitespace": {
            "type": "checkbox",
            "label": "Espacio en Blanco de Adelante",
            "show_if": "remove_characters.all_whitespace=='false'",
            "val": "false",
            "isOn": "true",
            "isOff": "false",
            "breakLine": true
          },
          "remove_characters.trailing_whitespace": {
            "type": "checkbox",
            "label": "Espacio en Blanco Móvil",
            "show_if": "remove_characters.all_whitespace=='false'",
            "val": "false",
            "isOn": "true",
            "isOff": "false",
            "breakLine": true
          },
          "remove_characters.whitespace": {
            "type": "checkbox",
            "label": "Espacio",
            "show_if": "remove_characters.all_whitespace=='false'",
            "val": "false",
            "isOn": "true",
            "isOff": "false",
            "breakLine": true
          },
          "remove_characters.tabs": {
            "type": "checkbox",
            "label": "Pestañas",
            "show_if": "remove_characters.all_whitespace=='false'",
            "val": "false",
            "isOn": "true",
            "isOff": "false",
            "breakLine": true
          },
          "remove_characters.line_breaks": {
            "type": "checkbox",
            "label": "Saltos de Línea",
            "show_if": "remove_characters.all_whitespace=='false'",
            "val": "false",
            "isOn": "true",
            "isOff": "false",
            "breakLine": true
          },
          "remove_characters.duplicate_whitespace": {
            "type": "checkbox",
            "label": "Duplicar Espacio en Blanco",
            "show_if": "remove_characters.all_whitespace=='false'",
            "val": "false",
            "isOn": "true",
            "isOff": "false",
            "breakLine": true
          }
        }
      },
      "remove_duplicates": {
        "type": "fieldset",
        "label": "Eliminar Duplicados",
        "repeat": false,
        "fields": {
          "remove_duplicates.cell": {
            "type": "checkbox",
            "label": "Duplica en la Celda",
            "val": "false",
            "isOn": "true",
            "isOff": "false",
            "breakLine": true
          },
          "remove_duplicates.delimiter": {
            "type": "text",
            "label": "Delimitador",
            "show_if": "remove_duplicates.cell=='true'",
            "val": ";",
            "required": true
          },
          "remove_duplicates.include_whitespace": {
            "type": "checkbox",
            "label": "(incluir espacio en blanco)",
            "show_if": "remove_duplicates.cell=='true'",
            "val": "true",
            "isOn": "true",
            "isOff": "false",
            "breakLine": true
          },
          "remove_duplicates.rows": {
            "type": "checkbox",
            "label": "Duplicar Filas",
            "val": "false",
            "isOn": "true",
            "isOff": "false",
            "breakLine": true
          }
        }
      },
      "remove_characters": {
        "type": "fieldset",
        "label": "Eliminar Caracteres",
        "repeat": false,
        "fields": {
          "remove_characters.letters": {
            "type": "checkbox",
            "label": "Letras",
            "val": "false",
            "isOn": "true",
            "isOff": "false",
            "breakLine": true
          },
          "remove_characters.numbers": {
            "type": "checkbox",
            "label": "Números",
            "val": "false",
            "isOn": "true",
            "isOff": "false",
            "breakLine": true
          },
          "remove_characters.punctuation": {
            "type": "checkbox",
            "label": "Puntuación",
            "val": "false",
            "isOn": "true",
            "isOff": "false",
            "breakLine": true
          },
          "remove_characters.trailing_zero": {
            "type": "checkbox",
            "label": "Trailing 0",
            "val": "false",
            "isOn": "true",
            "isOff": "false",
            "breakLine": true
          },
          "remove_characters.custom": {
            "type": "checkbox",
            "label": "To-be-delted Characters",
            "val": "false",
            "isOn": "true",
            "isOff": "false",
            "breakLine": true
          },
          "remove_characters.custom_chars": {
            "type": "text",
            "label": "to be deleted characters",
            "show_if": "remove_characters.custom=='true'",
            "val": "",
            "required": true
          },
          "remove_characters.custom_mode": {
            "type": "select",
            "label": "Posición",
            "show_if": "remove_characters.custom=='true'",
            "options": {
              "all": {
                "label": "all"
              },
              "leading only": {
                "label": "leading only"
              },
              "trailing only": {
                "label": "trailing only"
              }
            },
            "val": "all"
          },
          "remove_characters.custom_delimiter": {
            "type": "select",
            "label": "Delimitador",
            "show_if": "remove_characters.custom=='true'",
            "options": {
              ",": {
                "label": ","
              },
              ";": {
                "label": ";"
              },
              ".": {
                "label": "."
              }
            },
            "val": ","
          }
        }
      },
      "modify_case": {
        "type": "fieldset",
        "label": "Modificar Caso",
        "repeat": false,
        "fields": {
          "modify_case": {
            "type": "checkbox",
            "label": "Modificar Caso",
            "val": "false",
            "isOn": "true",
            "isOff": "false",
            "breakLine": true
          },
          "modify_case_by": {
            "type": "radio",
            "show_if": "modify_case=='true'",
            "val": "upper",
            "values": {
              "upper": "to Upper Case",
              "lower": "to Lower Case",
              "title": "to Title Case"
            },
            "breakLine": true
          }
        }
      },
      "rounding": {
        "type": "fieldset",
        "label": "Redondeo",
        "repeat": false,
        "fields": {
          "is_rounding": {
            "type": "checkbox",
            "label": "Redondeo",
            "val": "false",
            "isOn": "true",
            "isOff": "false",
            "breakLine": true
          },
          "rounding": {
            "type": "select",
            "label": "Redondeo",
            "show_if": "is_rounding=='true'",
            "required": true,
            "options": {
              "no_rounding": {
                "label": "Redondeo de números"
              },
              "0": {
                "label": "0 dígitos"
              },
              "1": {
                "label": "1 dígitos"
              },
              "2": {
                "label": "2 dígitos"
              },
              "3": {
                "label": "3 dígitos"
              },
              "4": {
                "label": "4 dígitos"
              },
              "5": {
                "label": "5 dígitos"
              },
              "6": {
                "label": "6 dígitos"
              },
              "7": {
                "label": "7 dígitos"
              },
              "8": {
                "label": "8 dígitos"
              },
              "9": {
                "label": "9 dígitos"
              }
            },
            "val": "no_rounding"
          }
        }
      },
      "options.use_filter": {
        "type": "checkbox",
        "label": "Usar Filtro",
        "val": "false",
        "isOn": "true",
        "isOff": "false",
        "style": "margin-top: 10px",
        "breakLine": true
      },
      "filter_box": {
        "type": "fieldset",
        "label": "Opciones del Filtro",
        "show_if": "options.use_filter=='true'",
        "repeat": false,
        "fields": {
          "options.filter": {
            "type": "fieldset",
            "label": "Agregar Condiciones del Filtro",
            "repeat": true,
            "fields": {
              "initial_op": {
                "type": "select",
                "label": "Seleccionar Operador Lógico",
                "required": true,
                "show_if" :  "$index == 0",
                "options": {
                  "if": {
                    "label": "SI"
                  },
                  "if_not": {
                    "label": "SI NO"
                  }
                },
                "val": "if"
              },
              "op": {
                "type": "select",
                "label": "Seleccionar Operador Lógico",
                "required": true,
                "show_if" :  "$index > 0",
                "options": {
                  "and": {
                    "label": "AND"
                  },
                  "or": {
                    "label": "OR"
                  },
                  "and_not": {
                    "label": "AND NOT"
                  },
                  "or_not": {
                    "label": "OR NOT"
                  }
                },
                "val": "and"
              },
              "column": {
                "type": "select",
                "label": "Seleccionar Columna",
                "table": "table_id",
                "bind": "COLUMNS",
                "required": true
              },
              "condition": {
                "type": "select",
                "label": "Seleccionar Operador",
                "required": true,
                "options": {
                  "CONTAINS": {
                    "label": "CONTIENE"
                  },
                  "CONTAINED IN STRING": {
                    "label": "CONTENIDO EN"
                  },
                  "STARTS WITH": {
                    "label": "EMPEZAR CON"
                  },
                  "ENDS WITH": {
                    "label": "TERMINA CON"
                  },
                  "=": {
                    "label": "IGUAL"
                  },
                  ">": {
                    "label": "MAYOR QUE"
                  },
                  ">=": {
                    "label": "MAYOR O IGUAL"
                  },
                  "<": {
                    "label": "MENOR QUE"
                  },
                  "<=": {
                    "label": "MENOR O IGUAL"
                  },
                  "EMPTY": {
                    "label": "ESTÁ VACÍO"
                  },
                  "BLANK": {
                    "label": "ESTÁ EN BLANCO"
                  },
                  "IS NULL": {
                    "label": "IS NULL"
                  },
                  "IS TRUE": {
                    "label": "ES VERDAD"
                  },
                  "IS FALSE": {
                    "label": "ES FALSO"
                  },
                  "REGEX MATCH": {
                    "label": "COINCIDENCIA CON LA EXPRESIÓN REGULAR"
                  },
                  "CONTAINED IN TABLE": {
                    "label": "EN TABLA"
                  },
                  "IN REGEX TABLE": {
                    "label": "EN LA TABLA DE LAS EXPRESIONES REGULARES"
                  },
                  "DATE": {
                    "label": "FECHA"
                  }
                },
                "val": "CONTAINS"
              },
              "date_condition": {
                "type": "select",
                "label": "Seleccionar Operador de la Fecha",
                "required": true,
                "show_if": "this.condition == 'DATE'",
                "options": {
                  "<": {
                    "label": "PREVIO"
                  },
                  "<=": {
                    "label": "PREVIO O IGUAL"
                  },
                  "=": {
                    "label": "IGUAL"
                  },
                  ">": {
                    "label": "DESPUÉS"
                  },
                  ">=": {
                    "label": "DESPUÉS O IGUAL"
                  }
                },
                "val": "="
              },
              "compare_with": {
                "type": "select",
                "label": "Compare with",
                "required": true,
                "show_if": "this.condition=='CONTAINS' || this.condition=='CONTAINED IN STRING' || this.condition=='STARTS WITH' || this.condition=='ENDS WITH' || this.condition=='=' || this.condition=='>' || this.condition=='>=' || this.condition=='<' || this.condition=='<='",
                "options": {
                  "custom_value": {
                    "label": "Custom value"
                  },
                  "column_value": {
                    "label": "Column value"
                  }
                },
                "val": "custom_value"
              },
              "self_compare_column": {
                "type": "select",
                "label": "Select Compare Column",
                "table": "table_id",
                "bind": "COLUMNS",
                "show_if": "this.compare_with=='column_value' && (this.condition=='CONTAINS' || this.condition=='CONTAINED IN STRING' || this.condition=='STARTS WITH' || this.condition=='ENDS WITH' || this.condition=='=' || this.condition=='>' || this.condition=='>=' || this.condition=='<' || this.condition=='<=')",
                "required": true
              },
              "date_value": {
                "type": "date",
                "label": "Seleccionar Fecha",
                "show_if": "this.condition=='DATE'",
                "val": "",
                "required": true
              },
              "value": {
                "type": "text",
                "label": "Ingresar Valor",
                "show_if": "this.compare_with=='custom_value' && this.condition != 'DATE' && this.condition != 'EMPTY' && this.condition != 'NOT EMPTY' && this.condition != 'BLANK' && this.condition != 'NOT BLANK' && this.condition != 'IS NULL' && this.condition != 'IS NOT NULL' && this.condition != 'IS TRUE' && this.condition != 'IS FALSE' && this.condition != 'CONTAINED IN TABLE' && this.condition != 'NOT CONTAINED IN TABLE' && this.condition != 'IN REGEX TABLE' && this.condition != 'NOT IN REGEX TABLE'",
                "val": "",
                "required": true
              },
              "case_sensitive": {
                "type": "select",
                "label": "Seleccionar Sensible a Mayúsculas y Minúsculas",
                "required": true,
                "show_if": "this.condition == 'CONTAINS' || this.condition == 'CONTAINED IN STRING' || this.condition == '=' || this.condition == 'STARTS WITH' || this.condition == 'ENDS WITH' || this.condition == 'REGEX MATCH' || this.condition == 'NOT REGEX MATCH' || this.condition == 'IN REGEX TABLE' || this.condition == 'NOT IN REGEX TABLE'",
                "options": {
                  "yes": {
                    "label": "Sensible a Mayúsculas y Minúsculas"
                  },
                  "no": {
                    "label": "Insensible a Mayúsculas y Minúsculas"
                  }
                },
                "val": "yes"
              },
              "compare_table": {
                "type": "select",
                "label": "Seleccionar Tabla",
                "bind": "TABLES",
                "empty": "",
                "show_if": "this.condition == 'CONTAINED IN TABLE' || this.condition == 'NOT CONTAINED IN TABLE' || this.condition == 'IN REGEX TABLE' || this.condition == 'NOT IN REGEX TABLE'",
                "required": true
              },
              "compare_column": {
                "type": "select",
                "label": "Seleccionar Columna",
                "table": "this.compare_table",
                "bind": "COLUMNS",
                "show_if": "this.condition == 'CONTAINED IN TABLE' || this.condition == 'NOT CONTAINED IN TABLE' || this.condition == 'IN REGEX TABLE' || this.condition == 'NOT IN REGEX TABLE'",
                "required": true
              },
              "delete": {
                "type": "button",
                "label": "Borrar",
                "function": "DELETE_ROW"
              }
            }
          },
          "options.apply_to_record": {
            "type": "checkbox",
            "label": "Aplicar al Registro",
            "val": "true",
            "isOn": "true",
            "isOff": "false",
            "style": "margin-top: 10px",
            "breakLine": true
          },
          "apply_to_group_box": {
            "type": "fieldset",
            "label": "Aplicar al Grupo",
            "show_if": "options.apply_to_record=='false'",
            "repeat": false,
            "fields": {
              "options.apply_to_group_columns": {
                "type": "select",
                "label": "Seleccionar Columnas del Grupo",
                "required": true,
                "width": "100%",
                "table": "table_id",
                "multiple": true,
                "bind": "COLUMNS",
                "breakLine": true
              }
            }
          }
        }
      }
    }
  }
}