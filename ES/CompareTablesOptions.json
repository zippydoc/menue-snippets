{
  "options": {
    "type": "fieldset",
    "label": "Opciones",
    "repeat": false,
    "fields": {
      "compare_set": {
        "type": "fieldset",
        "label": "Seleccionar Patrones",
        "repeat": true,
        "fields": {
          "pattern": {
            "type": "select",
            "label": "Tipo de Patrón de la Tabla",
            "options": {
              "all_col": {
                "label": "Todas las columnas son idénticas"
              },
              "two_col": {
                "label": "Dos Columnas Idénticas"
              }
            },
            "val": "all_col",
            "required": true
          },
          "compare_type": {
            "type": "select",
            "label": "Tipo de Patrón de la Celda",
            "options": {
              "equal": {
                "label": "Igual"
              },
              "contain": {
                "label": "Contiene"
              },
              "start_with": {
                "label": "Empezar Con"
              },
              "end_with": {
                "label": "Termina Con"
              },
              "reg_exp": {
                "label": "Expresión Regular"
              }
            },
            "val": "equal",
            "required": true
          },
          "case_sensitive": {
            "type": "select",
            "label": "Sensible a Mayúsculas y Minúsculas",
            "required": true,
            "options": {
              "yes": {
                "label": "Sensible a Mayúsculas y Minúsculas"
              },
              "no": {
                "label": "Insensible a Mayúsculas y Minúsculas"
              }
            },
            "val": "no"
          },
          "expression": {
            "type": "text",
            "label": "Expresión",
            "show_if": "this.compare_type != 'equal'",
            "required": true,
            "val": ""
          },
          "delete": {
            "show_if": "this.field_name != ''",
            "type": "button",
            "label": "Borrar",
            "function": "DELETE_ROW"
          }
        },
        "breakLine": true
      },
      "target": {
        "type": "select",
        "label": "Objetivo",
        "options": {
          "to_table": {
            "label": "Aplicar a la tabla"
          },
          "to_rows": {
            "label": "Fila por Fila"
          }
        },
        "val": "to_table",
        "required": true
      },
      "match_type": {
        "type": "select",
        "show_if": "target != 'to_table'",
        "label": "Tipo de Coincidencia",
        "options": {
          "match": {
            "label": "Solo Coincidencias"
          },
          "nonmatch": {
            "label": "No Solo las Coincidencias"
          },
          "all": {
            "label": "Todo"
          }
        },
        "val": "match",
        "required": true,
        "breakLine": true
      },
      "create_sql": {
        "type": "checkbox",
        "label": "Mostrar Detalles de SQL",
        "val": "false",
        "isOn": "true",
        "isOff": "false"
      }
    }
  }
}