{
  "input": {
    "type": "fieldset",
    "Etiqueta": "Consolidate Options",
    "repeat": true,
    "fields": {
      "new_position": {
        "type": "text",
        "Etiqueta": "New Position",
        "val": "",
        "placeholder": "Texto",
        "required": true
      },
      "input_table": {
        "type": "select",
        "Etiqueta": "Seleccionar Tabla",
        "bind": "TABLES",
        "empty": "",
        "required": true,
        "breakLine": true
      },
      "multiple_selection": {
        "type": "select",
        "Etiqueta": "Multiple Selection",
        "required": true,
        "table": "this.input_table",
        "multiple": true,
        "bind": "LABELS",
        "breakLine": true
      },
      "format__category": {
        "type": "select",
        "Etiqueta": "Formato",
        "required": true,
        "options": {
          "standard": {
            "Etiqueta": "Estándar"
          },
          "number": {
            "Etiqueta": "Número"
          },
          "percentage": {
            "Etiqueta": "Porcentaje"
          },
          "text": {
            "Etiqueta": "Texto"
          },
          "date": {
            "Etiqueta": "Fecha"
          },
          "currency": {
            "Etiqueta": "Moneda"
          }
        },
        "val": "standard"
      },
      "format__rounding": {
        "type": "select",
        "Etiqueta": "Redondeo",
        "show_if": "this.format__category=='number' || this.format__category=='percentage' || this.format__category=='currency'",
        "required": true,
        "options": {
          "no_rounding": {
            "Etiqueta": "Redondeo de números"
          },
          "0": {
            "Etiqueta": "0 dígitos"
          },
          "1": {
            "Etiqueta": "1 dígitos"
          },
          "2": {
            "Etiqueta": "2 dígitos"
          },
          "3": {
            "Etiqueta": "3 dígitos"
          },
          "4": {
            "Etiqueta": "4 dígitos"
          },
          "5": {
            "Etiqueta": "5 dígitos"
          },
          "6": {
            "Etiqueta": "6 dígitos"
          },
          "7": {
            "Etiqueta": "7 dígitos"
          },
          "8": {
            "Etiqueta": "8 dígitos"
          },
          "9": {
            "Etiqueta": "9 dígitos"
          }
        },
        "val": "2"
      },
      "format__use_1000_separator": {
        "type": "checkbox",
        "Etiqueta": "Use 1000 Separator(.)",
        "show_if": "this.format__category=='number' || this.format__category=='percentage' || this.format__category=='currency'",
        "val": "true",
        "isOn": "true",
        "isOff": "false"
      },
      "format__currency_symbol": {
        "type": "select",
        "Etiqueta": "Símbolo",
        "show_if": "this.format__category=='currency'",
        "required": true,
        "options": {
          "dollar": {
            "Etiqueta": "$"
          },
          "euro": {
            "Etiqueta": "€"
          },
          "pound": {
            "Etiqueta": "£"
          }
        },
        "val": "dollar"
      },
      "format__date_format": {
        "type": "select",
        "Etiqueta": "Tipo",
        "show_if": "this.format__category=='date'",
        "required": true,
        "options": {
          "mm/dd/yyyy": {
            "Etiqueta": "3/14/2012"
          },
          "D, M dd, Y": {
            "Etiqueta": "Miércoles, 14 de marzo de 2012"
          },
          "yyyy-mm-dd": {
            "Etiqueta": "40982"
          }
        },
        "val": "mm/dd/yyyy",
        "breakLine": true
      }
    }
  }
}