{
  "options_frame": {
    "type": "fieldset",
    "label": "Opciones",
    "repeat": false,
    "fields": {
      "options.group_by": {
        "type": "select",
        "label": "Agrupar por",
        "required": true,
        "options": {
          "Region": {
            "label": "Región"
          },
          "Income Group": {
            "label": "Grupo de ingresos"
          }
        },
        "val": "Region"
      },
      "options.stat_var_name": {
        "type": "select",
        "label": "Datos estadísticos",
        "required": true,
        "options": {
          "Count_Person": {
            "label": "Población"
          },
          "GrowthRate_Count_Person": {
            "label": "Tasa de crecimiento de la población"
          },
          "Count_Person_PerArea": {
            "label": "Densidad de población"
          },
          "Count_Person_IsInternetUser_PerCapita": {
            "label": "Porcentaje de usuarios de Internet"
          },
          "Count_Person_ResidingLessThan5MetersAboveSeaLevel_AsFractionOf_Count_Person": {
            "label": "Porcentaje de población que vive en zonas por debajo de los 5 metros de altitud"
          },
          "Count_Person_Rural": {
            "label": "Población rural"
          },
          "Count_Person_Urban": {
            "label": "Población urbana"
          }
        },
        "val": "Count_Person"
      },
      "options.per_capita": {
        "type": "checkbox",
        "label": "Per cápita",
        "show_if": "false",
        "val": "false",
        "isOn": "true",
        "isOff": "false",
        "breakLine": true
      },
      "options.all_countries": {
        "type": "checkbox",
        "label": "Todos los países",
        "val": "true",
        "isOn": "true",
        "isOff": "false",
        "breakLine": true
      },
      "options.output_as_chart": {
        "type": "checkbox",
        "label": "Salida de un gráfico",
        "val": "true",
        "isOn": "true",
        "isOff": "false"
      },
      "options.graph_type": {
        "type": "select",
        "label": "Tipo de gráfico",
        "show_if": "options.output_as_chart=='true'",
        "required": true,
        "options": {
          "zoomable_sunburst": {
            "label": "Rayo de sol"
          },
          "tidy_tree": {
            "label": "Árbol"
          },
          "treemap": {
            "label": "Mapa del árbol"
          }
        },
        "val": "zoomable_sunburst",
        "breakLine": true
      }
    }
  }
}