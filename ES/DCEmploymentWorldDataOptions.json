{
  "options_frame": {
    "type": "fieldset",
    "label": "Opciones",
    "repeat": false,
    "fields": {
      "options.group_by": {
        "type": "select",
        "label": "Agrupar por",
        "required": true,
        "options": {
          "Region": {
            "label": "Región"
          },
          "Income Group": {
            "label": "Grupo de ingresos"
          }
        },
        "val": "Region",
        "breakLine": true
      },
      "options.stat_var_name": {
        "type": "select",
        "label": "Datos estadísticos",
        "required": true,
        "options": {
          "UnemploymentRate_Person": {
            "label": "Tasa de desempleo"
          },
          "Count_Person_InLaborForce": {
            "label": "Población activa"
          },
          "Count_Person_15To64Years_InLaborForce_AsFractionOf_Count_Person_15To64Years": {
            "label": "15 - 64 años, en población activa per cápita 15 a 64 años"
          },
          "Count_Person_15To64Years_Female_InLaborForce_AsFractionOf_Count_Person_15To64Years_Female": {
            "label": "15 - 64 años, en la fuerza laboral, población femenina per cápita 15 a 64 años mujer"
          },
          "Count_Person_15To64Years_Male_InLaborForce_AsFractionOf_Count_Person_15To64Years_Male": {
            "label": "15 - 64 años, en la fuerza de trabajo, población masculina per cápita 15 a 64 años Hombre"
          },
          "Count_Person_15OrMoreYears_InLaborForce_Female_AsFractionOf_Count_Person_InLaborForce": {
            "label": "15 años o más, en la fuerza laboral, población femenina per cápita en la fuerza laboral"
          }
        },
        "val": "UnemploymentRate_Person"
      },
      "options.per_capita": {
        "type": "checkbox",
        "label": "Per cápita",
        "show_if": "false",
        "val": "false",
        "isOn": "true",
        "isOff": "false",
        "breakLine": true
      },
      "options.all_countries": {
        "type": "checkbox",
        "label": "Todos los países",
        "val": "true",
        "isOn": "true",
        "isOff": "false",
        "breakLine": true
      },
      "options.output_as_chart": {
        "type": "checkbox",
        "label": "Salida de un gráfico",
        "val": "true",
        "isOn": "true",
        "isOff": "false"
      },
      "options.graph_type": {
        "type": "select",
        "label": "Tipo de gráfico",
        "show_if": "options.output_as_chart=='true'",
        "required": true,
        "options": {
          "zoomable_sunburst": {
            "label": "Rayo de sol"
          },
          "treemap": {
            "label": "Mapa del árbol"
          }
        },
        "val": "zoomable_sunburst",
        "breakLine": true
      }
