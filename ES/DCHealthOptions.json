{
  "options_frame": {
    "type": "fieldset",
    "label": "Opciones",
    "repeat": false,
    "fields": {
      "options.all_countries": {
        "type": "hidden",
        "val": "false"
      },
      "options.stat_vars": {
        "type": "fieldset",
        "label": "Statistics data category(s)",
        "repeat": true,
        "fields": {
          "stat_var_name": {
            "type": "select",
            "label": "Datos estadísticos",
            "required": true,
            "options": {
              "FertilityRate_Person_Female": {
                "label": "Tasa de fecundidad"
              },
              "LifeExpectancy_Person": {
                "label": "Esperanza de vida"
              },
              "Count_BirthEvent_LiveBirth_AsFractionOf_Count_Person": {
                "label": "Número de nacidos vivos ocurridos durante el año, per cápita"
              },
              "Count_Person_20To79Years_Diabetes_AsFractionOf_Count_Person_20To79Years": {
                "label": "Porcentaje de personas de 20 a 79 años que tienen diabetes tipo 1 o tipo 2"
              },
              "Count_Death": {
                "label": "Recuento de eventos de mortalidad"
              },
              "Count_Person_15OrMoreYears_Smoking_AsFractionOf_Count_Person_15OrMoreYears": {
                "label": "15 años o más, población fumadora per cápita de 15 años o más"
              },
              "Count_Person_Upto4Years_Overweight_AsFractionOf_Count_Person_Upto4Years": {
                "label": "4 años o menos, población con sobrepeso per cápita hasta 4 años"
              }
            }
          },
          "per_capita": {
            "type": "checkbox",
            "label": "Per cápita",
            "show_if": "false",
            "val": "false",
            "isOn": "true",
            "isOff": "false",
            "breakLine": true
          },
          "delete": {
            "type": "button",
            "label": "Borrar",
            "function": "DELETE_ROW"
          }
        }
      },
      "options.output_as_chart": {
        "type": "checkbox",
        "label": "Salida de un gráfico",
        "val": "true",
        "isOn": "true",
        "isOff": "false",
        "breakLine": true
      }
    }
  }
}