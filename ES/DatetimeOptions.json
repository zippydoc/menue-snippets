{
  "options": {
    "type": "fieldset",
    "Etiqueta": "Seleccionar Opciones",
    "repeat": false,
    "fields": {
      "options.convert_type": {
        "type": "select",
        "Etiqueta": "Convertir Tipo",
        "required": true,
        "options": {
          "complete_date": {
            "Etiqueta": "Fecha completa"
          },
          "expand_date": {
            "Etiqueta": "Expandir fecha"
          },
          "convert_date": {
            "Etiqueta": "Convertir fecha"
          },
          "convert_date_range": {
            "Etiqueta": "Convertir intervalo de fechas"
          }
        },
        "val": "complete_date"
      },
      "options.date_convert_type": {
        "type": "select",
        "Etiqueta": "Convertir a",
        "show_if": "options.convert_type=='convert_date'",
        "required": true,
        "options": {
          "begin_of_month": {
            "Etiqueta": "Inicio de Mes"
          },
          "end_of_month": {
            "Etiqueta": "Fin de Mes"
          },
          "hy_year": {
            "Etiqueta": "Semestre"
          },
          "qrt_year": {
            "Etiqueta": "Trimestre Año"
          },
          "month_year": {
            "Etiqueta": "Mes Año"
          },
          "week_year": {
            "Etiqueta": "Semana Año"
          }
        },
        "val": "begin_of_month"
      },
      "options.date_range_convert_type": {
        "type": "select",
        "Etiqueta": "Convertir a",
        "show_if": "options.convert_type=='convert_date_range'",
        "required": true,
        "options": {
          "begin_of_month": {
            "Etiqueta": "Inicio de Mes"
          },
          "end_of_month": {
            "Etiqueta": "Fin de Mes"
          },
          "begin_of_range": {
            "Etiqueta": "Inicio de Intervalo"
          },
          "end_of_range": {
            "Etiqueta": "Fin de Intervalo"
          },
          "begin_of_quarter": {
            "Etiqueta": "Inicio del Trimestre"
          },
          "end_of_quarter": {
            "Etiqueta": "Fin de Trimestre"
          }
        },
        "val": "begin_of_month",
        "breakLine": true
      },
      "options.output_in_new_column": {
        "type": "checkbox",
        "Etiqueta": "Imprimir en Nueva Columna",
        "val": "false",
        "isOn": "true",
        "isOff": "false"
      },
      "options.new_field_name": {
        "type": "text",
        "Etiqueta": "Nuevo nombre de columna",
        "show_if": "options.output_in_new_column=='true'",
        "val": "GeneratedDate",
        "required": true,
        "minLength": 1,
        "maxLength": 255,
        "breakLine": true
      }
    }
  }
}