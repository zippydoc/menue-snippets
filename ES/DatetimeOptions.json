{
  "options": {
    "type": "fieldset", 
    "label": "Seleccionar Opciones", 
    "repeat": false, 
    "fields": {
      "options.convert_type": {
        "type": "select", 
        "label": "Convertir Tipo", 
        "required": true, 
        "options": {
          "complete_date": {
            "label": "Fecha completa"
          }, 
          "expand_date": {
            "label": "Expandir fecha"
          }, 
          "convert_date": {
            "label": "Convertir fecha"
          }, 
          "convert_date_range": {
            "label": "Convertir intervalo de fechas"
          }
        }, 
        "val": "complete_date"
      }, 
      "options.date_convert_type": {
        "type": "select", 
        "label": "Convertir a", 
        "show_if": "options.convert_type=='convert_date'", 
        "required": true, 
        "options": {
          "begin_of_month": {
            "label": "Inicio de Mes"
          }, 
          "end_of_month": {
            "label": "Fin de Mes"
          }, 
          "hy_year": {
            "label": "Semestre"
          }, 
          "qrt_year": {
            "label": "Trimestre Año"
          }, 
          "month_year": {
            "label": "Mes Año"
          }, 
          "week_year": {
            "label": "Semana Año"
          }
        }, 
        "val": "begin_of_month"
      }, 
      "options.date_range_convert_type": {
        "type": "select", 
        "label": "Convertir a", 
        "show_if": "options.convert_type=='convert_date_range'", 
        "required": true, 
        "options": {
          "begin_of_month": {
            "label": "Inicio de Mes"
          }, 
          "end_of_month": {
            "label": "Fin de Mes"
          }, 
          "begin_of_range": {
            "label": "Inicio de Intervalo"
          }, 
          "end_of_range": {
            "label": "Fin de Intervalo"
          }, 
          "begin_of_quarter": {
            "label": "Inicio del Trimestre"
          }, 
          "end_of_quarter": {
            "label": "Fin de Trimestre"
          }
        }, 
        "val": "begin_of_month", 
        "breakLine": true
      }, 
      "options.output_in_new_column": {
        "type": "checkbox", 
        "label": "Imprimir en Nueva Columna", 
        "val": "false", 
        "isOn": "true", 
        "isOff": "false"
      }, 
      "options.new_field_name": {
        "type": "text", 
        "label": "Nuevo nombre de columna", 
        "show_if": "options.output_in_new_column=='true'", 
        "val": "GeneratedDate", 
        "required": true, 
        "minLength": 1, 
        "maxLength": 255, 
        "breakLine": true
      }
    }
  }
}