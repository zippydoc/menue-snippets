{
  "placeholder": {
    "breakLine": true
  },
  "delete_options": {
    "type": "select",
    "label": "Borrar opciones",
    "required": true,
    "options": {
      "del_table": {
        "label": "Borrar tabla"
      },
      "del_columns": {
        "label": "Borrar columnas seleccionadas"
      },
      "del_rows": {
        "label": "Borrar filas seleccionadas"
      }
    },
    "val": "del_table"
  },
  "apply_columns": {
    "type": "select",
    "label": "Seleccionar columnas para eliminar",
    "show_if": "delete_options=='del_columns'",
    "required": true,
    "table": "input_table",
    "multiple": true,
    "bind": "COLUMNS"
  },
  "apply_rows": {
    "type": "select",
    "label": "Seleccionar filas para eliminar",
    "show_if": "delete_options=='del_rows'",
    "required": true,
    "table": "input_table",
    "multiple": true,
    "bind": "LABELS"
  }
}