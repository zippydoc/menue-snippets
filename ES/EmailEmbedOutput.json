{
  "embed_output_frame": {
    "type": "fieldset",
    "label": "Define Embed(Attach) Table Name",
    "show_if": "options.add_table=='true'",
    "repeat": false,
    "fields": {
      "embed_output.custom_text": {
        "type": "text",
        "label": "Imprimir Nombre de Tabla",
        "show_if": "embed_output.compose_table_name=='false'",
        "minLength": 1,
        "val": "",
        "width": "100%",
        "breakLine": true
      },
      "embed_output.custom_options": {
        "type": "checkbox",
        "label": "Imprimir Opciones",
        "val": "false",
        "isOn": "true",
        "isOff": "false",
        "breakLine": true
      },
      "embed_output.compose_table_name": {
        "type": "checkbox",
        "label": "Componer Nombre de la Tabla",
        "show_if": "embed_output.custom_options=='true'",
        "val": "false",
        "isOn": "true",
        "isOff": "false",
        "breakLine": true
      },
      "embed_output_compose_table_name_frame": {
        "type": "fieldset",
        "label": "Componer Nombre de la Tabla de Salida",
        "show_if": "embed_output.custom_options=='true' && embed_output.compose_table_name=='true'",
        "repeat": false,
        "fields": {
          "embed_output.prefix_rule": {
            "type": "select",
            "label": "Prefijo",
            "val": "empty",
            "options": {
              "empty": {
                "label": "-"
              },
              "script_name": {
                "label": "Nombre de la Tarea"
              },
              "date_string_type1": {
                "label": "Fecha: dd_mm_aaaa"
              },
              "date_string_type2": {
                "label": "Fecha: aaaa_mm_dd"
              },
              "time_string": {
                "label": "Tempo: h_min_seg"
              },
              "datetime_string": {
                "label": "Fecha_Hora"
              },
              "custom": {
                "label": "Personalizado"
              }
            }
          },
          "embed_output.prefix_text": {
            "type": "text",
            "label": "Texto del Prefijo",
            "show_if": "embed_output.prefix_rule=='custom'",
            "minLength": 1,
            "maxLength": 31,
            "val": ""
          },
          "embed_output.core_delimiter": {
            "type": "text",
            "label": "Delimitador",
            "show_if": "embed_output.prefix_rule!='empty'",
            "val": "_",
            "width": "50px"
          },
          "embed_output.core_rule": {
            "type": "select",
            "label": "Nombre de la Tabla",
            "val": "input_table_name",
            "options": {
              "input_table_name": {
                "label": "Ingresar Nombre de la Tabla"
              },
              "script_name": {
                "label": "Nombre de la Tarea"
              },
              "custom": {
                "label": "Personalizado"
              }
            }
          },
          "embed_output.core_text": {
            "type": "text",
            "label": "Nombre de la Tabla Principal",
            "show_if": "embed_output.core_rule=='custom'",
            "required": true,
            "minLength": 1,
            "maxLength": 31,
            "val": ""
          },
          "embed_output.suffix_delimiter": {
            "type": "text",
            "label": "Delimitador",
            "val": "_",
            "width": "50px"
          },
          "embed_output.suffix_rule": {
            "type": "select",
            "label": "Sufijo",
            "val": "script_name",
            "options": {
              "empty": {
                "label": "-"
              },
              "script_name": {
                "label": "Nombre de la Tarea"
              },
              "date_string_type1": {
                "label": "Fecha: dd_mm_aaaa"
              },
              "date_string_type2": {
                "label": "Fecha: aaaa_mm_dd"
              },
              "time_string": {
                "label": "Tempo: h_min_seg"
              },
              "datetime_string": {
                "label": "Fecha_Hora"
              },
              "custom": {
                "label": "Personalizado"
              }
            }
          },
          "embed_output.suffix_text": {
            "type": "text",
            "label": "Texto del Sufijo",
            "show_if": "embed_output.suffix_rule=='custom'",
            "minLength": 1,
            "maxLength": 31,
            "val": "",
            "breakLine": true
          },
          "embed_output.timezone": {
            "type": "select",
            "label": "Zona Horaria",
            "show_if": "embed_output.prefix_rule=='date_string_type1' || embed_output.prefix_rule=='date_string_type2' || embed_output.prefix_rule=='time_string' || embed_output.prefix_rule=='datetime_string' || embed_output.suffix_rule=='date_string_type1' || embed_output.suffix_rule=='date_string_type2' || embed_output.suffix_rule=='time_string' || embed_output.suffix_rule=='datetime_string'",
            "val": "Europe/Berlin",
            "required": true,
            "options": {
              "America/Adak": {
                "label": "Adak, Gambier"
              },
              "Australia/Adelaide": {
                "label": "Adelaida, Broken Hill, Darwin"
              },
              "America/Anchorage": {
                "label": "Anchorage, Juneau, Sitka"
              },
              "Pacific/Apia": {
                "label": "Apia, Asau, Mulifanua, Afega, Leulumoega"
              },
              "Pacific/Enderbury": {
                "label": "Apia, Fiji, Tongatapu"
              },
              "Antarctica/McMurdo": {
                "label": "Auckland, Casey, Davis"
              },
              "Pacific/Auckland": {
                "label": "Auckland, Wellington, Christchurch"
              },
              "Asia/Bangkok": {
                "label": "Bangkok, Yakarta, Novosibirsk"
              },
              "Europe/Berlin": {
                "label": "Berlín, París, Ámsterdam"
              },
              "Australia/Perth": {
                "label": "Brunei, Singapur, Perth"
              },
              "Atlantic/Cape_Verde": {
                "label": "Cabo Verde, Praia, Mindelo, Santa Maria"
              },
              "America/Belize": {
                "label": "Costa Rica, El Salvador, Denver"
              },
              "Antarctica/Vostok": {
                "label": "Dhaka, Bishkek, Omsk"
              },
              "Europe/Astrakhan": {
                "label": "Dubai, Bakú, Tbilisi"
              },
              "Australia/Eucla": {
                "label": "Eucla"
              },
              "Pacific/Honolulu": {
                "label": "Honolulu, Rarotonga, Tahití"
              },
              "Asia/Kabul": {
                "label": "Kabul, Kandahār, Mazār-e Sharīf, Herāt, Jalālābād"
              },
              "Asia/Aqtau": {
                "label": "Karachi, Tashkent, Maldivas"
              },
              "Asia/Kathmandu": {
                "label": "Katmandú, Pokhara, Pātan, Biratnagar"
              },
              "Pacific/Kiritimati": {
                "label": "Kiritimati"
              },
              "Asia/Colombo": {
                "label": "Calcuta, Colombo, Jaffna, Negombo"
              },
              "Europe/Guernsey": {
                "label": "Londres, Dublín, Lisboa"
              },
              "Australia/Lord_Howe": {
                "label": "Lord Howe"
              },
              "America/Creston": {
                "label": "Los Ángeles, Vancouver, Hermosillo"
              },
              "Pacific/Marquesas": {
                "label": "Marquesas, Taiohae"
              },
              "America/Cayman": {
                "label": "Ciudad de México, Panamá, Chicago"
              },
              "Pacific/Niue": {
                "label": "Midway, Niue, Pago Pago"
              },
              "Europe/Mariehamn": {
                "label": "Moscú, Estambul, Qatar"
              },
              "Asia/Kolkata": {
                "label": "Bombay, Delhi, Bengaluru, Calcuta, Chennai"
              },
              "America/Miquelon": {
                "label": "Noronha, Nuuk, Miquelon"
              },
              "America/Danmarkshavn": {
                "label": "Reikiavik, Dakar, Casablanca"
