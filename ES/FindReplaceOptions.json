                    "label": "MENOR O IGUAL"
                  },
                  "EMPTY": {
                    "label": "ESTÁ VACÍO"
                  },
                  "BLANK": {
                    "label": "ESTÁ EN BLANCO"
                  },
                  "IS NULL": {
                    "label": "IS NULL"
                  },
                  "IS TRUE": {
                    "label": "ES VERDAD"
                  },
                  "IS FALSE": {
                    "label": "ES FALSO"
                  },
                  "REGEX MATCH": {
                    "label": "COINCIDENCIA CON LA EXPRESIÓN REGULAR"
                  },
                  "CONTAINED IN TABLE": {
                    "label": "EN TABLA"
                  },
                  "IN REGEX TABLE": {
                    "label": "EN LA TABLA DE LAS EXPRESIONES REGULARES"
                  },
                  "DATE": {
                    "label": "FECHA"
                  }
                },
                "val": "CONTAINS"
              },
