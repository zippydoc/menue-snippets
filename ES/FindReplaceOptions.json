                "required": true,
                "show_if" :  "$index > 0",
                "options": {
                  "and": {
                    "label": "AND"
                  },
                  "or": {
                    "label": "OR"
                  },
                  "and_not": {
                    "label": "AND NOT"
                  },
                  "or_not": {
                    "label": "OR NOT"
                  }
                },
                "val": "and"
              },
              "column": {
                "type": "select",
                "label": "Seleccionar Columna",
                "table": "replace_with.table_id",
                "bind": "COLUMNS",
                "required": true
              },
              "condition": {
                "type": "select",
                "label": "Seleccionar Operador",
                "required": true,
                "options": {
                  "CONTAINS": {
                    "label": "CONTIENE"
                  },
