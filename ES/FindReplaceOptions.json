{
  "options": {
    "type": "fieldset", 
    "label": "Specificare le opzioni", 
    "repeat": false, 
    "fields": {
      "fields_to_impute": {
        "type": "select", 
        "label": "Seleziona le colonne da trovare", 
        "required": true, 
        "table": "input_table", 
        "multiple": true, 
        "bind": "COLUMNS", 
        "width": "100%", 
        "breakLine": true
      }, 
      "incoming_value_to_replace.type": {
        "type": "select", 
        "label": "Valor entrante", 
        "options": {
          "user_specified_value": {
            "label": "Es igual a"
          }, 
          "begins_with": {
            "label": "Comienza con"
          }, 
          "ends_with": {
            "label": "Termina con"
          }, 
          "empty": {
            "label": "Vacío"
          }, 
          "regex": {
            "label": "Espressione regolare"
          }, 
          "replacement_table": {
            "label": "Tabella di sostituzione"
          }
        }, 
        "val": "user_specified_value", 
        "style": "min-width: 190px"
      }, 
      "incoming_value_to_replace.value": {
        "type": "text", 
        "label": "Valor de entrada para reemplazar", 
        "required": true, 
        "show_if": "incoming_value_to_replace.type=='user_specified_value' || incoming_value_to_replace.type=='begins_with' || incoming_value_to_replace.type=='ends_with'", 
        "val": "", 
        "style": "min-width: 170px"
      }, 
      "incoming_value_to_replace.regex": {
        "type": "text", 
        "label": "Espressione regolare", 
        "required": true, 
        "show_if": "incoming_value_to_replace.type=='regex'", 
        "val": "(.*), (.*), (.*)\\\\s(\\\\d{5})(-\\\\d{4})?", 
        "style": "min-width: 200px"
      }, 
      "replace_table.table_id": {
        "type": "select", 
        "label": "Tabella di sostituzione", 
        "show_if": "incoming_value_to_replace.type=='replacement_table'", 
        "bind": "TABLES", 
        "required": true
      }, 
      "replace_table.find_column": {
        "type": "select", 
        "label": "Seleziona Trova colonna", 
        "show_if": "incoming_value_to_replace.type=='replacement_table'", 
        "table": "replace_table.table_id", 
        "bind": "COLUMNS", 
        "required": true
      }, 
      "replace_table.replace_with_column": {
        "type": "select", 
        "label": "Seleziona Sostituisci con colonna", 
        "show_if": "incoming_value_to_replace.type=='replacement_table'", 
        "table": "replace_table.table_id", 
        "bind": "COLUMNS", 
        "required": true
      }, 
      "incoming_value_to_replace.partial_match": {
        "type": "checkbox", 
        "label": "Corrispondenza parziale", 
        "show_if": "incoming_value_to_replace.type=='user_specified_value' || incoming_value_to_replace.type=='replacement_table'", 
        "val": "true", 
        "isOn": "true", 
        "isOff": "false", 
        "breakLine": true
      }, 
      "replace_with.value_type": {
        "type": "select", 
        "label": "Sostituire con", 
        "show_if": "incoming_value_to_replace.type!='regex' && incoming_value_to_replace.type!='replacement_table'", 
        "options": {
          "user_specified_value": {
            "label": "Valor personalizado"
          }, 
          "average": {
            "label": "Media de la columna"
          }, 
          "median": {
            "label": "Columna Mediana"
          }, 
          "mode": {
            "label": "Modo columna"
          }, 
          "sum": {
            "label": "Suma de columnas"
          }, 
          "table": {
            "label": "Colonna della tabella"
          }
        }, 
        "val": "user_specified_value", 
        "style": "min-width: 190px"
      }, 
      "replace_with.value": {
        "type": "text", 
        "label": "Sostituisci con valore", 
        "show_if": "incoming_value_to_replace.type!='regex' && replace_with.value_type=='user_specified_value' && incoming_value_to_replace.type!='replacement_table'", 
        "val": ""
      }, 
      "replace_with.regex_value": {
        "type": "text", 
        "label": "Sostituisci con valore", 
        "show_if": "incoming_value_to_replace.type=='regex' && incoming_value_to_replace.type!='replacement_table'", 
        "val": "\\\\4: \\\\2,  \\\\3"
      }, 
      "replace_with.table": {
        "type": "select", 
        "label": "Seleziona la tabella", 
        "show_if": "incoming_value_to_replace.type!='regex' && replace_with.value_type=='table' && incoming_value_to_replace.type!='replacement_table'", 
        "bind": "TABLES", 
        "required": true
      }, 
      "replace_with.column": {
        "type": "select", 
        "label": "Seleziona la colonna", 
        "show_if": "incoming_value_to_replace.type!='regex' && replace_with.value_type=='table' && incoming_value_to_replace.type!='replacement_table'", 
        "table": "replace_with.table", 
        "bind": "COLUMNS", 
        "required": true, 
        "breakLine": true
      }, 
      "use_filter": {
        "type": "checkbox", 
        "label": "Usa il filtro", 
        "val": "false", 
        "isOn": "true", 
        "isOff": "false", 
        "breakLine": true
      }, 
      "filter_options_box": {
        "type": "fieldset", 
        "label": "Opciones de filtrado", 
        "show_if": "use_filter=='true'", 
        "repeat": false, 
        "fields": {
          "filter": {
            "type": "fieldset", 
            "label": "Añadir condiciones de filtrado", 
            "show_if": "use_filter=='true'", 
            "repeat": true, 
            "fields": {
              "initial_op": {
                "type": "select", 
                "label": "Seleziona l'operatore logico", 
                "required": true, 
                "show_if" :  "$index == 0", 
                "options": {
                  "if": {
                    "label": "SI"
                  }, 
                  "if_not": {
                    "label": "SI NO"
                  }
                }, 
                "val": "if"
              }, 
              "op": {
                "type": "select", 
                "label": "Seleziona l'operatore logico", 
                "required": true, 
                "show_if" :  "$index > 0", 
                "options": {
                  "and": {
                    "label": "Y"
                  }, 
                  "or": {
                    "label": "O"
                  }, 
                  "and_not": {
                    "label": "Y NO"
                  }, 
                  "or_not": {
                    "label": "O NON"
                  }
                }, 
                "val": "and"
              }, 
              "column": {
                "type": "select", 
                "label": "Seleziona la colonna", 
                "table": "input_table", 
                "bind": "COLUMNS", 
                "required": true
              }, 
              "condition": {
                "type": "select", 
                "label": "Seleziona l'operatore", 
                "required": true, 
                "options": {
                  "CONTIENE": {
                    "label": "CONTIENE"
                  }, 
                  "CONTAINED IN STRING": {
                    "label": "CONTENIDO EN"
                  }, 
                  "INIZIA CON": {
                    "label": "INIZIA CON"
                  }, 
                  "FINALIZA CON": {
                    "label": "FINALIZA CON"
                  }, 
                  "=": {
                    "label": "EQUAL"
                  }, 
                  ">": {
                    "label": "MAYOR QUE"
                  }, 
                  ">=": {
                    "label": "MAYOR O IGUAL"
                  }, 
                  "<": {
                    "label": "PIÙ PICCOLO DI"
                  }, 
                  "<=": {
                    "label": "PIÙ PICCOLO O UGUALE"
                  }, 
                  "EMPTY": {
                    "label": "ESTÁ VACÍO"
                  }, 
                  "BLANK": {
                    "label": "ESTÁ EN BLANCO"
                  }, 
                  "ES NULO": {
                    "label": "ES NULO"
                  }, 
                  "CORRISPONDENZA REGEX": {
                    "label": "CORRISPONDENZA REGEX"
                  }, 
                  "CONTAINED IN TABLE": {
                    "label": "EN TABLA"
                  }, 
                  "EN LA TABLA REGEX": {
                    "label": "EN LA TABLA REGEX"
                  }, 
                  "FECHA": {
                    "label": "FECHA"
                  }
                }, 
                "val": "CONTIENE"
              }, 
              "date_condition": {
                "type": "select", 
                "label": "Seleziona l'operatore della data", 
                "required": true, 
                "show_if": "this.condition == 'DATE'", 
                "options": {
                  "<": {
                    "label": "PRIORE"
                  }, 
                  "<=": {
                    "label": "PRIORE O PARI"
                  }, 
                  "=": {
                    "label": "EQUAL"
                  }, 
                  ">": {
                    "label": "DESPUÉS DE"
                  }, 
                  ">=": {
                    "label": "DESPUÉS O IGUAL"
                  }
                }, 
                "val": "="
              }, 
              "case_sensitive": {
                "type": "select", 
                "label": "Selezionare Case Sensitive", 
                "required": true, 
                "show_if": "this.condition == 'CONTAINS' || this.condition == 'CONTAINED IN STRING' || this.condition == '=' || this.condition == 'STARTS WITH' || this.condition == 'ENDS WITH' || this.condition == 'REGEX MATCH' || this.condition == 'NOT REGEX MATCH' || this.condition == 'IN REGEX TABLE' || this.condition == 'NOT IN REGEX TABLE'", 
                "options": {
                  "yes": {
                    "label": "Sensible al caso"
                  }, 
                  "no": {
                    "label": "Insensible a las mayúsculas y minúsculas"
                  }
                }, 
                "val": "yes"
              }, 
              "date_value": {
                "type": "date", 
                "label": "Seleziona la data", 
                "show_if": "this.condition=='DATE'", 
                "val": "1/31/2020", 
                "required": true
              }, 
              "value": {
                "type": "text", 
                "label": "Valor de entrada", 
                "show_if": "this.condition != 'DATE' && this.condition != 'EMPTY' && this.condition != 'NOT EMPTY' && this.condition != 'BLANK' && this.condition != 'NOT BLANK' && this.condition != 'IS NULL' && this.condition != 'IS NOT NULL' && this.condition != 'CONTAINED IN TABLE' && this.condition != 'NOT CONTAINED IN TABLE' && this.condition != 'IN REGEX TABLE' && this.condition != 'NOT IN REGEX TABLE'", 
                "val": "", 
                "required": true
              }, 
              "compare_table": {
                "type": "select", 
                "label": "Seleziona la tabella", 
                "bind": "TABLES", 
                "empty": "", 
                "show_if": "this.condition == 'CONTAINED IN TABLE' || this.condition == 'NOT CONTAINED IN TABLE' || this.condition == 'IN REGEX TABLE' || this.condition == 'NOT IN REGEX TABLE'", 
                "required": true
              }, 
              "compare_column": {
                "type": "select", 
                "label": "Seleziona la colonna", 
                "table": "this.compare_table", 
                "bind": "COLUMNS", 
                "show_if": "this.condition == 'CONTAINED IN TABLE' || this.condition == 'NOT CONTAINED IN TABLE' || this.condition == 'IN REGEX TABLE' || this.condition == 'NOT IN REGEX TABLE'", 
                "required": true
              }, 
              "delete": {
                "type": "button", 
                "label": "Borrar", 
                "function": "DELETE_ROW"
              }
            }
          }, 
          "apply_to_record": {
            "type": "checkbox", 
            "label": "Aplicar al registro", 
            "val": "true", 
            "isOn": "true", 
            "isOff": "false", 
            "style": "margin-top: 10px", 
            "breakLine": true
          }, 
          "apply_to_group_box": {
            "type": "fieldset", 
            "label": "Aplicar al Grupo", 
            "show_if": "apply_to_record=='false'", 
            "repeat": false, 
            "fields": {
              "apply_to_group_columns": {
                "type": "select", 
                "label": "Seleziona le colonne del gruppo", 
                "required": true, 
                "width": "100%", 
                "table": "input_table", 
                "multiple": true, 
                "bind": "COLUMNS", 
                "breakLine": true
              }
            }
          }
        }
      }, 
      "use_replace_table_filter": {
        "type": "checkbox", 
        "label": "Use Filter on \\"Replace With\\" Table", 
        "show_if": "incoming_value_to_replace.type=='replacement_table' || (incoming_value_to_replace.type!='regex' && replace_with.value_type=='table')", 
        "val": "false", 
        "isOn": "true", 
        "isOff": "false", 
        "style": "margin-top: 10px", 
        "breakLine": true
      }, 
      "replace_table_filter_option_box": {
        "type": "fieldset", 
        "label": "\\"Replace With\\" Table Filter Options", 
        "show_if": "use_replace_table_filter=='true' && (incoming_value_to_replace.type=='replacement_table' || (incoming_value_to_replace.type!='regex' && replace_with.value_type=='table'))", 
        "repeat": false, 
        "fields": {
          "replace_table_filter_option.filter": {
            "type": "fieldset", 
            "label": "Add Filter Conditions on \\"Replace With\\" Table", 
            "repeat": true, 
            "fields": {
              "initial_op": {
                "type": "select", 
                "label": "Seleziona l'operatore logico", 
                "required": true, 
                "show_if" :  "$index == 0", 
                "options": {
                  "if": {
                    "label": "SI"
                  }, 
                  "if_not": {
                    "label": "SI NO"
                  }
                }, 
                "val": "if"
              }, 
              "op": {
                "type": "select", 
                "label": "Seleziona l'operatore logico", 
                "required": true, 
                "show_if" :  "$index > 0", 
                "options": {
                  "and": {
                    "label": "Y"
                  }, 
                  "or": {
                    "label": "O"
                  }, 
                  "and_not": {
                    "label": "Y NO"
                  }, 
                  "or_not": {
                    "label": "O NON"
                  }
                }, 
                "val": "and"
              }, 
              "column": {
                "type": "select", 
                "label": "Seleziona la colonna", 
                "table": "replace_with.table", 
                "bind": "COLUMNS", 
                "required": true
              }, 
              "condition": {
                "type": "select", 
                "label": "Seleziona l'operatore", 
                "required": true, 
                "options": {
                  "CONTIENE": {
                    "label": "CONTIENE"
                  }, 
                  "CONTAINED IN STRING": {
                    "label": "CONTENIDO EN"
                  }, 
                  "INIZIA CON": {
                    "label": "INIZIA CON"
                  }, 
                  "FINALIZA CON": {
                    "label": "FINALIZA CON"
                  }, 
                  "=": {
                    "label": "EQUAL"
                  }, 
                  ">": {
                    "label": "MAYOR QUE"
                  }, 
                  ">=": {
                    "label": "MAYOR O IGUAL"
                  }, 
                  "<": {
                    "label": "PIÙ PICCOLO DI"
                  }, 
                  "<=": {
                    "label": "PIÙ PICCOLO O UGUALE"
                  }, 
                  "EMPTY": {
                    "label": "ESTÁ VACÍO"
                  }, 
                  "BLANK": {
                    "label": "ESTÁ EN BLANCO"
                  }, 
                  "ES NULO": {
                    "label": "ES NULO"
                  }, 
                  "CORRISPONDENZA REGEX": {
                    "label": "CORRISPONDENZA REGEX"
                  }, 
                  "CONTAINED IN TABLE": {
                    "label": "EN TABLA"
                  }, 
                  "EN LA TABLA REGEX": {
                    "label": "EN LA TABLA REGEX"
                  }, 
                  "FECHA": {
                    "label": "FECHA"
                  }
                }, 
                "val": "CONTIENE"
              }, 
              "date_condition": {
                "type": "select", 
                "label": "Seleziona l'operatore della data", 
                "required": true, 
                "show_if": "this.condition == 'DATE'", 
                "options": {
                  "<": {
                    "label": "PRIORE"
                  }, 
                  "<=": {
                    "label": "PRIORE O PARI"
                  }, 
                  "=": {
                    "label": "EQUAL"
                  }, 
                  ">": {
                    "label": "DESPUÉS DE"
                  }, 
                  ">=": {
                    "label": "DESPUÉS O IGUAL"
                  }
                }, 
                "val": "="
              }, 
              "case_sensitive": {
                "type": "select", 
                "label": "Selezionare Case Sensitive", 
                "required": true, 
                "show_if": "this.condition == 'CONTAINS' || this.condition == 'CONTAINED IN STRING' || this.condition == '=' || this.condition == 'STARTS WITH' || this.condition == 'ENDS WITH' || this.condition == 'REGEX MATCH' || this.condition == 'NOT REGEX MATCH' || this.condition == 'IN REGEX TABLE' || this.condition == 'NOT IN REGEX TABLE'", 
                "options": {
                  "yes": {
                    "label": "Sensible al caso"
                  }, 
                  "no": {
                    "label": "Insensible a las mayúsculas y minúsculas"
                  }
                }, 
                "val": "yes"
              }, 
              "date_value": {
                "type": "date", 
                "label": "Seleziona la data", 
                "show_if": "this.condition=='DATE'", 
                "val": "1/31/2020", 
                "required": true
              }, 
              "value": {
                "type": "text", 
                "label": "Valor de entrada", 
                "show_if": "this.condition != 'DATE' && this.condition != 'EMPTY' && this.condition != 'NOT EMPTY' && this.condition != 'BLANK' && this.condition != 'NOT BLANK' && this.condition != 'IS NULL' && this.condition != 'IS NOT NULL' && this.condition != 'CONTAINED IN TABLE' && this.condition != 'NOT CONTAINED IN TABLE' && this.condition != 'IN REGEX TABLE' && this.condition != 'NOT IN REGEX TABLE'", 
                "val": "", 
                "required": true
              }, 
              "compare_table": {
                "type": "select", 
                "label": "Seleziona la tabella", 
                "bind": "TABLES", 
                "empty": "", 
                "show_if": "this.condition == 'CONTAINED IN TABLE' || this.condition == 'NOT CONTAINED IN TABLE' || this.condition == 'IN REGEX TABLE' || this.condition == 'NOT IN REGEX TABLE'", 
                "required": true
              }, 
              "compare_column": {
                "type": "select", 
                "label": "Seleziona la colonna", 
                "table": "this.compare_table", 
                "bind": "COLUMNS", 
                "show_if": "this.condition == 'CONTAINED IN TABLE' || this.condition == 'NOT CONTAINED IN TABLE' || this.condition == 'IN REGEX TABLE' || this.condition == 'NOT IN REGEX TABLE'", 
                "required": true
              }, 
              "delete": {
                "type": "button", 
                "label": "Borrar", 
                "function": "DELETE_ROW"
              }
            }
          }, 
          "replace_table_filter_option.apply_to_record": {
            "type": "checkbox", 
            "label": "Apply to Record on \\"Replace With\\" Table", 
            "val": "true", 
            "isOn": "true", 
            "isOff": "false", 
            "style": "margin-top: 10px", 
            "breakLine": true
          }, 
          "replace_table_filter_option_apply_to_group_box": {
            "type": "fieldset", 
            "label": "Apply to Group on \\"Replace With\\" Table", 
            "show_if": "replace_table_filter_option.apply_to_record=='false' && (incoming_value_to_replace.type!='replacement_table' && incoming_value_to_replace.type!='regex' && replace_with.value_type=='table')", 
            "repeat": false, 
            "fields": {
              "replace_table_filter_option.apply_to_group_columns": {
                "type": "select", 
                "label": "Seleziona le colonne del gruppo", 
                "required": true, 
                "width": "100%", 
                "table": "replace_with.table", 
                "multiple": true, 
                "bind": "COLUMNS", 
                "breakLine": true
              }
            }
          }, 
          "replacement_table_filter_option_apply_to_group_box": {
            "type": "fieldset", 
            "label": "Apply to Group on \\"Replace With\\" Table", 
            "show_if": "replace_table_filter_option.apply_to_record=='false' && incoming_value_to_replace.type=='replacement_table'", 
            "repeat": false, 
            "fields": {
              "replacement_table_filter_option.apply_to_group_columns": {
                "type": "select", 
                "label": "Seleziona le colonne del gruppo", 
                "required": true, 
                "width": "100%", 
                "table": "replace_table.table_id", 
                "multiple": true, 
                "bind": "COLUMNS", 
                "breakLine": true
              }
            }
          }
        }
      }
    }
  }
}
