  "options_box": {
    "type": "fieldset",
    "label": "Opción de informe Flexmonster",
    "show_if": "options.configure_manual=='true'",
    "repeat": false,
    "fields": {
      "options.type": {
        "type": "hidden",
        "val": "grid"
      },
      "options.add_column_cols": {
        "type": "hidden",
        "val": "true"
      },
      "options.column_cols": {
        "type": "fieldset",
        "label": "Column column(s)",
        "repeat": true,
        "fields": {
          "column_col": {
            "type": "select",
            "label": "Columna",
            "table": "table_id",
            "bind": "COLUMNS",
            "required": true
          },
          "is_date": {
            "type": "checkbox",
            "label": "Es la fecha",
            "val": "false",
            "isOn": "true",
            "isOff": "false",
            "breakLine": true
          },
          "delete": {
            "type": "button",
            "label": "Borrar",
            "function": "DELETE_ROW"
          }
        }
      },
      "options.row_cols": {
        "type": "fieldset",
        "label": "Row column(s)",
        "repeat": true,
        "fields": {
          "row_col": {
            "type": "select",
            "label": "Columna de filas",
            "table": "table_id",
