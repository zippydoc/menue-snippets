{
  "output": {
    "type": "fieldset",
    "label": "Definir Tabla de Salida",
    "repeat": false,
    "fields": {
      "output.custom_text": {
        "type": "text",
        "label": "Imprimir Nombre de Tabla",
        "show_if": "(output.set_options==''true'' && output.compose_table_name==''false'') || output.set_options==''false''",
        "minLength": 1,
        "val": "",
        "width": "100%",
        "breakLine": true
      },
      "output.select_output_columns": {
        "type": "checkbox",
        "label": "Seleccionar Columnas de Salida",
        "show_if": "options.is_generate_holiday_table==''add_column''",
        "val": "false",
        "isOn": "true",
        "isOff": "false"
      },
      "output_columns": {
        "type": "fieldset",
        "label": "Select OutputColumns",
        "show_if": "options.is_generate_holiday_table==''add_column'' && output.select_output_columns==''true''",
        "repeat": true,
        "fields": {
          "field_name": {
            "type": "select",
            "label": “Seleccionar Columna”,
            "table": "input_table",
            "bind": "COLUMNS",
            "required": true
          },
          "order": {
            "type": "select",
            "label": "Orden",
            "options": {
              "-": {
                "label": "-"
              },
              "asc": {
                "label": "Ascendente"
              },
              "desc": {
                "label": "Descendente"
              }
            },
            "val": "-"
          },
          "is_rename": {
            "type": "checkbox",
            "label": “Renombrar”,
            "val": "false",
            "isOn": "true",
            "isOff": "false"
          },
          "rename": {
            "show_if": "this.is_rename == ''true''",
            "type": "text",
            "required": true,
            "minLength": 1,
            "maxLength": 255,
            "label": “Nuevo Nombre de Columna”,
            "style": "margin-left: 10px"
          },
          "format__category": {
            "type": "select",
            "label": "Formato",
            "required": true,
            "options": {
              "standard": {
                "label": "Estándar"
              },
              "number": {
                "label": "Número"
              },
              "percentage": {
                "label": "Porcentaje"
              },
              "text": {
                "label": "Texto"
              },
              "date": {
                "label": "Fecha"
              },
              "currency": {
                "label": "Moneda"
              }
            },
            "val": "standard"
          },
          "format__rounding": {
            "type": "select",
            "label": "Redondeo",
            "show_if": "this.format__category==''number'' || this.format__category==''percentage'' || this.format__category==''currency''",
            "required": true,
            "options": {
              "no_rounding": {
                "label": "Redondeo de números"
              },
              "0": {
                "label": "0 dígitos"
              },
              "1": {
                "label": "1 dígitos"
              },
              "2": {
                "label": "2 dígitos"
              },
              "3": {
                "label": "3 dígitos"
              },
              "4": {
                "label": "4 dígitos"
              },
              "5": {
                "label": "5 dígitos"
              },
              "6": {
                "label": "6 dígitos"
              },
              "7": {
                "label": "7 dígitos"
              },
              "8": {
                "label": "8 dígitos"
              },
              "9": {
                "label": "9 dígitos"
              }
            },
            "val": "2"
          },
          "format__use_1000_separator": {
            "type": "checkbox",
            "label": "Usar el Separador de 1000(.)",
            "show_if": "this.format__category==''number'' || this.format__category==''percentage'' || this.format__category==''currency''",
            "val": "true",
            "isOn": "true",
            "isOff": "false"
          },
          "format__currency_symbol": {
            "type": "select",
            "label": "Símbolo",
            "show_if": "this.format__category==''currency''",
            "required": true,
            "options": {
              "dollar": {
                "label": "$"
              },
              "euro": {
                "label": "€"
              },
              "pound": {
                "label": "£"
              }
            },
            "val": "dollar"
          },
          "format__date_format": {
            "type": "select",
            "label": "Tipo",
            "show_if": "this.format__category==''date''",
            "required": true,
            "options": {
              "mm/dd/yyyy": {
                "label": "3/14/2012"
              },
              "D, M dd, Y": {
                "label": "Wednesday, March 14, 2012"
              },
              "yyyy-mm-dd": {
                "label": "2012-03-14"
              }
            },
            "val": "mm/dd/yyyy",
            "breakLine": true
          },
          "delete": {
            "type": "button",
            "label": "Borrar",
            "function": "DELETE_ROW"
          }
        },
        "breakLine": true,
        "style": "margin-bottom: 20px"
      },
      "output.set_options": {
        "type": "checkbox",
        "label": "Imprimir Opciones",
        "val": "false",
        "isOn": "true",
        "isOff": "false"
      },
      "output_options_box": {
        "type": "fieldset",
        "label": "Imprimir Opciones",
        "show_if": "output.set_options==''true''",
        "repeat": false,
        "fields": {
          "output.compose_table_name": {
            "type": "checkbox",
            "label": “Componer Nombre de la Tabla”,
            "val": "false",
            "isOn": "true",
            "isOff": "false",
            "breakLine": true
          },
          "compose_table_name_frame": {
            "type": "fieldset",
            "label": “Componer Nombre de la Tabla de Salida”,
            "show_if": "output.compose_table_name==''true''",
            "repeat": false,
            "fields": {
              "output.prefix_rule": {
                "type": "select",
                "label": "Prefijo",
                "show_if": "output.compose_table_name==''true''",
                "val": "empty",
                "options": {
                  "empty": {
                    "label": "-"
                  },
                  "script_name": {
                    "label": "Nombre de la Tarea"
                  },
                  "date_string_type1": {
                    "label": "Fecha: dd_mm_aaaa"
                  },
                  "date_string_type2": {
                    "label": "Fecha: aaaa_mm_dd"
                  },
                  "time_string": {
                    "label": "Tempo: h_min_seg"
                  },
                  "datetime_string": {
                    "label": "Fecha_Hora"
                  },
                  "custom": {
                    "label": "Personalizado"
                  }
                }
              },
              "output.prefix_text": {
                "type": "text",
                "label": "Texto del Prefijo",
                "show_if": "output.compose_table_name==''true'' && output.prefix_rule==''custom''",
                "minLength": 1,
                "maxLength": 31,
                "val": ""
              },
              "output.core_delimiter": {
                "type": "text",
                "label": "Delimitador",
                "show_if": "output.compose_table_name==''true'' && output.prefix_rule!=''empty''",
                "val": "_",
                "width": "50px"
              },
              "output.generate_core_rule": {
                "type": "select",
                "label": "Nombre de la Tabla",
                "show_if": "output.compose_table_name==''true'' && options.is_generate_holiday_table==''generate_table''",
                "val": "script_name",
                "options": {
                  "script_name": {
                    "label": "Nombre de la Tarea"
                  },
                  "custom": {
                    "label": "Personalizado"
                  }
                }
              },
              "output.core_rule": {
                "type": "select",
                "label": "Nombre de la Tabla",
                "show_if": "output.compose_table_name==''true'' && options.is_generate_holiday_table==''add_column''",
                "val": "input_table_name",
                "options": {
                  "input_table_name": {
                    "label": "Ingresar Nombre de la Tabla"
                  },
                  "script_name": {
                    "label": "Nombre de la Tarea"
                  },
                  "custom": {
                    "label": "Personalizado"
                  }
                }
              },
              "output.core_text": {
                "type": "text",
                "label": "Nombre de la Tabla Principal",
                "show_if": "output.compose_table_name==''true'' && output.core_rule==''custom''",
                "required": true,
                "minLength": 1,
                "maxLength": 31,
                "val": ""
              },
              "output.suffix_delimiter": {
                "type": "text",
                "label": "Delimitador",
                "show_if": "output.compose_table_name==''true''",
                "val": "_",
                "width": "50px"
              },
              "output.suffix_rule": {
                "type": "select",
                "label": "Sufijo",
                "show_if": "output.compose_table_name==''true''",
                "val": "script_name",
                "options": {
                  "empty": {
                    "label": "-"
                  },
                  "script_name": {
                    "label": "Nombre de la Tarea"
                  },
                  "date_string_type1": {
                    "label": "Fecha: dd_mm_aaaa"
                  },
                  "date_string_type2": {
                    "label": "Fecha: aaaa_mm_dd"
                  },
                  "time_string": {
                    "label": "Tempo: h_min_seg"
                  },
                  "datetime_string": {
                    "label": "Fecha_Hora"
                  },
                  "custom": {
                    "label": "Personalizado"
                  }
                }
              },
              "output.suffix_text": {
                "type": "text",
                "label": "Texto del Sufijo",
                "show_if": "output.compose_table_name==''true'' && output.suffix_rule==''custom''",
                "minLength": 1,
                "maxLength": 31,
                "val": "",
                "breakLine": true
              },
              "output.timezone": {
                "type": "select",
                "label": "Zona Horaria",
                "show_if": "output.compose_table_name==''true'' && (output.prefix_rule==''date_string_type1'' || output.prefix_rule==''date_string_type2'' || output.prefix_rule==''time_string'' || output.prefix_rule==''datetime_string'' || output.suffix_rule==''date_string_type1'' || output.suffix_rule==''date_string_type2'' || output.suffix_rule==''time_string'' || output.suffix_rule==''datetime_string'')",
                "val": "Europa/Berlín",
                "required": true,
                "options": {
                  "America/Adak": {
                    "label": "Adak, Gambier"
                  },
                  "Australia/Adelaide": {
                    "label": "Adelaide, Broken_Hill, Darwin"
                  },
                  "America/Anchorage": {
                    "label": "Anchorage, Juneau, Sitka"
                  },
                  "Pacific/Apia": {
                    "label": "Apia, Asau, Mulifanua, Afega, Leulumoega"
                  },
                  "Pacific/Enderbury": {
                    "label": "Apia, Fiji, Tongatapu"
                  },
                  "Antarctica/McMurdo": {
                    "label": "Auckland, Casey, Davis"
                  },
                  "Pacific/Auckland": {
                    "label": "Auckland, Wellington, Christchurch"
                  },
                  "Asia/Bangkok": {
                    "label": "Bangkok, Jakarta, Novosibirsk"
                  },
                  "Europa/Berlín": {
                    "label": "Berlin, Paris, Amsterdam"
                  },
                  "Australia/Perth": {
                    "label": "Brunei, Singapore, Perth"
                  },
                  "Atlantic/Cape_Verde": {
                    "label": "Cape_Verde,Praia, Mindelo, Santa Maria"
                  },
                  "America/Belize": {
                    "label": "Costa_Rica, El_Salvador, Denver"
                  },
                  "Antarctica/Vostok": {
                    "label": "Dhaka, Bishkek, Omsk"
                  },
                  "Europe/Astrakhan": {
                    "label": "Dubai, Baku, Tbilisi"
                  },
                  "Australia/Eucla": {
                    "label": "Eucla"
                  },
                  "Pacific/Honolulu": {
                    "label": "Honolulu, Rarotonga, Tahiti"
                  },
                  "Asia/Kabul": {
                    "label": "Kabul, Kandahār, Mazār-e Sharīf, Herāt, Jalālābād"
                  },
                  "Asia/Aqtau": {
                    "label": "Karachi, Tashkent, Maldives"
                  },
                  "Asia/Kathmandu": {
                    "label": "Kathmandu,Pokhara, Pātan, Biratnagar"
                  },
                  "Pacific/Kiritimati": {
                    "label": "Kiritimati"
                  },
                  "Asia/Colombo": {
                    "label": "Kolkata, Colombo,Jaffna, Negombo"
                  },
                  "Europe/Guernsey": {
                    "label": "London. Dublin, Lisbon"
                  },
                  "Australia/Lord_Howe": {
                    "label": "Lord_Howe"
                  },
                  "America/Creston": {
                    "label": "Los_Angeles, Vancouver, Hermosillo"
                  },
                  "Pacific/Marquesas": {
                    "label": "Marquesas,Taiohae"
                  },
                  "America/Cayman": {
                    "label": "Mexico_City, Panama, Chicago"
                  },
                  "Pacific/Niue": {
                    "label": "Midway, Niue, Pago_Pago"
                  },
                  "Europe/Mariehamn": {
                    "label": "Moscow, Istanbul, Qatar"
                  },
                  "Asia/Kolkata": {
                    "label": "Mumbai, Delhi, Bengaluru, Kolkata, Chennai"
                  },
                  "America/Miquelon": {
                    "label": "Noronha, Nuuk, Miquelon"
                  },
                  "America/Danmarkshavn": {
                    "label": "Reykjavik, Dakar, Casablanca"
                  },
                  "Pacific/Bougainville": {
                    "label": "Sakhalin, Norfolk, Bougainville"
                  },
                  "America/Araguaina": {
                    "label": "Sao_Paulo, Buenos_Aires, Montevideo"
                  },
                  "America/St_Johns": {
                    "label": "St_Johns,Mount Pearl, Corner Brook"
                  },
                  "Asia/Tehran": {
                    "label": "Tehran, Mashhad, Isfahan, Karaj, Tabriz"
                  },
                  "Asia/Tokyo": {
                    "label": "Tokyo, Seoul, Jayapura"
                  },
                  "America/New_York": {
                    "label": "Toronto, Santiago, New_York"
                  },
                  "Australia/Sydney": {
                    "label": "Vladivostok, Sydney, Brisbane"
                  },
                  "Asia/Yangon": {
                    "label": "Yangon, Cocos,Mandalay, Nay Pyi Taw, Mawlamyine"
                  },
                  "Indian/Cocos": {
                    "label": "Yangon, Cocos,West Island"
                  }
                },
                "breakLine": true
              }
            }
          },
          "download_table_setting_frame": {
            "type": "fieldset",
            "label": "Download Table Settings",
            "repeat": false,
            "fields": {
              "output.options.location": {
                "type": "text",
                "label": "Especificar Ubicación",
                "val": "A1",
                "breakLine": true
              },
              "output.options.formatted_as_table": {
                "type": "checkbox",
                "label": "Formateado como Tabla",
                "val": "true",
                "isOn": "true",
                "isOff": "false",
                "breakLine": true
              },
              "output.add_tag": {
                "type": "checkbox",
                "label": “Agregar Marcador”,
                "val": "false",
                "isOn": "true",
                "isOff": "false"
              },
              "output.tag_name": {
                "type": "text",
                "label": "Nombre del marcador",
                "show_if": "output.add_tag==''true''",
                "required": true,
                "minLength": 1,
                "maxLength": 255,
                "val": "",
                "breakLine": true,
                "style": "margin-left: 15px"
              },
              "output.options.visibility": {
                "type": "select",
                "label": "Visibilidad",
                "val": "current_flow",
                "options": {
                  "current_flow": {
                    "label": "Caudal Actual"
                  },
                  "all_flow": {
                    "label": "Todos los Caudales"
                  }
                },
                "breakLine": true
              },
              "output.options.table_mode": {
                "type": "select",
                "label": "Pegar",
                "val": "content_only",
                "options": {
                  "content_only": {
                    "label": "Sólo Contenido"
                  },
                  "fully_formatted": {
                    "label": "Completamente Formateada"
                  }
                }
              }
            }
          }
        },
        "breakLine": true
      }
    }
  }
}