{
  "output": {
    "type": "fieldset",
    "Etiqueta": "Definir Tabla de Salida",
    "repeat": false,
    "fields": {
      "output.custom_text": {
        "type": "text",
        "Etiqueta": "Imprimir Nombre de Tabla",
        "show_if": "output.custom_options=='false' || (output.custom_options=='true' && output.compose_table_name=='false')",
        "minLength": 1,
        "val": "",
        "width": "100%",
        "breakLine": true
      },
      "output.custom_options": {
        "type": "checkbox",
        "Etiqueta": "Imprimir Opciones",
        "val": "false",
        "isOn": "true",
        "isOff": "false",
        "breakLine": true
      },
      "output.select_output_columns": {
        "type": "checkbox",
        "Etiqueta": "Seleccionar columnas de salida",
        "show_if": "options.is_generate_holiday_table=='add_column' && output.custom_options=='true'",
        "val": "false",
        "isOn": "true",
        "isOff": "false"
      },
      "output_columns": {
        "type": "fieldset",
        "Etiqueta": "Select OutputColumns",
        "show_if": "options.is_generate_holiday_table=='add_column' && output.custom_options=='true' && output.select_output_columns=='true'",
        "repeat": true,
        "fields": {
          "field_name": {
            "type": "select",
            "Etiqueta": "Seleccionar Columna",
            "table": "input_table",
            "bind": "COLUMNS",
            "required": true
          },
          "order": {
            "type": "select",
            "Etiqueta": "Orden",
            "options": {
              "-": {
                "Etiqueta": "-"
              },
              "asc": {
                "Etiqueta": "Ascendente"
              },
              "desc": {
                "Etiqueta": "Descendente"
              }
            },
            "val": "-"
          },
          "is_rename": {
            "type": "checkbox",
            "Etiqueta": "Renombrar",
            "val": "false",
            "isOn": "true",
            "isOff": "false"
          },
          "rename": {
            "show_if": "this.is_rename == 'true'",
            "type": "text",
            "required": true,
            "minLength": 1,
            "maxLength": 255,
            "Etiqueta": "Nuevo Nombre de Columna",
            "style": "margin-left: 10px"
          },
          "format__category": {
            "type": "select",
            "Etiqueta": "Formato",
            "required": true,
            "options": {
              "standard": {
                "Etiqueta": "Estándar"
              },
              "number": {
                "Etiqueta": "Número"
              },
              "percentage": {
                "Etiqueta": "Porcentaje"
              },
              "text": {
                "Etiqueta": "Texto"
              },
              "date": {
                "Etiqueta": "Fecha"
              },
              "currency": {
                "Etiqueta": "Moneda"
              }
            },
            "val": "standard"
          },
          "format__rounding": {
            "type": "select",
            "Etiqueta": "Redondeo",
            "show_if": "this.format__category=='number' || this.format__category=='percentage' || this.format__category=='currency'",
            "required": true,
            "options": {
              "no_rounding": {
                "Etiqueta": "Redondeo de números"
              },
              "0": {
                "Etiqueta": "0 dígitos"
              },
              "1": {
                "Etiqueta": "1 dígitos"
              },
              "2": {
                "Etiqueta": "2 dígitos"
              },
              "3": {
                "Etiqueta": "3 dígitos"
              },
              "4": {
                "Etiqueta": "4 dígitos"
              },
              "5": {
                "Etiqueta": "5 dígitos"
              },
              "6": {
                "Etiqueta": "6 dígitos"
              },
              "7": {
                "Etiqueta": "7 dígitos"
              },
              "8": {
                "Etiqueta": "8 dígitos"
              },
              "9": {
                "Etiqueta": "9 dígitos"
              }
            },
            "val": "2"
          },
          "format__use_1000_separator": {
            "type": "checkbox",
            "Etiqueta": "Use 1000 Separator(.)",
            "show_if": "this.format__category=='number' || this.format__category=='percentage' || this.format__category=='currency'",
            "val": "true",
            "isOn": "true",
            "isOff": "false"
          },
          "format__currency_symbol": {
            "type": "select",
            "Etiqueta": "Símbolo",
            "show_if": "this.format__category=='currency'",
            "required": true,
            "options": {
              "dollar": {
                "Etiqueta": "$"
              },
              "euro": {
                "Etiqueta": "€"
              },
              "pound": {
                "Etiqueta": "£"
              }
            },
            "val": "dollar"
          },
          "format__date_format": {
            "type": "select",
            "Etiqueta": "Tipo",
            "show_if": "this.format__category=='date'",
            "required": true,
            "options": {
              "mm/dd/yyyy": {
                "Etiqueta": "3/14/2012"
              },
              "D, M dd, Y": {
                "Etiqueta": "Miércoles, 14 de marzo de 2012"
              },
              "yyyy-mm-dd": {
                "Etiqueta": "40982"
              }
            },
            "val": "mm/dd/yyyy",
            "breakLine": true
          },
          "delete": {
            "type": "button",
            "Etiqueta": "Borrar",
            "function": "DELETE_ROW"
          }
        },
        "breakLine": true,
        "style": "margin-bottom: 20px"
      },
      "output.compose_table_name": {
        "type": "checkbox",
        "Etiqueta": "Componer Nombre de la Tabla",
        "show_if": "output.custom_options=='true'",
        "val": "false",
        "isOn": "true",
        "isOff": "false",
        "breakLine": true
      },
      "compose_table_name_frame": {
        "type": "fieldset",
        "Etiqueta": "Componer Nombre de la Tabla de Salida",
        "show_if": "output.custom_options=='true' && output.compose_table_name=='true'",
        "repeat": false,
        "fields": {
          "output.prefix_rule": {
            "type": "select",
            "Etiqueta": "Prefijo",
            "show_if": "output.compose_table_name=='true'",
            "val": "empty",
            "options": {
              "empty": {
                "Etiqueta": "-"
              },
              "script_name": {
                "Etiqueta": "Nombre de la Tarea"
              },
              "date_string_type1": {
                "Etiqueta": "Fecha: dd_mm_aaaa"
              },
              "date_string_type2": {
                "Etiqueta": "Fecha: aaaa_mm_dd"
              },
              "time_string": {
                "Etiqueta": "Tempo: h_min_seg"
              },
              "datetime_string": {
                "Etiqueta": "Fecha_Hora"
              },
              "custom": {
                "Etiqueta": "Personalizado"
              }
            }
          },
          "output.prefix_text": {
            "type": "text",
            "Etiqueta": "Texto del Prefijo",
            "show_if": "output.compose_table_name=='true' && output.prefix_rule=='custom'",
            "minLength": 1,
            "maxLength": 31,
            "val": ""
          },
          "output.core_delimiter": {
            "type": "text",
            "Etiqueta": "Delimitador",
            "show_if": "output.compose_table_name=='true' && output.prefix_rule!='empty'",
            "val": "_",
            "width": "50px"
          },
          "output.generate_core_rule": {
            "type": "select",
            "Etiqueta": "Nombre de la Tabla",
            "show_if": "output.compose_table_name=='true' && options.is_generate_holiday_table=='generate_table'",
            "val": "script_name",
            "options": {
              "script_name": {
                "Etiqueta": "Nombre de la Tarea"
              },
              "custom": {
                "Etiqueta": "Personalizado"
              }
            }
          },
          "output.core_rule": {
            "type": "select",
            "Etiqueta": "Nombre de la Tabla",
            "show_if": "output.compose_table_name=='true' && options.is_generate_holiday_table=='add_column'",
            "val": "input_table_name",
            "options": {
              "input_table_name": {
                "Etiqueta": "Ingresar Nombre de la Tabla"
              },
              "script_name": {
                "Etiqueta": "Nombre de la Tarea"
              },
              "custom": {
                "Etiqueta": "Personalizado"
              }
            }
          },
          "output.core_text": {
            "type": "text",
            "Etiqueta": "Nombre de la Tabla Principal",
            "show_if": "output.compose_table_name=='true' && output.core_rule=='custom'",
            "required": true,
            "minLength": 1,
            "maxLength": 31,
            "val": ""
          },
          "output.suffix_delimiter": {
            "type": "text",
            "Etiqueta": "Delimitador",
            "show_if": "output.compose_table_name=='true'",
            "val": "_",
            "width": "50px"
          },
          "output.suffix_rule": {
            "type": "select",
            "Etiqueta": "Sufijo",
            "show_if": "output.compose_table_name=='true'",
            "val": "script_name",
            "options": {
              "empty": {
                "Etiqueta": "-"
              },
              "script_name": {
                "Etiqueta": "Nombre de la Tarea"
              },
              "date_string_type1": {
                "Etiqueta": "Fecha: dd_mm_aaaa"
              },
              "date_string_type2": {
                "Etiqueta": "Fecha: aaaa_mm_dd"
              },
              "time_string": {
                "Etiqueta": "Tempo: h_min_seg"
              },
              "datetime_string": {
                "Etiqueta": "Fecha_Hora"
              },
              "custom": {
                "Etiqueta": "Personalizado"
              }
            }
          },
          "output.suffix_text": {
            "type": "text",
            "Etiqueta": "Texto del Sufijo",
            "show_if": "output.compose_table_name=='true' && output.suffix_rule=='custom'",
            "minLength": 1,
            "maxLength": 31,
            "val": "",
            "breakLine": true
          },
          "output.timezone": {
            "type": "select",
            "Etiqueta": "Zona Horaria",
            "show_if": "output.compose_table_name=='true' && (output.prefix_rule=='date_string_type1' || output.prefix_rule=='date_string_type2' || output.prefix_rule=='time_string' || output.prefix_rule=='datetime_string' || output.suffix_rule=='date_string_type1' || output.suffix_rule=='date_string_type2' || output.suffix_rule=='time_string' || output.suffix_rule=='datetime_string')",
            "val": "Europa/Berlín",
            "required": true,
            "options": {
              "America/Adak": {
                "Etiqueta": "Adak, Gambier"
              },
              "Australia/Adelaide": {
                "Etiqueta": "Adelaide, Broken_Hill, Darwin"
              },
              "America/Anchorage": {
                "Etiqueta": "Anchorage, Juneau, Sitka"
              },
              "Pacific/Apia": {
                "Etiqueta": "Apia, Asau, Mulifanua, Afega, Leulumoega"
              },
              "Pacific/Enderbury": {
                "Etiqueta": "Apia, Fiji, Tongatapu"
              },
