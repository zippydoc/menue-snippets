              },
              "Europe/Guernsey": {
                "Etiqueta": "London. Dublin, Lisbon"
              },
              "Australia/Lord_Howe": {
                "Etiqueta": "Lord_Howe"
              },
              "America/Creston": {
                "Etiqueta": "Los_Angeles, Vancouver, Hermosillo"
              },
              "Pacific/Marquesas": {
                "Etiqueta": "Marquesas,Taiohae"
              },
              "America/Cayman": {
                "Etiqueta": "Mexico_City, Panama, Chicago"
              },
              "Pacific/Niue": {
                "Etiqueta": "Midway, Niue, Pago_Pago"
              },
              "Europe/Mariehamn": {
                "Etiqueta": "Moscú, Estambul, Qatar"
              },
              "Asia/Kolkata": {
