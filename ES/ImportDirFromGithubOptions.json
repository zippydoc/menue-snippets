{
  "options": {
    "type": "fieldset",
    "label": "Seleccionar Opciones",
    "repeat": false,
    "fields": {
      "options.from": {
        "type": "hidden",
        "val": "web"
      },
      "options.webservice": {
        "type": "hidden",
        "val": "github"
      },
      "options.output_format": {
        "type": "select",
        "label": "Seleccionar Tipo",
        "options": {
          "csv_table": {
            "label": "Tabla CSV"
          },
          "line_by_line": {
            "label": "Línea por Línea"
          },
          "word_by_word": {
            "label": "Texto"
          },
          "file_list": {
            "label": "Lista de Archivos"
          }
        },
        "val": "csv_table",
        "required": true
      },
      "options.word_pattern_type": {
        "type": "select",
        "label": "Patrón de Palabras",
        "show_if": "options.output_format=='word_by_word' && options.specify_word_pattern=='true'",
        "options": {
          "regex": {
            "label": "Expresión Regular"
          }
        },
        "val": "regex",
        "required": true
      },
      "options.word_pattern_regex": {
        "type": "text",
        "label": "Expresión Regular",
        "show_if": "options.output_format=='word_by_word' && options.specify_word_pattern=='true' && options.word_pattern_type=='regex'",
        "required": true,
        "val": "[a-z][1-9]",
        "breakLine": true
      },
      "options.column_setting": {
        "type": "checkbox",
        "label": "Configuración de la columna",
        "show_if": "options.output_format!='file_list'",
        "val": "false",
        "isOn": "true",
        "isOff": "false",
        "breakLine": true
      },
      "column_setting_frame": {
        "type": "fieldset",
        "label": "Configuración de la columna",
        "show_if": "options.output_format!='file_list' && options.column_setting=='true'",
        "repeat": false,
        "fields": {
          "options.add_row_column": {
            "type": "checkbox",
            "label": "Agregar Columna Fila",
            "show_if": "options.output_format=='word_by_word'",
            "val": "true",
            "isOn": "true",
            "isOff": "false",
            "breakLine": true
          },
          "options.add_word_count_column": {
            "type": "checkbox",
            "label": "Añadir columna de 'Recuento de palabras'",
            "show_if": "options.output_format=='word_by_word'",
            "val": "false",
            "isOn": "true",
            "isOff": "false",
            "breakLine": true
          },
          "options.add_word_nonword_column": {
            "type": "checkbox",
            "label": "Añadir columna de 'palabras y no palabras'",
            "show_if": "options.output_format=='word_by_word'",
            "val": "false",
            "isOn": "true",
            "isOff": "false",
            "breakLine": true
          },
          "options.add_section_column": {
            "type": "checkbox",
            "label": "Agregar Columna Sección",
            "show_if": "options.output_format=='word_by_word'",
            "val": "false",
            "isOn": "true",
            "isOff": "false",
            "breakLine": true
          },
          "section_column_types_frame": {
            "type": "fieldset",
            "label": "Tipos de columnas de sección",
            "show_if": "options.output_format=='word_by_word' && options.add_section_column=='true'",
            "repeat": false,
            "fields": {
              "options.section_columns.add_paragraph_section_column": {
                "type": "checkbox",
                "label": "Párrafo",
                "val": "false",
                "isOn": "true",
                "isOff": "false",
                "breakLine": true
              },
              "options.section_columns.add_custom_section_column": {
                "type": "checkbox",
                "label": "Delimitador de Sección Personalizado",
                "val": "false",
                "isOn": "true",
                "isOff": "false"
              },
              "options.section_columns.custom_delimiter": {
                "type": "text",
                "label": "Delimitador(1 carácter)",
                "show_if": "options.section_columns.add_custom_section_column=='true'",
                "maxLength": 1,
                "required": true,
                "breakLine": true
              },
              "options.section_columns.add_brackets_punctuation_section_column": {
                "type": "checkbox",
                "label": "Corchetes y Puntuación",
                "val": "false",
                "isOn": "true",
                "isOff": "false"
              },
              "options.section_columns.brackets_punctuation_section_column_type": {
                "type": "select",
                "label": "Tipo de paréntesis",
                "show_if": "options.section_columns.add_brackets_punctuation_section_column=='true'",
                "options": {
                  "nesting_structure": {
                    "label": "Estructura de anidamiento"
                  },
                  "inner_bracket": {
                    "label": "Corchete interior"
                  },
                  "brackets_only": {
                    "label": "Brackets Only"
                  },
                  "show_all": {
                    "label": "Mostrar Todo"
                  }
                },
                "val": "show_all",
                "required": true,
                "breakLine": true
              }
            }
          },
          "options.add_filename_column": {
            "type": "checkbox",
            "label": "Agregar Columna por Nombre de Archivo",
            "val": "true",
            "isOn": "true",
            "isOff": "false",
            "style": "margin-top: 10px",
            "breakLine": true
          },
          "options.add_filepath_column": {
            "type": "checkbox",
            "label": "Añadir columna para la ruta del archivo",
            "val": "false",
            "isOn": "true",
            "isOff": "false",
            "breakLine": true
          }
        }
      },
      "options.is_cloud_dir": {
        "type": "hidden",
        "val": "true"
      },
      "if_dir_frame": {
        "type": "fieldset",
        "label": "Opciones de importación de directorios",
        "show_if": "options.output_format!='file_list' && options.is_cloud_dir=='true'",
        "repeat": false,
        "fields": {
          "options.unify_in_one_table": {
            "type": "checkbox",
            "label": "Unificar en una Tabla",
            "val": "true",
            "isOn": "true",
            "isOff": "false",
            "breakLine": true
          },
          "options.suffix": {
            "type": "select",
            "label": "Sufijo de las tablas de salida",
            "show_if": "options.unify_in_one_table=='false'",
            "options": {
              "number": {
                "label": "Número"
              },
              "filename": {
                "label": "Nombre de Archivo"
              }
            },
            "val": "number",
            "required": true,
            "breakLine": true
          },
          "options.include_subdir": {
            "type": "checkbox",
            "label": "Incluir Subdirectorios",
            "show_if": "options.output_format!='file_list' && options.is_cloud_dir=='true'",
            "val": "false",
            "isOn": "true",
            "isOff": "false",
            "breakLine": true,
            "style": "margin-top: 10px"
          }
        }
      },
      "options.file_list.include_subdir": {
        "type": "checkbox",
        "label": "Incluir Subdirectorios",
        "show_if": "options.output_format=='file_list'",
        "val": "true",
        "isOn": "true",
        "isOff": "false",
        "breakLine": true,
        "style": "margin-top: 10px"
      },
      "options.file_list.add_change_date_column": {
        "type": "checkbox",
        "label": "Agregar Cambiar Columna de Fecha",
        "show_if": "options.output_format=='file_list'",
        "val": "false",
        "isOn": "true",
        "isOff": "false",
        "breakLine": true
      }
    }
  },
  "import_link_token_frame": {
    "type": "fieldset",
    "label": "Enlace de importación",
    "repeat": false,
    "fields": {
      "options.link_csv_file": {
        "type": "text",
        "label": "Ingresar enlace, por ejemplo, https://github.com/zippydoc/test/blob/main/example.csv",
        "show_if": "options.webservice=='github' && options.is_cloud_dir=='false' && options.output_format=='csv_table'",
        "required": true,
        "width": "100%",
        "breakLine": true
      },
      "options.link_text_file": {
        "type": "text",
        "label": "Introducir enlace, por ejemplo, https://github.com/zippydoc/test/blob/main/example.txt(json, csv, etc)",
        "show_if": "options.webservice=='github' && options.is_cloud_dir=='false' && options.output_format!='csv_table' && options.output_format!='file_list'",
        "required": true,
        "width": "100%",
        "breakLine": true
      },
      "options.one_drive.file_path": {
        "type": "text",
        "label": "Introduzca la ruta del archivo, por ejemplo, ZippyDoc/Subdir1/test.txt",
        "show_if": "options.webservice=='ms_one_drive' && options.output_format!='file_list' && options.is_cloud_dir=='false'",
        "required": true,
        "width": "100%",
        "breakLine": true
      },
      "options.dropbox.file_path": {
        "type": "text",
        "label": "Introduzca la ruta del archivo, por ejemplo, ZippyDoc/Subdir1/test.txt",
        "show_if": "options.webservice=='dropbox' && options.output_format!='file_list' && options.is_cloud_dir=='false'",
        "required": true,
        "width": "100%",
        "breakLine": true
      },
      "options.dir_path_filter": {
        "type": "fieldset",
        "label": "Ruta de importación y filtro de archivos",
        "show_if": "options.output_format=='file_list' || options.is_cloud_dir=='true'",
        "repeat": true,
        "fields": {
          "github__link_directory": {
            "type": "text",
            "label": "Ingresar enlace, por ejemplo, https://github.com/zippydoc/test",
            "show_if": "options.webservice=='github'",
            "required": true,
            "width": "100%",
            "breakLine": true
          },
          "one_drive__dir_path": {
            "type": "text",
            "label": "Introduzca la ruta al directorio, por ejemplo, ZippyDoc/Subdir1",
            "show_if": "options.webservice=='ms_one_drive'",
            "width": "100%",
            "breakLine": true
          },
          "dropbox__dir_path": {
            "type": "text",
            "label": "Introduzca la ruta al directorio, por ejemplo, ZippyDoc/Subdir1",
            "show_if": "options.webservice=='dropbox'",
            "width": "100%",
            "breakLine": true
          },
          "google_drive__dir_path": {
            "type": "text",
            "label": "Introducir ruta de acceso al directorio",
            "show_if": "options.webservice=='google_drive'",
            "width": "100%",
            "breakLine": true
          },
          "dir_filter__filter_type": {
            "type": "select",
            "label": "Tipo de Filtro",
            "options": {
              "all": {
                "label": "Todo"
              },
              "contains": {
                "label": "Contiene"
              },
              "starts_with": {
                "label": "Empezar Con"
              },
              "ends_with": {
                "label": "Termina Con"
              },
              "regex": {
                "label": "Expresión Regular"
              }
            },
            "val": "all",
            "required": true
          },
          "dir_filter__filter_string": {
            "type": "text",
            "label": "Filtro",
            "show_if": "this.dir_filter__filter_type!='all'",
            "required": true
          },
          "delete": {
            "type": "button",
            "label": "Borrar",
            "function": "DELETE_ROW"
          }
        }
      },
      "options.is_github_private": {
        "type": "checkbox",
        "label": "El repositorio de Github es privado",
        "show_if": "options.webservice=='github'",
        "val": "false",
        "isOn": "true",
        "isOff": "false",
        "style": "margin-top: 10px",
        "breakLine": true
      },
      "options.github_token": {
        "type": "text",
        "label": "Token de acceso personal a Github",
        "show_if": "options.webservice=='github' && options.is_github_private=='true'",
        "required": true,
        "width": "100%",
        "breakLine": true
      },
      "options.one_drive.access_token": {
        "type": "text",
        "label": "Token de acceso a One Drive",
        "show_if": "options.webservice=='ms_one_drive'",
        "required": true,
        "width": "100%",
        "breakLine": true
      },
      "options.dropbox.auth_code": {
        "type": "text",
        "label": "Código de autorización de Dropbox",
        "show_if": "options.webservice=='dropbox'",
        "required": true,
        "width": "100%",
        "breakLine": true
      },
      "options.google_drive.access_token": {
        "type": "text",
        "label": "Token de acceso a Google Drive",
        "show_if": "options.webservice=='google_drive'",
        "required": true,
        "width": "100%",
        "breakLine": true
      }
    }
  }
}