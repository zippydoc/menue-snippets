{
  "output.output_table_types": {
    "type": "select",
    "label": “Tipo(s) de Tabla(s) de Salida”,
    "required": true,
    "options": {
      "inner": {
        "label": “Unión Interna de Tablas”
      },
      "left": {
        "label": “Unión por Izquierda de las Tablas”
      },
      "right": {
        "label": “Unión por Derecha de las Tablas”
      }
    },
    "multiple": true,
    "val": ["inner"]
  },
  "output.standard_ending": {
    "type": "checkbox",
    "label": "Termina con Estándar",
    "val": "true",
    "isOn": "true",
    "isOff": "false",
    "breakLine": true
  },
  "output.inner_ending": {
    "show_if": "output.standard_ending == 'false'",
    "type": "text",
    "label": "Termina con interna",
    "val": "_j",
    "style": "margin-left: 10px"
  },
  "output.left_ending": {
    "show_if": "output.standard_ending == 'false'",
    "type": "text",
    "label": "Termina con izquierda",
    "val": "_l",
    "style": "margin-left: 10px"
  },
  "output.right_ending": {
    "show_if": "output.standard_ending == 'false'",
    "type": "text",
    "label": "Termina con derecha",
    "val": "_r",
    "style": "margin-left: 10px",
    "breakLine": true
  }
}