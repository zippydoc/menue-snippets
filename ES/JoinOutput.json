{
  "output.standard_ending": {
    "type": "checkbox", 
    "label": "Finale standard", 
    "val": "true", 
    "isOn": "true", 
    "isOff": "false", 
    "breakLine": true
  }, 
  "output.inner_ending": {
    "show_if": "output.standard_ending == 'false'", 
    "type": "text", 
    "required": true, 
    "label": "Final interno", 
    "val": "_j", 
    "style": "margin-left: 10px"
  }, 
  "output.left_ending": {
    "show_if": "output.standard_ending == 'false'", 
    "type": "text", 
    "required": true, 
    "label": "Final de la izquierda", 
    "val": "_l", 
    "style": "margin-left: 10px"
  }, 
  "output.right_ending": {
    "show_if": "output.standard_ending == 'false'", 
    "type": "text", 
    "required": true, 
    "label": "Fine giusta", 
    "val": "_r", 
    "style": "margin-left: 10px", 
    "breakLine": true
  }
}
