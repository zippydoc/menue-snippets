{
  "options": {
    "type": "fieldset",
    "Etiqueta": "Agregar Etiqueta",
    "repeat": false,
    "fields": {
      "add_column": {
        "type": "text",
        "Etiqueta": "Nombre de columna para la etiqueta",
        "val": "Etiqueta",
        "required": true,
        "width": "100%",
        "breakLine": true
      },
      "add_label": {
        "type": "text",
        "Etiqueta": "Agregar etiqueta",
        "val": "label1",
        "required": true
      },
      "use_custom_delimiter": {
        "type": "checkbox",
        "Etiqueta": "Delimitador Personalizado",
        "val": "false",
        "isOn": "true",
        "isOff": "false"
      },
      "custom_delimiter": {
        "type": "text",
        "Etiqueta": "Delimitador Personalizado",
        "show_if": "use_custom_delimiter=='true'",
        "val": ";",
        "required": true
      },
      "do_not_duplicate": {
        "type": "checkbox",
        "Etiqueta": "No duplicar",
        "val": "true",
        "isOn": "true",
        "isOff": "false",
        "breakLine": true
      },
      "filter_box": {
        "type": "fieldset",
        "Etiqueta": "Opciones del Filtro",
        "repeat": false,
        "fields": {
          "filter": {
            "type": "fieldset",
            "Etiqueta": "Seleccionar Condiciones para el Etiquetado",
            "repeat": true,
            "fields": {
              "initial_op": {
                "type": "select",
                "Etiqueta": "Seleccionar Operador Lógico",
                "required": true,
                "show_if" :  "$index == 0",
                "options": {
                  "if": {
                    "Etiqueta": "SI"
                  },
                  "if_not": {
                    "Etiqueta": "SI NO"
                  }
                },
                "val": "if"
              },
              "op": {
                "type": "select",
                "Etiqueta": "Seleccionar Operador Lógico",
                "show_if" :  "$index > 0",
                "options": {
                  "and": {
                    "Etiqueta": "AND"
                  },
                  "or": {
                    "Etiqueta": "OR"
                  },
                  "and_not": {
                    "Etiqueta": "AND NOT"
                  },
                  "or_not": {
                    "Etiqueta": "OR NOT"
                  }
                },
                "val": "and"
              },
              "column": {
                "type": "select",
                "Etiqueta": "Seleccionar Columna",
                "table": "input_table",
                "bind": "COLUMNS",
                "required": true
              },
              "condition": {
                "type": "select",
                "Etiqueta": "Seleccionar Operador",
                "required": true,
                "options": {
                  "CONTIENE": {
                    "Etiqueta": "CONTIENE"
                  },
                  "CONTAINED IN STRING": {
                    "Etiqueta": "CONTENIDO EN"
                  },
                  "EMPEZAR CON": {
                    "Etiqueta": "EMPEZAR CON"
                  },
                  "TERMINA CON": {
                    "Etiqueta": "TERMINA CON"
                  },
                  "=": {
                    "Etiqueta": "IGUAL"
                  },
                  ">": {
                    "Etiqueta": "MAYOR QUE"
                  },
                  ">=": {
                    "Etiqueta": "MAYOR O IGUAL"
                  },
                  "<": {
                    "Etiqueta": "MENOR QUE"
                  },
                  "<=": {
                    "Etiqueta": "MENOR O IGUAL"
                  },
                  "EMPTY": {
                    "Etiqueta": "ESTÁ VACÍO"
                  },
                  "BLANK": {
                    "Etiqueta": "ESTÁ EN BLANCO"
                  },
                  "IS NULL": {
                    "Etiqueta": "IS NULL"
                  },
                  "COINCIDENCIA CON LA EXPRESIÓN REGULAR": {
                    "Etiqueta": "COINCIDENCIA CON LA EXPRESIÓN REGULAR"
                  },
                  "CONTAINED IN TABLE": {
                    "Etiqueta": "EN TABLA"
                  },
                  "EN LA TABLA DE LAS EXPRESIONES REGULARES": {
                    "Etiqueta": "EN LA TABLA DE LAS EXPRESIONES REGULARES"
                  },
                  "FECHA": {
                    "Etiqueta": "FECHA"
                  }
                },
                "val": "CONTIENE"
              },
              "date_condition": {
                "type": "select",
                "Etiqueta": "Seleccionar Operador de la Fecha",
                "required": true,
                "show_if": "this.condition == 'DATE'",
                "options": {
                  "<": {
                    "Etiqueta": "PREVIO"
                  },
                  "<=": {
                    "Etiqueta": "PREVIO O IGUAL"
                  },
                  "=": {
                    "Etiqueta": "IGUAL"
                  },
                  ">": {
                    "Etiqueta": "DESPUÉS"
                  },
                  ">=": {
                    "Etiqueta": "DESPUÉS O IGUAL"
                  }
                },
                "val": "="
              },
              "case_sensitive": {
                "type": "select",
                "Etiqueta": "Seleccionar Sensible a Mayúsculas y Minúsculas",
                "required": true,
                "show_if": "this.condition == 'CONTAINS' || this.condition == 'CONTAINED IN STRING' || this.condition == '=' || this.condition == 'STARTS WITH' || this.condition == 'ENDS WITH' || this.condition == 'REGEX MATCH' || this.condition == 'NOT REGEX MATCH' || this.condition == 'IN REGEX TABLE' || this.condition == 'NOT IN REGEX TABLE'",
                "options": {
                  "yes": {
                    "Etiqueta": "Sensible a Mayúsculas y Minúsculas"
                  },
                  "no": {
                    "Etiqueta": "Insensible a Mayúsculas y Minúsculas"
                  }
                },
                "val": "yes"
              },
              "date_value": {
                "type": "date",
                "Etiqueta": "Seleccionar Fecha",
                "show_if": "this.condition=='DATE'",
                "val": "",
                "required": true
              },
              "value": {
                "type": "text",
                "Etiqueta": "Ingresar Valor",
                "show_if": "this.condition != 'DATE' && this.condition != 'EMPTY' && this.condition != 'NOT EMPTY' && this.condition != 'BLANK' && this.condition != 'NOT BLANK' && this.condition != 'IS NULL' && this.condition != 'IS NOT NULL' && this.condition != 'CONTAINED IN TABLE' && this.condition != 'NOT CONTAINED IN TABLE' && this.condition != 'IN REGEX TABLE' && this.condition != 'NOT IN REGEX TABLE'",
                "val": "",
                "required": true
              },
              "compare_table": {
                "type": "select",
                "Etiqueta": "Seleccionar Tabla",
                "bind": "TABLES",
                "empty": "",
                "show_if": "this.condition == 'CONTAINED IN TABLE' || this.condition == 'NOT CONTAINED IN TABLE' || this.condition == 'IN REGEX TABLE' || this.condition == 'NOT IN REGEX TABLE'",
                "required": true
              },
              "compare_column": {
                "type": "select",
                "Etiqueta": "Seleccionar Columna",
                "table": "this.compare_table",
                "bind": "COLUMNS",
                "show_if": "this.condition == 'CONTAINED IN TABLE' || this.condition == 'NOT CONTAINED IN TABLE' || this.condition == 'IN REGEX TABLE' || this.condition == 'NOT IN REGEX TABLE'",
                "required": true
              },
              "delete": {
                "type": "button",
                "Etiqueta": "Borrar",
                "function": "DELETE_ROW"
              }
            }
          },
          "apply_to_record": {
            "type": "checkbox",
            "Etiqueta": "Aplicar al Registro",
            "val": "true",
            "isOn": "true",
            "isOff": "false",
            "style": "margin-top: 10px",
            "breakLine": true
          },
          "apply_to_group_box": {
            "type": "fieldset",
            "Etiqueta": "Aplicar al Grupo",
            "show_if": "apply_to_record=='false'",
            "repeat": false,
            "fields": {
              "apply_to_group_columns": {
                "type": "select",
                "Etiqueta": "Seleccionar Columnas del Grupo",
                "required": true,
                "width": "100%",
                "table": "input_table",
                "multiple": true,
                "bind": "COLUMNS",
                "breakLine": true
              }
            }
          }
        }
      }
    }
  }
}