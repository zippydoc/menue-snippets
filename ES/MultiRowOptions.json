
    "type": "fieldset", {
  "options_box": {
    "label": "Opciones", 
    "repeat": false, 
    "fields": {
      "options.task": {
        "type": "hidden", 
        "val": "multi_row"
      }, 
      "options.add_columns": {
        "type": "fieldset", 
        "label": "Agregar Nuevas Columnas", 
        "repeat": true, 
        "fields": {
          "new_column_name": {
            "type": "text", 
            "label": “Nuevo Nombre de Columna”, 
            "val": "", 
            "breakLine": true
          }, 
          "multi_row_functions__function": {
            "type": "select", 
            "label": "Función", 
            "required": true, 
            "options": {
              "trailing_row_n_average": {
                "label": "Promedio Móvil de la Fila N"
              }, 
              "index_from_row_n": {
                "label": "Índice de la Fila N"
              }, 
              "absolute_growth": {
                "label": "Crecimiento Absoluto"
              }, 
              "relative_growth": {
                "label": "Crecimiento Relativo"
              }, 
              "compound_growth": {
                "label": "Crecimiento Compuesto"
              }
            }, 
            "val": "trailing_row_n_average"
          }, 
          "multi_row_functions__sel_column": {
            "type": "select", 
            "label": "Columna", 
            "show_if": "this.multi_row_functions__function=='trailing_row_n_average' || this.multi_row_functions__function=='index_from_row_n' || this.multi_row_functions__function=='absolute_growth' || this.multi_row_functions__function=='relative_growth' || this.multi_row_functions__function=='compound_growth'", 
            "table": "input_table", 
            "bind": "COLUMNS", 
            "required": true
          }, 
          "multi_row_functions__n_value": {
            "type": "text", 
            "label": "N (entero positivo)", 
            "show_if": "this.multi_row_functions__function=='trailing_row_n_average' || this.multi_row_functions__function=='index_from_row_n' || this.multi_row_functions__function=='compound_growth'", 
            "val": "1", 
            "required": true, 
            "breakLine": true
          }, 
          "format__category": {
            "type": "select", 
            "label": "Formato", 
            "required": true, 
            "options": {
              "standard": {
                "label": "Estándar"
              }, 
              "number": {
                "label": "Número"
              }, 
              "percentage": {
                "label": "Porcentaje"
              }, 
              "text": {
                "label": "Texto"
              }, 
              "date": {
                "label": "Fecha"
              }, 
              "currency": {
                "label": "Moneda"
              }
            }, 
            "val": "standard"
          }, 
          "format__rounding": {
            "type": "select", 
            "label": "Redondeo", 
            "show_if": "this.format__category=='number' || this.format__category=='percentage' || this.format__category=='currency'", 
            "required": true, 
            "options": {
              "no_rounding": {
                "label": "Redondeo de números"
              }, 
              "0": {
                "label": "0 dígitos"
              }, 
              "1": {
                "label": "1 dígitos"
              }, 
              "2": {
                "label": "2 dígitos"
              }, 
              "3": {
                "label": "3 dígitos"
              }, 
              "4": {
                "label": "4 dígitos"
              }, 
              "5": {
                "label": "5 dígitos"
              }, 
              "6": {
                "label": "6 dígitos"
              }, 
              "7": {
                "label": "7 dígitos"
              }, 
              "8": {
                "label": "8 dígitos"
              }, 
              "9": {
                "label": "9 dígitos"
              }
            }, 
            "val": "2"
          }, 
          "format__use_1000_separator": {
            "type": "checkbox", 
            "label": "Usar el Separador de 1000(.)", 
            "show_if": "this.format__category=='number' || this.format__category=='percentage' || this.format__category=='currency'", 
            "val": "true", 
            "isOn": "true", 
            "isOff": "false"
          }, 
          "format__currency_symbol": {
            "type": "select", 
            "label": "Símbolo", 
            "show_if": "this.format__category=='currency'", 
            "required": true, 
            "options": {
              "dollar": {
                "label": "$"
              }, 
              "euro": {
                "label": "€"
              }, 
              "pound": {
                "label": "£"
              }
            }, 
            "val": "dollar"
          }, 
          "format__date_format": {
            "type": "select", 
            "label": "Tipo", 
            "show_if": "this.format__category=='date'", 
            "required": true, 
            "options": {
              "mm/dd/yyyy": {
                "label": "3/14/2012"
              }, 
              "D,  M dd,  Y": {
                "label": "Miércoles 14 de marzo de 2012"
              }, 
              "yyyy-mm-dd": {
                "label": "2012-03-14"
              }
            }, 
            "val": "mm/dd/yyyy", 
            "breakLine": true
          }, 
          "apply_to_group__enabled": {
            "type": "checkbox", 
            "label": "Aplicar al Grupo", 
            "show_if": "this.task_type!='static_value'", 
            "val": "false", 
            "isOn": "true", 
            "isOff": "false"
          }, 
          "apply_to_group__group_columns": {
            "type": "select", 
            "label": "Columna(s) del Grupo", 
            "show_if": "this.apply_to_group__enabled == 'true' && this.task_type!='static_value'", 
            "table": "input_table", 
            "bind": "COLUMNS", 
            "multiple": true, 
            "required": true
          }, 
          "delete": {
            "type": "button", 
            "label": "Borrar", 
            "function": "DELETE_ROW"
          }
        }
      }, 
      "options.use_filter": {
        "type": "checkbox", 
        "label": "Usar Filtro", 
        "val": "false", 
        "isOn": "true", 
        "isOff": "false", 
        "style": "margin-top: 10px", 
        "breakLine": true
      }, 
      "filter_box": {
        "type": "fieldset", 
        "label": "Opciones del Filtro", 
        "show_if": "options.use_filter=='true'", 
        "repeat": false, 
        "fields": {
          "options.filter": {
            "type": "fieldset", 
            "label": "Agregar Condiciones del Filtro", 
            "repeat": true, 
            "fields": {
              "initial_op": {
                "type": "select", 
                "label": "Seleccionar Operador Lógico", 
                "required": true, 
                "show_if" :  "$index == 0", 
                "options": {
                  "if": {
                    "label": "SI"
                  }, 
                  "if_not": {
                    "label": "SI NO"
                  }
                }, 
                "val": "if"
              }, 
              "op": {
                "type": "select", 
                "label": "Seleccionar Operador Lógico", 
                "required": true, 
                "show_if" :  "$index > 0", 
                "options": {
                  "and": {
                    "label": "AND"
                  }, 
                  "or": {
                    "label": "OR"
                  }, 
                  "and_not": {
                    "label": "AND NOT"
                  }, 
                  "or_not": {
                    "label": "OR NOT"
                  }
                }, 
                "val": "and"
              }, 
              "column": {
                "type": "select", 
                "label": “Seleccionar Columna”, 
                "table": "input_table", 
                "bind": "COLUMNS", 
                "required": true
              }, 
              "condition": {
                "type": "select", 
                "label": "Seleccionar Operador", 
                "required": true, 
                "options": {
                  "CONTIENE": {
                    "label": "CONTIENE"
                  }, 
                  "CONTAINED IN STRING": {
                    "label": "CONTENIDO EN"
                  }, 
                  “EMPEZAR CON”: {
                    "label": “EMPEZAR CON”
                  }, 
                  "TERMINA CON”: {
                    "label": "TERMINA CON”
                  }, 
                  "=": {
                    "label": "IGUAL"
                  }, 
                  ">": {
                    "label": "MAYOR QUE"
                  }, 
                  ">=": {
                    "label": "MAYOR O IGUAL"
                  }, 
                  "<": {
                    "label": "MENOR QUE"
                  }, 
                  "<=": {
                    "label": "MENOR O IGUAL"
                  }, 
                  "EMPTY": {
                    "label": "ESTÁ VACÍO"
                  }, 
                  "BLANK": {
                    "label": "ESTÁ EN BLANCO"
                  }, 
                  "IS NULL": {
                    "label": "IS NULL"
                  }, 
                  "COINCIDENCIA CON LA EXPRESIÓN REGULAR": {
                    "label": "COINCIDENCIA CON LA EXPRESIÓN REGULAR"
                  }, 
                  "CONTAINED IN TABLE": {
                    "label": "EN TABLA"
                  }, 
                  "EN LA TABLA DE LAS EXPRESIONES REGULARES": {
                    "label": "EN LA TABLA DE LAS EXPRESIONES REGULARES"
                  }, 
                  "FECHA": {
                    "label": "FECHA"
                  }
                }, 
                "val": "CONTIENE"
              }, 
              "date_condition": {
                "type": "select", 
                "label": "Seleccionar Operador de la Fecha", 
                "required": true, 
                "show_if": "this.condition == 'DATE'", 
                "options": {
                  "<": {
                    "label": "PREVIO"
                  }, 
                  "<=": {
                    "label": "PREVIO O IGUAL"
                  }, 
                  "=": {
                    "label": "IGUAL"
                  }, 
                  ">": {
                    "label": "DESPUÉS"
                  }, 
                  ">=": {
                    "label": "DESPUÉS O IGUAL"
                  }
                }, 
                "val": "="
              }, 
              "case_sensitive": {
                "type": "select", 
                "label": "Seleccionar Sensible a Mayúsculas y Minúsculas", 
                "required": true, 
                "show_if": "this.condition == 'CONTAINS' || this.condition == 'CONTAINED IN STRING' || this.condition == '=' || this.condition == 'STARTS WITH' || this.condition == 'ENDS WITH' || this.condition == 'REGEX MATCH' || this.condition == 'NOT REGEX MATCH' || this.condition == 'IN REGEX TABLE' || this.condition == 'NOT IN REGEX TABLE'", 
                "options": {
                  "yes": {
                    "label": "Sensible a Mayúsculas y Minúsculas"
                  }, 
                  "no": {
                    "label": "Insensible a Mayúsculas y Minúsculas"
                  }
                }, 
                "val": "yes"
              }, 
              "date_value": {
                "type": "date", 
                "label": "Seleccionar Fecha", 
                "show_if": "this.condition=='DATE'", 
                "val": "1/31/2020", 
                "required": true
              }, 
              "value": {
                "type": "text", 
                "label": "Ingresar Valor", 
                "show_if": "this.condition != 'DATE' && this.condition != 'EMPTY' && this.condition != 'NOT EMPTY' && this.condition != 'BLANK' && this.condition != 'NOT BLANK' && this.condition != 'IS NULL' && this.condition != 'IS NOT NULL' && this.condition != 'CONTAINED IN TABLE' && this.condition != 'NOT CONTAINED IN TABLE' && this.condition != 'IN REGEX TABLE' && this.condition != 'NOT IN REGEX TABLE'", 
                "val": "", 
                "required": true
              }, 
              "compare_table": {
                "type": "select", 
                "label": "Seleccionar Tabla", 
                "bind": "TABLES", 
                "empty": "", 
                "show_if": "this.condition == 'CONTAINED IN TABLE' || this.condition == 'NOT CONTAINED IN TABLE' || this.condition == 'IN REGEX TABLE' || this.condition == 'NOT IN REGEX TABLE'", 
                "required": true
              }, 
              "compare_column": {
                "type": "select", 
                "label": “Seleccionar Columna”, 
                "table": "this.compare_table", 
                "bind": "COLUMNS", 
                "show_if": "this.condition == 'CONTAINED IN TABLE' || this.condition == 'NOT CONTAINED IN TABLE' || this.condition == 'IN REGEX TABLE' || this.condition == 'NOT IN REGEX TABLE'", 
                "required": true
              }, 
              "delete": {
                "type": "button", 
                "label": "Borrar", 
                "function": "DELETE_ROW"
              }
            }
          }, 
          "options.apply_to_record": {
            "type": "checkbox", 
            "label": "Aplicar al Registro", 
            "val": "true", 
            "isOn": "true", 
            "isOff": "false", 
            "style": "margin-top: 10px", 
            "breakLine": true
          }, 
          "apply_to_group_box": {
            "type": "fieldset", 
            "label": "Aplicar al Grupo", 
            "show_if": "options.apply_to_record=='false'", 
            "repeat": false, 
            "fields": {
              "options.apply_to_group_columns": {
                "type": "select", 
                "label": "Seleccionar Columnas del Grupo", 
                "required": true, 
                "width": "100%", 
                "table": "input_table", 
                "multiple": true, 
                "bind": "COLUMNS", 
                "breakLine": true
              }
            }
          }
        }
      }
    }
  }
}