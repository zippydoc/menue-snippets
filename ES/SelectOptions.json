{
  "options": {
    "type": "fieldset", 
    "label": "Seleccionar Opciones", 
    "repeat": false, 
    "fields": {
      "type": {
        "type": "select", 
        "label": "Seleccionar Tipo", 
        "val": "first_n", 
        "options": {
          "first_n": {
            "label": "Primeros N"
          }, 
          "last_n": {
            "label": "Últimos N"
          }, 
          "custom_selection": {
            "label": "Selección Personalizada"
          }, 
          "skip_first_n": {
            "label": "Saltear Primeros N"
          }, 
          "skip_last_n": {
            "label": "Saltear Últimos N"
          }, 
          "first_n_percent": {
            "label": "Primeros N%"
          }, 
          "last_n_percent": {
            "label": "Últimos N%"
          }, 
          "random_n_records": {
            "label": "N Aleatorios"
          }, 
          "random_n_percent": {
            "label": "N% Aleatorio"
          }, 
          "random_choice_1_n": {
            "label": "Elección 1:N Aleatoria"
          }, 
          "every_nth_record": {
            "label": "Cada Enésimo Registro"
          }
        }, 
        "required": true, 
        "width": "100%", 
        "breakLine": true
      }, 
      "n": {
        "type": "text", 
        "label": "Ingresar N", 
        "show_if": "type!='custom_selection'", 
        "val": "1", 
        "required": true, 
        "width": "100%", 
        "breakLine": true
      }, 
      "ranges": {
        "type": "text", 
        "label": "Ingresar Intervalos", 
        "show_if": "type=='custom_selection'", 
        "val": "-2; 4-6; 9; 12+", 
        "required": true, 
        "width": "100%", 
        "breakLine": true
      }, 
      "set_offset": {
        "type": "checkbox", 
        "label": "Compensación", 
        "show_if": "type=='every_nth_record'", 
        "val": "false", 
        "isOn": "true", 
        "isOff": "false"
      }, 
      "offset": {
        "type": "text", 
        "label": "Compensación", 
        "show_if": "type=='every_nth_record' && set_offset=='true'", 
        "val": "1", 
        "required": true, 
        "breakLine": true
      }, 
      "group_by_column": {
        "type": "checkbox", 
        "label": "Agrupar por columna", 
        "val": "false", 
        "isOn": "true", 
        "isOff": "false", 
        "breakLine": true
      }, 
      "columns_to_group_box": {
        "type": "fieldset", 
        "label": "Agrupar por columna", 
        "show_if": "group_by_column=='true'", 
        "repeat": false, 
        "fields": {
          "columns_to_group": {
            "type": "select", 
            "label": "Seleccionar Columnas del Grupo", 
            "required": true, 
            "width": "100%", 
            "table": "input_table", 
            "multiple": true, 
            "bind": "COLUMNS", 
            "breakLine": true
          }
        }
      }
    }
  }
}