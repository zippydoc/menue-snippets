{
  "options": {
    "type": "fieldset",
    "Etiqueta": "Seleccionar Opciones",
    "repeat": false,
    "fields": {
      "type": {
        "type": "select",
        "Etiqueta": "Seleccionar Tipo",
        "val": "first_n",
        "options": {
          "first_n": {
            "Etiqueta": "Primeros N"
          },
          "last_n": {
            "Etiqueta": "Últimos N"
          },
          "custom_selection": {
            "Etiqueta": "Selección Personalizada"
          },
          "skip_first_n": {
            "Etiqueta": "Saltear Primeros N"
          },
          "skip_last_n": {
            "Etiqueta": "Saltear Últimos N"
          },
          "first_n_percent": {
            "Etiqueta": "Primeros N%"
          },
          "last_n_percent": {
            "Etiqueta": "Últimos N%"
          },
          "random_n_records": {
            "Etiqueta": "N Aleatorios"
          },
          "random_n_percent": {
            "Etiqueta": "N% Aleatorio"
          },
          "random_choice_1_n": {
            "Etiqueta": "Elección 1:N Aleatoria"
          },
          "every_nth_record": {
            "Etiqueta": "Cada Enésimo Registro"
          }
        },
        "required": true,
        "width": "100%",
        "breakLine": true
      },
      "n": {
        "type": "text",
        "Etiqueta": "Ingresar N",
        "show_if": "type!='custom_selection'",
        "val": "1",
        "required": true,
        "width": "100%",
        "breakLine": true
      },
      "ranges": {
        "type": "text",
        "Etiqueta": "Ingresar Intervalos",
        "show_if": "type=='custom_selection'",
        "val": "-2; 4-6; 9; 12+",
        "required": true,
        "width": "100%",
        "breakLine": true
      },
      "set_offset": {
        "type": "checkbox",
        "Etiqueta": "Compensación",
        "show_if": "type=='every_nth_record'",
        "val": "false",
        "isOn": "true",
        "isOff": "false"
      },
      "offset": {
        "type": "text",
        "Etiqueta": "Compensación",
        "show_if": "type=='every_nth_record' && set_offset=='true'",
        "val": "1",
        "required": true,
        "breakLine": true
      },
      "group_by_column": {
        "type": "checkbox",
        "Etiqueta": "Agrupar por columna",
        "val": "false",
        "isOn": "true",
        "isOff": "false",
        "breakLine": true
      },
      "columns_to_group_box": {
        "type": "fieldset",
        "Etiqueta": "Agrupar por columna",
        "show_if": "group_by_column=='true'",
        "repeat": false,
        "fields": {
          "columns_to_group": {
            "type": "select",
            "Etiqueta": "Seleccionar Columnas del Grupo",
            "required": true,
            "width": "100%",
            "table": "input_table",
            "multiple": true,
            "bind": "COLUMNS",
            "breakLine": true
          }
        }
      }
    }
  }
}