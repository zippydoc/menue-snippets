        "fields": {
          "options.input_table.filter": {
            "type": "fieldset",
            "Etiqueta": "Agregar Condiciones del Filtro para la Tabla de Entrada",
            "repeat": true,
            "fields": {
              "initial_op": {
                "type": "select",
