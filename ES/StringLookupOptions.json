                  "and": {
                    "Etiqueta": "AND"
                  },
                  "or": {
                    "Etiqueta": "OR"
                  },
                  "and_not": {
                    "Etiqueta": "AND NOT"
                  },
                  "or_not": {
                    "Etiqueta": "OR NOT"
                  }
                },
                "val": "and"
              },
              "column": {
                "type": "select",
                "Etiqueta": "Seleccionar Columna",
                "table": "input_table",
                "bind": "COLUMNS",
                "required": true
              },
              "condition": {
                "type": "select",
                "Etiqueta": "Seleccionar Operador",
