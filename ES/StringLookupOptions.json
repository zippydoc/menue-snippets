{
  "options": {
    "type": "fieldset",
    "Etiqueta": "Seleccionar Opciones",
    "repeat": false,
    "fields": {
      "options.task_type": {
        "type": "select",
        "Etiqueta": "Tipo de Tarea",
        "required": true,
        "options": {
          "generate_lookup_table": {
            "Etiqueta": "Generar Tabla de Búsqueda"
          },
          "apply_lookup_table": {
            "Etiqueta": "Aplicar Tabla de Búsqueda"
          },
          "apply_regex_table": {
            "Etiqueta": "Aplicar la Tabla de Expresiones Regulares"
          },
          "apply_lookup_table_to_cells": {
            "Etiqueta": "Aplicar Tabla de Búsqueda a las Celdas"
          },
          "apply_regex_table_to_cells": {
            "Etiqueta": "Aplicar la Tabla de Expresiones Regulares a las Celdas"
          }
        },
        "val": "generate_lookup_table"
      },
      "options.pattern_type": {
        "type": "hidden",
        "val": "duplicate_pattern"
      },
      "duplicate_option_box": {
        "type": "fieldset",
        "Etiqueta": "Opción para Duplicar Patrones",
        "show_if": "options.task_type=='label_duplicates' || (options.task_type=='generate_lookup_table' && options.pattern_type=='duplicate_pattern')",
        "repeat": false,
        "fields": {
          "options.minimum_number_of_matching_characters": {
            "type": "text",
            "Etiqueta": "Número Mínimo de Caracteres coincidentes",
            "val": "3",
            "required": true,
            "style": "min-width: 240px"
          },
          "options.minimum_number_of_duplicates": {
            "type": "text",
            "Etiqueta": "Número Mínimo de duplicados",
            "val": "2",
            "required": true,
            "style": "min-width: 240px",
            "breakLine": true
          }
        }
      },
      "number_range_option_box": {
        "type": "fieldset",
        "Etiqueta": "Opción de Intervalos Numéricos",
        "show_if": "options.task_type=='generate_lookup_table' && options.pattern_type=='number_range'",
        "repeat": false,
        "fields": {
          "options.number_range.range_type": {
            "type": "select",
            "Etiqueta": "Tipo de Intervalo",
            "required": true,
            "options": {
              "equal_interval": {
                "Etiqueta": "N Intervalos Iguales"
              },
              "equal_range": {
                "Etiqueta": "Intervalo Igual"
              },
              "custom_range": {
                "Etiqueta": "Intervalo Personalizado"
              }
            },
            "val": "equal_interval"
          },
          "options.number_range.n_value": {
            "type": "text",
            "Etiqueta": "N",
            "show_if": "options.number_range.range_type=='equal_interval'",
            "val": "3",
            "required": true
          },
          "options.number_range.range_value": {
            "type": "text",
            "Etiqueta": "Intervalo",
            "show_if": "options.number_range.range_type=='equal_range'",
            "val": "10",
            "required": true
          },
          "options.number_range.custom_ranges": {
            "type": "fieldset",
            "Etiqueta": "Agregar Patrón de Intervalo Personalizado",
            "show_if": "options.number_range.range_type=='custom_range'",
            "repeat": true,
            "fields": {
              "pattern": {
                "type": "text",
                "Etiqueta": "Patrón",
                "val": "0<=x<10",
                "required": true
              },
              "Etiqueta": {
                "type": "text",
                "Etiqueta": "Etiqueta",
                "val": "positive10",
                "required": true
              },
              "delete": {
                "type": "button",
                "Etiqueta": "Borrar",
                "function": "DELETE_ROW"
              }
            }
          }
        }
      },
      "options.is_custom_lookup_table": {
        "type": "checkbox",
        "Etiqueta": "Tabla de búsqueda personalizada",
        "show_if": "options.task_type=='apply_lookup_table' || options.task_type=='apply_regex_table' || options.task_type=='apply_lookup_table_to_cells' || options.task_type=='apply_regex_table_to_cells'",
        "val": "true",
        "isOn": "true",
        "isOff": "false",
        "breakLine": true
      },
      "lookup_table_frame": {
        "type": "fieldset",
        "Etiqueta": "",
        "show_if": "options.task_type=='apply_lookup_table' || options.task_type=='apply_regex_table' || options.task_type=='apply_lookup_table_to_cells' || options.task_type=='apply_regex_table_to_cells'",
        "repeat": false,
        "fields": {
          "options.lookup_table_id": {
            "type": "select",
            "Etiqueta": "Tabla de Búsqueda",
            "show_if": "options.is_custom_lookup_table=='false'",
            "bind": "TABLES",
            "empty": "",
            "required": true,
            "breakLine": true
          },
          "options.custom_lookup_table": {
            "type": "fieldset",
            "Etiqueta": "Tabla de búsqueda personalizada",
            "show_if": "options.is_custom_lookup_table=='true'",
            "repeat": true,
            "fields": {
              "lookup": {
                "type": "text",
                "Etiqueta": "Buscar",
                "val": "",
                "required": true
              },
              "Etiqueta": {
                "type": "text",
                "Etiqueta": "Etiqueta",
                "val": "",
                "required": true
              },
              "delete": {
                "type": "button",
                "Etiqueta": "Borrar",
                "function": "DELETE_ROW"
              }
            }
          },
          "options.partial_match": {
            "type": "checkbox",
            "Etiqueta": "Coincidencia Parcial",
            "show_if": "options.pattern_type=='duplicate_pattern' && (options.task_type=='apply_lookup_table' || options.task_type=='apply_lookup_table_to_cells')",
            "val": "false",
            "isOn": "true",
            "isOff": "false",
            "style": "margin-top: 10px",
            "breakLine": true
          },
          "options.case_sensitive": {
            "type": "checkbox",
            "Etiqueta": "Sensible a Mayúsculas y Minúsculas",
            "show_if": "options.pattern_type=='duplicate_pattern' && (options.task_type=='apply_lookup_table' || options.task_type=='apply_lookup_table_to_cells')",
            "val": "false",
            "isOn": "true",
            "isOff": "false",
            "breakLine": true
          },
          "options.use_delimiter": {
            "type": "checkbox",
            "Etiqueta": "Delimitador Personalizado",
            "show_if": "options.task_type=='apply_lookup_table' || options.task_type=='apply_lookup_table_to_cells'",
            "val": "false",
            "isOn": "true",
            "isOff": "false",
            "style": "margin-top: 10px"
          },
          "options.delimiter": {
            "type": "text",
