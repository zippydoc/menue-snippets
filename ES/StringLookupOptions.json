          "options.input_table.apply_to_record": {
            "type": "checkbox",
            "Etiqueta": "Aplicar al Registro para el filtro de la tabla de Entrada",
            "val": "true",
            "isOn": "true",
            "isOff": "false",
            "style": "margin-top: 10px",
            "breakLine": true
          },
          "input_table_apply_to_group_box": {
            "type": "fieldset",
            "Etiqueta": "Aplicar al Grupo de la tabla de Entrada",
            "show_if": "options.input_table.apply_to_record=='false'",
