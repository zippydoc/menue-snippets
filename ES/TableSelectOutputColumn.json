{
  "output.select_output_columns": {
    "type": "checkbox", 
    "label": "Seleccionar Columnas de Salida", 
    "show_if": "options.type=='transform_group'", 
    "val": "false", 
    "isOn": "true", 
    "isOff": "false"
  }, 
  "output_columns": {
    "type": "fieldset", 
    "label": "Select OutputColumns", 
    "show_if": "options.type=='transform_group' && output.select_output_columns=='true'", 
    "repeat": true, 
    "fields": {
      "field_name": {
        "type": "select", 
        "label": “Seleccionar Columna”, 
        "table": "input_table", 
        "bind": "COLUMNS", 
        "required": true
      }, 
      "is_rename": {
        "type": "checkbox", 
        "label": "Renombrar", 
        "val": "false", 
        "isOn": "true", 
        "isOff": "false"
      }, 
      "rename": {
        "show_if": "this.is_rename == 'true'", 
        "type": "text", 
        "required": true, 
        "minLength": 1, 
        "maxLength": 255, 
        "label": “Nuevo Nombre de Columna”, 
        "style": "margin-left: 10px"
      }, 
      "delete": {
        "type": "button", 
        "label": "Borrar", 
        "function": "DELETE_ROW"
      }
    }, 
    "breakLine": true, 
    "style": "margin-bottom: 20px"
  }
}