{
  "options": {
    "type": "fieldset",
    "Etiqueta": "Seleccionar Opciones",
    "repeat": false,
    "fields": {
      "options.type": {
        "type": "radio",
        "val": "transform_group",
        "show_if": "false",
        "values": {
          "transform_group": "Transform Group"
        },
        "breakLine": true
      },
      "options.group_label_direction": {
        "type": "select",
        "Etiqueta": "Dirección de Transformación",
        "required": true,
        "options": {
          "group_to_label": {
            "Etiqueta": "Grupo a Etiqueta"
          },
          "label_to_group": {
            "Etiqueta": "Etiqueta a Grupo"
          }
        },
        "val": "group_to_label"
      },
      "options.group_column": {
        "type": "select",
        "Etiqueta": "Columna del Grupo",
        "show_if": "options.group_label_direction=='group_to_label'",
        "table": "input_table",
        "bind": "COLUMNS",
        "required": true
      },
      "options.group_column_name": {
        "type": "text",
        "Etiqueta": "Nombre de Columna del Grupo",
        "show_if": "options.group_label_direction=='label_to_group'",
        "val": "Group",
        "required": true
      }
    }
  }
}