{
  "output.select_output_columns": {
    "type": "checkbox",
    "label": "Seleccionar columnas de salida",
    "show_if": "options.type=='transform_group' && output.custom_options=='true'",
    "val": "false",
    "isOn": "true",
    "isOff": "false"
  },
  "output_columns": {
    "type": "fieldset",
    "label": "Seleccionar columnas de salida",
    "show_if": "options.type=='transform_group' && output.custom_options=='true' && output.select_output_columns=='true'",
    "repeat": true,
    "fields": {
      "field_name": {
        "type": "select",
        "label": "Seleccionar Columna",
        "table": "table_id",
        "bind": "COLUMNS",
        "required": true
      },
      "is_rename": {
        "type": "checkbox",
        "label": "Renombrar",
        "val": "false",
        "isOn": "true",
        "isOff": "false"
      },
      "rename": {
        "show_if": "this.is_rename == 'true'",
        "type": "text",
        "required": true,
        "minLength": 1,
        "maxLength": 255,
        "label": "Nuevo Nombre de Columna",
        "style": "margin-left: 10px"
      },
      "delete": {
        "type": "button",
        "label": "Borrar",
        "function": "DELETE_ROW"
      }
    },
    "breakLine": true,
    "style": "margin-bottom: 20px"
  }
}