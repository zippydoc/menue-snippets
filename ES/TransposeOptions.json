{
  "options": {
    "type": "fieldset", 
    "label": "Seleccionar Opciones", 
    "repeat": false, 
    "fields": {
      "transpose_type": {
        "type": "select", 
        "label": "Tipo de Transposición", 
        "options": {
          "mirror_matrix": {
            "label": "Matriz Espejo"
          }, 
          "column_name_to_label": {
            "label": “Nombre de Columna para la Etiqueta”
          }, 
          "crosstab": {
            "label": "Referencias cruzadas"
          }
        }, 
        "val": "mirror_matrix", 
        "required": true, 
        "width": "100%", 
        "breakLine": true
      }, 
      "mirror_matrix.convert_header_to_label": {
        "type": "checkbox", 
        "label": "Convertir la cabecera a etiqueta", 
        "show_if": "transpose_type=='mirror_matrix'", 
        "val": "true", 
        "isOn": "true", 
        "isOff": "false", 
        "breakLine": true
      }, 
      "column_name_to_label.columns_to_label": {
        "type": "select", 
        "label": "Seleccionar Columnas para Convertir en Etiquetas", 
        "show_if": "transpose_type=='column_name_to_label'", 
        "table": "input_table", 
        "multiple": true, 
        "bind": "COLUMNS", 
        "required": true, 
        "width": "100%", 
        "breakLine": true
      }, 
      "column_name_to_label.label_column_name": {
        "type": "text", 
        "label": "Nombre de Columna de la Columna de Etiquetas", 
        "show_if": "transpose_type=='column_name_to_label'", 
        "required": true, 
        "val": "Etiqueta", 
        "width": "100%", 
        "breakLine": true
      }, 
      "column_name_to_label.value_column_name": {
        "type": "text", 
        "label": "Nombre de Columna de la Columna de Valores", 
        "show_if": "transpose_type=='column_name_to_label'", 
        "required": true, 
        "val": "Valor", 
        "width": "100%", 
        "breakLine": true
      }, 
      "crosstab.group_data_by_these_values": {
        "type": "select", 
        "label": "Agrupar los datos por estos valores", 
        "show_if": "transpose_type=='crosstab'", 
        "table": "input_table", 
        "multiple": true, 
        "bind": "COLUMNS", 
        "required": true, 
        "width": "100%", 
        "breakLine": true
      }, 
      "crosstab.change_column_headers": {
        "type": "select", 
        "label": "Cambiar la Cabecera de la Columna", 
        "show_if": "transpose_type=='crosstab'", 
        "table": "input_table", 
        "bind": "COLUMNS", 
        "required": true, 
        "width": "100%", 
        "breakLine": true
      }, 
      "crosstab.values_for_new_columns": {
        "type": "select", 
        "label": "Valores Para las Nuevas Columnas", 
        "show_if": "transpose_type=='crosstab'", 
        "table": "input_table", 
        "bind": "COLUMNS", 
        "required": true, 
        "width": "100%", 
        "breakLine": true
      }, 
      "crosstab.method_for_aggregating_values": {
        "type": "select", 
        "label": “Método para Agregar Valores”, 
        "show_if": "transpose_type=='crosstab'", 
        "options": {
          "Concatenar": {
            "label": "Concatenar"
          }, 
          "Primero": {
            "label": "Primero"
          }, 
          "Última": {
            "label": "Última"
          }, 
          "Suma": {
            "label": "Suma"
          }, 
          "Promedio": {
            "label": "Promedio"
          }, 
          "Cuenta": {
            "label": "Cuenta"
          }, 
          “Definido por el Usuario”: {
            "label": “Definido por el Usuario”
          }
        }, 
        "val": "Concatenar", 
        "required": true
      }, 
      "crosstab.user_defined_str": {
        "type": "text", 
        "label": “Valor Definido por el Usuario”, 
        "show_if": "transpose_type=='crosstab' && crosstab.method_for_aggregating_values=='User Defined'", 
        "val": "x", 
        "trim": "false", 
        "breakLine": true
      }, 
      "crosstab.separator": {
        "type": "text", 
        "label": "Separador", 
        "show_if": "transpose_type=='crosstab' && method_for_aggregating_values=='Concatenate'", 
        "val": ", ,
        trim": "false", 
        "required": true, 
        "breakLine": true
      }
    }
  }
}