{
  "output.output_table_types": {
    "type": "select", 
    "label": “Tipo(s) de Tabla(s) de Salida”, 
    "required": true, 
    "options": {
      "unique": {
        "label": “Tabla Única”
      }, 
      "duplicates": {
        "label": “Duplica la Tabla”
      }
    }, 
    "multiple": true, 
    "val": ["unique"]
  }, 
  "output.standard_ending": {
    "type": "checkbox", 
    "label": "Termina con Estándar", 
    "val": "true", 
    "isOn": "true", 
    "isOff": "false", 
    "breakLine": true
  }, 
  "output.unique_ending": {
    "show_if": "output.standard_ending == 'false'", 
    "type": "text", 
    "label": "Finalización de Salida Única", 
    "val": "_u", 
    "style": "width: 160px; margin-left: 10px"
  }, 
  "output.duplicates_ending": {
    "show_if": "output.standard_ending == 'false'", 
    "type": "text", 
    "label": "Duplica Finalización de Salida", 
    "val": "_d", 
    "style": "width: 160px; margin-left: 10px", 
    "breakLine": true
  }
}