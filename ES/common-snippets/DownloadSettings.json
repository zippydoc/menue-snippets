{
  "download_setting_frame": {
    "type": "fieldset",
    "label": "Descargar configuración",
    "show_if": "output.custom_options=='true'",
    "repeat": false,
    "fields": {
      "output.options.location": {
        "type": "text",
        "label": "Especificar Ubicación",
        "val": "A1",
        "breakLine": true
      },
      "output.options.formatted_as_table": {
        "type": "checkbox",
        "label": "Formateado como Tabla",
        "val": "true",
        "isOn": "true",
        "isOff": "false",
        "breakLine": true
      },
      "output.options.visibility": {
        "type": "select",
        "label": "Visibilidad",
        "val": "current_flow",
        "options": {
          "current_flow": {
            "label": "Caudal Actual"
          },
          "all_flow": {
            "label": "Todos los Caudales"
          }
        },
        "breakLine": true
      },
      "output.options.table_mode": {
        "type": "select",
        "label": "Pegar",
        "val": "content_only",
        "options": {
          "content_only": {
            "label": "Sólo Contenido"
          },
          "fully_formatted": {
            "label": "Completamente Formateada"
          }
        }
      }
    },
    "breakLine": true,
    "style": "margin-bottom: 20px"
  }
}