{
  "options.cut_off_run": {
    "type": "checkbox",
    "label": "Corte de la tabla de entrada",
    "val": "false",
    "isOn": "true",
    "isOff": "false"
  },
  "options.cut_off.type": {
    "type": "select",
    "label": "Tipo de corte",
    "show_if": "options.cut_off_run=='true'",
    "required": true,
    "options": {
      "from_first_row": {
        "label": "Desde la primera fila"
      },
      "range": {
        "label": "Intervalo"
      }
    },
    "val": "from_first_row"
  },
  "options.cut_off.start_row_num": {
    "type": "text",
    "label": "Inicio de fila",
    "show_if": "options.cut_off_run=='true' && options.cut_off.type=='range'",
    "required": true,
    "minLength": 1,
    "maxLength": 7,
    "val": "1"
  },
  "options.cut_off.row_count": {
    "type": "text",
    "label": "Recuento de filas",
    "show_if": "options.cut_off_run=='true'",
    "required": true,
    "minLength": 1,
    "maxLength": 7,
    "val": "100"
  }
}