{
  "output.select_output_columns": {
    "type": "checkbox",
    "label": "Seleccionar columnas de salida",
    "show_if": "output.custom_options=='true'",
    "val": "false",
    "isOn": "true",
    "isOff": "false",
    "style": "margin-top: 10px"
  },
  "output_columns": {
    "type": "fieldset",
    "label": "Seleccionar columnas de salida",
    "show_if": "output.custom_options=='true' && output.select_output_columns=='true'",
    "repeat": true,
    "fields": {
      "field_name": {
        "type": "select",
        "label": "Seleccionar Columna",
        "table": "input_table",
        "bind": "COLUMNS",
        "required": true
      },
      "order": {
        "type": "select",
        "label": "Orden",
        "options": {
          "-": {
            "label": "-"
          },
          "asc": {
            "label": "Ascendente"
          },
          "desc": {
            "label": "Descendente"
          }
        },
        "val": "-"
      },
      "is_rename": {
        "type": "checkbox",
        "label": "Renombrar",
        "val": "false",
        "isOn": "true",
        "isOff": "false"
      },
      "rename": {
        "show_if": "this.is_rename == 'true'",
        "type": "text",
        "required": true,
        "minLength": 1,
        "maxLength": 255,
        "label": "Nuevo Nombre de Columna",
        "style": "margin-left: 10px"
      },
      "format__category": {
        "type": "select",
        "label": "Formato",
        "required": true,
        "options": {
          "standard": {
            "label": "Estándar"
          },
          "number": {
            "label": "Número"
          },
          "percentage": {
            "label": "Porcentaje"
          },
          "text": {
            "label": "Texto"
          },
          "date": {
            "label": "Fecha"
          },
          "currency": {
            "label": "Moneda"
          }
        },
        "val": "standard"
      },
      "format__rounding": {
        "type": "select",
        "label": "Redondeo",
        "show_if": "this.format__category=='number' || this.format__category=='percentage' || this.format__category=='currency'",
        "required": true,
        "options": {
          "no_rounding": {
            "label": "Redondeo de números"
          },
          "0": {
            "label": "0 dígitos"
          },
          "1": {
            "label": "1 dígitos"
          },
          "2": {
            "label": "2 dígitos"
          },
          "3": {
            "label": "3 dígitos"
          },
          "4": {
            "label": "4 dígitos"
          },
          "5": {
            "label": "5 dígitos"
          },
          "6": {
            "label": "6 dígitos"
          },
          "7": {
            "label": "7 dígitos"
          },
          "8": {
            "label": "8 dígitos"
          },
          "9": {
            "label": "9 dígitos"
          }
        },
        "val": "2"
      },
      "format__use_1000_separator": {
        "type": "checkbox",
        "label": "Use 1000 Separator(.)",
        "show_if": "this.format__category=='number' || this.format__category=='percentage' || this.format__category=='currency'",
        "val": "true",
        "isOn": "true",
        "isOff": "false"
      },
      "format__currency_symbol": {
        "type": "select",
        "label": "Símbolo",
        "show_if": "this.format__category=='currency'",
        "required": true,
        "options": {
          "dollar": {
            "label": "$"
          },
          "euro": {
            "label": "€"
          },
          "pound": {
            "label": "£"
          }
        },
        "val": "dollar"
      },
      "format__date_format": {
        "type": "select",
        "label": "Tipo",
        "show_if": "this.format__category=='date'",
        "required": true,
        "options": {
          "mm/dd/yyyy": {
            "label": "3/14/2012"
          },
          "D, M dd, Y": {
            "label": "Miércoles, 14 de marzo de 2012"
          },
          "yyyy-mm-dd": {
            "label": "40982"
          }
        },
        "val": "mm/dd/yyyy",
        "breakLine": true
      },
      "delete": {
        "type": "button",
        "label": "Borrar",
        "function": "DELETE_ROW"
      }
    },
    "breakLine": true,
    "style": "margin-bottom: 20px"
  }
}