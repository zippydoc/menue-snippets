          },
          "output.options.location": {
            "type": "text",
            "label": "Specify Location",
            "val": "A1"
          },
          "output.options.table_mode": {
            "type": "select",
            "label": "Paste",
            "val": "content_only",
            "options": {
              "content_only": {
                "label": "Content Only"
              },
              "fully_formatted": {
                "label": "Fully Formatted"
              }
            }
          },
          "output.options.formatted_as_table": {
            "type": "checkbox",
            "label": "Formatted as Table",
            "val": "true",
            "isOn": "true",
            "isOff": "false",
            "breakLine": true
          },
          "output.add_tag": {
            "type": "checkbox",
            "label": "Add Tag",
            "val": "false",
            "isOn": "true",
            "isOff": "false"
          },
          "output.tag_name": {
            "type": "text",
            "label": "Tag Name",
            "show_if": "output.add_tag=='true'",
            "required": true,
            "minLength": 1,
            "maxLength": 255,
            "val": "",
            "breakLine": true,
            "style": "margin-left: 15px"
          },
          "output.options.visibility": {
            "type": "select",
            "label": "Visibility",
            "val": "current_flow",
            "options": {
