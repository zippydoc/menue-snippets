{
  "input": {
    "type": "fieldset",
    "Étiquette": "Sélectionner un tableau d'entrée",
    "repeat": false,
    "fields": {
      "input.table_or_flow": {
        "type": "select",
        "Étiquette": "Type",
        "required": true,
        "options": {
          "set_table_permissions": {
            "Étiquette": "Définir les autorisations de la table"
          },
          "set_flow_permissions": {
            "Étiquette": "Définir les autorisations de flux"
          }
        },
        "val": "set_table_permissions"
      },
      "input.table_id": {
        "type": "select",
        "Étiquette": "Sélectionner un tableau",
        "show_if": "input.table_or_flow=='set_table_permissions'",
        "bind": "TABLES",
        "empty": "",
        "required": true
      }
    }
  }
}