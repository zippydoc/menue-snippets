{
  "input": {
    "type": "fieldset",
    "Étiquette": "Sélectionner un tableau d'entrée",
    "repeat": false,
    "fields": {
      "target_table.table": {
        "type": "select",
        "Étiquette": "Tableau de gauche",
        "bind": "TABLES"
      },
      "target_table.remove_input_table_after_processing": {
        "type": "hidden",
        "val": "false"
      },
      "target_table.is_word": {
        "type": "hidden",
        "val": "false"
      },
      "source_table.table": {
        "type": "select",
        "Étiquette": "Table de droite",
        "bind": "TABLES",
        "breakLine": true
      },
      "source_table.remove_input_table_after_processing": {
        "type": "hidden",
        "val": "false"
      },
      "source_table.is_word": {
        "type": "hidden",
        "val": "false"
      },
      "options.cut_off_run": {
        "type": "checkbox",
        "Étiquette": "Couper la table d'entrée et courir",
        "val": "false",
        "isOn": "true",
        "isOff": "false"
      },
      "options.input_table_rows_limit": {
        "type": "text",
        "Étiquette": "Limite du tableau d'entrée (max : 800)",
        "show_if": "options.cut_off_run=='true'",
        "minLength": 1,
        "maxLength": 3,
        "val": "500",
        "maxVal": 800
      }
    }
  }
}