{
  "input": {
    "type": "fieldset",
    "label": "Sélectionner un tableau d'entrée",
    "repeat": false,
    "fields": {
      "target_table.table_id": {
        "type": "select",
        "label": "Tableau de gauche",
        "bind": "TABLES"
      },
      "target_table.remove_input_table_after_processing": {
        "type": "hidden",
        "val": "false"
      },
      "target_table.is_word": {
        "type": "hidden",
        "val": "false"
      },
      "source_table.table_id": {
        "type": "select",
        "label": "Table de droite",
        "bind": "TABLES",
        "breakLine": true
      },
      "source_table.remove_input_table_after_processing": {
        "type": "hidden",
        "val": "false"
      },
      "source_table.is_word": {
        "type": "hidden",
        "val": "false"
      }
    }
  }
}