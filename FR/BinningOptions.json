{
  "options": {
    "type": "fieldset",
    "label": "Sélectionner les options de binning",
    "repeat": false,
    "fields": {
      "number_of_tiles": {
        "type": "number",
        "label": "Nombre de classes",
        "val": "2",
        "required": true,
        "breakLine": true
      },
      "binning_type": {
        "type": "select",
        "label": "Type de binning",
        "options": {
          "equal_records": {
            "label": "Binning simple"
          },
          "equal_intervals": {
            "label": "Binning à intervalles égaux"
          }
        },
        "val": "equal_records"
      },
      "equal_records_order": {
        "type": "select",
        "label": "Commander",
        "show_if": "binning_type=='equal_records'",
        "options": {
          "keep_current": {
            "label": "Tenez-vous au courant"
          },
          "asc": {
            "label": "Ascendant"
          },
          "desc": {
            "label": "Descendant"
          }
        },
        "val": "keep_current"
      },
      "equal_intervals_order": {
        "type": "select",
        "label": "Commander",
        "show_if": "binning_type=='equal_intervals'",
        "options": {
          "asc": {
            "label": "Ascendant"
          },
          "desc": {
            "label": "Descendant"
          }
        },
        "val": "asc",
        "breakLine": true
      }
    },
    "breakLine": true
  }
}