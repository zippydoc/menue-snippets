{
  "options": {
    "type": "fieldset",
    "Étiquette": "Sélectionner les options de binning",
    "repeat": false,
    "fields": {
      "number_of_tiles": {
        "type": "text",
        "Étiquette": "Nombre de classes",
        "val": "2",
        "required": true,
        "breakLine": true
      },
      "binning_type": {
        "type": "select",
        "Étiquette": "Type de binning",
        "options": {
          "equal_records": {
            "Étiquette": "Binning simple"
          },
          "equal_intervals": {
            "Étiquette": "Binning à intervalles égaux"
          }
        },
        "val": "equal_records"
      },
      "equal_records_order": {
        "type": "select",
        "Étiquette": "Commander",
        "show_if": "binning_type=='equal_records'",
        "options": {
          "keep_current": {
            "Étiquette": "Tenez-vous au courant"
          },
          "asc": {
            "Étiquette": "Ascendant"
          },
          "desc": {
            "Étiquette": "Descendant"
          }
        },
        "val": "keep_current"
      },
      "equal_intervals_order": {
        "type": "select",
        "Étiquette": "Commander",
        "show_if": "binning_type=='equal_intervals'",
        "options": {
          "asc": {
            "Étiquette": "Ascendant"
          },
          "desc": {
            "Étiquette": "Descendant"
          }
        },
        "val": "asc"
      }
    },
    "breakLine": true
  }
}