{
  "options": {
    "type": "fieldset",
    "label": "Sélectionner les options de nettoyage",
    "repeat": false,
    "fields": {
      "replace_null": {
        "type": "fieldset",
        "label": "Registres vides",
        "repeat": false,
        "fields": {
          "replace_null": {
            "type": "checkbox",
            "label": "Registres vides",
            "val": "false",
            "isOn": "true",
            "isOff": "false",
            "breakLine": true
          },
          "replace_null_target": {
            "type": "select",
            "label": "Remplacer",
            "show_if": "replace_null=='true'",
            "options": {
              "empty": {
                "label": "Vide"
              },
              "blank": {
                "label": "Vide"
              },
              "null": {
                "label": "NULL"
              }
            },
            "val": "empty",
            "breakLine": true
          },
          "replace_null_by": {
            "type": "radio",
            "show_if": "replace_null=='true'",
            "val": "by_blank",
            "values": {
              "by_blank": "By Blank",
              "by_zero": "By Zero"
            },
            "breakLine": true
          }
        }
      },
      "remove_whitespace": {
        "type": "fieldset",
        "label": "Supprimer les blancs",
        "repeat": false,
        "fields": {
          "remove_characters.all_whitespace": {
            "type": "checkbox",
            "label": "Tous les blancs",
            "val": "false",
            "isOn": "true",
            "isOff": "false",
            "breakLine": true
          },
          "remove_characters.leading_whitespace": {
            "type": "checkbox",
            "label": "Espace de premier plan",
            "show_if": "remove_characters.all_whitespace=='false'",
            "val": "false",
            "isOn": "true",
            "isOff": "false",
            "breakLine": true
          },
          "remove_characters.trailing_whitespace": {
            "type": "checkbox",
            "label": "Espaces mobiles",
            "show_if": "remove_characters.all_whitespace=='false'",
            "val": "false",
            "isOn": "true",
            "isOff": "false",
            "breakLine": true
          },
          "remove_characters.whitespace": {
            "type": "checkbox",
            "label": "Espace",
            "show_if": "remove_characters.all_whitespace=='false'",
            "val": "false",
            "isOn": "true",
            "isOff": "false",
            "breakLine": true
          },
          "remove_characters.tabs": {
            "type": "checkbox",
            "label": "Onglets",
            "show_if": "remove_characters.all_whitespace=='false'",
            "val": "false",
            "isOn": "true",
            "isOff": "false",
            "breakLine": true
          },
          "remove_characters.line_breaks": {
            "type": "checkbox",
            "label": "Sauts de ligne",
            "show_if": "remove_characters.all_whitespace=='false'",
            "val": "false",
            "isOn": "true",
            "isOff": "false",
            "breakLine": true
          },
          "remove_characters.duplicate_whitespace": {
            "type": "checkbox",
            "label": "Dupliquer les blancs",
            "show_if": "remove_characters.all_whitespace=='false'",
            "val": "false",
            "isOn": "true",
            "isOff": "false",
            "breakLine": true
          }
        }
      },
      "remove_duplicates": {
        "type": "fieldset",
        "label": "Supprimer les doublons",
        "repeat": false,
        "fields": {
          "remove_duplicates.cell": {
            "type": "checkbox",
            "label": "Duplicatas dans la cellule",
            "val": "false",
            "isOn": "true",
            "isOff": "false",
            "breakLine": true
          },
          "remove_duplicates.delimiter": {
            "type": "text",
            "label": "Délimiteur",
            "show_if": "remove_duplicates.cell=='true'",
            "val": ";",
            "required": true
          },
          "remove_duplicates.include_whitespace": {
            "type": "checkbox",
            "label": "(espaces compris)",
            "show_if": "remove_duplicates.cell=='true'",
            "val": "true",
            "isOn": "true",
            "isOff": "false",
            "breakLine": true
          },
          "remove_duplicates.rows": {
            "type": "checkbox",
            "label": "Dupliquer des lignes",
            "val": "false",
            "isOn": "true",
            "isOff": "false",
            "breakLine": true
          }
        }
      },
      "remove_characters": {
        "type": "fieldset",
        "label": "Supprimer des caractères",
        "repeat": false,
        "fields": {
          "remove_characters.letters": {
            "type": "checkbox",
            "label": "Lettres",
            "val": "false",
            "isOn": "true",
            "isOff": "false",
            "breakLine": true
          },
          "remove_characters.numbers": {
            "type": "checkbox",
            "label": "Numéros",
            "val": "false",
            "isOn": "true",
            "isOff": "false",
            "breakLine": true
          },
          "remove_characters.punctuation": {
            "type": "checkbox",
            "label": "Ponctuation",
            "val": "false",
            "isOn": "true",
            "isOff": "false",
            "breakLine": true
          },
          "remove_characters.trailing_zero": {
            "type": "checkbox",
            "label": "Trailing 0",
            "val": "false",
            "isOn": "true",
            "isOff": "false",
            "breakLine": true
          },
          "remove_characters.custom": {
            "type": "checkbox",
            "label": "To-be-delted Characters",
            "val": "false",
            "isOn": "true",
            "isOff": "false",
            "breakLine": true
          },
          "remove_characters.custom_chars": {
            "type": "text",
            "label": "to be deleted characters",
            "show_if": "remove_characters.custom=='true'",
            "val": "",
            "required": true
          },
          "remove_characters.custom_mode": {
            "type": "select",
            "label": "Position",
            "show_if": "remove_characters.custom=='true'",
            "options": {
              "all": {
                "label": "all"
              },
              "leading only": {
                "label": "leading only"
              },
              "trailing only": {
                "label": "trailing only"
              }
            },
            "val": "all"
          },
          "remove_characters.custom_delimiter": {
            "type": "select",
            "label": "Délimiteur",
            "show_if": "remove_characters.custom=='true'",
            "options": {
              ",": {
                "label": ","
              },
              ";": {
                "label": ";"
              },
              ".": {
                "label": "."
              }
            },
            "val": ","
          }
        }
      },
      "modify_case": {
        "type": "fieldset",
        "label": "Modifier la case",
        "repeat": false,
        "fields": {
          "modify_case": {
            "type": "checkbox",
            "label": "Modifier la case",
            "val": "false",
            "isOn": "true",
            "isOff": "false",
            "breakLine": true
          },
          "modify_case_by": {
            "type": "radio",
            "show_if": "modify_case=='true'",
            "val": "upper",
            "values": {
              "upper": "to Upper Case",
              "lower": "to Lower Case",
              "title": "to Title Case"
            },
            "breakLine": true
          }
        }
      },
      "rounding": {
        "type": "fieldset",
        "label": "Arrondi",
        "repeat": false,
        "fields": {
          "is_rounding": {
            "type": "checkbox",
            "label": "Arrondi",
            "val": "false",
            "isOn": "true",
            "isOff": "false",
            "breakLine": true
          },
          "rounding": {
            "type": "select",
            "label": "Arrondi",
            "show_if": "is_rounding=='true'",
            "required": true,
            "options": {
              "no_rounding": {
                "label": "Pas d'arrondi"
              },
              "0": {
                "label": "0 chiffre"
              },
              "1": {
                "label": "1 chiffre"
              },
              "2": {
                "label": "2 chiffres"
              },
              "3": {
                "label": "3 chiffres"
              },
              "4": {
                "label": "4 chiffres"
              },
              "5": {
                "label": "5 chiffres"
              },
              "6": {
                "label": "6 chiffres"
              },
              "7": {
                "label": "7 chiffres"
              },
              "8": {
                "label": "8 chiffres"
              },
              "9": {
                "label": "9 chiffres"
              }
            },
            "val": "no_rounding"
          }
        }
      },
      "options.use_filter": {
        "type": "checkbox",
        "label": "Utiliser un filtre",
        "val": "false",
        "isOn": "true",
        "isOff": "false",
        "style": "margin-top: 10px",
        "breakLine": true
      },
      "filter_box": {
        "type": "fieldset",
        "label": "Options de filtrage",
        "show_if": "options.use_filter=='true'",
        "repeat": false,
        "fields": {
          "options.filter": {
            "type": "fieldset",
            "label": "Ajouter des filtres",
            "repeat": true,
            "fields": {
              "initial_op": {
                "type": "select",
                "label": "Sélectionner l'opérateur logique",
                "required": true,
                "show_if" :  "$index == 0",
                "options": {
                  "if": {
                    "label": "SI"
                  },
                  "if_not": {
                    "label": "SINON"
                  }
                },
                "val": "if"
              },
              "op": {
                "type": "select",
                "label": "Sélectionner l'opérateur logique",
                "required": true,
                "show_if" :  "$index > 0",
                "options": {
                  "and": {
                    "label": "ET"
                  },
                  "or": {
                    "label": "OU"
                  },
                  "and_not": {
                    "label": "ET NON"
                  },
                  "or_not": {
                    "label": "OU NON"
                  }
                },
                "val": "and"
              },
              "column": {
                "type": "select",
                "label": "Sélectionner une colonne",
                "table": "table_id",
                "bind": "COLUMNS",
                "required": true
              },
              "condition": {
                "type": "select",
                "label": "Sélectionner un opérateur",
                "required": true,
                "options": {
                  "CONTAINS": {
                    "label": "CONTIENT"
                  },
                  "CONTAINED IN STRING": {
                    "label": "CONTENU DANS"
                  },
                  "STARTS WITH": {
                    "label": "COMMENCE AVEC"
                  },
                  "ENDS WITH": {
                    "label": "SE TERMINE PAR"
                  },
                  "=": {
                    "label": "ÉGAL"
                  },
                  ">": {
                    "label": "SUPÉRIEUR À"
                  },
                  ">=": {
                    "label": "SUPÉRIEUR OU ÉGAL"
                  },
                  "<": {
                    "label": "INFÉRIEUR À"
                  },
                  "<=": {
                    "label": "INFÉRIEUR OU ÉGAL"
                  },
                  "EMPTY": {
                    "label": "EST VIDE"
                  },
                  "BLANK": {
                    "label": "EST BLANC"
                  },
                  "IS NULL": {
                    "label": "EST NUL"
                  },
                  "IS TRUE": {
                    "label": "EST VRAI"
                  },
                  "IS FALSE": {
                    "label": "EST FAUX"
                  },
                  "REGEX MATCH": {
                    "label": "CORRESPONDANCE EXPRESSION RÉGULIÈRE"
                  },
                  "CONTAINED IN TABLE": {
                    "label": "EN TABLE"
                  },
                  "IN REGEX TABLE": {
                    "label": "DANS LE TABLEAU D'EXPRESSIONS RÉGULIÈRES"
                  },
                  "DATE": {
                    "label": "DATE"
                  }
                },
                "val": "CONTAINS"
              },
              "date_condition": {
                "type": "select",
                "label": "Sélectionner l'opérateur de date",
                "required": true,
                "show_if": "this.condition == 'DATE'",
                "options": {
                  "<": {
                    "label": "AVANT"
                  },
                  "<=": {
                    "label": "AVANT OU ÉGAL"
                  },
                  "=": {
                    "label": "ÉGAL"
                  },
                  ">": {
                    "label": "APRÈS"
                  },
                  ">=": {
                    "label": "APRÈS OU ÉGAL"
                  }
                },
                "val": "="
              },
              "compare_with": {
                "type": "select",
                "label": "Compare with",
                "required": true,
                "show_if": "this.condition=='CONTAINS' || this.condition=='CONTAINED IN STRING' || this.condition=='STARTS WITH' || this.condition=='ENDS WITH' || this.condition=='=' || this.condition=='>' || this.condition=='>=' || this.condition=='<' || this.condition=='<='",
                "options": {
                  "custom_value": {
                    "label": "Custom value"
                  },
                  "column_value": {
                    "label": "Column value"
                  }
                },
                "val": "custom_value"
              },
              "self_compare_column": {
                "type": "select",
                "label": "Select Compare Column",
                "table": "table_id",
                "bind": "COLUMNS",
                "show_if": "this.compare_with=='column_value' && (this.condition=='CONTAINS' || this.condition=='CONTAINED IN STRING' || this.condition=='STARTS WITH' || this.condition=='ENDS WITH' || this.condition=='=' || this.condition=='>' || this.condition=='>=' || this.condition=='<' || this.condition=='<=')",
                "required": true
              },
              "date_value": {
                "type": "date",
                "label": "Sélectionner une date",
                "show_if": "this.condition=='DATE'",
                "val": "",
                "required": true
              },
              "value": {
                "type": "text",
                "label": "Valeur d'entrée",
                "show_if": "this.compare_with=='custom_value' && this.condition != 'DATE' && this.condition != 'EMPTY' && this.condition != 'NOT EMPTY' && this.condition != 'BLANK' && this.condition != 'NOT BLANK' && this.condition != 'IS NULL' && this.condition != 'IS NOT NULL' && this.condition != 'IS TRUE' && this.condition != 'IS FALSE' && this.condition != 'CONTAINED IN TABLE' && this.condition != 'NOT CONTAINED IN TABLE' && this.condition != 'IN REGEX TABLE' && this.condition != 'NOT IN REGEX TABLE'",
                "val": "",
                "required": true
              },
              "case_sensitive": {
                "type": "select",
                "label": "Sélectionner la sensibilité à la casse",
                "required": true,
                "show_if": "this.condition == 'CONTAINS' || this.condition == 'CONTAINED IN STRING' || this.condition == '=' || this.condition == 'STARTS WITH' || this.condition == 'ENDS WITH' || this.condition == 'REGEX MATCH' || this.condition == 'NOT REGEX MATCH' || this.condition == 'IN REGEX TABLE' || this.condition == 'NOT IN REGEX TABLE'",
                "options": {
                  "yes": {
                    "label": "Respecter la casse"
                  },
                  "no": {
                    "label": "Ignorer la casse"
                  }
                },
                "val": "yes"
              },
              "compare_table": {
                "type": "select",
                "label": "Sélectionner un tableau",
                "bind": "TABLES",
                "empty": "",
                "show_if": "this.condition == 'CONTAINED IN TABLE' || this.condition == 'NOT CONTAINED IN TABLE' || this.condition == 'IN REGEX TABLE' || this.condition == 'NOT IN REGEX TABLE'",
                "required": true
              },
              "compare_column": {
                "type": "select",
                "label": "Sélectionner une colonne",
                "table": "this.compare_table",
                "bind": "COLUMNS",
                "show_if": "this.condition == 'CONTAINED IN TABLE' || this.condition == 'NOT CONTAINED IN TABLE' || this.condition == 'IN REGEX TABLE' || this.condition == 'NOT IN REGEX TABLE'",
                "required": true
              },
              "delete": {
                "type": "button",
                "label": "Supprimer",
                "function": "DELETE_ROW"
              }
            }
          },
          "options.apply_to_record": {
            "type": "checkbox",
            "label": "Appliquer au registre",
            "val": "true",
            "isOn": "true",
            "isOff": "false",
            "style": "margin-top: 10px",
            "breakLine": true
          },
          "apply_to_group_box": {
            "type": "fieldset",
            "label": "Appliquer au groupe",
            "show_if": "options.apply_to_record=='false'",
            "repeat": false,
            "fields": {
              "options.apply_to_group_columns": {
                "type": "select",
                "label": "Sélectionner les colonnes du groupe",
                "required": true,
                "width": "100%",
                "table": "table_id",
                "multiple": true,
                "bind": "COLUMNS",
                "breakLine": true
              }
            }
          }
        }
      }
    }
  }
}