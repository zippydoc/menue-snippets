          "add_single_row_at_table_end": {
            "Étiquette": "Ajouter une seule ligne à la fin du tableau"
          },
          "add_row_after_each_group": {
            "Étiquette": "Ajouter une ligne après chaque groupe"
          }
        },
        "val": "add_rows_below_selected_row",
        "breakLine": true
      },
