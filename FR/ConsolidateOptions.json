{
  "input": {
    "type": "fieldset",
    "label": "Consolider les options",
    "repeat": true,
    "fields": {
      "new_position": {
        "type": "text",
        "label": "Nouveau poste",
        "val": "",
        "placeholder": "Text",
        "required": true,
        "breakLine": true
      },
      "table_id": {
        "type": "select",
        "label": "Sélectionner un tableau",
        "bind": "TABLES",
        "empty": "",
        "required": true
      },
      "rows": {
        "type": "select",
        "label": "Sélection des rangs",
        "required": true,
        "table": "this.table_id",
        "multiple": true,
        "bind": "LABELS"
      },
      "multiplier": {
        "type": "select",
        "label": "Multiplicateur",
        "required": true,
        "options": {
          "positive": {
            "label": "Gardez le signe"
          },
          "negative": {
            "label": "Switch sign"
          }
        },
        "val": "positive",
        "breakLine": true
      },
      "format__category": {
        "type": "select",
        "label": "Format d'affichage",
        "required": true,
        "options": {
          "standard": {
            "label": "Standard"
          },
          "number": {
            "label": "Numéro"
          },
          "percentage": {
            "label": "Pourcentage"
          },
          "text": {
            "label": "Texte"
          },
          "date": {
            "label": "Date"
          },
          "currency": {
            "label": "Monnaie"
          }
        },
        "val": "standard"
      },
      "format__rounding": {
        "type": "select",
        "label": "Places décimales pour l'affichage",
        "show_if": "this.format__category=='number' || this.format__category=='percentage' || this.format__category=='currency'",
        "required": true,
        "options": {
          "no_rounding": {
            "label": "Pas d'arrondi"
          },
          "0": {
            "label": "0 chiffre"
          },
          "1": {
            "label": "1 chiffre"
          },
          "2": {
            "label": "2 chiffres"
          },
          "3": {
            "label": "3 chiffres"
          },
          "4": {
            "label": "4 chiffres"
          },
          "5": {
            "label": "5 chiffres"
          },
          "6": {
            "label": "6 chiffres"
          },
          "7": {
            "label": "7 chiffres"
          },
          "8": {
            "label": "8 chiffres"
          },
          "9": {
            "label": "9 chiffres"
          }
        },
        "val": "2"
      },
      "format__use_1000_separator": {
        "type": "checkbox",
        "label": "Utilisez le séparateur 1000(.)",
        "show_if": "this.format__category=='number' || this.format__category=='percentage' || this.format__category=='currency'",
        "val": "true",
        "isOn": "true",
        "isOff": "false"
      },
      "format__currency_symbol": {
        "type": "select",
        "label": "Symbole",
        "show_if": "this.format__category=='currency'",
        "required": true,
        "options": {
          "dollar": {
            "label": "$"
          },
          "euro": {
            "label": "€"
          },
          "pound": {
            "label": "£"
          }
        },
        "val": "dollar"
      },
      "format__date_format": {
        "type": "select",
        "label": "Type",
        "show_if": "this.format__category=='date'",
        "required": true,
        "options": {
          "mm/dd/yyyy": {
            "label": "3/14/2012"
          },
          "D, M dd, Y": {
            "label": "Mercredi 14 mars 2012"
          },
          "yyyy-mm-dd": {
            "label": "2012-03-14"
          },
          "m.dd.yyyy": {
            "label": "3.14.2012"
          },
          "m.dd.yy": {
            "label": "3.14.12"
          },
          "dd.m.yyyy": {
            "label": "14.3.2012"
          },
          "dd.m.yy": {
            "label": "14.3.12"
          },
          "M dd, Y": {
            "label": "14 mars 2012"
          },
          "dd-mm-yyyy": {
            "label": "14-03-2012"
          },
          "mm-dd-yyyy": {
            "label": "03-14-2012"
          },
          "dd-mm-yy": {
            "label": "14-03-12"
          },
          "mm-dd-yy": {
            "label": "03-14-12"
          }
        },
        "val": "mm/dd/yyyy",
        "breakLine": true
      },
      "delete": {
        "type": "button",
        "label": "Supprimer",
        "function": "DELETE_ROW"
      }
    }
  }
}