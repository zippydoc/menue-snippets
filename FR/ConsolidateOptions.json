{
  "input": {
    "type": "fieldset",
    "Étiquette": "Consolidate Options",
    "repeat": true,
    "fields": {
      "new_position": {
        "type": "text",
        "Étiquette": "New Position",
        "val": "",
        "placeholder": "Texte",
        "required": true
      },
      "input_table": {
        "type": "select",
        "Étiquette": "Sélectionner un tableau",
        "bind": "TABLES",
        "empty": "",
        "required": true,
        "breakLine": true
      },
      "multiple_selection": {
        "type": "select",
        "Étiquette": "Multiple Selection",
        "required": true,
        "table": "this.input_table",
        "multiple": true,
        "bind": "LABELS",
        "breakLine": true
      },
      "format__category": {
        "type": "select",
        "Étiquette": "Format",
        "required": true,
        "options": {
          "standard": {
            "Étiquette": "Standard"
          },
          "number": {
            "Étiquette": "Numéro"
          },
          "percentage": {
            "Étiquette": "Pourcentage"
          },
          "text": {
            "Étiquette": "Texte"
          },
          "date": {
            "Étiquette": "Date"
          },
          "currency": {
            "Étiquette": "Monnaie"
          }
        },
        "val": "standard"
      },
      "format__rounding": {
        "type": "select",
        "Étiquette": "Arrondi",
        "show_if": "this.format__category=='number' || this.format__category=='percentage' || this.format__category=='currency'",
        "required": true,
        "options": {
          "no_rounding": {
            "Étiquette": "Pas d'arrondi"
          },
          "0": {
            "Étiquette": "0 chiffre"
          },
          "1": {
            "Étiquette": "1 chiffre"
          },
          "2": {
            "Étiquette": "2 chiffres"
          },
          "3": {
            "Étiquette": "3 chiffres"
          },
          "4": {
            "Étiquette": "4 chiffres"
          },
          "5": {
            "Étiquette": "5 chiffres"
          },
          "6": {
            "Étiquette": "6 chiffres"
          },
          "7": {
            "Étiquette": "7 chiffres"
          },
          "8": {
            "Étiquette": "8 chiffres"
          },
          "9": {
            "Étiquette": "9 chiffres"
          }
        },
        "val": "2"
      },
      "format__use_1000_separator": {
        "type": "checkbox",
        "Étiquette": "Use 1000 Separator(.)",
        "show_if": "this.format__category=='number' || this.format__category=='percentage' || this.format__category=='currency'",
        "val": "true",
        "isOn": "true",
        "isOff": "false"
      },
      "format__currency_symbol": {
        "type": "select",
        "Étiquette": "Symbole",
        "show_if": "this.format__category=='currency'",
        "required": true,
        "options": {
          "dollar": {
            "Étiquette": "$"
          },
          "euro": {
            "Étiquette": "€"
          },
          "pound": {
            "Étiquette": "£"
          }
        },
        "val": "dollar"
      },
      "format__date_format": {
        "type": "select",
        "Étiquette": "Type",
        "show_if": "this.format__category=='date'",
        "required": true,
        "options": {
          "mm/dd/yyyy": {
            "Étiquette": "3/14/2012"
          },
          "D, M dd, Y": {
            "Étiquette": "Mercredi 14 mars 2012"
          },
          "yyyy-mm-dd": {
            "Étiquette": "2012-03-14"
          }
        },
        "val": "mm/dd/yyyy",
        "breakLine": true
      }
    }
  }
}