{
  "options_frame": {
    "type": "fieldset",
    "label": "Options",
    "repeat": false,
    "fields": {
      "options.group_by": {
        "type": "select",
        "label": "Grouper par",
        "required": true,
        "options": {
          "Region": {
            "label": "Région"
          },
          "Income Group": {
            "label": "Groupe de revenu"
          }
        },
        "val": "Region",
        "breakLine": true
      },
      "options.stat_var_name": {
        "type": "select",
        "label": "Données statistiques",
        "required": true,
        "options": {
          "UnemploymentRate_Person": {
            "label": "Taux de chômage"
          },
          "Count_Person_InLaborForce": {
            "label": "Population active"
          },
          "Count_Person_15To64Years_InLaborForce_AsFractionOf_Count_Person_15To64Years": {
            "label": "15 - 64 ans, dans la population active Population par habitant 15 à 64 ans"
          },
          "Count_Person_15To64Years_Female_InLaborForce_AsFractionOf_Count_Person_15To64Years_Female": {
            "label": "15 - 64 ans, dans la population active, population féminine par habitant de 15 à 64 ans"
          },
          "Count_Person_15To64Years_Male_InLaborForce_AsFractionOf_Count_Person_15To64Years_Male": {
            "label": "15 - 64 ans, dans la population active, population masculine par habitant 15 à 64 ans Hommes"
          },
          "Count_Person_15OrMoreYears_InLaborForce_Female_AsFractionOf_Count_Person_InLaborForce": {
            "label": "15 ans ou plus, dans la population active, population féminine par habitant dans la population active"
          }
        },
        "val": "UnemploymentRate_Person"
      },
      "options.per_capita": {
        "type": "checkbox",
        "label": "Par habitant",
        "show_if": "false",
        "val": "false",
        "isOn": "true",
        "isOff": "false",
        "breakLine": true
      },
      "options.all_countries": {
        "type": "checkbox",
        "label": "Tous les pays",
        "val": "true",
        "isOn": "true",
        "isOff": "false",
        "breakLine": true
      },
      "options.output_as_chart": {
        "type": "checkbox",
        "label": "Produire un graphique",
        "val": "true",
        "isOn": "true",
        "isOff": "false"
      },
      "options.graph_type": {
        "type": "select",
        "label": "Type de graphique",
        "show_if": "options.output_as_chart=='true'",
        "required": true,
        "options": {
          "zoomable_sunburst": {
            "label": "Sunburst"
          },
          "treemap": {
            "label": "Treemap"
          }
        },
        "val": "zoomable_sunburst",
        "breakLine": true
      }
    }
  }
}