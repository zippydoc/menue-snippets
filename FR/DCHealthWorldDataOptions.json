{
  "options_frame": {
    "type": "fieldset",
    "label": "Options",
    "repeat": false,
    "fields": {
      "options.group_by": {
        "type": "select",
        "label": "Grouper par",
        "required": true,
        "options": {
          "Region": {
            "label": "Région"
          },
          "Income Group": {
            "label": "Groupe de revenu"
          }
        },
        "val": "Region",
        "breakLine": true
      },
      "options.stat_var_name": {
        "type": "select",
        "label": "Données statistiques",
        "required": true,
        "options": {
          "FertilityRate_Person_Female": {
            "label": "Taux de fertilité"
          },
          "LifeExpectancy_Person": {
            "label": "Espérance de vie"
          },
          "Count_BirthEvent_LiveBirth_AsFractionOf_Count_Person": {
            "label": "Nombre de naissances vivantes survenues au cours de l'année, par habitant"
          },
          "Count_Person_20To79Years_Diabetes_AsFractionOf_Count_Person_20To79Years": {
            "label": "Pourcentage de personnes âgées de 20 à 79 ans qui ont un diabète de type 1 ou de type 2"
          },
          "Count_Death": {
            "label": "Compte de l'événement de mortalité"
          },
          "Count_Person_15OrMoreYears_Smoking_AsFractionOf_Count_Person_15OrMoreYears": {
            "label": "15 ans ou plus, Population fumeuse par habitant 15 ans ou plus"
          },
          "Count_Person_Upto4Years_Overweight_AsFractionOf_Count_Person_Upto4Years": {
            "label": "4 ans ou moins, population en surpoids par habitant jusqu'à 4 ans"
          }
        },
        "val": "FertilityRate_Person_Female"
      },
      "options.per_capita": {
        "type": "checkbox",
        "label": "Par habitant",
        "show_if": "false",
        "val": "false",
        "isOn": "true",
        "isOff": "false",
        "breakLine": true
      },
      "options.all_countries": {
        "type": "checkbox",
        "label": "Tous les pays",
        "val": "true",
        "isOn": "true",
        "isOff": "false",
        "breakLine": true
      },
      "options.output_as_chart": {
        "type": "checkbox",
        "label": "Produire un graphique",
        "val": "true",
        "isOn": "true",
        "isOff": "false"
      },
      "options.graph_type": {
        "type": "select",
        "label": "Type de graphique",
        "show_if": "options.output_as_chart=='true'",
        "required": true,
        "options": {
          "zoomable_sunburst": {
            "label": "Sunburst"
          },
          "treemap": {
            "label": "Treemap"
          }
        },
        "val": "zoomable_sunburst",
        "breakLine": true
      }
    }
  }
}