{
  "placeholder": {
    "breakLine": true
  },
  "delete_options": {
    "type": "select",
    "Étiquette": "Supprimer les options",
    "required": true,
    "options": {
      "del_table": {
        "Étiquette": "Supprimer le tableau"
      },
      "del_columns": {
        "Étiquette": "Supprimer les colonnes sélectionnées"
      },
      "del_rows": {
        "Étiquette": "Supprimer les lignes sélectionnées"
      }
    },
    "val": "del_table"
  },
  "apply_columns": {
    "type": "select",
    "Étiquette": "Sélectionner les colonnes à supprimer",
    "show_if": "delete_options=='del_columns'",
    "required": true,
    "table": "input_table",
    "multiple": true,
    "bind": "COLUMNS"
  },
  "apply_rows": {
    "type": "select",
    "Étiquette": "Sélectionner les lignes à supprimer",
    "show_if": "delete_options=='del_rows'",
    "required": true,
    "table": "input_table",
    "multiple": true,
    "bind": "LABELS"
  }
}