{
  "placeholder": {
    "breakLine": true
  },
  "delete_options": {
    "type": "select",
    "label": "Supprimer les options",
    "required": true,
    "options": {
      "del_table": {
        "label": "Supprimer le tableau"
      },
      "del_columns": {
        "label": "Supprimer les colonnes sélectionnées"
      },
      "del_rows": {
        "label": "Supprimer les lignes sélectionnées"
      }
    },
    "val": "del_table"
  },
  "apply_columns": {
    "type": "select",
    "label": "Sélectionner les colonnes à supprimer",
    "show_if": "delete_options=='del_columns'",
    "required": true,
    "table": "table_id",
    "multiple": true,
    "bind": "COLUMNS"
  },
  "apply_rows": {
    "type": "select",
    "label": "Sélectionner les lignes à supprimer",
    "show_if": "delete_options=='del_rows'",
    "required": true,
    "table": "table_id",
    "multiple": true,
    "bind": "LABELS"
  }
}