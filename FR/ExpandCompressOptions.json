{
  "options": {
    "type": "fieldset",
    "Étiquette": "Sélectionner les options",
    "repeat": false,
    "fields": {
      "options.type": {
        "type": "radio",
        "val": "expand",
        "values": {
          "expand": "Étendre",
          "compress": "Compress"
        },
        "breakLine": true
      },
      "options.column_to_expand": {
        "type": "select",
        "Étiquette": "Colonne à développer",
        "show_if": "options.type=='expand'",
        "table": "input_table",
        "bind": "COLUMNS",
        "required": true
      },
      "options.column_to_group": {
        "type": "select",
        "Étiquette": "Colonne à grouper",
        "show_if": "options.type=='compress'",
        "table": "input_table",
        "bind": "COLUMNS",
        "required": true
      },
      "options.column_to_compress": {
        "type": "select",
        "Étiquette": "Colonne à compresser",
        "show_if": "options.type=='compress'",
        "table": "input_table",
        "bind": "COLUMNS",
        "required": true
      },
      "options.delimiter": {
        "type": "text",
        "Étiquette": "Délimiteur",
        "val": ";",
        "trim": "false",
        "required": true
      },
      "options.new_col_name": {
        "type": "text",
        "Étiquette": "Nouveau nom de colonne",
        "show_if": "options.type=='expand' && options.expand__output_in_new_column=='true'",
        "val": "Étiquette",
        "required": true,
        "breakLine": true
      },
      "options.expand__output_in_new_column": {
        "type": "checkbox",
        "Étiquette": "Sortie dans une nouvelle colonne",
        "show_if": "options.type=='expand'",
        "val": "false",
        "isOn": "true",
        "isOff": "false",
        "breakLine": true
      },
      "options.create_new_table": {
        "type": "checkbox",
        "Étiquette": "Sortie en nouveau tableau",
        "val": "true",
        "isOn": "true",
        "isOff": "false",
        "breakLine": true
      }
    }
  }
}