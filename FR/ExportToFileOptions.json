{
  "options": {
    "type": "fieldset",
    "Étiquette": "Sélectionner les options",
    "repeat": false,
    "fields": {
      "options.to": {
        "type": "hidden",
        "val": "file"
      },
      "options.webservice": {
        "type": "select",
        "Étiquette": "Sélectionner un service web",
        "options": {
          "github": {
            "Étiquette": "Github"
          },
          "dropbox": {
            "Étiquette": "Dropbox"
          },
          "ms_one_drive": {
            "Étiquette": " Microsoft OneDrive"
          },
          "google_drive": {
            "Étiquette": "Google Drive"
          }
        },
        "val": "github",
        "required": true,
        "breakLine": true
      },
      "options.export_format": {
        "type": "select",
        "Étiquette": "Format d'exportation",
        "options": {
          "csv_file": {
            "Étiquette": "Fichier(s) Csv"
          },
          "word_by_word": {
            "Étiquette": "Mot par mot"
          },
          "line_by_line": {
            "Étiquette": "Ligne par ligne"
          }
        },
        "val": "csv_file",
        "required": true,
        "breakLine": true
      },
      "options.if_duplicated": {
        "type": "select",
        "Étiquette": "Si existant",
        "show_if": "options.webservice=='github'",
        "val": "drop_if_exist",
        "options": {
          "drop_if_exist": {
            "Étiquette": "Créer ou supprimer (si déjà existant)"
          },
          "error_if_exist": {
            "Étiquette": "Créer (erreur si déjà existant)"
          },
          "error_if_not_exist": {
            "Étiquette": "Ignorer (erreur si non-existant)"
          },
          "append": {
            "Étiquette": "Annexer"
          }
        },
        "breakLine": true
      },
      "options.export_series": {
        "type": "checkbox",
        "Étiquette": "Exporter série",
        "val": "false",
        "isOn": "true",
        "isOff": "false",
        "breakLine": true
      },
      "options.file_name_column": {
        "type": "select",
        "Étiquette": "Sélectionner la colonne de nom du fichier",
        "show_if": "options.export_series=='true'",
        "table": "input_table",
        "bind": "COLUMNS",
        "required": true,
        "breakLine": true
      },
      "options.set_file_path_column": {
        "type": "checkbox",
        "Étiquette": "Exporter vers un chemin spécifié",
        "show_if": "options.export_series=='true'",
        "val": "false",
        "isOn": "true",
        "isOff": "false",
        "breakLine": true
      },
      "options.file_path_column": {
        "type": "select",
        "Étiquette": "Colonne de sélection du chemin d'accès au fichier",
        "show_if": "options.export_series=='true' && options.set_file_path_column=='true'",
        "table": "input_table",
        "bind": "COLUMNS",
        "required": true,
        "breakLine": true
      }
    }
  },
  "export_link_token_frame": {
    "type": "fieldset",
    "Étiquette": "Exporter le lien",
    "repeat": false,
    "fields": {
      "options.github_url": {
        "type": "text",
        "Étiquette": "Insérer lien, p. ex. https://github.com/zippydoc/test",
        "show_if": "options.webservice=='github'",
        "val": "",
        "required": true,
        "width": "100%",
        "breakLine": true
      },
      "options.github_token": {
        "type": "text",
        "Étiquette": "Spécifiez le jeton d'accès personnel Github qui a le droit d'écriture :",
        "show_if": "options.webservice=='github'",
        "required": true,
        "width": "100%",
        "breakLine": true
      },
      "options.github_create_pr": {
        "type": "checkbox",
        "Étiquette": "Créer une requête d'extraction",
        "show_if": "options.webservice=='github'",
        "val": "false",
        "isOn": "true",
        "isOff": "false",
        "breakLine": true
      },
      "options.ms_one_drive_token": {
        "type": "text",
        "Étiquette": "Code d'accès à l'API Microsoft Graph. Vous pouvez l'obtenir ici https://ms-oauth.herokuapp.com/",
        "show_if": "options.webservice=='ms_one_drive'",
        "required": true,
        "width": "100%",
        "breakLine": true
      },
      "options.google_authorization_code": {
        "type": "text",
        "Étiquette": "Code d'autorisation Google OAuth avec la portée https://www.googleapis.com/auth/drive. Vous pouvez l'obtenir ici https://developers.google.com/oauthplayground/",
        "show_if": "options.webservice=='google_drive'",
        "required": true,
        "width": "100%",
        "breakLine": true
      },
      "options.dropbox_authorization_code": {
        "type": "text",
        "Étiquette": "Code d'autorisation Dropbox. Vous pouvez l'obtenir ici https://www.dropbox.com/oauth2/authorize?client_id=9i7woztjp94jrkw&response_type=code",
        "show_if": "options.webservice=='dropbox'",
        "required": true,
        "width": "100%",
        "breakLine": true
      }
    }
  }
}