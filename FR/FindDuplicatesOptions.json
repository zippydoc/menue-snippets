        "val": "false",
        "isOn": "true",
        "isOff": "false",
        "breakLine": true
      },
      "input_table_filter_box": {
        "type": "fieldset",
        "label": "Options de filtrage des tableaux d'entrée",
        "show_if": "options.input_table.use_filter==''true'' && (options.task_type==''apply_lookup_table'' || options.task_type==''apply_regex_table'' || options.task_type==''apply_lookup_table_to_cells'' || options.task_type==''apply_regex_table_to_cells'')",
        "repeat": false,
        "fields": {
          "options.input_table.filter": {
            "type": "fieldset",
            "label": "Ajouter des filtres pour le tableau d'entrée",
            "repeat": true,
            "fields": {
              "initial_op": {
                "type": "select",
                "label": "Sélectionner l'opérateur logique",
                "required": true,
                "show_if": "$index == 0",
                "options": {
                  "if": {
                    "label": "SI"
                  },
                  "if_not": {
                    "label": "SINON"
                  }
                },
                "val": "if"
              },
              "op": {
                "type": "select",
                "label": "Sélectionner l'opérateur logique",
                "required": true,
                "show_if": "$index > 0",
                "options": {
                  "and": {
                    "label": "ET"
                  },
                  "or": {
                    "label": "OU"
                  },
                  "and_not": {
                    "label": "ET NON"
                  },
                  "or_not": {
                    "label": "OU NON"
                  }
                },
                "val": "and"
              },
              "column": {
                "type": "select",
                "label": "Sélectionner une colonne",
                "table": "input_table",
                "bind": "COLUMNS",
                "required": true
              },
              "condition": {
                "type": "select",
                "label": "Sélectionner un opérateur",
                "required": true,
                "options": {
                  "CONTIENT": {
                    "label": "CONTIENT"
                  },
                  "CONTAINED IN STRING": {
                    "label": "CONTENU DANS"
                  },
                  "COMMENCE AVEC": {
                    "label": "COMMENCE AVEC"
                  },
                  "SE TERMINE PAR": {
                    "label": "SE TERMINE PAR"
                  },
                  "=": {
                    "label": "ÉGAL"
                  },
                  ">": {
                    "label": "SUPÉRIEUR À"
                  },
                  ">=": {
                    "label": "SUPÉRIEUR OU ÉGAL"
                  },
                  "<": {
                    "label": "INFÉRIEUR À"
                  },
                  "<=": {
                    "label": "INFÉRIEUR OU ÉGAL"
                  },
                  "EMPTY": {
                    "label": "EST VIDE"
                  },
                  "BLANK": {
                    "label": "EST BLANC"
                  },
                  "EST NUL": {
                    "label": "EST NUL"
                  },
                  "CORRESPONDANCE EXPRESSION RÉGULIÈRE": {
                    "label": "CORRESPONDANCE EXPRESSION RÉGULIÈRE"
                  },
                  "CONTAINED IN TABLE": {
                    "label": "EN TABLE"
                  },
