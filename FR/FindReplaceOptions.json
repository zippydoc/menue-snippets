{
  "options": {
    "type": "fieldset",
    "Étiquette": "Indiquer les options",
    "repeat": false,
    "fields": {
      "fields_to_impute": {
        "type": "select",
        "Étiquette": "Sélectionner les colonnes à trouver",
        "required": true,
        "table": "input_table",
        "multiple": true,
        "bind": "COLUMNS",
        "width": "100%",
        "breakLine": true
      },
      "incoming_value_to_replace.type": {
        "type": "select",
        "Étiquette": "Valeur entrante",
        "options": {
          "user_specified_value": {
            "Étiquette": "Égaux"
          },
          "begins_with": {
            "Étiquette": "Commence par"
          },
          "ends_with": {
            "Étiquette": "Se termine par"
          },
          "empty": {
            "Étiquette": "Vide"
          },
          "regex": {
            "Étiquette": "Expression régulière"
          }
        },
        "val": "user_specified_value",
        "style": "min-width: 190px"
      },
      "incoming_value_to_replace.value": {
        "type": "text",
        "Étiquette": "Valeur entrante à remplacer",
        "required": true,
        "show_if": "incoming_value_to_replace.type=='user_specified_value' || incoming_value_to_replace.type=='begins_with' || incoming_value_to_replace.type=='ends_with'",
        "val": "",
        "style": "min-width: 170px"
      },
      "incoming_value_to_replace.regex": {
        "type": "text",
        "Étiquette": "Expression régulière",
        "required": true,
        "show_if": "incoming_value_to_replace.type=='regex'",
        "val": "(.*),(.*),(.*)\\s(\\d{5})(-\\d{4})?",
        "style": "min-width: 200px"
      },
      "replace_table.table_id": {
        "type": "select",
        "Étiquette": "Tableau de remplacement",
        "show_if": "incoming_value_to_replace.type=='replacement_table'",
        "bind": "TABLES",
        "required": true
      },
      "replace_table.find_column": {
        "type": "select",
        "Étiquette": "Sélectionner la colonne de recherche",
        "show_if": "incoming_value_to_replace.type=='replacement_table'",
        "table": "replace_table.table_id",
        "bind": "COLUMNS",
        "required": true
      },
      "replace_table.replace_with_column": {
        "type": "select",
        "Étiquette": "Sélectionner remplacer par une colonne",
        "show_if": "incoming_value_to_replace.type=='replacement_table'",
        "table": "replace_table.table_id",
        "bind": "COLUMNS",
        "required": true
      },
      "incoming_value_to_replace.partial_match": {
        "type": "select",
        "Étiquette": "Correspondance partielle",
        "show_if": "incoming_value_to_replace.type=='user_specified_value' || incoming_value_to_replace.type=='replacement_table'",
        "options": {
          "full_match": {
            "Étiquette": "Correspondance complète"
          },
          "full_and_partial_match": {
            "Étiquette": "Correspondance complète et partielle"
          },
          "partial_match_only": {
            "Étiquette": "Correspondance partielle uniquement"
          }
        },
        "val": "full_and_partial_match"
      },
      "replace_with.value_type": {
        "type": "select",
        "Étiquette": "Remplacer par",
        "show_if": "incoming_value_to_replace.type!='regex' && incoming_value_to_replace.type!='replacement_table'",
        "options": {
          "user_specified_value": {
            "Étiquette": "Valeur personnalisée"
          },
          "average": {
            "Étiquette": "Moyenne des colonnes"
          },
          "median": {
            "Étiquette": "Médiane de la colonne"
          },
          "mode": {
            "Étiquette": "Mode de la colonne"
          },
          "sum": {
            "Étiquette": "Somme de la colonne"
          },
          "table": {
            "Étiquette": "Colonne du tableau"
          }
        },
        "val": "user_specified_value",
        "style": "min-width: 190px"
      },
      "replace_with.value": {
        "type": "text",
        "Étiquette": "Remplacer par la valeur",
        "show_if": "incoming_value_to_replace.type!='regex' && replace_with.value_type=='user_specified_value' && incoming_value_to_replace.type!='replacement_table'",
        "val": ""
      },
      "replace_with.regex_value": {
        "type": "text",
        "Étiquette": "Remplacer par la valeur",
        "show_if": "incoming_value_to_replace.type=='regex' && incoming_value_to_replace.type!='replacement_table'",
        "val": "\\4: \\2, \\3"
      },
      "replace_with.table": {
        "type": "select",
        "Étiquette": "Sélectionner un tableau",
        "show_if": "incoming_value_to_replace.type!='regex' && replace_with.value_type=='table' && incoming_value_to_replace.type!='replacement_table'",
        "bind": "TABLES",
        "required": true
      },
      "replace_with.column": {
        "type": "select",
        "Étiquette": "Sélectionner une colonne",
        "show_if": "incoming_value_to_replace.type!='regex' && replace_with.value_type=='table' && incoming_value_to_replace.type!='replacement_table'",
        "table": "replace_with.table",
        "bind": "COLUMNS",
        "required": true,
        "breakLine": true
      },
      "replacement_table_notice": {
        "type": "text",
        "Étiquette": "",
        "show_if": "incoming_value_to_replace.type=='replacement_table'",
        "val": "Line-break will be ignored in replacement table",
        "disabled": true,
        "width": "100%"
      },
      "use_filter": {
        "type": "checkbox",
        "Étiquette": "Utiliser un filtre",
        "val": "false",
        "isOn": "true",
        "isOff": "false",
        "breakLine": true
      },
      "filter_options_box": {
        "type": "fieldset",
        "Étiquette": "Options de filtrage",
        "show_if": "use_filter=='true'",
        "repeat": false,
        "fields": {
          "filter": {
            "type": "fieldset",
            "Étiquette": "Ajouter des filtres",
            "show_if": "use_filter=='true'",
            "repeat": true,
            "fields": {
              "initial_op": {
                "type": "select",
                "Étiquette": "Sélectionner l'opérateur logique",
                "required": true,
                "show_if" :  "$index == 0",
                "options": {
                  "if": {
                    "Étiquette": "SI"
                  },
                  "if_not": {
                    "Étiquette": "SINON"
                  }
                },
                "val": "if"
              },
              "op": {
                "type": "select",
                "Étiquette": "Sélectionner l'opérateur logique",
                "required": true,
                "show_if" :  "$index > 0",
                "options": {
                  "and": {
                    "Étiquette": "ET"
                  },
                  "or": {
                    "Étiquette": "OU"
                  },
                  "and_not": {
                    "Étiquette": "ET NON"
                  },
                  "or_not": {
                    "Étiquette": "OU NON"
                  }
                },
                "val": "and"
              },
              "column": {
                "type": "select",
                "Étiquette": "Sélectionner une colonne",
                "table": "input_table",
                "bind": "COLUMNS",
                "required": true
              },
              "condition": {
                "type": "select",
                "Étiquette": "Sélectionner un opérateur",
                "required": true,
                "options": {
                  "CONTIENT": {
                    "Étiquette": "CONTIENT"
                  },
                  "CONTAINED IN STRING": {
                    "Étiquette": "CONTENU DANS"
                  },
                  "COMMENCE AVEC": {
                    "Étiquette": "COMMENCE AVEC"
                  },
                  "SE TERMINE PAR": {
                    "Étiquette": "SE TERMINE PAR"
                  },
                  "=": {
                    "Étiquette": "ÉGAL"
                  },
                  ">": {
                    "Étiquette": "SUPÉRIEUR À"
                  },
                  ">=": {
                    "Étiquette": "SUPÉRIEUR OU ÉGAL"
                  },
                  "<": {
                    "Étiquette": "INFÉRIEUR À"
                  },
                  "<=": {
                    "Étiquette": "INFÉRIEUR OU ÉGAL"
                  },
                  "EMPTY": {
                    "Étiquette": "EST VIDE"
                  },
                  "BLANK": {
                    "Étiquette": "EST BLANC"
                  },
                  "EST NUL": {
                    "Étiquette": "EST NUL"
                  },
                  "CORRESPONDANCE EXPRESSION RÉGULIÈRE": {
                    "Étiquette": "CORRESPONDANCE EXPRESSION RÉGULIÈRE"
                  },
                  "CONTAINED IN TABLE": {
                    "Étiquette": "EN TABLE"
                  },
                  "DANS LE TABLEAU D'EXPRESSIONS RÉGULIÈRES": {
                    "Étiquette": "DANS LE TABLEAU D'EXPRESSIONS RÉGULIÈRES"
                  },
                  "DATE": {
                    "Étiquette": "DATE"
                  }
                },
                "val": "CONTIENT"
              },
              "date_condition": {
                "type": "select",
                "Étiquette": "Sélectionner l'opérateur de date",
                "required": true,
                "show_if": "this.condition == 'DATE'",
                "options": {
                  "<": {
                    "Étiquette": "AVANT"
                  },
                  "<=": {
                    "Étiquette": "AVANT OU ÉGAL"
                  },
                  "=": {
                    "Étiquette": "ÉGAL"
                  },
                  ">": {
                    "Étiquette": "APRÈS"
                  },
                  ">=": {
                    "Étiquette": "APRÈS OU ÉGAL"
                  }
                },
                "val": "="
              },
              "case_sensitive": {
                "type": "select",
                "Étiquette": "Sélectionner la sensibilité à la casse",
                "required": true,
                "show_if": "this.condition == 'CONTAINS' || this.condition == 'CONTAINED IN STRING' || this.condition == '=' || this.condition == 'STARTS WITH' || this.condition == 'ENDS WITH' || this.condition == 'REGEX MATCH' || this.condition == 'NOT REGEX MATCH' || this.condition == 'IN REGEX TABLE' || this.condition == 'NOT IN REGEX TABLE'",
                "options": {
                  "yes": {
                    "Étiquette": "Respecter la casse"
                  },
                  "no": {
                    "Étiquette": "Ignorer la casse"
                  }
                },
                "val": "yes"
              },
              "date_value": {
                "type": "date",
                "Étiquette": "Sélectionner une date",
                "show_if": "this.condition=='DATE'",
                "val": "1/31/2020",
                "required": true
              },
              "value": {
                "type": "text",
                "Étiquette": "Valeur d'entrée",
                "show_if": "this.condition != 'DATE' && this.condition != 'EMPTY' && this.condition != 'NOT EMPTY' && this.condition != 'BLANK' && this.condition != 'NOT BLANK' && this.condition != 'IS NULL' && this.condition != 'IS NOT NULL' && this.condition != 'CONTAINED IN TABLE' && this.condition != 'NOT CONTAINED IN TABLE' && this.condition != 'IN REGEX TABLE' && this.condition != 'NOT IN REGEX TABLE'",
                "val": "",
                "required": true
              },
              "compare_table": {
                "type": "select",
                "Étiquette": "Sélectionner un tableau",
                "bind": "TABLES",
                "empty": "",
                "show_if": "this.condition == 'CONTAINED IN TABLE' || this.condition == 'NOT CONTAINED IN TABLE' || this.condition == 'IN REGEX TABLE' || this.condition == 'NOT IN REGEX TABLE'",
                "required": true
              },
              "compare_column": {
                "type": "select",
                "Étiquette": "Sélectionner une colonne",
                "table": "this.compare_table",
                "bind": "COLUMNS",
                "show_if": "this.condition == 'CONTAINED IN TABLE' || this.condition == 'NOT CONTAINED IN TABLE' || this.condition == 'IN REGEX TABLE' || this.condition == 'NOT IN REGEX TABLE'",
                "required": true
              },
              "delete": {
                "type": "button",
                "Étiquette": "Supprimer",
                "function": "DELETE_ROW"
              }
            }
          },
          "apply_to_record": {
            "type": "checkbox",
            "Étiquette": "Appliquer au registre",
            "val": "true",
            "isOn": "true",
            "isOff": "false",
            "style": "margin-top: 10px",
            "breakLine": true
          },
          "apply_to_group_box": {
            "type": "fieldset",
            "Étiquette": "Appliquer au groupe",
            "show_if": "apply_to_record=='false'",
            "repeat": false,
            "fields": {
              "apply_to_group_columns": {
                "type": "select",
                "Étiquette": "Sélectionner les colonnes du groupe",
                "required": true,
                "width": "100%",
                "table": "input_table",
                "multiple": true,
                "bind": "COLUMNS",
                "breakLine": true
              }
            }
          }
        }
      },
      "use_replace_table_filter": {
        "type": "checkbox",
        "Étiquette": "Use Filter on \"Replace With\" Table",
        "show_if": "incoming_value_to_replace.type=='replacement_table' || (incoming_value_to_replace.type!='regex' && replace_with.value_type=='table')",
        "val": "false",
        "isOn": "true",
        "isOff": "false",
        "style": "margin-top: 10px",
        "breakLine": true
      },
      "replace_table_filter_option_box": {
        "type": "fieldset",
        "Étiquette": "\"Replace With\" Table Filter Options",
        "show_if": "use_replace_table_filter=='true' && (incoming_value_to_replace.type=='replacement_table' || (incoming_value_to_replace.type!='regex' && replace_with.value_type=='table'))",
        "repeat": false,
        "fields": {
          "replace_table_filter_option.filter": {
            "type": "fieldset",
            "Étiquette": "Add Filter Conditions on \"Replace With\" Table",
            "repeat": true,
            "fields": {
              "initial_op": {
                "type": "select",
                "Étiquette": "Sélectionner l'opérateur logique",
                "required": true,
                "show_if" :  "$index == 0",
                "options": {
                  "if": {
                    "Étiquette": "SI"
                  },
                  "if_not": {
                    "Étiquette": "SINON"
                  }
                },
                "val": "if"
              },
              "op": {
                "type": "select",
                "Étiquette": "Sélectionner l'opérateur logique",
                "required": true,
                "show_if" :  "$index > 0",
                "options": {
                  "and": {
                    "Étiquette": "ET"
                  },
                  "or": {
                    "Étiquette": "OU"
                  },
                  "and_not": {
                    "Étiquette": "ET NON"
                  },
                  "or_not": {
                    "Étiquette": "OU NON"
                  }
                },
                "val": "and"
              },
              "column": {
                "type": "select",
                "Étiquette": "Sélectionner une colonne",
                "table": "replace_with.table",
                "bind": "COLUMNS",
                "required": true
              },
              "condition": {
                "type": "select",
                "Étiquette": "Sélectionner un opérateur",
                "required": true,
                "options": {
                  "CONTIENT": {
                    "Étiquette": "CONTIENT"
                  },
                  "CONTAINED IN STRING": {
                    "Étiquette": "CONTENU DANS"
                  },
                  "COMMENCE AVEC": {
                    "Étiquette": "COMMENCE AVEC"
                  },
                  "SE TERMINE PAR": {
                    "Étiquette": "SE TERMINE PAR"
                  },
                  "=": {
                    "Étiquette": "ÉGAL"
                  },
                  ">": {
                    "Étiquette": "SUPÉRIEUR À"
                  },
                  ">=": {
                    "Étiquette": "SUPÉRIEUR OU ÉGAL"
                  },
                  "<": {
                    "Étiquette": "INFÉRIEUR À"
                  },
                  "<=": {
                    "Étiquette": "INFÉRIEUR OU ÉGAL"
                  },
                  "EMPTY": {
                    "Étiquette": "EST VIDE"
                  },
                  "BLANK": {
                    "Étiquette": "EST BLANC"
                  },
                  "EST NUL": {
                    "Étiquette": "EST NUL"
                  },
                  "CORRESPONDANCE EXPRESSION RÉGULIÈRE": {
                    "Étiquette": "CORRESPONDANCE EXPRESSION RÉGULIÈRE"
                  },
                  "CONTAINED IN TABLE": {
                    "Étiquette": "EN TABLE"
                  },
                  "DANS LE TABLEAU D'EXPRESSIONS RÉGULIÈRES": {
                    "Étiquette": "DANS LE TABLEAU D'EXPRESSIONS RÉGULIÈRES"
                  },
                  "DATE": {
                    "Étiquette": "DATE"
                  }
                },
                "val": "CONTIENT"
              },
              "date_condition": {
                "type": "select",
                "Étiquette": "Sélectionner l'opérateur de date",
                "required": true,
                "show_if": "this.condition == 'DATE'",
                "options": {
                  "<": {
                    "Étiquette": "AVANT"
                  },
                  "<=": {
                    "Étiquette": "AVANT OU ÉGAL"
                  },
                  "=": {
                    "Étiquette": "ÉGAL"
                  },
                  ">": {
                    "Étiquette": "APRÈS"
                  },
                  ">=": {
                    "Étiquette": "APRÈS OU ÉGAL"
                  }
                },
                "val": "="
              },
              "case_sensitive": {
                "type": "select",
                "Étiquette": "Sélectionner la sensibilité à la casse",
                "required": true,
                "show_if": "this.condition == 'CONTAINS' || this.condition == 'CONTAINED IN STRING' || this.condition == '=' || this.condition == 'STARTS WITH' || this.condition == 'ENDS WITH' || this.condition == 'REGEX MATCH' || this.condition == 'NOT REGEX MATCH' || this.condition == 'IN REGEX TABLE' || this.condition == 'NOT IN REGEX TABLE'",
                "options": {
                  "yes": {
                    "Étiquette": "Respecter la casse"
                  },
                  "no": {
                    "Étiquette": "Ignorer la casse"
                  }
                },
                "val": "yes"
              },
              "date_value": {
                "type": "date",
                "Étiquette": "Sélectionner une date",
                "show_if": "this.condition=='DATE'",
                "val": "1/31/2020",
                "required": true
              },
              "value": {
                "type": "text",
                "Étiquette": "Valeur d'entrée",
                "show_if": "this.condition != 'DATE' && this.condition != 'EMPTY' && this.condition != 'NOT EMPTY' && this.condition != 'BLANK' && this.condition != 'NOT BLANK' && this.condition != 'IS NULL' && this.condition != 'IS NOT NULL' && this.condition != 'CONTAINED IN TABLE' && this.condition != 'NOT CONTAINED IN TABLE' && this.condition != 'IN REGEX TABLE' && this.condition != 'NOT IN REGEX TABLE'",
                "val": "",
                "required": true
              },
              "compare_table": {
                "type": "select",
                "Étiquette": "Sélectionner un tableau",
                "bind": "TABLES",
                "empty": "",
                "show_if": "this.condition == 'CONTAINED IN TABLE' || this.condition == 'NOT CONTAINED IN TABLE' || this.condition == 'IN REGEX TABLE' || this.condition == 'NOT IN REGEX TABLE'",
                "required": true
              },
              "compare_column": {
                "type": "select",
                "Étiquette": "Sélectionner une colonne",
                "table": "this.compare_table",
                "bind": "COLUMNS",
                "show_if": "this.condition == 'CONTAINED IN TABLE' || this.condition == 'NOT CONTAINED IN TABLE' || this.condition == 'IN REGEX TABLE' || this.condition == 'NOT IN REGEX TABLE'",
                "required": true
              },
              "delete": {
                "type": "button",
                "Étiquette": "Supprimer",
                "function": "DELETE_ROW"
              }
            }
          },
          "replace_table_filter_option.apply_to_record": {
            "type": "checkbox",
            "Étiquette": "Apply to Record on \"Replace With\" Table",
            "val": "true",
            "isOn": "true",
            "isOff": "false",
            "style": "margin-top: 10px",
            "breakLine": true
          },
          "replace_table_filter_option_apply_to_group_box": {
            "type": "fieldset",
            "Étiquette": "Apply to Group on \"Replace With\" Table",
            "show_if": "replace_table_filter_option.apply_to_record=='false' && (incoming_value_to_replace.type!='replacement_table' && incoming_value_to_replace.type!='regex' && replace_with.value_type=='table')",
            "repeat": false,
            "fields": {
              "replace_table_filter_option.apply_to_group_columns": {
                "type": "select",
                "Étiquette": "Sélectionner les colonnes du groupe",
