{
  "output": {
    "type": "fieldset",
    "label": "Définir le tableau des résultats",
    "show_if": "create_new_table=='true'",
    "repeat": false,
    "fields": {
      "output.custom_text": {
        "type": "text",
        "label": "Sortir le nom du tableau",
        "show_if": "output.custom_options=='false' || (output.custom_options=='true' && output.compose_table_name=='false')",
        "minLength": 1,
        "val": "",
        "width": "100%",
        "breakLine": true
      },
      "output.custom_options": {
        "type": "checkbox",
        "label": "Options de sortie",
        "val": "false",
        "isOn": "true",
        "isOff": "false",
        "breakLine": true
      },
      "output.compose_table_name": {
        "type": "checkbox",
        "label": "Saisissez le nom du tableau",
        "show_if": "output.custom_options=='true'",
        "val": "false",
        "isOn": "true",
        "isOff": "false",
        "breakLine": true
      },
      "compose_table_name_frame": {
        "type": "fieldset",
        "label": "Saisissez le nom du tableau de sortie",
        "show_if": "output.custom_options=='true' && output.compose_table_name=='true'",
        "repeat": false,
        "fields": {
          "output.prefix_rule": {
            "type": "select",
            "label": "Préfixe",
            "show_if": "output.compose_table_name=='true'",
            "val": "empty",
            "options": {
              "empty": {
                "label": "-"
              },
              "script_name": {
                "label": "Nom de la tâche"
              },
              "date_string_type1": {
                "label": "Date : jj_mm_aaaa"
              },
              "date_string_type2": {
                "label": "Date : aaaa_mm_jj"
              },
              "time_string": {
                "label": "Heure : h_min_sec"
              },
              "datetime_string": {
                "label": "Date_heure"
              },
              "custom": {
                "label": "Personnaliser"
              }
            }
          },
          "output.prefix_text": {
            "type": "text",
            "label": "Texte du préfixe",
            "show_if": "output.compose_table_name=='true' && output.prefix_rule=='custom'",
            "minLength": 1,
            "maxLength": 31,
            "val": ""
          },
          "output.core_delimiter": {
            "type": "text",
            "label": "Délimiteur",
            "show_if": "output.compose_table_name=='true' && output.prefix_rule!='empty'",
            "val": "_",
            "width": "50px"
          },
          "output.core_rule": {
            "type": "select",
            "label": "Nom du tableau",
            "show_if": "output.compose_table_name=='true'",
            "val": "input_table_name",
            "options": {
              "input_table_name": {
                "label": "Nom du tableau d'entrée"
              },
              "script_name": {
                "label": "Nom de la tâche"
              },
              "custom": {
                "label": "Personnaliser"
              }
            }
          },
          "output.core_text": {
            "type": "text",
            "label": "Nom du tableau principal",
            "show_if": "output.compose_table_name=='true' && output.core_rule=='custom'",
            "required": true,
            "minLength": 1,
            "maxLength": 31,
            "val": ""
          },
          "output.suffix_delimiter": {
            "type": "text",
            "label": "Délimiteur",
            "show_if": "output.compose_table_name=='true'",
            "val": "_",
            "width": "50px"
          },
          "output.suffix_rule": {
            "type": "select",
            "label": "Suffixe",
            "show_if": "output.compose_table_name=='true'",
            "val": "script_name",
            "options": {
              "empty": {
                "label": "-"
              },
              "script_name": {
                "label": "Nom de la tâche"
              },
              "date_string_type1": {
                "label": "Date : jj_mm_aaaa"
              },
              "date_string_type2": {
                "label": "Date : aaaa_mm_jj"
              },
              "time_string": {
                "label": "Heure : h_min_sec"
              },
              "datetime_string": {
                "label": "Date_heure"
              },
              "custom": {
                "label": "Personnaliser"
              }
            }
          },
          "output.suffix_text": {
            "type": "text",
            "label": "Texte du suffixe",
            "show_if": "output.compose_table_name=='true' && output.suffix_rule=='custom'",
            "minLength": 1,
            "maxLength": 31,
            "val": "",
            "breakLine": true
          },
          "output.timezone": {
            "type": "select",
            "label": "Fuseau horaire",
            "show_if": "output.compose_table_name=='true' && (output.prefix_rule=='date_string_type1' || output.prefix_rule=='date_string_type2' || output.prefix_rule=='time_string' || output.prefix_rule=='datetime_string' || output.suffix_rule=='date_string_type1' || output.suffix_rule=='date_string_type2' || output.suffix_rule=='time_string' || output.suffix_rule=='datetime_string')",
            "val": "Europe/Berlin",
            "required": true,
            "options": {
              "America/Adak": {
                "label": "Adak, Gambier"
              },
              "Australia/Adelaide": {
                "label": "Adelaïde, Broken_Hill, Darwin"
              },
              "America/Anchorage": {
                "label": "Anchorage, Juneau, Sitka"
              },
