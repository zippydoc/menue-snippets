{
  "output": {
    "type": "fieldset",
    "Étiquette": "Définir le tableau des résultats",
    "repeat": false,
    "fields": {
      "output.custom_text": {
        "type": "text",
        "Étiquette": "Sortir le nom du tableau",
        "show_if": "output.compose_table_name=='false'",
        "minLength": 1,
        "val": "",
        "width": "100%",
        "breakLine": true
      },
      "output.select_output_columns": {
        "type": "checkbox",
        "Étiquette": "Sélectionner les colonnes de sortie",
        "show_if": "options.is_generate_holiday_table=='add_column'",
        "val": "false",
        "isOn": "true",
        "isOff": "false"
      },
      "output_columns": {
        "type": "fieldset",
        "Étiquette": "Sélectionner les colonnes de sortie",
        "show_if": "options.is_generate_holiday_table=='add_column' && output.select_output_columns=='true'",
        "repeat": true,
        "fields": {
          "field_name": {
            "type": "select",
            "Étiquette": "Sélectionner une colonne",
            "table": "input_table",
            "bind": "COLUMNS",
            "required": true
          },
          "order": {
            "type": "select",
            "Étiquette": "Commander",
            "options": {
              "-": {
                "Étiquette": "-"
              },
              "asc": {
                "Étiquette": "Asc"
              },
              "desc": {
                "Étiquette": "Desc"
              }
            },
            "val": "-"
          },
          "is_rename": {
            "type": "checkbox",
            "Étiquette": "Renommer",
            "val": "false",
            "isOn": "true",
            "isOff": "false"
          },
          "rename": {
            "show_if": "this.is_rename == 'true'",
            "type": "text",
            "required": true,
            "minLength": 1,
            "maxLength": 255,
            "Étiquette": "Nouveau nom de colonne",
            "style": "margin-left: 10px"
          },
          "format__category": {
            "type": "select",
            "Étiquette": "Format",
            "required": true,
            "options": {
              "standard": {
                "Étiquette": "Standard"
              },
              "number": {
                "Étiquette": "Numéro"
              },
              "percentage": {
                "Étiquette": "Pourcentage"
              },
              "text": {
                "Étiquette": "Texte"
              },
              "date": {
                "Étiquette": "Date"
              },
              "currency": {
                "Étiquette": "Monnaie"
              }
            },
            "val": "standard"
          },
          "format__rounding": {
            "type": "select",
            "Étiquette": "Arrondi",
            "show_if": "this.format__category=='number' || this.format__category=='percentage' || this.format__category=='currency'",
            "required": true,
            "options": {
              "no_rounding": {
                "Étiquette": "Pas d'arrondi"
              },
              "0": {
                "Étiquette": "0 chiffre"
              },
              "1": {
                "Étiquette": "1 chiffre"
              },
              "2": {
                "Étiquette": "2 chiffres"
              },
              "3": {
                "Étiquette": "3 chiffres"
              },
              "4": {
                "Étiquette": "4 chiffres"
              },
              "5": {
                "Étiquette": "5 chiffres"
              },
              "6": {
                "Étiquette": "6 chiffres"
              },
              "7": {
                "Étiquette": "7 chiffres"
              },
              "8": {
                "Étiquette": "8 chiffres"
              },
              "9": {
                "Étiquette": "9 chiffres"
              }
            },
            "val": "2"
          },
          "format__use_1000_separator": {
            "type": "checkbox",
            "Étiquette": "Utilisez le séparateur 1000 (.)",
            "show_if": "this.format__category=='number' || this.format__category=='percentage' || this.format__category=='currency'",
            "val": "true",
            "isOn": "true",
            "isOff": "false"
          },
          "format__currency_symbol": {
            "type": "select",
            "Étiquette": "Symbole",
            "show_if": "this.format__category=='currency'",
            "required": true,
            "options": {
              "dollar": {
                "Étiquette": "$"
              },
              "euro": {
                "Étiquette": "€"
              },
              "pound": {
                "Étiquette": "£"
              }
            },
            "val": "dollar"
          },
          "format__date_format": {
            "type": "select",
            "Étiquette": "Type",
            "show_if": "this.format__category=='date'",
            "required": true,
            "options": {
              "mm/dd/yyyy": {
                "Étiquette": "3/14/2012"
              },
              "D, M dd, Y": {
                "Étiquette": "Mercredi 14 mars 2012"
              },
              "yyyy-mm-dd": {
                "Étiquette": "2012-03-14"
              }
            },
            "val": "mm/dd/yyyy",
            "breakLine": true
          },
          "delete": {
            "type": "button",
            "Étiquette": "Supprimer",
            "function": "DELETE_ROW"
          }
        },
        "breakLine": true,
        "style": "margin-bottom: 20px"
      },
      "output.compose_table_name": {
        "type": "checkbox",
        "Étiquette": "Saisissez le nom du tableau",
        "val": "false",
        "isOn": "true",
        "isOff": "false",
        "breakLine": true
      },
      "compose_table_name_frame": {
        "type": "fieldset",
        "Étiquette": "Saisissez le nom du tableau de sortie",
        "show_if": "output.compose_table_name=='true'",
        "repeat": false,
        "fields": {
          "output.prefix_rule": {
            "type": "select",
            "Étiquette": "Préfixe",
            "show_if": "output.compose_table_name=='true'",
            "val": "empty",
            "options": {
              "empty": {
                "Étiquette": "-"
              },
              "script_name": {
                "Étiquette": "Nom de la tâche"
              },
              "date_string_type1": {
                "Étiquette": "Date : jj_mm_aaaa"
              },
              "date_string_type2": {
                "Étiquette": "Date : aaaa_mm_jj"
              },
              "time_string": {
                "Étiquette": "Heure : h_min_sec"
              },
              "datetime_string": {
                "Étiquette": "Date_heure"
              },
              "custom": {
                "Étiquette": "Personnaliser"
              }
            }
          },
          "output.prefix_text": {
            "type": "text",
            "Étiquette": "Texte du préfixe",
            "show_if": "output.compose_table_name=='true' && output.prefix_rule=='custom'",
            "minLength": 1,
            "maxLength": 31,
            "val": ""
          },
          "output.core_delimiter": {
            "type": "text",
            "Étiquette": "Délimiteur",
            "show_if": "output.compose_table_name=='true' && output.prefix_rule!='empty'",
            "val": "_",
            "width": "50px"
          },
          "output.generate_core_rule": {
            "type": "select",
            "Étiquette": "Nom du tableau",
            "show_if": "output.compose_table_name=='true' && options.is_generate_holiday_table=='generate_table'",
            "val": "script_name",
            "options": {
              "script_name": {
                "Étiquette": "Nom de la tâche"
              },
              "custom": {
                "Étiquette": "Personnaliser"
              }
            }
          },
          "output.core_rule": {
            "type": "select",
            "Étiquette": "Nom du tableau",
            "show_if": "output.compose_table_name=='true' && options.is_generate_holiday_table=='add_column'",
            "val": "input_table_name",
            "options": {
              "input_table_name": {
                "Étiquette": "Nom du tableau d'entrée"
              },
              "script_name": {
                "Étiquette": "Nom de la tâche"
              },
              "custom": {
                "Étiquette": "Personnaliser"
              }
            }
          },
          "output.core_text": {
            "type": "text",
            "Étiquette": "Nom du tableau principal",
            "show_if": "output.compose_table_name=='true' && output.core_rule=='custom'",
            "required": true,
            "minLength": 1,
            "maxLength": 31,
            "val": ""
          },
          "output.suffix_delimiter": {
            "type": "text",
            "Étiquette": "Délimiteur",
            "show_if": "output.compose_table_name=='true'",
            "val": "_",
            "width": "50px"
          },
          "output.suffix_rule": {
            "type": "select",
            "Étiquette": "Suffixe",
            "show_if": "output.compose_table_name=='true'",
            "val": "script_name",
            "options": {
              "empty": {
                "Étiquette": "-"
              },
              "script_name": {
                "Étiquette": "Nom de la tâche"
