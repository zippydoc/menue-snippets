              "Asia/Tehran": {
                "label": "Téhéran, Mashhad, Ispahan, Karaj, Tabriz"
              },
              "Asia/Tokyo": {
                "label": "Tokyo, Séoul, Jayapura"
              },
              "America/New_York": {
                "label": "Toronto, Santiago, New_York"
              },
              "Australia/Sydney": {
                "label": "Vladivostok, Sydney, Brisbane"
              },
              "Asia/Yangon": {
                "label": "Yangon, Cocos,Mandalay, Nay Pyi Taw, Mawlamyine"
              },
              "Indian/Cocos": {
                "label": "Yangon, Cocos, île de l'Ouest"
              }
            },
            "breakLine": true
          }
        }
      }
    }
  }
}