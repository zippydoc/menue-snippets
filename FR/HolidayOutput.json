          "output.suffix_text": {
            "type": "text",
            "Étiquette": "Texte du suffixe",
            "show_if": "output.compose_table_name=='true' && output.suffix_rule=='custom'",
            "minLength": 1,
            "maxLength": 31,
            "val": "",
