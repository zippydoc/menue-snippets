{
  "input": {
    "type": "fieldset",
    "label": "Sélectionner un tableau d'entrée",
    "repeat": false,
    "fields": {
      "left_table.table_id": {
        "type": "select",
        "label": "Sélectionner le tableau de gauche",
        "bind": "TABLES"
      },
      "left_table.remove_input_table_after_processing": {
        "type": "hidden",
        "val": "false"
      },
      "left_table.is_word": {
        "type": "hidden",
        "val": "false",
        "breakLine": true
      },
      "right_table.table_id": {
        "type": "select",
        "label": "Sélectionner",
        "bind": "TABLES"
      },
      "right_table.remove_input_table_after_processing": {
        "type": "hidden",
        "val": "false"
      },
      "right_table.is_word": {
        "type": "hidden",
        "val": "false"
      }
    }
  }
}