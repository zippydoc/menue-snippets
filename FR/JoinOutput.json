{
  "output.standard_ending": {
    "type": "checkbox", 
    "label": "Fin standard", 
    "val": "true", 
    "isOn": "true", 
    "isOff": "false", 
    "breakLine": true
  }, 
  "output.inner_ending": {
    "show_if": "output.standard_ending == 'false'", 
    "type": "text", 
    "required": true, 
    "label": "La fin intérieure", 
    "val": "_j", 
    "style": "margin-left: 10px"
  }, 
  "output.left_ending": {
    "show_if": "output.standard_ending == 'false'", 
    "type": "text", 
    "required": true, 
    "label": "Fin à gauche", 
    "val": "_l", 
    "style": "margin-left: 10px"
  }, 
  "output.right_ending": {
    "show_if": "output.standard_ending == 'false'", 
    "type": "text", 
    "required": true, 
    "label": "La bonne fin", 
    "val": "_r", 
    "style": "margin-left: 10px", 
    "breakLine": true
  }
}
