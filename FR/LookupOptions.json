{
  "options": {
    "type": "fieldset",
    "label": "Sélectionner les options",
    "repeat": false,
    "fields": {
      "options.task_type": {
        "type": "select",
        "label": "Type de tâche",
        "required": true,
        "options": {
          "generate_lookup_table": {
            "label": "Générer un tableau de recherche"
          },
          "apply_lookup_table": {
            "label": "Appliquer le tableau de recherche"
          },
          "apply_regex_table": {
            "label": "Appliquer le tableau d'expressions régulières"
          },
          "apply_lookup_table_to_cells": {
            "label": "Appliquer le tableau de recherche aux cellules"
          },
          "apply_regex_table_to_cells": {
            "label": "Appliquer le tableau d'expressions régulières aux cellules"
          }
        },
        "val": "generate_lookup_table"
      },
      "options.pattern_type": {
        "type": "select",
        "label": "Type de modèle",
        "show_if": "options.task_type=='generate_lookup_table' || options.task_type=='apply_lookup_table' || options.task_type=='apply_lookup_table_to_cells'",
        "required": true,
        "options": {
          "duplicate_pattern": {
            "label": "Pattern de mots"
          },
          "number_range": {
            "label": "Intervalles (p. ex. 10 < x <= 100)"
          }
        },
        "val": "duplicate_pattern"
      },
      "duplicate_option_box": {
        "type": "fieldset",
        "label": "Option de duplication des patterns",
        "show_if": "options.task_type=='label_duplicates' || (options.task_type=='generate_lookup_table' && options.pattern_type=='duplicate_pattern')",
        "repeat": false,
        "fields": {
          "options.minimum_number_of_matching_characters": {
            "type": "text",
            "label": "Nombre minimum de caractères correspondants",
            "val": "3",
            "required": true,
            "style": "min-width: 240px"
          },
          "options.minimum_number_of_duplicates": {
            "type": "text",
            "label": "Nombre minimum de doublons",
            "val": "2",
            "required": true,
            "style": "min-width: 240px",
            "breakLine": true
          }
        }
      },
      "number_range_option_box": {
        "type": "fieldset",
        "label": "Option Fourchettes de numéros",
        "show_if": "options.task_type=='generate_lookup_table' && options.pattern_type=='number_range'",
        "repeat": false,
        "fields": {
          "options.number_range.range_type": {
            "type": "select",
            "label": "Type d'intervalle",
            "required": true,
            "options": {
              "equal_interval": {
                "label": "N intervalles égaux"
              },
              "equal_range": {
                "label": "Égaliser l'intervalle"
              },
              "custom_range": {
                "label": "Intervalle personnalisé"
              }
            },
            "val": "equal_interval"
          },
          "options.number_range.n_value": {
            "type": "text",
            "label": "N",
            "show_if": "options.number_range.range_type=='equal_interval'",
            "val": "3",
            "required": true
          },
          "options.number_range.range_value": {
            "type": "text",
            "label": "Intervalle",
            "show_if": "options.number_range.range_type=='equal_range'",
            "val": "10",
            "required": true
          },
          "options.number_range.custom_ranges": {
            "type": "fieldset",
            "label": "Ajouter un modèle de gamme personnalisé",
            "show_if": "options.number_range.range_type=='custom_range'",
            "repeat": true,
            "fields": {
              "pattern": {
                "type": "text",
                "label": "Pattern",
                "val": "0<=x<10",
                "required": true
              },
              "label": {
                "type": "text",
                "label": "Étiqueter",
                "val": "positive10",
                "required": true
              },
              "delete": {
                "type": "button",
                "label": "Supprimer",
                "function": "DELETE_ROW"
              }
            }
          }
        }
      },
      "options.custom_lookup_table": {
        "type": "checkbox",
        "label": "Table de recherche personnalisée",
        "show_if": "options.task_type=='apply_lookup_table' || options.task_type=='apply_regex_table' || options.task_type=='apply_lookup_table_to_cells' || options.task_type=='apply_regex_table_to_cells'",
        "val": "true",
        "isOn": "true",
        "isOff": "false",
        "breakLine": true
      },
      "options.lookup_table_type": {
        "type": "select",
        "label": "Type de table de recherche",
        "show_if": "options.task_type=='apply_lookup_table' || options.task_type=='apply_regex_table' || options.task_type=='apply_lookup_table_to_cells' || options.task_type=='apply_regex_table_to_cells'",
        "required": true,
        "options": {
          "select_lookup_table": {
            "label": "Sélectionner la table de recherche"
          },
          "custom_lookup_table": {
            "label": "Table de recherche personnalisée"
          }
        },
        "val": "select_lookup_table"
      },
      "lookup_table_frame": {
        "type": "fieldset",
        "label": "",
        "show_if": "options.task_type=='apply_lookup_table' || options.task_type=='apply_regex_table' || options.task_type=='apply_lookup_table_to_cells' || options.task_type=='apply_regex_table_to_cells'",
        "repeat": false,
        "fields": {
          "options.lookup_table_id": {
            "type": "select",
            "label": "Tableau de recherche",
            "show_if": "options.custom_lookup_table=='false'",
            "bind": "TABLES",
            "empty": "",
            "required": true,
            "breakLine": true
          },
          "options.custom_lookup_table": {
            "type": "fieldset",
            "label": "Table de recherche personnalisée",
            "show_if": "options.custom_lookup_table=='true'",
            "repeat": true,
            "fields": {
              "lookup": {
                "type": "text",
                "label": "Détecter",
                "placeholder": "10 < x <= 100",
                "val": "",
                "required": true
              },
              "label": {
                "type": "text",
                "label": "Étiqueter",
                "placeholder": "10 < x <= 100",
                "val": "",
                "required": true
              },
              "delete": {
                "type": "button",
                "label": "Supprimer",
                "function": "DELETE_ROW"
              }
            }
          },
          "options.partial_match": {
            "type": "checkbox",
            "label": "Correspondance partielle",
            "show_if": "options.pattern_type=='duplicate_pattern' && (options.task_type=='apply_lookup_table' || options.task_type=='apply_lookup_table_to_cells')",
            "val": "false",
            "isOn": "true",
            "isOff": "false",
            "style": "margin-top: 10px",
            "breakLine": true
          },
          "options.case_sensitive": {
            "type": "checkbox",
            "label": "Respecter la casse",
            "show_if": "options.pattern_type=='duplicate_pattern' && (options.task_type=='apply_lookup_table' || options.task_type=='apply_lookup_table_to_cells')",
            "val": "false",
            "isOn": "true",
            "isOff": "false",
            "breakLine": true
          },
          "options.use_delimiter": {
            "type": "checkbox",
            "label": "Personnaliser délimiteur",
            "show_if": "options.task_type=='apply_lookup_table' || options.task_type=='apply_lookup_table_to_cells'",
            "val": "false",
            "isOn": "true",
            "isOff": "false",
            "style": "margin-top: 10px"
          },
          "options.delimiter": {
            "type": "text",
            "label": "Délimiteur d'entrée",
            "show_if": "options.use_delimiter=='true' && (options.task_type=='apply_lookup_table' || options.task_type=='apply_lookup_table_to_cells')",
            "val": ";",
            "required": true,
            "breakLine": true
          }
        }
      },
      "options.input_table.use_filter": {
        "type": "checkbox",
        "label": "Utiliser le filtre pour le tableau d'entrée",
        "show_if": "options.task_type=='apply_lookup_table' || options.task_type=='apply_regex_table' || options.task_type=='apply_lookup_table_to_cells' || options.task_type=='apply_regex_table_to_cells'",
        "val": "false",
        "isOn": "true",
        "isOff": "false",
        "style": "margin-top: 10px",
        "breakLine": true
      },
      "input_table_filter_box": {
        "type": "fieldset",
        "label": "Options de filtrage des tableaux d'entrée",
        "show_if": "options.input_table.use_filter=='true' && (options.task_type=='apply_lookup_table' || options.task_type=='apply_regex_table' || options.task_type=='apply_lookup_table_to_cells' || options.task_type=='apply_regex_table_to_cells')",
        "repeat": false,
        "fields": {
          "options.input_table.filter": {
            "type": "fieldset",
            "label": "Ajouter des filtres pour le tableau d'entrée",
            "repeat": true,
            "fields": {
              "initial_op": {
                "type": "select",
                "label": "Sélectionner l'opérateur logique",
                "required": true,
                "show_if": "$index == 0",
                "options": {
                  "if": {
                    "label": "SI"
                  },
                  "if_not": {
                    "label": "SINON"
                  }
                },
                "val": "if"
              },
              "op": {
                "type": "select",
                "label": "Sélectionner l'opérateur logique",
                "required": true,
                "show_if": "$index > 0",
                "options": {
                  "and": {
                    "label": "ET"
                  },
                  "or": {
                    "label": "OU"
                  },
                  "and_not": {
                    "label": "ET NON"
                  },
                  "or_not": {
                    "label": "OU NON"
                  }
                },
                "val": "and"
              },
              "column": {
                "type": "select",
                "label": "Sélectionner une colonne",
                "table": "input_table",
                "bind": "COLUMNS",
                "required": true
              },
              "condition": {
                "type": "select",
                "label": "Sélectionner un opérateur",
                "required": true,
                "options": {
                  "CONTAINS": {
                    "label": "CONTIENT"
                  },
                  "CONTAINED IN STRING": {
                    "label": "CONTENU DANS"
                  },
                  "STARTS WITH": {
                    "label": "COMMENCE AVEC"
                  },
                  "ENDS WITH": {
                    "label": "SE TERMINE PAR"
                  },
                  "=": {
                    "label": "ÉGAL"
                  },
                  ">": {
                    "label": "SUPÉRIEUR À"
                  },
                  ">=": {
                    "label": "SUPÉRIEUR OU ÉGAL"
                  },
                  "<": {
                    "label": "INFÉRIEUR À"
                  },
                  "<=": {
                    "label": "INFÉRIEUR OU ÉGAL"
                  },
                  "EMPTY": {
                    "label": "EST VIDE"
                  },
                  "BLANK": {
                    "label": "EST BLANC"
                  },
                  "IS NULL": {
                    "label": "EST NUL"
                  },
                  "IS TRUE": {
                    "label": "EST VRAI"
                  },
                  "IS FALSE": {
                    "label": "EST FAUX"
                  },
                  "REGEX MATCH": {
                    "label": "CORRESPONDANCE EXPRESSION RÉGULIÈRE"
                  },
                  "CONTAINED IN TABLE": {
                    "label": "EN TABLE"
                  },
                  "IN REGEX TABLE": {
                    "label": "DANS LE TABLEAU D'EXPRESSIONS RÉGULIÈRES"
                  },
                  "DATE": {
                    "label": "DATE"
                  }
                },
                "val": "CONTAINS"
              },
              "date_condition": {
                "type": "select",
                "label": "Sélectionner l'opérateur de date",
                "required": true,
                "show_if": "this.condition == 'DATE'",
                "options": {
                  "<": {
                    "label": "AVANT"
                  },
                  "<=": {
                    "label": "AVANT OU ÉGAL"
                  },
                  "=": {
                    "label": "ÉGAL"
                  },
                  ">": {
                    "label": "APRÈS"
                  },
                  ">=": {
                    "label": "APRÈS OU ÉGAL"
                  }
                },
                "val": "="
              },
              "compare_with": {
                "type": "select",
                "label": "Compare with",
                "required": true,
                "show_if": "this.condition=='CONTAINS' || this.condition=='CONTAINED IN STRING' || this.condition=='STARTS WITH' || this.condition=='ENDS WITH' || this.condition=='=' || this.condition=='>' || this.condition=='>=' || this.condition=='<' || this.condition=='<='",
                "options": {
                  "custom_value": {
                    "label": "Custom value"
                  },
                  "column_value": {
                    "label": "Column value"
                  }
                },
                "val": "custom_value"
              },
              "self_compare_column": {
                "type": "select",
                "label": "Select Compare Column",
                "table": "table_id",
                "bind": "COLUMNS",
                "show_if": "this.compare_with=='column_value' && (this.condition=='CONTAINS' || this.condition=='CONTAINED IN STRING' || this.condition=='STARTS WITH' || this.condition=='ENDS WITH' || this.condition=='=' || this.condition=='>' || this.condition=='>=' || this.condition=='<' || this.condition=='<=')",
                "required": true
              },
              "date_value": {
                "type": "date",
                "label": "Sélectionner une date",
                "show_if": "this.condition=='DATE'",
                "val": "",
                "required": true
              },
              "value": {
                "type": "text",
                "label": "Valeur d'entrée",
                "show_if": "this.compare_with=='custom_value' && this.condition != 'DATE' && this.condition != 'EMPTY' && this.condition != 'NOT EMPTY' && this.condition != 'BLANK' && this.condition != 'NOT BLANK' && this.condition != 'IS NULL' && this.condition != 'IS NOT NULL' && this.condition != 'IS TRUE' && this.condition != 'IS FALSE' && this.condition != 'CONTAINED IN TABLE' && this.condition != 'NOT CONTAINED IN TABLE' && this.condition != 'IN REGEX TABLE' && this.condition != 'NOT IN REGEX TABLE'",
                "val": "",
                "required": true
              },
              "case_sensitive": {
                "type": "select",
                "label": "Sélectionner la sensibilité à la casse",
                "required": true,
                "show_if": "this.condition == 'CONTAINS' || this.condition == 'CONTAINED IN STRING' || this.condition == '=' || this.condition == 'STARTS WITH' || this.condition == 'ENDS WITH' || this.condition == 'REGEX MATCH' || this.condition == 'NOT REGEX MATCH' || this.condition == 'IN REGEX TABLE' || this.condition == 'NOT IN REGEX TABLE'",
                "options": {
                  "yes": {
                    "label": "Respecter la casse"
                  },
                  "no": {
                    "label": "Ignorer la casse"
                  }
                },
                "val": "yes"
              },
              "compare_table": {
                "type": "select",
                "label": "Sélectionner un tableau",
                "bind": "TABLES",
                "empty": "",
                "show_if": "this.condition == 'CONTAINED IN TABLE' || this.condition == 'NOT CONTAINED IN TABLE' || this.condition == 'IN REGEX TABLE' || this.condition == 'NOT IN REGEX TABLE'",
                "required": true
              },
              "compare_column": {
                "type": "select",
                "label": "Sélectionner une colonne",
                "table": "this.compare_table",
                "bind": "COLUMNS",
                "show_if": "this.condition == 'CONTAINED IN TABLE' || this.condition == 'NOT CONTAINED IN TABLE' || this.condition == 'IN REGEX TABLE' || this.condition == 'NOT IN REGEX TABLE'",
                "required": true
              },
              "delete": {
                "type": "button",
                "label": "Supprimer",
                "function": "DELETE_ROW"
              }
            }
          },
          "options.input_table.apply_to_record": {
            "type": "checkbox",
            "label": "Appliquer au registre pour le filtre de tableau d'entrée",
            "val": "true",
            "isOn": "true",
            "isOff": "false",
            "style": "margin-top: 10px",
            "breakLine": true
          },
          "input_table_apply_to_group_box": {
            "type": "fieldset",
            "label": "Appliquer au groupe des tableaux d'entrée",
