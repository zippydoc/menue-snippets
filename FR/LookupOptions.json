            "repeat": false,
            "fields": {
              "options.input_table.apply_to_group_columns": {
                "type": "select",
                "label": "Sélectionner les colonnes du groupe",
                "required": true,
                "width": "100%",
                "table": "input_table",
                "multiple": true,
                "bind": "COLUMNS",
                "breakLine": true
              }
            }
          }
        }
      },
