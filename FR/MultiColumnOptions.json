{
  "options_box": {
    "type": "fieldset",
    "Étiquette": "Options",
    "repeat": false,
    "fields": {
      "options.task": {
        "type": "hidden",
        "val": "multi_column"
      },
      "options.add_columns": {
        "type": "fieldset",
        "Étiquette": "Ajouter des colonnes",
        "repeat": true,
        "fields": {
          "new_column_name": {
            "type": "text",
            "Étiquette": "Nouveau nom de colonne",
            "val": "",
            "breakLine": true
          },
          "multi_column_functions__function": {
            "type": "select",
            "Étiquette": "Fonction",
            "required": true,
            "options": {
              "sum": {
                "Étiquette": "Somme"
              },
              "average": {
                "Étiquette": "Moyenne"
              },
              "median": {
                "Étiquette": "Médiane"
              },
              "max": {
                "Étiquette": "Max"
              },
              "min": {
                "Étiquette": "Min"
              },
              "standard_deviation": {
                "Étiquette": "Écart type"
              },
              "product": {
                "Étiquette": "Produit"
              },
              "quota": {
                "Étiquette": "Quota"
              },
              "absolute_growth": {
                "Étiquette": "Croissance absolue"
              },
              "relative_growth": {
                "Étiquette": "Croissance relative"
              },
              "compound_growth": {
                "Étiquette": "Croissance composée"
              },
              "static_value": {
                "Étiquette": "Valeur statique"
              },
              "numeric_formula": {
                "Étiquette": "Formule numérique"
              },
              "text_formula": {
                "Étiquette": "Formule du texte"
              }
            },
            "val": "sum"
          },
          "multi_column_functions__sel_columns": {
            "type": "select",
            "Étiquette": "Sélectionner Colonne(s)",
            "show_if": "this.multi_column_functions__function=='sum' || this.multi_column_functions__function=='average' || this.multi_column_functions__function=='median' || this.multi_column_functions__function=='max' || this.multi_column_functions__function=='min' || this.multi_column_functions__function=='standard_deviation' || this.multi_column_functions__function=='product'",
            "table": "input_table",
            "bind": "COLUMNS",
            "multiple": true,
            "required": true
          },
          "multi_column_functions__column_a": {
            "type": "select",
            "Étiquette": "Sélectionner la colonne A",
            "show_if": "this.multi_column_functions__function=='quota' || this.multi_column_functions__function=='absolute_growth' || this.multi_column_functions__function=='relative_growth' || this.multi_column_functions__function=='compound_growth'",
            "table": "input_table",
            "bind": "COLUMNS",
            "required": true
          },
          "multi_column_functions__column_b": {
            "type": "select",
            "Étiquette": "Sélectionner la colonne B",
            "show_if": "this.multi_column_functions__function=='quota' || this.multi_column_functions__function=='absolute_growth' || this.multi_column_functions__function=='relative_growth' || this.multi_column_functions__function=='compound_growth'",
            "table": "input_table",
            "bind": "COLUMNS",
            "required": true
          },
          "multi_column_functions__n_value": {
            "type": "text",
            "Étiquette": "N (nombre entier positif)",
            "show_if": "this.multi_column_functions__function=='compound_growth'",
            "val": "1",
            "required": true
          },
          "static_value": {
            "type": "text",
            "Étiquette": "Valeur d'entrée",
            "show_if": "this.multi_column_functions__function=='static_value'",
            "val": "",
            "width": "100%"
          },
          "numeric_formula": {
            "type": "text",
            "Étiquette": "Formule d'entrée, par exemple [Column1] + 2 * ([Row-1:Column3] - [Column1])",
            "show_if": "this.multi_column_functions__function=='numeric_formula'",
            "val": "",
            "width": "100%"
          },
          "text_formula": {
            "type": "text",
            "Étiquette": "Formule d'entrée, par exemple [firstName] + ' ' + [lastName] + ' ' + [Row-1:Column3]".,
            "show_if": "this.multi_column_functions__function=='text_formula'",
            "val": "",
            "width": "100%"
          },
          "use_multi_row": {
            "type": "checkbox",
            "Étiquette": "Options à plusieurs rangées",
            "show_if": "this.multi_column_functions__function=='numeric_formula' || this.multi_column_functions__function=='text_formula'",
            "val": "false",
            "isOn": "true",
            "isOff": "false"
          },
          "if_row_not_exist": {
            "type": "select",
            "Étiquette": "Si ligne désignée inexistante",
            "show_if": "this.use_multi_row=='true' && (this.multi_column_functions__function=='numeric_formula' || this.multi_column_functions__function=='text_formula')",
            "required": true,
            "options": {
              "empty": {
                "Étiquette": "Vide ou 0"
              },
              "closest": {
                "Étiquette": "Valeur de la ligne la plus proche"
              }
            },
            "val": "empty",
            "breakLine": true
          },
          "format__category": {
            "type": "select",
            "Étiquette": "Format",
            "required": true,
            "options": {
              "standard": {
                "Étiquette": "Standard"
              },
              "number": {
                "Étiquette": "Numéro"
              },
              "percentage": {
                "Étiquette": "Pourcentage"
              },
              "text": {
                "Étiquette": "Texte"
              },
              "date": {
                "Étiquette": "Date"
              },
              "currency": {
                "Étiquette": "Monnaie"
              }
            },
            "val": "standard"
          },
          "format__rounding": {
            "type": "select",
            "Étiquette": "Arrondi",
            "show_if": "this.format__category=='number' || this.format__category=='percentage' || this.format__category=='currency'",
            "required": true,
            "options": {
              "no_rounding": {
                "Étiquette": "Pas d'arrondi"
              },
              "0": {
                "Étiquette": "0 chiffre"
              },
              "1": {
                "Étiquette": "1 chiffre"
              },
              "2": {
                "Étiquette": "2 chiffres"
              },
              "3": {
                "Étiquette": "3 chiffres"
              },
              "4": {
                "Étiquette": "4 chiffres"
              },
              "5": {
                "Étiquette": "5 chiffres"
              },
              "6": {
                "Étiquette": "6 chiffres"
              },
              "7": {
                "Étiquette": "7 chiffres"
              },
              "8": {
                "Étiquette": "8 chiffres"
              },
              "9": {
                "Étiquette": "9 chiffres"
              }
            },
            "val": "2"
          },
          "format__use_1000_separator": {
            "type": "checkbox",
            "Étiquette": "Use 1000 Separator(.)",
            "show_if": "this.format__category=='number' || this.format__category=='percentage' || this.format__category=='currency'",
            "val": "true",
            "isOn": "true",
            "isOff": "false"
          },
          "format__currency_symbol": {
            "type": "select",
            "Étiquette": "Symbole",
            "show_if": "this.format__category=='currency'",
            "required": true,
            "options": {
              "dollar": {
                "Étiquette": "$"
              },
              "euro": {
                "Étiquette": "€"
              },
              "pound": {
                "Étiquette": "£"
              }
            },
            "val": "dollar"
          },
          "format__date_format": {
            "type": "select",
            "Étiquette": "Type",
            "show_if": "this.format__category=='date'",
            "required": true,
            "options": {
              "mm/dd/yyyy": {
                "Étiquette": "3/14/2012"
              },
              "D, M dd, Y": {
                "Étiquette": "Mercredi 14 mars 2012"
              },
              "yyyy-mm-dd": {
                "Étiquette": "2012-03-14"
              }
            },
            "val": "mm/dd/yyyy",
            "breakLine": true
          },
          "group_counter__inc_dec_curly_bracket": {
            "type": "checkbox",
            "Étiquette": "Augmenter/diminuer avec {}",
            "show_if": "this.task_type=='group_counter'",
            "val": "false",
            "isOn": "true",
            "isOff": "false",
            "breakLine": true
          },
          "group_counter__inc_dec_square_bracket": {
            "type": "checkbox",
            "Étiquette": "Augmenter/diminuer avec []",
            "show_if": "this.task_type=='group_counter'",
            "val": "false",
            "isOn": "true",
            "isOff": "false",
            "breakLine": true
          },
          "group_counter__inc_dec_parenthesis": {
            "type": "checkbox",
            "Étiquette": "Augmenter/diminuer avec ()",
            "show_if": "this.task_type=='group_counter'",
            "val": "false",
            "isOn": "true",
            "isOff": "false",
            "breakLine": true
          },
          "group_counter__custom": {
            "type": "checkbox",
            "Étiquette": "Compteur de groupe personnalisé",
            "show_if": "this.task_type=='group_counter'",
            "val": "false",
            "isOn": "true",
            "isOff": "false",
            "breakLine": true
          },
          "group_counter__column": {
            "type": "select",
            "Étiquette": "Colonne",
            "show_if": "this.task_type=='group_counter' && this.group_counter__custom=='true'",
            "table": "input_table",
            "bind": "COLUMNS",
            "required": true
          },
          "group_counter__type": {
            "type": "select",
            "Étiquette": "Quand",
            "show_if": "this.task_type=='group_counter' && this.group_counter__custom=='true'",
            "required": true,
            "options": {
              "starts_with": {
                "Étiquette": "COMMENCE AVEC"
              },
              "ends_with": {
                "Étiquette": "SE TERMINE PAR"
              },
              "starts_with_regex": {
                "Étiquette": "COMMENCE AVEC L'EXPRESSION RÉGULIÈRE"
              },
              "ends_with_regex": {
                "Étiquette": "SE TERMINE PAR EXPRESSION RÉGULIÈRE"
              }
            },
            "val": "starts_with"
          },
          "group_counter__value": {
            "type": "text",
            "Étiquette": "Valeur d'entrée",
            "show_if": "this.task_type=='group_counter' && this.group_counter__custom=='true'",
            "val": ".",
            "breakLine": true
          },
          "group_counter__label": {
            "type": "text",
            "Étiquette": "Étiquette d'entrée",
            "show_if": "this.task_type=='group_counter' && this.group_counter__custom=='true'",
            "val": "sentence",
            "breakLine": true
          },
          "group_counter__if_end_of_string_start_labeling_with_next_record": {
            "type": "checkbox",
            "Étiquette": "Si fin de chaîne, commencer à étiqueter avec le registre suivant",
            "show_if": "this.task_type=='group_counter' && this.group_counter__custom=='true' && (this.group_counter__type=='starts_with' || this.group_counter__type=='starts_with_regex')",
            "val": "true",
            "isOn": "true",
            "isOff": "false"
          },
          "group_counter__if_begin_of_string_start_labeling_with_previous_record": {
            "type": "checkbox",
            "Étiquette": "Si début de chaîne, commencer à étiqueter avec le disque précédent",
            "show_if": "this.task_type=='group_counter' && this.group_counter__custom=='true' && (this.group_counter__type=='ends_with' || this.group_counter__type=='ends_with_regex')",
            "val": "true",
            "isOn": "true",
            "isOff": "false"
          },
          "group_counter__always_include": {
            "type": "checkbox",
            "Étiquette": "Toujours inclure",
            "show_if": "this.task_type=='group_counter' && this.group_counter__custom=='true'",
            "val": "true",
            "isOn": "true",
            "isOff": "false"
          },
          "apply_to_group__enabled": {
            "type": "checkbox",
            "Étiquette": "Appliquer au groupe",
            "show_if": "this.multi_column_functions__function!='static_value'",
            "val": "false",
            "isOn": "true",
            "isOff": "false"
          },
          "apply_to_group__group_columns": {
            "type": "select",
            "Étiquette": "Colonne(s) de groupe",
            "show_if": "this.apply_to_group__enabled == 'true' && this.multi_column_functions__function!='static_value'",
            "table": "input_table",
            "bind": "COLUMNS",
            "multiple": true,
            "required": true
          },
          "delete": {
            "type": "button",
            "Étiquette": "Supprimer",
            "function": "DELETE_ROW"
          }
        }
      },
      "options.use_filter": {
        "type": "checkbox",
        "Étiquette": "Utiliser un filtre",
        "val": "false",
        "isOn": "true",
        "isOff": "false",
        "style": "margin-top: 10px",
        "breakLine": true
      },
      "filter_box": {
        "type": "fieldset",
        "Étiquette": "Options de filtrage",
        "show_if": "options.use_filter=='true'",
        "repeat": false,
        "fields": {
          "options.filter": {
            "type": "fieldset",
            "Étiquette": "Ajouter des filtres",
            "repeat": true,
            "fields": {
              "initial_op": {
                "type": "select",
                "Étiquette": "Sélectionner l'opérateur logique",
                "required": true,
                "show_if" :  "$index == 0",
                "options": {
                  "if": {
                    "Étiquette": "SI"
                  },
                  "if_not": {
                    "Étiquette": "SINON"
                  }
                },
                "val": "if"
              },
              "op": {
                "type": "select",
                "Étiquette": "Sélectionner l'opérateur logique",
                "required": true,
                "show_if" :  "$index > 0",
                "options": {
                  "and": {
                    "Étiquette": "ET"
                  },
                  "or": {
                    "Étiquette": "OU"
                  },
                  "and_not": {
                    "Étiquette": "ET NON"
                  },
                  "or_not": {
                    "Étiquette": "OU NON"
                  }
                },
                "val": "and"
              },
              "column": {
                "type": "select",
                "Étiquette": "Sélectionner une colonne",
                "table": "input_table",
                "bind": "COLUMNS",
                "required": true
              },
              "condition": {
                "type": "select",
                "Étiquette": "Sélectionner un opérateur",
                "required": true,
                "options": {
                  "CONTIENT": {
                    "Étiquette": "CONTIENT"
                  },
                  "CONTAINED IN STRING": {
                    "Étiquette": "CONTENU DANS"
                  },
                  "COMMENCE AVEC": {
                    "Étiquette": "COMMENCE AVEC"
                  },
                  "SE TERMINE PAR": {
                    "Étiquette": "SE TERMINE PAR"
                  },
                  "=": {
                    "Étiquette": "ÉGAL"
                  },
                  ">": {
                    "Étiquette": "SUPÉRIEUR À"
                  },
                  ">=": {
                    "Étiquette": "SUPÉRIEUR OU ÉGAL"
                  },
                  "<": {
                    "Étiquette": "INFÉRIEUR À"
                  },
                  "<=": {
                    "Étiquette": "INFÉRIEUR OU ÉGAL"
                  },
                  "EMPTY": {
                    "Étiquette": "EST VIDE"
                  },
                  "BLANK": {
                    "Étiquette": "EST BLANC"
                  },
                  "EST NUL": {
                    "Étiquette": "EST NUL"
                  },
                  "CORRESPONDANCE EXPRESSION RÉGULIÈRE": {
                    "Étiquette": "CORRESPONDANCE EXPRESSION RÉGULIÈRE"
                  },
                  "CONTAINED IN TABLE": {
                    "Étiquette": "EN TABLE"
                  },
                  "DANS LE TABLEAU D'EXPRESSIONS RÉGULIÈRES": {
                    "Étiquette": "DANS LE TABLEAU D'EXPRESSIONS RÉGULIÈRES"
                  },
                  "DATE": {
                    "Étiquette": "DATE"
                  }
                },
                "val": "CONTIENT"
              },
              "date_condition": {
                "type": "select",
                "Étiquette": "Sélectionner l'opérateur de date",
                "required": true,
                "show_if": "this.condition == 'DATE'",
                "options": {
                  "<": {
                    "Étiquette": "AVANT"
                  },
                  "<=": {
                    "Étiquette": "AVANT OU ÉGAL"
                  },
                  "=": {
                    "Étiquette": "ÉGAL"
                  },
                  ">": {
                    "Étiquette": "APRÈS"
                  },
                  ">=": {
                    "Étiquette": "APRÈS OU ÉGAL"
                  }
                },
                "val": "="
              },
              "case_sensitive": {
                "type": "select",
                "Étiquette": "Sélectionner la sensibilité à la casse",
                "required": true,
                "show_if": "this.condition == 'CONTAINS' || this.condition == 'CONTAINED IN STRING' || this.condition == '=' || this.condition == 'STARTS WITH' || this.condition == 'ENDS WITH' || this.condition == 'REGEX MATCH' || this.condition == 'NOT REGEX MATCH' || this.condition == 'IN REGEX TABLE' || this.condition == 'NOT IN REGEX TABLE'",
                "options": {
                  "yes": {
                    "Étiquette": "Respecter la casse"
                  },
                  "no": {
                    "Étiquette": "Ignorer la casse"
                  }
                },
                "val": "yes"
              },
              "date_value": {
                "type": "date",
                "Étiquette": "Sélectionner une date",
                "show_if": "this.condition=='DATE'",
                "val": "",
                "required": true
              },
              "value": {
                "type": "text",
                "Étiquette": "Valeur d'entrée",
                "show_if": "this.condition != 'DATE' && this.condition != 'EMPTY' && this.condition != 'NOT EMPTY' && this.condition != 'BLANK' && this.condition != 'NOT BLANK' && this.condition != 'IS NULL' && this.condition != 'IS NOT NULL' && this.condition != 'CONTAINED IN TABLE' && this.condition != 'NOT CONTAINED IN TABLE' && this.condition != 'IN REGEX TABLE' && this.condition != 'NOT IN REGEX TABLE'",
                "val": "",
                "required": true
              },
              "compare_table": {
                "type": "select",
                "Étiquette": "Sélectionner un tableau",
                "bind": "TABLES",
                "empty": "",
                "show_if": "this.condition == 'CONTAINED IN TABLE' || this.condition == 'NOT CONTAINED IN TABLE' || this.condition == 'IN REGEX TABLE' || this.condition == 'NOT IN REGEX TABLE'",
                "required": true
              },
              "compare_column": {
                "type": "select",
                "Étiquette": "Sélectionner une colonne",
                "table": "this.compare_table",
                "bind": "COLUMNS",
                "show_if": "this.condition == 'CONTAINED IN TABLE' || this.condition == 'NOT CONTAINED IN TABLE' || this.condition == 'IN REGEX TABLE' || this.condition == 'NOT IN REGEX TABLE'",
                "required": true
              },
              "delete": {
                "type": "button",
                "Étiquette": "Supprimer",
                "function": "DELETE_ROW"
              }
            }
          },
          "options.apply_to_record": {
            "type": "checkbox",
            "Étiquette": "Appliquer au registre",
            "val": "true",
            "isOn": "true",
            "isOff": "false",
            "style": "margin-top: 10px",
            "breakLine": true
          },
          "apply_to_group_box": {
            "type": "fieldset",
            "Étiquette": "Appliquer au groupe",
            "show_if": "options.apply_to_record=='false'",
            "repeat": false,
            "fields": {
              "options.apply_to_group_columns": {
                "type": "select",
                "Étiquette": "Sélectionner les colonnes du groupe",
                "required": true,
                "width": "100%",
                "table": "input_table",
                "multiple": true,
                "bind": "COLUMNS",
                "breakLine": true
              }
            }
          }
        }
      }
    }
  }
}