{
  "output.select_output_columns": {
    "type": "checkbox",
    "label": "Sélectionner les colonnes de sortie",
    "show_if": "options.task_symbol=='periodicy_by_table' && output.custom_options=='true'",
    "val": "false",
    "isOn": "true",
    "isOff": "false"
  },
  "output_columns": {
    "type": "fieldset",
    "label": "Sélectionnez OutputColumns",
    "show_if": "options.task_symbol=='periodicy_by_table' && output.custom_options=='true' && output.select_output_columns=='true'",
    "repeat": true,
    "fields": {
      "field_name": {
        "type": "select",
        "label": "Sélectionner une colonne",
        "table": "table_id",
        "bind": "COLUMNS",
        "required": true
      },
      "order": {
        "type": "select",
        "label": "Commander",
        "options": {
          "-": {
            "label": "-"
          },
          "asc": {
            "label": "Asc"
          },
          "desc": {
            "label": "Desc"
          }
        },
        "val": "-"
      },
      "is_rename": {
        "type": "checkbox",
        "label": "Renommer",
        "val": "false",
        "isOn": "true",
        "isOff": "false"
      },
      "rename": {
        "show_if": "this.is_rename == 'true'",
        "type": "text",
        "required": true,
        "minLength": 1,
        "maxLength": 255,
        "label": "Nouveau nom de colonne",
        "style": "margin-left: 10px"
      },
      "format__category": {
        "type": "select",
        "label": "Format d'affichage",
        "required": true,
        "options": {
          "standard": {
            "label": "Standard"
          },
          "number": {
            "label": "Numéro"
          },
          "percentage": {
            "label": "Pourcentage"
          },
          "text": {
            "label": "Texte"
          },
          "date": {
            "label": "Date"
          },
          "currency": {
            "label": "Monnaie"
          }
        },
        "val": "standard"
      },
      "format__rounding": {
        "type": "select",
        "label": "Places décimales pour l'affichage",
        "show_if": "this.format__category=='number' || this.format__category=='percentage' || this.format__category=='currency'",
        "required": true,
        "options": {
          "no_rounding": {
            "label": "Pas d'arrondi"
          },
          "0": {
            "label": "0 chiffre"
          },
          "1": {
            "label": "1 chiffre"
          },
          "2": {
            "label": "2 chiffres"
          },
          "3": {
            "label": "3 chiffres"
          },
          "4": {
            "label": "4 chiffres"
          },
          "5": {
            "label": "5 chiffres"
          },
          "6": {
            "label": "6 chiffres"
          },
          "7": {
            "label": "7 chiffres"
          },
          "8": {
            "label": "8 chiffres"
          },
          "9": {
            "label": "9 chiffres"
          }
        },
        "val": "2"
      },
      "format__use_1000_separator": {
        "type": "checkbox",
        "label": "Utilisez le séparateur 1000(.)",
        "show_if": "this.format__category=='number' || this.format__category=='percentage' || this.format__category=='currency'",
        "val": "true",
        "isOn": "true",
        "isOff": "false"
      },
      "format__currency_symbol": {
        "type": "select",
        "label": "Symbole",
        "show_if": "this.format__category=='currency'",
        "required": true,
        "options": {
          "dollar": {
            "label": "$"
          },
          "euro": {
            "label": "€"
          },
          "pound": {
            "label": "£"
          },
          "yen": {
            "label": "¥ (Yen, currency of Japan)"
          },
          "colon": {
            "label": "₡ (Colón, currency of Costa Rica)"
          },
          "naira": {
            "label": "₦ (Naira, currency of Nigeria)"
          },
          "rupee": {
            "label": "₨ (Rupee, currency of India)"
          },
          "rupie": {
            "label": "₹ (Rupee, currency of India)"
          },
          "won": {
            "label": "₩ (Won, currency in South Korea and North Korea)"
          },
          "hebrew": {
            "label": "₪ (Currency of Israel)"
          },
          "dong": {
            "label": "₫ (Currency of Vietnam)"
          },
          "kip": {
            "label": "₭ (Kip, currency of Laos)"
          },
          "mongolia": {
            "label": "₮ (Currency of Mongolia)"
          },
          "peso": {
            "label": "₱ (Peso, currency of the Philippines)"
          },
          "guarani": {
            "label": "₲ (Guaraní, currency of Paraguay)"
          },
          "hryvnia": {
            "label": "₴ (Hryvnia, currency of Ukraine)"
          },
          "tenge": {
            "label": "₸ (Tenge, currency of Kazakhstan)"
          },
          "lira": {
            "label": "₺ (Lira, currency of Turkey)"
          },
          "manat": {
            "label": "₼ (Manat, currency of Azerbaijan)"
          },
          "rubles": {
            "label": "₽ (Rubles, currency of Russia)"
          },
          "lari": {
            "label": "₾ (Lari, currency of Georgia)"
          },
          "bitcoin": {
            "label": "₿ (Bitcoin, crypto currency)"
          }
        },
        "val": "dollar"
      },
      "format__date_format": {
        "type": "select",
        "label": "Type",
        "show_if": "this.format__category=='date'",
        "required": true,
        "options": {
          "mm/dd/yyyy": {
            "label": "3/14/2012"
          },
          "D, M dd, Y": {
            "label": "Mercredi 14 mars 2012"
          },
          "yyyy-mm-dd": {
            "label": "2012-03-14"
          },
          "m.dd.yyyy": {
            "label": "3.14.2012"
          },
          "m.dd.yy": {
            "label": "3.14.12"
          },
          "dd.m.yyyy": {
            "label": "14.3.2012"
          },
          "dd.m.yy": {
            "label": "14.3.12"
          },
          "M dd, Y": {
            "label": "14 mars 2012"
          },
          "dd-mm-yyyy": {
            "label": "14-03-2012"
          },
          "mm-dd-yyyy": {
            "label": "03-14-2012"
          },
          "dd-mm-yy": {
            "label": "14-03-12"
          },
          "mm-dd-yy": {
            "label": "03-14-12"
          }
        },
        "val": "mm/dd/yyyy",
        "breakLine": true
      },
      "delete": {
        "type": "button",
        "label": "Supprimer",
        "function": "DELETE_ROW"
      }
    },
    "breakLine": true,
    "style": "margin-bottom: 20px"
  }
}