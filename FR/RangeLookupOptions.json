              },
              "Étiquette": {
                "type": "text",
                "Étiquette": "Étiqueter",
                "val": "positive10",
                "required": true
              },
              "delete": {
                "type": "button",
                "Étiquette": "Supprimer",
                "function": "DELETE_ROW"
              }
            }
          }
        }
      },
