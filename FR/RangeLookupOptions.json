        "repeat": false,
        "fields": {
          "options.number_range.range_type": {
            "type": "select",
            "Étiquette": "Type d'intervalle",
            "required": true,
            "options": {
              "equal_interval": {
                "Étiquette": "N intervalles égaux"
              },
              "equal_range": {
                "Étiquette": "Égaliser l'intervalle"
              },
              "custom_range": {
                "Étiquette": "Intervalle personnalisé"
              }
            },
            "val": "equal_interval"
          },
          "options.number_range.n_value": {
            "type": "text",
            "Étiquette": "N",
            "show_if": "options.number_range.range_type=='equal_interval'",
            "val": "3",
            "required": true
          },
          "options.number_range.range_value": {
            "type": "text",
            "Étiquette": "Intervalle",
            "show_if": "options.number_range.range_type=='equal_range'",
            "val": "10",
            "required": true
          },
          "options.number_range.custom_ranges": {
            "type": "fieldset",
