          },
          "apply_lookup_table": {
            "Étiquette": "Appliquer le tableau de recherche"
          },
          "apply_lookup_table_to_cells": {
            "Étiquette": "Appliquer le tableau de recherche aux cellules"
          }
        },
        "val": "generate_lookup_table"
      },
      "options.pattern_type": {
        "type": "hidden",
        "val": "number_range"
      },
      "duplicate_option_box": {
        "type": "fieldset",
        "Étiquette": "Option de duplication des patterns",
        "show_if": "options.task_type=='label_duplicates' || (options.task_type=='generate_lookup_table' && options.pattern_type=='duplicate_pattern')",
        "repeat": false,
        "fields": {
          "options.minimum_number_of_matching_characters": {
            "type": "text",
            "Étiquette": "Nombre minimum de caractères correspondants",
            "val": "3",
            "required": true,
            "style": "min-width: 240px"
          },
          "options.minimum_number_of_duplicates": {
            "type": "text",
            "Étiquette": "Nombre minimum de doublons",
            "val": "2",
            "required": true,
            "style": "min-width: 240px",
            "breakLine": true
          }
        }
      },
      "number_range_option_box": {
        "type": "fieldset",
