              "and_not": {
                "label": "ET NON"
              },
              "or_not": {
                "label": "OU NON"
              }
            },
            "val": "and"
          },
          "type": {
            "type": "select",
            "label": "Type",
            "required": true,
            "options": {
              "Table Name": {
                "label": "Nom du tableau"
              },
              "Tags": {
                "label": "Tags"
              }
            },
            "val": "Table Name"
          },
          "rows": {
            "type": "select",
            "label": "Condition",
            "show_if": "this.type == 'Tags'",
            "required": true,
            "options": {
              "All Tags": {
                "label": "All Tags"
              },
              "Some Tags": {
                "label": "Some Tags"
              }
            },
            "val": "Some Tags"
          },
          "condition": {
            "type": "select",
            "label": "Opérateur",
            "required": true,
            "options": {
              "CONTAINS": {
                "label": "CONTIENT"
              },
