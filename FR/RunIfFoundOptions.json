            "required": true,
            "show_if" :  "$index > 0",
            "options": {
              "and": {
                "label": "ET"
              },
              "or": {
                "label": "OU"
              },
              "and_not": {
                "label": "ET NON"
              },
              "or_not": {
                "label": "OU NON"
              }
            },
            "val": "and"
          },
          "column": {
            "type": "select",
            "label": "Colonne",
            "table": "table_id",
            "bind": "COLUMNS",
            "required": true
          },
          "rows": {
            "type": "select",
            "label": "Condition",
            "required": true,
            "options": {
              "All rows": {
                "label": "All rows"
              },
              "Some rows": {
                "label": "Some rows"
              }
            },
            "val": "All rows"
          },
          "condition": {
            "type": "select",
            "label": "Opérateur",
            "required": true,
            "options": {
              "CONTAINS": {
                "label": "CONTIENT"
              },
              "CONTAINED IN STRING": {
                "label": "CONTENU DANS"
              },
              "STARTS WITH": {
                "label": "COMMENCE AVEC"
              },
              "ENDS WITH": {
                "label": "SE TERMINE PAR"
              },
              "=": {
                "label": "ÉGAL"
              },
              "REGEX MATCH": {
                "label": "CORRESPONDANCE EXPRESSION RÉGULIÈRE"
              },
              "Only Numbers": {
                "label": "Only Numbers"
              },
              "Only Characters": {
                "label": "Only Characters"
              },
              "Contains Characters": {
                "label": "Contains Characters"
              },
              "Contains Whitespace": {
                "label": "Contains Whitespace"
              },
              "Contains Punctuation": {
                "label": "Contains Punctuation"
              }
            },
            "val": "CONTAINS"
          },
          "value": {
            "type": "text",
            "label": "Valeur d'entrée",
            "show_if": "this.condition == 'CONTAINS' || this.condition == 'CONTAINED IN STRING' || this.condition == '=' || this.condition == 'STARTS WITH' || this.condition == 'ENDS WITH' || this.condition == 'REGEX MATCH' || this.condition == 'NOT REGEX MATCH' || this.condition == 'IN REGEX TABLE' || this.condition == 'NOT IN REGEX TABLE'",
            "val": "",
            "required": true
          },
          "case_sensitive": {
            "type": "select",
            "label": "Respecter la casse",
            "required": true,
            "show_if": "this.condition == 'CONTAINS' || this.condition == 'CONTAINED IN STRING' || this.condition == '=' || this.condition == 'STARTS WITH' || this.condition == 'ENDS WITH' || this.condition == 'REGEX MATCH' || this.condition == 'NOT REGEX MATCH' || this.condition == 'IN REGEX TABLE' || this.condition == 'NOT IN REGEX TABLE'",
            "options": {
              "yes": {
                "label": "Respecter la casse"
              },
              "no": {
                "label": "Ignorer la casse"
              }
            },
            "val": "yes"
          },
          "delete": {
            "type": "button",
            "label": "Supprimer",
            "function": "DELETE_ROW"
          }
        }
      },
      "options.use_table_condition": {
        "type": "checkbox",
        "label": "Table Condition",
        "val": "false",
        "isOn": "true",
        "isOff": "false"
      },
      "options.table_condition": {
        "type": "fieldset",
