{
  "options": {
    "type": "fieldset", 
    "label": "Sélectionner les options", 
    "repeat": false, 
    "fields": {
      "type": {
        "type": "select", 
        "label": "Sélectionner le type", 
        "val": "first_n", 
        "options": {
          "first_n": {
            "label": "Premier N"
          }, 
          "last_n": {
            "label": "Dernier N"
          }, 
          "custom_selection": {
            "label": "Sélection personnalisée"
          }, 
          "skip_first_n": {
            "label": "Sauter le premier N"
          }, 
          "skip_last_n": {
            "label": "Sauter le dernier N"
          }, 
          "first_n_percent": {
            "label": "Premier N%"
          }, 
          "last_n_percent": {
            "label": "Dernier N%"
          }, 
          "random_n_records": {
            "label": "N aléatoire"
          }, 
          "random_n_percent": {
            "label": "N aléatoire"
          }, 
          "random_choice_1_n": {
            "label": "Choix aléatoire 1 : N"
          }, 
          "every_nth_record": {
            "label": "Chaque nième registre"
          }
        }, 
        "required": true, 
        "width": "100%", 
        "breakLine": true
      }, 
      "n": {
        "type": "text", 
        "label": "Entrée N", 
        "show_if": "type!='custom_selection'", 
        "val": "1", 
        "required": true, 
        "width": "100%", 
        "breakLine": true
      }, 
      "ranges": {
        "type": "text", 
        "label": "Intervalles d'entrée", 
        "show_if": "type=='custom_selection'", 
        "val": "-2; 4-6; 9; 12+", 
        "required": true, 
        "width": "100%", 
        "breakLine": true
      }, 
      "set_offset": {
        "type": "checkbox", 
        "label": "Compensation", 
        "show_if": "type=='every_nth_record'", 
        "val": "false", 
        "isOn": "true", 
        "isOff": "false"
      }, 
      "offset": {
        "type": "text", 
        "label": "Compensation", 
        "show_if": "type=='every_nth_record' && set_offset=='true'", 
        "val": "1", 
        "required": true, 
        "breakLine": true
      }, 
      "group_by_column": {
        "type": "checkbox", 
        "label": "Groupe par colonne", 
        "val": "false", 
        "isOn": "true", 
        "isOff": "false", 
        "breakLine": true
      }, 
      "columns_to_group_box": {
        "type": "fieldset", 
        "label": "Groupe par colonne", 
        "show_if": "group_by_column=='true'", 
        "repeat": false, 
        "fields": {
          "columns_to_group": {
            "type": "select", 
            "label": "Sélectionner les colonnes du groupe", 
            "required": true, 
            "width": "100%", 
            "table": "input_table", 
            "multiple": true, 
            "bind": "COLUMNS", 
            "breakLine": true
          }
        }
      }
    }
  }
}