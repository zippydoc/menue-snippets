{
  "output.add_period": {
    "type": "checkbox",
    "label": "Ajouter une périodicité",
    "show_if": "options.type=='process'",
    "val": "false",
    "isOn": "true",
    "isOff": "false",
    "style": "margin-top: 10px",
    "breakLine": true
  },
  "output.period": {
    "type": "select",
    "label": "Périodicité",
    "show_if": "options.type=='process' && output.add_period=='true'",
    "multiple": true,
    "options": {
      "year": {
        "label": "Année"
      },
      "half-year": {
        "label": "Semestre"
      },
      "quarter": {
        "label": "Trimestre"
      },
      "month": {
        "label": "Mois"
      },
      "week": {
        "label": "Semaine"
      }
    },
    "val": "",
    "required": true
  },
  "output.period_concatenate": {
    "type": "checkbox",
    "label": "Concaténer",
    "show_if": "options.type=='process' && output.add_period=='true'",
    "val": "false",
    "isOn": "true",
    "isOff": "false",
    "breakLine": true
  }
}