{
  "input": {
    "type": "fieldset",
    "label": "Sélectionner un tableau d'entrée",
    "repeat": false,
    "fields": {
      "input.table_or_flow": {
        "type": "hidden",
        "val": "set_table_permissions"
      },
      "input.table_id": {
        "type": "select",
        "label": "Sélectionner un tableau",
        "show_if": "input.table_or_flow=='set_table_permissions'",
        "bind": "TABLES",
        "empty": "",
        "required": true
      }
    }
  }
}