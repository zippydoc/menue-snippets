{
  "options": {
    "type": "fieldset",
    "label": "Résultat de l'en-tête et de l'étiquette du tableau",
    "repeat": false,
    "fields": {
      "has_header": {
        "type": "button",
        "label": "A un en-tête !",
        "show_if": "$input_table.numOfHeader >= 1"
      },
      "has_label": {
        "type": "button",
        "label": "A une étiquette !",
        "show_if": "$input_table.numOfLabel >= 1"
      },
      "has_both": {
        "type": "button",
        "label": "A les deux !",
        "show_if": "$input_table.numOfHeader >= 1 && $input_table.numOfLabel >= 1"
      },
      "has_both2": {
        "type": "text",
        "label": "A les deux2 !",
        "show_if": "$input_table.numOfHeader>=1 && $input_table.numOfLabel>=1",
        "val": "both"
      },
      "header_only": {
        "type": "button",
        "label": "En-tête seulement !",
        "show_if": "$input_table.numOfHeader >= 1 && $input_table.numOfLabel < 1"
      },
      "label_only": {
        "type": "button",
        "label": "Étiquette seulement !",
        "show_if": "$input_table.numOfHeader < 1 && $input_table.numOfLabel >= 1"
      },
      "no_header": {
        "type": "button",
        "label": "Pas d'en-tête !",
        "show_if": "$input_table.numOfHeader < 1"
      },
      "no_label": {
        "type": "button",
        "label": "Pas d'étiquette !",
        "show_if": "$input_table.numOfLabel < 1"
      }
    }
  }
}