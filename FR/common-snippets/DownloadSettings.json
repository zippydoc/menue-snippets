{
  "download_setting_frame": {
    "type": "fieldset",
    "label": "Paramètres de téléchargement",
    "show_if": "output.custom_options=='true'",
    "repeat": false,
    "fields": {
      "output.options.location": {
        "type": "text",
        "label": "Indiquer le lieu",
        "val": "A1",
        "breakLine": true
      },
      "output.options.formatted_as_table": {
        "type": "checkbox",
        "label": "Formaté comme tableau",
        "val": "true",
        "isOn": "true",
        "isOff": "false",
        "breakLine": true
      },
      "output.options.visibility": {
        "type": "select",
        "label": "Visibilité",
        "val": "current_flow",
        "options": {
          "current_flow": {
            "label": "Flux actuel"
          },
          "all_flow": {
            "label": "Tous les flux"
          }
        },
        "breakLine": true
      },
      "output.options.table_mode": {
        "type": "select",
        "label": "Coller",
        "val": "content_only",
        "options": {
          "content_only": {
            "label": "Contenu uniquement"
          },
          "fully_formatted": {
            "label": "Entièrement formaté"
          }
        }
      }
    },
    "breakLine": true,
    "style": "margin-bottom: 20px"
  }
}