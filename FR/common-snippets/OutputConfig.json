    "type": "fieldset",
    "label": "Output Configuration",
    "show_if": "output.custom_options=='true'",
    "repeat": false,
    "fields": {
      "output.add_tag": {
        "type": "checkbox",
        "label": "Ajouter une étiquette",
        "val": "false",
        "isOn": "true",
        "isOff": "false"
      },
      "output.tag_name": {
        "type": "text",
        "label": "Nom de l'étiquette",
        "show_if": "output.add_tag=='true'",
        "required": true,
        "minLength": 1,
        "maxLength": 255,
        "val": "",
        "breakLine": true,
        "style": "margin-left: 15px"
      }
    }
  }
}