{
  "output.select_output_columns": {
    "type": "checkbox",
    "label": "Sélectionner les colonnes de sortie",
    "show_if": "output.custom_options=='true'",
    "val": "false",
    "isOn": "true",
    "isOff": "false",
    "style": "margin-top: 10px"
  },
  "output_columns": {
    "type": "fieldset",
    "label": "Sélectionner les colonnes de sortie",
    "show_if": "output.custom_options=='true' && output.select_output_columns=='true'",
    "repeat": true,
    "fields": {
      "field_name": {
        "type": "select",
        "label": "Sélectionner une colonne",
        "table": "input_table",
        "bind": "COLUMNS",
        "required": true
      },
      "is_rename": {
        "type": "checkbox",
        "label": "Renommer",
        "val": "false",
        "isOn": "true",
        "isOff": "false"
      },
      "rename": {
        "show_if": "this.is_rename == 'true'",
        "type": "text",
        "required": true,
        "minLength": 1,
        "maxLength": 255,
        "label": "Nouveau nom de colonne",
        "style": "margin-left: 10px"
      },
      "format__category": {
        "type": "select",
        "label": "Format",
        "required": true,
        "options": {
          "standard": {
            "label": "Standard"
          },
          "number": {
            "label": "Numéro"
          },
          "percentage": {
            "label": "Pourcentage"
          },
          "text": {
            "label": "Texte"
          },
          "date": {
            "label": "Date"
          },
          "currency": {
            "label": "Monnaie"
          }
        },
        "val": "standard"
      },
      "format__rounding": {
        "type": "select",
        "label": "Arrondi",
        "show_if": "this.format__category=='number' || this.format__category=='percentage' || this.format__category=='currency'",
        "required": true,
        "options": {
          "no_rounding": {
            "label": "Pas d'arrondi"
          },
          "0": {
            "label": "0 chiffre"
          },
          "1": {
            "label": "1 chiffre"
          },
          "2": {
            "label": "2 chiffres"
          },
          "3": {
            "label": "3 chiffres"
          },
          "4": {
            "label": "4 chiffres"
          },
          "5": {
            "label": "5 chiffres"
          },
          "6": {
            "label": "6 chiffres"
          },
          "7": {
            "label": "7 chiffres"
          },
          "8": {
            "label": "8 chiffres"
          },
          "9": {
            "label": "9 chiffres"
          }
        },
        "val": "2"
      },
      "format__use_1000_separator": {
        "type": "checkbox",
        "label": "Use 1000 Separator(.)",
        "show_if": "this.format__category=='number' || this.format__category=='percentage' || this.format__category=='currency'",
        "val": "true",
        "isOn": "true",
        "isOff": "false"
      },
      "format__currency_symbol": {
        "type": "select",
        "label": "Symbole",
        "show_if": "this.format__category=='currency'",
        "required": true,
        "options": {
          "dollar": {
            "label": "$"
          },
          "euro": {
            "label": "€"
          },
          "pound": {
            "label": "£"
          }
        },
        "val": "dollar"
      },
      "format__date_format": {
        "type": "select",
        "label": "Type",
        "show_if": "this.format__category=='date'",
        "required": true,
        "options": {
          "mm/dd/yyyy": {
            "label": "3/14/2012"
          },
          "D, M dd, Y": {
            "label": "Mercredi 14 mars 2012"
          },
          "yyyy-mm-dd": {
            "label": "2012-03-14"
          }
        },
        "val": "mm/dd/yyyy",
        "breakLine": true
      },
      "delete": {
        "type": "button",
        "label": "Supprimer",
        "function": "DELETE_ROW"
      }
    },
    "breakLine": true,
    "style": "margin-bottom: 20px"
  }
}