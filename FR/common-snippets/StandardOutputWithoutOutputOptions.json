{
  "output": {
    "type": "fieldset", 
    "label": "Définir le tableau des résultats", 
    "repeat": false, 
    "fields": {
      "output.compose_table_name": {
        "type": "checkbox",
        "label": "Composer",
        "val": "false",
        "isOn": "true",
        "isOff": "false",
        "breakLine": true
      },
      "output.custom_text": {
        "type": "text", 
        "label": "Nom du tableau de sortie", 
        "show_if": "output.compose_table_name=='false'",
        "minLength": 1,
        "val": "",
        "width": "100%",
        "breakLine": true
      }, 
      "output.prefix_rule": {
        "type": "select", 
        "label": "Préfixe", 
        "show_if": "output.compose_table_name=='true'", 
        "val": "empty", 
        "options": {
          "empty": {
            "label": "-"
          }, 
          "script_name": {
            "label": "Nom de la tâche"
          }, 
          "date_string_type1": {
            "label": "Date : jj_mm_aaaa"
          }, 
          "date_string_type2": {
            "label": "Date : aaaa_mm_jj"
          }, 
          "time_string": {
            "label": "Heure : h_min_sec"
          }, 
          "datetime_string": {
            "label": "Date_Heure"
          }, 
          "custom": {
            "label": "Personnalisation"
          }
        }
      }, 
      "output.prefix_text": {
        "type": "text", 
        "label": "Préfixe Texte", 
        "show_if": "output.compose_table_name=='true' && output.prefix_rule=='custom'", 
        "minLength": 1, 
        "maxLength": 31, 
        "val": ""
      }, 
      "output.core_delimiter": {
        "type": "text", 
        "label": "Délimiteur", 
        "show_if": "output.compose_table_name=='true' && output.prefix_rule!='empty'", 
        "val": "_", 
        "width": "50px"
      }, 
      "output.core_rule": {
        "type": "select", 
        "label": "Nom de la table", 
        "show_if": "output.compose_table_name=='true'", 
        "val": "input_table_name", 
        "options": {
          "input_table_name": {
            "label": "Nom du tableau d'entrée"
          }, 
          "script_name": {
            "label": "Nom de la tâche"
          }, 
          "custom": {
            "label": "Personnalisation"
          }
        }
      }, 
      "output.core_text": {
        "type": "text", 
        "label": "Nom de la table principale", 
        "show_if": "output.compose_table_name=='true' && output.core_rule=='custom'", 
        "required": true, 
        "minLength": 1, 
        "maxLength": 31, 
        "val": ""
      }, 
      "output.suffix_delimiter": {
        "type": "text", 
        "label": "Délimiteur", 
        "show_if": "output.compose_table_name=='true'", 
        "val": "_", 
        "width": "50px"
      }, 
      "output.suffix_rule": {
        "type": "select", 
        "label": "Suffixe", 
        "show_if": "output.compose_table_name=='true'", 
        "val": "script_name", 
        "options": {
          "empty": {
            "label": "-"
          }, 
          "script_name": {
            "label": "Nom de la tâche"
          }, 
          "date_string_type1": {
            "label": "Date : jj_mm_aaaa"
          }, 
          "date_string_type2": {
            "label": "Date : aaaa_mm_jj"
          }, 
          "time_string": {
            "label": "Heure : h_min_sec"
          }, 
          "datetime_string": {
            "label": "Date_Heure"
          }, 
          "custom": {
            "label": "Personnalisation"
          }
        }
      }, 
      "output.suffix_text": {
        "type": "text", 
        "label": "Texte du suffixe", 
        "show_if": "output.compose_table_name=='true' && output.suffix_rule=='custom'", 
        "minLength": 1, 
        "maxLength": 31, 
        "val": "", 
        "breakLine": true
      }, 
      "output.timezone": {
        "type": "select", 
        "label": "Fuseau horaire", 
