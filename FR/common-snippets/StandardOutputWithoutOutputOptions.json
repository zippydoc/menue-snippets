{
  "output": {
    "type": "fieldset", 
    "label": "Définir le tableau des résultats", 
    "repeat": false, 
    "fields": {
      "output.type": {
        "type": "select", 
        "label": "Nom de la table", 
        "val": "auto", 
        "options": {
          "auto": {
            "label": "Auto"
          }, 
          "custom": {
            "label": "Personnalisation"
          }, 
          "compose": {
            "label": "Composer"
          }
        }
      }, 
      "output.custom_text": {
        "type": "text", 
        "label": "Nom du tableau de sortie", 
        "show_if": "output.type=='custom'", 
        "required": true, 
        "minLength": 1, 
        "maxLength": 31, 
        "val": "", 
        "breakLine": true
      }, 
      "output.prefix_rule": {
        "type": "select", 
        "label": "Préfixe", 
        "show_if": "output.type=='compose'", 
        "val": "empty", 
        "options": {
          "empty": {
            "label": "-"
          }, 
          "script_name": {
            "label": "Nom de la tâche"
          }, 
          "date_string_type1": {
            "label": "Date : jj_mm_aaaa"
          }, 
          "date_string_type2": {
            "label": "Date : aaaa_mm_jj"
          }, 
          "time_string": {
            "label": "Heure : h_min_sec"
          }, 
          "datetime_string": {
            "label": "Date_Heure"
          }, 
          "custom": {
            "label": "Personnalisation"
          }
        }
      }, 
      "output.prefix_text": {
        "type": "text", 
        "label": "Préfixe Texte", 
        "show_if": "output.type=='compose' && output.prefix_rule=='custom'", 
        "minLength": 1, 
        "maxLength": 31, 
        "val": ""
      }, 
      "output.core_delimiter": {
        "type": "text", 
        "label": "Délimiteur", 
        "show_if": "output.type=='compose' && output.prefix_rule!='empty'", 
        "val": "_", 
        "width": "50px"
      }, 
      "output.core_rule": {
        "type": "select", 
        "label": "Nom de la table", 
        "show_if": "output.type=='compose'", 
        "val": "input_table_name", 
        "options": {
          "input_table_name": {
            "label": "Nom du tableau d'entrée"
          }, 
          "script_name": {
            "label": "Nom de la tâche"
          }, 
          "custom": {
            "label": "Personnalisation"
          }
        }
      }, 
      "output.core_text": {
        "type": "text", 
        "label": "Nom de la table principale", 
        "show_if": "output.type=='compose' && output.core_rule=='custom'", 
        "required": true, 
        "minLength": 1, 
        "maxLength": 31, 
        "val": ""
      }, 
      "output.suffix_delimiter": {
        "type": "text", 
        "label": "Délimiteur", 
        "show_if": "output.type=='compose'", 
        "val": "_", 
        "width": "50px"
      }, 
      "output.suffix_rule": {
        "type": "select", 
        "label": "Suffixe", 
        "show_if": "output.type=='compose'", 
        "val": "script_name", 
        "options": {
          "empty": {
            "label": "-"
          }, 
          "script_name": {
            "label": "Nom de la tâche"
          }, 
          "date_string_type1": {
            "label": "Date : jj_mm_aaaa"
          }, 
          "date_string_type2": {
            "label": "Date : aaaa_mm_jj"
          }, 
          "time_string": {
            "label": "Heure : h_min_sec"
          }, 
          "datetime_string": {
            "label": "Date_Heure"
          }, 
          "custom": {
            "label": "Personnalisation"
          }
        }
      }, 
      "output.suffix_text": {
        "type": "text", 
        "label": "Texte du suffixe", 
        "show_if": "output.type=='compose' && output.suffix_rule=='custom'", 
        "minLength": 1, 
        "maxLength": 31, 
        "val": "", 
        "breakLine": true
      }, 
      "output.timezone": {
        "type": "select", 
        "label": "Fuseau horaire", 
        "show_if": "output.type=='compose' && (output.prefix_rule=='date_string_type1' || output.prefix_rule=='date_string_type2' || output.prefix_rule=='time_string' || output.prefix_rule=='datetime_string' || output.suffix_rule=='date_string_type1' || output.suffix_rule=='date_string_type2' || output.suffix_rule=='time_string' || output.suffix_rule=='datetime_string')", 
        "val": "Europe/Berlin", 
        "required": true, 
        "options": {
          "Europe/Amsterdam": {
            "label": "Europe/Amsterdam"
          }, 
          "Europe/Berlin": {
            "label": "Europe/Berlin"
          }, 
          "Europe/Helsinki": {
            "label": "Europe/Helsinki"
          }, 
          "Europe/Londres": {
            "label": "Europe/Londres"
          }, 
          "Europe/Luxembourg": {
            "label": "Europe/Luxembourg"
          }, 
          "Europe/Oslo": {
            "label": "Europe/Oslo"
          }, 
          "Europe/Paris": {
            "label": "Europe/Paris"
          }, 
          "Europe/Rome": {
            "label": "Europe/Rome"
          }, 
          "États-Unis/Alaska": {
            "label": "États-Unis/Alaska"
          }, 
          "États-Unis/Central": {
            "label": "États-Unis/Central"
          }, 
          "États-Unis/Est": {
            "label": "États-Unis/Est"
          }, 
          "États-Unis/Pacifique": {
            "label": "États-Unis/Pacifique"
          }
        }, 
        "breakLine": true
      }
    }
  }
}
