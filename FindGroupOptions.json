{
  "options": {
    "type": "fieldset",
    "label": "Find Group Options",
    "repeat": false,
    "fields": {
      "options.group_column": {
        "type": "select",
        "label": "Select Group Column",
        "required": true,
        "table": "input_table",
        "bind": "COLUMNS",
        "multiple": false,
        "breakLine": true
      },
      "options.item_column": {
        "type": "select",
        "label": "Select Item Column",
        "required": true,
        "table": "input_table",
        "bind": "COLUMNS",
        "multiple": false,
        "breakLine": true
      },
      "options.min_item": {
        "type": "number",
        "label": "Minimum number of same sub-group items per group",
        "required": true,
        "val": "3",
        "breakLine": true
      },
      "options.order": {
        "type": "select",
        "label": "Orderd / Unordered",
        "required": true,
        "options": {
          "ordered": {
            "label": "Ordered"
          },
          "unordered": {
            "label": "Unordered"
          }
        },
        "val": "ordered",
        "breakLine": true
      },
      "options.output_type": {
        "type": "select",
        "label": "Output Type",
        "required": true,
        "options": {
          "item_group_list": {
            "label": "Item Group List"
          },
          "item_group_column": {
            "label": "Item Group Column"
          }
        },
        "val": "item_group_list"
      }
    }
  }
}