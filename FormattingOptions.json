      "all_column.format__category": {
        "type": "select",
        "label": "Display Format",
        "required": true,
        "options": {
          "standard": {
            "label": "Standard"
          },
          "number": {
            "label": "Number"
          },
          "percentage": {
            "label": "Percentage"
          },
          "text": {
            "label": "Text"
          },
          "date": {
            "label": "Date"
          },
          "currency": {
            "label": "Currency"
          }
        },
        "val": "standard",
