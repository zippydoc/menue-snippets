{
  "options": {
    "type": "fieldset",
    "label": "Select Options",
    "repeat": false,
    "fields": {
      "options.type": {
        "type": "hidden",
        "val": "custom"
      },
      "options.custom.table": {
        "type": "fieldset",
        "label": "Planning Position",
        "repeat": true,
        "fields": {
          "item": {
            "type": "text",
            "label": "Item",
            "val": "",
            "required": true
          },
          "position": {
            "type": "select",
            "label": "Position",
            "options": {
              "sales revenues": {
                "label": "sales revenues"
              },
              "other revenues": {
                "label": "other revenues"
              },
              "cost of goods sold": {
                "label": "cost of goods sold"
              },
              "marketing expenses": {
                "label": "marketing expenses"
              },
              "operating expenses": {
                "label": "operating expenses"
              },
              "general and administration expenses": {
                "label": "general and administration expenses"
              },
              "rental expenses": {
                "label": "rental expenses"
              },
              "salaries": {
                "label": "salaries"
              },
              "tax": {
                "label": "tax"
              },
              "interest income": {
                "label": "interest income"
              },
              "interest expenses": {
                "label": "interest expenses"
              },
              "capital expenditure": {
                "label": "capital expenditure"
              },
              "maintenance capex": {
                "label": "maintenance capex"
              },
              "expansion capex": {
                "label": "expansion capex,"
              },
              "depreciation": {
                "label": "depreciation"
              },
              "amortization": {
                "label": "amortization"
              },
              "extraordinary income": {
                "label": "extraordinary income"
              },
              "extraordinary expenses": {
                "label": "extraordinary expenses"
              }
            },
            "val": "sales revenues",
            "required": true,
            "breakLine": true
          },
          "amount": {
            "type": "number",
            "label": "Amount",
            "val": "",
            "required": true
          },
          "amount_format__category": {
            "type": "hidden",
            "val": "currency"
          },
          "amount_format__rounding": {
            "type": "hidden",
            "val": "2"
          },
          "amount_format__use_1000_separator": {
            "type": "hidden",
            "val": "true"
          },
          "amount_format__currency_symbol": {
            "type": "hidden",
            "val": "dollar"
          },
          "initial_date": {
            "type": "date",
            "label": "Initial Date",
            "val": "",
            "required": true
          },
          "initial_date_format__date_format": {
            "type": "select",
            "label": "Initial Date Format",
            "show_if": "$index == 0",
            "required": true,
            "options": {
              "mm/dd/yyyy": {
                "label": "3/14/2012"
              },
              "D, M dd, Y": {
                "label": "Wednesday, March 14, 2012"
              },
              "yyyy-mm-dd": {
                "label": "2012-03-14"
              }
            },
            "val": "mm/dd/yyyy",
            "breakLine": true
          },
          "periodicy": {
            "type": "select",
            "label": "Periodicy",
            "required": true,
            "options": {
              "One-Off": {
                "label": "One-Off"
              },
              "Daily": {
                "label": "Daily"
              },
              "Work-Daily": {
                "label": "Work-Daily"
              },
              "Weekly": {
                "label": "Weekly"
              },
              "Monthly": {
                "label": "Monthly"
              },
              "Quarterly": {
                "label": "Quarterly"
              },
              "Yearly": {
                "label": "Yearly"
              }
            },
            "val": "One-Off"
          },
          "growth": {
            "type": "number",
            "label": "Growth",
            "show_if": "this.periodicy!='One-Off'",
            "val": "",
            "required": true
          },
          "growth_period": {
            "type": "select",
            "label": "Growth Period",
            "show_if": "this.periodicy!='One-Off'",
            "options": {
              "Weekly": {
                "label": "Weekly"
              },
              "Monthly": {
                "label": "Month on Month"
              },
              "Quarterly": {
                "label": "Quarter on Quarter"
              },
              "Yearly": {
                "label": "Year on Year"
              }
            },
            "val": "Weekly"
          },
          "growth_pattern": {
            "type": "select",
            "label": "Growth Pattern",
            "show_if": "this.periodicy!='One-Off'",
            "options": {
              "Compounding": {
                "label": "Compounding"
              },
              "Linear": {
                "label": "Linear"
              },
              "Jump-Fix": {
                "label": "Jump-Fix"
              }
            },
            "val": "Compounding"
          },
          "final_date": {
            "type": "date",
            "label": "Final Date",
            "show_if": "this.periodicy!='One-Off'",
            "val": ""
          },
          "delete": {
            "type": "button",
            "label": "Delete",
            "function": "DELETE_ROW"
          }
        }
      },
      "options.end_date": {
        "type": "date",
        "label": "End Date of Table",
        "show_if": "options.type=='process' || options.type=='custom'",
        "val": "",
        "required": true,
        "style": "width: 300px"
      }
    }
  }
}