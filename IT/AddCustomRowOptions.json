{
  "options": {
    "type": "fieldset", 
    "label": "Seleziona le opzioni", 
    "repeat": false, 
    "fields": {
      "options.position": {
        "type": "select", 
        "label": "Selezionare la posizione", 
        "required": true, 
        "options": {
          "add_rows_below_selected_row": {
            "label": "Add rows below selected rows"
          }, 
          "add_rows_above_selected_row": {
            "label": "Add rows above selected rows"
          }, 
          "add_rows_at_table_end": {
            "label": "Aggiungere righe alla fine della tabella"
          }, 
          "add_single_row_at_table_end": {
            "label": "Aggiungere una riga singola alla fine della tabella"
          }, 
          "add_row_after_each_group": {
            "label": "Aggiungere la riga dopo ogni gruppo"
          }
        }, 
        "val": "add_rows_below_selected_row", 
        "breakLine": true
      }, 
      "options.group_column": {
        "type": "select", 
        "label": "Colonna del gruppo", 
        "show_if": "options.position=='add_row_after_each_group'", 
        "table": "input_table", 
        "bind": "COLUMNS", 
        "required": true, 
        "breakLine": true
      }, 
      "options.use_filter": {
        "type": "checkbox", 
        "label": "Usa il filtro", 
        "show_if": "options.position=='add_rows_below_selected_row' || options.position=='add_rows_above_selected_row' || options.position=='add_rows_at_table_end'", 
        "val": "false", 
        "isOn": "true", 
        "isOff": "false", 
        "breakLine": true
      }, 
      "filter_box": {
        "type": "fieldset", 
        "label": "Opzioni del filtro", 
        "show_if": "options.use_filter=='true' && (options.position=='add_rows_below_selected_row' || options.position=='add_rows_above_selected_row' || options.position=='add_rows_at_table_end')", 
        "repeat": false, 
        "fields": {
          "options.filter": {
            "type": "fieldset", 
            "label": "Aggiungi condizioni di filtro", 
            "repeat": true, 
            "fields": {
              "initial_op": {
                "type": "select", 
                "label": "Seleziona l'operatore logico", 
                "required": true, 
                "show_if": "$index == 0", 
                "options": {
                  "if": {
                    "label": "SE"
                  }, 
                  "if_not": {
                    "label": "SE NON"
                  }
                }, 
                "val": "if"
              }, 
              "op": {
                "type": "select", 
                "label": "Seleziona l'operatore logico", 
                "required": true, 
                "show_if": "$index > 0", 
                "options": {
                  "and": {
                    "label": "E"
                  }, 
                  "or": {
                    "label": "O"
                  }, 
                  "and_not": {
                    "label": "E NON"
                  }, 
                  "or_not": {
                    "label": "O NON"
                  }
                }, 
                "val": "and"
              }, 
              "column": {
                "type": "select", 
                "label": "Seleziona la colonna", 
                "table": "input_table", 
                "bind": "COLUMNS", 
                "required": true
              }, 
              "condition": {
                "type": "select", 
                "label": "Seleziona l'operatore", 
                "required": true, 
                "options": {
                  "CONTIENE": {
                    "label": "CONTIENE"
                  }, 
                  "CONTAINED IN STRING": {
                    "label": "CONTENUTO IN"
                  }, 
                  "INIZIA CON": {
                    "label": "INIZIA CON"
                  }, 
                  "FINISCE CON": {
                    "label": "FINISCE CON"
                  }, 
                  "=": {
                    "label": "EQUAL"
                  }, 
                  ">": {
                    "label": "PIÙ GRANDE DI"
                  }, 
                  ">=": {
                    "label": "MAGGIORE O UGUALE"
                  }, 
                  "<": {
                    "label": "PIÙ PICCOLO DI"
                  }, 
                  "<=": {
                    "label": "PIÙ PICCOLO O UGUALE"
                  }, 
                  "EMPTY": {
                    "label": "È VUOTO"
                  }, 
                  "BLANK": {
                    "label": "È VUOTO"
                  }, 
                  "È NULLO": {
                    "label": "È NULLO"
                  }, 
                  "CORRISPONDENZA REGEX": {
                    "label": "CORRISPONDENZA REGEX"
                  }, 
                  "CONTAINED IN TABLE": {
                    "label": "IN TAVOLA"
                  }, 
                  "IN TAVOLA REGEX": {
                    "label": "IN TAVOLA REGEX"
                  }, 
                  "DATA": {
                    "label": "DATA"
                  }
                }, 
                "val": "CONTIENE"
              }, 
              "date_condition": {
                "type": "select", 
                "label": "Seleziona l'operatore della data", 
                "required": true, 
                "show_if": "this.condition == 'DATE'", 
                "options": {
                  "<": {
                    "label": "PRIORE"
                  }, 
                  "<=": {
                    "label": "PRIORE O PARI"
                  }, 
                  "=": {
                    "label": "EQUAL"
                  }, 
                  ">": {
                    "label": "DOPO"
                  }, 
                  ">=": {
                    "label": "DOPO O PARI"
                  }
                }, 
                "val": "="
              }, 
              "case_sensitive": {
                "type": "select", 
                "label": "Selezionare Case Sensitive", 
                "required": true, 
                "show_if": "this.condition == 'CONTAINS' || this.condition == 'CONTAINED IN STRING' || this.condition == '=' || this.condition == 'STARTS WITH' || this.condition == 'ENDS WITH' || this.condition == 'REGEX MATCH' || this.condition == 'NOT REGEX MATCH' || this.condition == 'IN REGEX TABLE' || this.condition == 'NOT IN REGEX TABLE'", 
                "options": {
                  "yes": {
                    "label": "Sensibile al caso"
                  }, 
                  "no": {
                    "label": "Insensibile alle maiuscole e alle minuscole"
                  }
                }, 
                "val": "yes"
              }, 
              "date_value": {
                "type": "date", 
                "label": "Seleziona la data", 
                "show_if": "this.condition=='DATE'", 
                "val": "1/31/2020", 
                "required": true
              }, 
              "value": {
                "type": "text", 
                "label": "Valore di ingresso", 
                "show_if": "this.condition != 'DATE' && this.condition != 'EMPTY' && this.condition != 'NOT EMPTY' && this.condition != 'BLANK' && this.condition != 'NOT BLANK' && this.condition != 'IS NULL' && this.condition != 'IS NOT NULL' && this.condition != 'CONTAINED IN TABLE' && this.condition != 'NOT CONTAINED IN TABLE' && this.condition != 'IN REGEX TABLE' && this.condition != 'NOT IN REGEX TABLE'", 
                "val": "", 
                "required": true
              }, 
              "compare_table": {
                "type": "select", 
                "label": "Seleziona la tabella", 
                "bind": "TABLES", 
                "empty": "", 
                "show_if": "this.condition == 'CONTAINED IN TABLE' || this.condition == 'NOT CONTAINED IN TABLE' || this.condition == 'IN REGEX TABLE' || this.condition == 'NOT IN REGEX TABLE'", 
                "required": true
              }, 
              "compare_column": {
                "type": "select", 
                "label": "Seleziona la colonna", 
                "table": "this.compare_table", 
                "bind": "COLUMNS", 
                "show_if": "this.condition == 'CONTAINED IN TABLE' || this.condition == 'NOT CONTAINED IN TABLE' || this.condition == 'IN REGEX TABLE' || this.condition == 'NOT IN REGEX TABLE'", 
                "required": true
              }, 
              "delete": {
                "type": "button", 
                "label": "Cancellare", 
                "function": "DELETE_ROW"
              }
            }
          }, 
          "options.apply_to_record": {
            "type": "checkbox", 
            "label": "Applicare per registrare", 
            "val": "true", 
            "isOn": "true", 
            "isOff": "false", 
            "style": "margin-top: 10px", 
            "breakLine": true
          }, 
          "apply_to_group_box": {
            "type": "fieldset", 
            "label": "Applica al gruppo", 
            "show_if": "options.apply_to_record=='false'", 
            "repeat": false, 
            "fields": {
              "options.apply_to_group_columns": {
                "type": "select", 
                "label": "Seleziona le colonne del gruppo", 
                "required": true, 
                "width": "100%", 
                "table": "input_table", 
                "multiple": true, 
                "bind": "COLUMNS", 
                "breakLine": true
              }
            }
          }
        }
      }, 
      "options.target_cells": {
        "type": "fieldset", 
        "label": "Definire le cellule bersaglio", 
        "repeat": true, 
        "fields": {
          "column": {
            "type": "select", 
            "label": "Seleziona la colonna", 
            "table": "input_table", 
            "bind": "COLUMNS", 
            "required": true
          }, 
          "formula_type": {
            "type": "select", 
            "label": "Tipo di formula", 
            "required": true, 
            "options": {
              "copy_selected": {
                "label": "Copia selezionata"
              }, 
              "static_value": {
                "label": "Valore statico"
              }, 
              "numeric_formula": {
                "label": "Formula numerica"
              }, 
              "text_formula": {
                "label": "Formula del testo"
              }
            }, 
            "val": "copy_selected"
          }, 
          "static_value": {
            "type": "text", 
            "label": "Valore", 
            "show_if": "this.formula_type=='static_value'", 
            "val": ""
          }, 
          "numeric_formula": {
            "type": "text", 
            "label": "Formula, ad esempio [Column1] + 2 * ([Row-1:Column3] - [Column1])", 
            "show_if": "this.formula_type=='numeric_formula'", 
            "val": "", 
            "style": "width: 100%"
          }, 
          "rounding": {
            "type": "select", 
            "label": "Arrotondamento", 
            "show_if": "this.formula_type=='numeric_formula'", 
            "required": true, 
            "options": {
              "no_rounding": {
                "label": "Nessun arrotondamento"
              }, 
              "0": {
                "label": "0 cifre"
              }, 
              "1": {
                "label": "1 cifra"
              }, 
              "2": {
                "label": "2 cifre"
              }, 
              "3": {
                "label": "3 cifre"
              }, 
              "4": {
                "label": "4 cifre"
              }, 
              "5": {
                "label": "5 cifre"
              }, 
              "6": {
                "label": "6 cifre"
              }, 
              "7": {
                "label": "7 cifre"
              }, 
              "8": {
                "label": "8 cifre"
              }, 
              "9": {
                "label": "9 cifre"
              }
            }, 
            "val": "no_rounding"
          }, 
          "text_formula": {
            "type": "text", 
            "label": "Formula, ad esempio [firstName] + ' ' + [lastName] + ' ' + [Row-1:Column3]", 
            "show_if": "this.formula_type=='text_formula'", 
            "val": "", 
            "style": "width: 100%"
          }, 
          "use_multi_row": {
            "type": "checkbox", 
            "label": "Opzioni multi-fila", 
            "show_if": "options.position!='add_row_after_each_group' && (this.formula_type=='numeric_formula' || this.formula_type=='text_formula')", 
            "val": "false", 
            "isOn": "true", 
            "isOff": "false"
          }, 
          "if_row_not_exist": {
            "type": "select", 
            "label": "Se la riga di riferimento non esiste", 
            "show_if": "options.position!='add_row_after_each_group' && this.use_multi_row=='true' && (this.formula_type=='numeric_formula' || this.formula_type=='text_formula')", 
            "required": true, 
            "options": {
              "empty": {
                "label": "Vuoto o 0"
              }, 
              "closest": {
                "label": "Valore della riga più vicina"
              }
            }, 
            "val": "empty", 
            "breakLine": true
          }, 
          "delete": {
            "type": "button", 
            "label": "Cancellare", 
            "function": "DELETE_ROW"
          }
        }, 
        "breakLine": true
      }, 
      "options.set_date_column": {
        "type": "checkbox", 
        "label": "Seleziona la colonna della data", 
        "val": "false", 
        "isOn": "true", 
        "isOff": "false", 
        "style": "margin-top: 20px", 
        "breakLine": true
      }, 
      "options.date_column": {
        "type": "select", 
        "label": "Seleziona la colonna della data", 
        "show_if": "options.set_date_column=='true'", 
        "table": "input_table", 
        "bind": "COLUMNS", 
        "required": true
      }, 
      "options.period_shift.number": {
        "type": "text", 
        "label": "Turno di periodo", 
        "show_if": "options.set_date_column=='true'", 
        "required": true, 
        "val": "0"
      }, 
      "options.period_shift.period": {
        "type": "select", 
        "label": "Seleziona il periodo", 
        "show_if": "options.set_date_column=='true'", 
        "options": {
          "day": {
            "label": "Giorni"
          }, 
          "week": {
            "label": "Settimane"
          }, 
          "month": {
            "label": "Mesi"
          }, 
          "year": {
            "label": "Anni"
          }
        }, 
        "val": "month"
      }
    }
  }
}
