{
  "output.add_period": {
    "type": "checkbox", 
    "label": "Aggiungere la periodicità", 
    "show_if": "options.type=='process'", 
    "val": "false", 
    "isOn": "true", 
    "isOff": "false", 
    "breakLine": true
  }, 
  "output.period": {
    "type": "select", 
    "label": "Periodicy", 
    "show_if": "options.type=='process' && output.add_period=='true'", 
    "multiple": true, 
    "options": {
      "year": {
        "label": "Anno"
      }, 
      "half-year": {
        "label": "Metà anno"
      }, 
      "quarter": {
        "label": "Trimestre"
      }, 
      "month": {
        "label": "Mese"
      }, 
      "week": {
        "label": "Settimana"
      }
    }, 
    "val": "", 
    "required": true
  }, 
  "output.period_concatenate": {
    "type": "checkbox", 
    "label": "Concatenare", 
    "show_if": "options.type=='process' && output.add_period=='true'", 
    "val": "false", 
    "isOn": "true", 
    "isOff": "false"
  }
}
