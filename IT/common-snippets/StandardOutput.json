{
  "output": {
    "type": "fieldset", 
    "label": "Definire la tabella di uscita", 
    "repeat": false, 
    "fields": {
      "output.type": {
        "type": "select", 
        "label": "Nome della tabella", 
        "val": "auto", 
        "options": {
          "auto": {
            "label": "Auto"
          }, 
          "custom": {
            "label": "Personalizzato"
          }, 
          "compose": {
            "label": "Comporre"
          }
        }
      }, 
      "output.custom_text": {
        "type": "text", 
        "label": "Nome della tabella di uscita", 
        "show_if": "output.type=='custom'", 
        "required": true, 
        "minLength": 1, 
        "val": "", 
        "breakLine": true
      }, 
      "output.prefix_rule": {
        "type": "select", 
        "label": "Prefisso", 
        "show_if": "output.type=='compose'", 
        "val": "empty", 
        "options": {
          "empty": {
            "label": "-"
          }, 
          "script_name": {
            "label": "Nome del compito"
          }, 
          "date_string_type1": {
            "label": "Data: gg_mm_aaaa"
          }, 
          "date_string_type2": {
            "label": "Data: aaaa_mm_gg"
          }, 
          "time_string": {
            "label": "Tempo: h_min_sec"
          }, 
          "datetime_string": {
            "label": "Data_Tempo"
          }, 
          "custom": {
            "label": "Personalizzato"
          }
        }
      }, 
      "output.prefix_text": {
        "type": "text", 
        "label": "Testo del prefisso", 
        "show_if": "output.type=='compose' && output.prefix_rule=='custom'", 
        "minLength": 1, 
        "maxLength": 31, 
        "val": ""
      }, 
      "output.core_delimiter": {
        "type": "text", 
        "label": "Delimitatore", 
        "show_if": "output.type=='compose' && output.prefix_rule!='empty'", 
        "val": "_", 
        "width": "50px"
      }, 
      "output.core_rule": {
        "type": "select", 
        "label": "Nome della tabella", 
        "show_if": "output.type=='compose'", 
        "val": "input_table_name", 
        "options": {
          "input_table_name": {
            "label": "Nome della tabella di ingresso"
          }, 
          "script_name": {
            "label": "Nome del compito"
          }, 
          "custom": {
            "label": "Personalizzato"
          }
        }
      }, 
      "output.core_text": {
        "type": "text", 
        "label": "Nome della tabella principale", 
        "show_if": "output.type=='compose' && output.core_rule=='custom'", 
        "required": true, 
        "minLength": 1, 
        "maxLength": 31, 
        "val": ""
      }, 
      "output.suffix_delimiter": {
        "type": "text", 
        "label": "Delimitatore", 
        "show_if": "output.type=='compose'", 
        "val": "_", 
        "width": "50px"
      }, 
      "output.suffix_rule": {
        "type": "select", 
        "label": "Suffisso", 
        "show_if": "output.type=='compose'", 
        "val": "script_name", 
        "options": {
          "empty": {
            "label": "-"
          }, 
          "script_name": {
            "label": "Nome del compito"
          }, 
          "date_string_type1": {
            "label": "Data: gg_mm_aaaa"
          }, 
          "date_string_type2": {
            "label": "Data: aaaa_mm_gg"
          }, 
          "time_string": {
            "label": "Tempo: h_min_sec"
          }, 
          "datetime_string": {
            "label": "Data_Tempo"
          }, 
          "custom": {
            "label": "Personalizzato"
          }
        }
      }, 
      "output.suffix_text": {
        "type": "text", 
        "label": "Testo del suffisso", 
        "show_if": "output.type=='compose' && output.suffix_rule=='custom'", 
        "minLength": 1, 
        "maxLength": 31, 
        "val": "", 
        "breakLine": true
      }, 
      "output.timezone": {
        "type": "select", 
        "label": "Fuso orario", 
        "show_if": "output.type=='compose' && (output.prefix_rule=='date_string_type1' || output.prefix_rule=='date_string_type2' || output.prefix_rule=='time_string' || output.prefix_rule=='datetime_string' || output.suffix_rule=='date_string_type1' || output.suffix_rule=='date_string_type2' || output.suffix_rule=='time_string' || output.suffix_rule=='datetime_string')", 
        "val": "Europa/Berlino", 
        "required": true, 
        "options": {
          "Europa/Amsterdam": {
            "label": "Europa/Amsterdam"
          }, 
          "Europa/Berlino": {
            "label": "Europa/Berlino"
          }, 
          "Europa/Helsinki": {
            "label": "Europa/Helsinki"
          }, 
          "Europa/Londra": {
            "label": "Europa/Londra"
          }, 
          "Europa/Lussemburgo": {
            "label": "Europa/Lussemburgo"
          }, 
          "Europa/Oslo": {
            "label": "Europa/Oslo"
          }, 
          "Europa/Parigi": {
            "label": "Europa/Parigi"
          }, 
          "Europa/Roma": {
            "label": "Europa/Roma"
          }, 
          "Stati Uniti/Alaska": {
            "label": "Stati Uniti/Alaska"
          }, 
          "Stati Uniti/Centrale": {
            "label": "Stati Uniti/Centrale"
          }, 
          "Stati Uniti/Estero": {
            "label": "Stati Uniti/Estero"
          }, 
          "Stati Uniti/Pacifico": {
            "label": "Stati Uniti/Pacifico"
          }
        }, 
        "breakLine": true
      }, 
      "output.set_options": {
        "type": "checkbox", 
        "label": "Opzioni di uscita", 
        "val": "false", 
        "isOn": "true", 
        "isOff": "false"
      }, 
      "output_options_box": {
        "type": "fieldset", 
        "label": "Opzioni di uscita", 
        "show_if": "output.set_options=='true'", 
        "repeat": false, 
        "fields": {
          "output.options.location": {
            "type": "text", 
            "label": "Specificare la posizione", 
            "val": "A1", 
            "breakLine": true
          }, 
          "output.options.table_mode": {
            "type": "select", 
            "label": "Incolla", 
            "val": "content_only", 
            "options": {
              "content_only": {
                "label": "Solo contenuto"
              }, 
              "fully_formatted": {
                "label": "Completamente formattato"
              }
            }
          }, 
          "output.options.formatted_as_table": {
            "type": "checkbox", 
            "label": "Formattato come tabella", 
            "val": "true", 
            "isOn": "true", 
            "isOff": "false", 
            "breakLine": true
          }, 
          "output.add_tag": {
            "type": "checkbox", 
            "label": "Aggiungi tag", 
            "val": "false", 
            "isOn": "true", 
            "isOff": "false"
          }, 
          "output.tag_name": {
            "type": "text", 
            "label": "Nome dell'etichetta", 
            "show_if": "output.add_tag=='true'", 
            "required": true, 
            "minLength": 1, 
            "maxLength": 255, 
            "val": "", 
            "breakLine": true, 
            "style": "margin-left: 15px"
          }, 
          "output.options.visibility": {
            "type": "select", 
            "label": "VisibilitÃ ", 
            "val": "current_flow", 
            "options": {
              "current_flow": {
                "label": "Flusso di corrente"
              }, 
              "all_flow": {
                "label": "Tutti i flussi"
              }
            }
          }
        }, 
        "breakLine": true
      }
    }
  }
}
