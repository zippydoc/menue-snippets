{
  "options": {
    "type": "fieldset",
    "label": "Select Options",
    "repeat": false,
    "fields": {
      "options.from": {
        "type": "radio",
        "val": "database",
        "values": {
          "database": "Import from Database",
          "web": "Import from Webservice"
        },
        "breakLine": true
      },
      "postgresql_db_credential_box": {
        "type": "fieldset",
        "label": "Database Connection Credentials",
        "show_if": "options.from=='database'",
        "repeat": false,
        "fields": {
          "options.dbms": {
            "type": "select",
            "label": "Select DBMS",
            "options": {
              "postgresql": {
                "label": "PostgreSQL"
              },
              "mysql": {
                "label": "MySQL"
              },
              "sql_server": {
                "label": "MS SQL Server"
              },
              "oracle": {
                "label": "Oracle"
              }
            },
            "val": "postgresql",
            "required": true,
            "breakLine": true
          },
          "options.postgresql_credential.user": {
            "type": "text",
            "label": "User",
            "show_if": "options.dbms=='postgresql'",
            "required": true,
            "width": "100%",
            "breakLine": true
          },
          "options.postgresql_credential.password": {
            "type": "text",
            "label": "Password",
            "show_if": "options.dbms=='postgresql'",
            "required": true,
            "width": "100%",
            "breakLine": true
          },
          "options.postgresql_credential.host": {
            "type": "text",
            "label": "Host",
            "show_if": "options.dbms=='postgresql'",
            "required": true,
            "width": "100%",
            "breakLine": true
          },
          "options.postgresql_credential.database": {
            "type": "text",
            "label": "Database",
            "show_if": "options.dbms=='postgresql'",
            "required": true,
            "width": "100%",
            "breakLine": true
          }
        }
      },
      "options.schema_name_to_import": {
        "type": "text",
        "label": "Schema name that includes table to import",
        "show_if": "options.from=='database' && options.dbms=='postgresql'",
        "required": true,
        "val": "public",
        "width": "100%",
        "breakLine": true
      },
      "options.table_name_to_import": {
        "type": "text",
        "label": "Table name to import",
        "show_if": "options.from=='database' && options.dbms=='postgresql'",
        "required": true,
        "width": "100%",
        "breakLine": true
      },
      "options.webservice": {
        "type": "select",
        "label": "Select Webservice",
        "show_if": "options.from=='web'",
        "options": {
          "github": {
            "label": "Github"
          }
        },
        "val": "github",
        "required": true
      },
      "options.is_csv_file": {
        "type": "checkbox",
        "label": "Import table",
        "show_if": "options.from=='web'",
        "val": "true",
        "isOn": "true",
        "isOff": "false"
      },
      "options.is_github_dir": {
        "type": "checkbox",
        "label": "Import directory",
        "show_if": "options.from=='web'",
        "val": "false",
        "isOn": "true",
        "isOff": "false"
      },
      "options.is_github_private": {
        "type": "checkbox",
        "label": "Repository is private",
        "show_if": "options.from=='web'",
        "val": "false",
        "isOn": "true",
        "isOff": "false",
        "breakLine": true
      },
      "options.unify_in_one_table": {
        "type": "checkbox",
        "label": "Unify in one table",
        "show_if": "options.from=='web' && options.is_github_dir=='true'",
        "val": "false",
        "isOn": "true",
        "isOff": "false"
      },
      "options.suffix": {
        "type": "select",
        "label": "Suffix",
        "show_if": "options.from=='web' && options.is_github_dir=='true' && options.unify_in_one_table=='false'",
        "options": {
          "number": {
            "label": "Number"
          },
          "filename": {
            "label": "Filename"
          }
        },
        "val": "number",
        "required": true
      },
      "options.add_filename_column": {
        "type": "checkbox",
        "label": "Add Column for File Name",
        "show_if": "options.from=='web' && options.is_github_dir=='true'",
        "val": "false",
        "isOn": "true",
        "isOff": "false",
        "breakLine": true
      },
      "options.github_token": {
        "type": "text",
        "label": "specify the Github personal access token",
        "show_if": "options.from=='web' && options.is_github_private=='true'",
        "required": true,
        "width": "100%",
        "breakLine": true
      },
      "options.link_directory": {
        "type": "text",
        "label": "Enter link e.g. https://github.com/zippydoc/test",
        "show_if": "options.from=='web' && options.is_github_dir=='true'",
        "required": true,
        "width": "100%",
        "breakLine": true
      },
      "options.link_file": {
        "type": "text",
        "label": "Enter link e.g. https://github.com/zippydoc/test/blob/main/example.csv",
        "show_if": "options.from=='web' && options.is_github_dir=='false' && options.is_csv_file=='true'",
        "required": true,
        "width": "100%",
        "breakLine": true
      },
      "options.link_text_file": {
        "type": "text",
        "label": "Enter link e.g. https://github.com/zippydoc/test/blob/main/example.txt",
        "show_if": "options.from=='web' && options.is_github_dir=='false' && options.is_csv_file=='false'",
        "required": true,
        "width": "100%",
        "breakLine": true
      },
      "options.txt_output_format": {
        "type": "select",
        "label": "Output Format",
        "show_if": "options.from=='web' && options.is_csv_file=='false'",
        "options": {
          "line_by_line": {
            "label": "Line by Line"
          },
          "word_by_word": {
            "label": "Word by Word"
          }
        },
        "val": "line_by_line",
        "required": true
      }
    }
  }
}