{
  "options": {
    "type": "fieldset",
    "label": "オプションの選択",
    "repeat": false,
    "fields": {
      "options.position": {
        "type": "select",
        "label": "位置の選択",
        "required": true,
        "options": {
          "add_rows_below_selected_row": {
            "label": "選択された行の下の行を追加"
          },
          "add_rows_above_selected_row": {
            "label": "選択された行の上の行を追加"
          },
          "add_rows_at_table_end": {
            "label": "テーブルの最後に行を追加"
          },
          "add_single_row_at_table_end": {
            "label": "テーブルの最後に1行追加"
          },
          "add_row_after_each_group": {
            "label": "各グループの後に行を追加"
          }
        },
        "val": "add_rows_below_selected_row",
        "breakLine": true
      },
      "options.group_column": {
        "type": "select",
        "label": "列のグループ",
        "show_if": "options.position=='add_row_after_each_group'",
        "table": "input_table",
        "bind": "COLUMNS",
        "required": true,
        "breakLine": true
      },
      "options.use_filter": {
        "type": "select",
        "label": "選択タイプ",
        "show_if": "options.position=='add_rows_below_selected_row' || options.position=='add_rows_above_selected_row' || options.position=='add_rows_at_table_end'",
        "required": true,
        "options": {
          "false": {
            "label": "テーブルの各行に1つの行を追加"
          },
          "true": {
            "label": "指定した行ごとに1つの行を追加"
          }
        },
        "val": "false",
        "breakLine": true
      },
      "filter_box": {
        "type": "fieldset",
        "label": "フィルターオプション",
        "show_if": "options.use_filter=='true' && (options.position=='add_rows_below_selected_row' || options.position=='add_rows_above_selected_row' || options.position=='add_rows_at_table_end')",
        "repeat": false,
        "fields": {
          "options.filter": {
            "type": "fieldset",
            "label": "フィルター条件の追加",
            "repeat": true,
            "fields": {
              "initial_op": {
                "type": "select",
                "label": "論理オペレータの選択",
                "required": true,
                "show_if": "$index == 0",
                "options": {
                  "if": {
                    "label": "IF"
                  },
                  "if_not": {
                    "label": "IF NOT"
                  }
                },
                "val": "if"
              },
              "op": {
                "type": "select",
                "label": "論理オペレータの選択",
                "required": true,
                "show_if": "$index > 0",
                "options": {
                  "and": {
                    "label": "AND"
                  },
                  "or": {
                    "label": "OR"
                  },
                  "and_not": {
                    "label": "AND NOT"
                  },
                  "or_not": {
                    "label": "OR NOT"
                  }
                },
                "val": "and"
              },
              "column": {
                "type": "select",
                "label": "列の選択",
                "table": "input_table",
                "bind": "COLUMNS",
                "required": true
              },
              "condition": {
                "type": "select",
                "label": "オペレータの選択",
                "required": true,
                "options": {
                  "含む": {
                    "label": "含む"
                  },
                  "CONTAINED IN STRING": {
                    "label": "次の中に含む"
                  },
                  "次で開始": {
                    "label": "次で開始"
                  },
                  "次で終了": {
                    "label": "次で終了"
                  },
                  "=": {
                    "label": "等しい"
                  },
                  ">": {
                    "label": "超"
                  },
                  ">=": {
                    "label": "以上"
                  },
                  "<": {
                    "label": "より小さい"
                  },
                  "<=": {
                    "label": "より小さいまたは等しい"
                  },
                  "EMPTY": {
                    "label": "空です"
                  },
                  "BLANK": {
                    "label": "空白です"
                  },
                  "無効です": {
                    "label": "無効です"
                  },
                  "REGEXの一致": {
                    "label": "REGEXの一致"
                  },
                  "CONTAINED IN TABLE": {
                    "label": "テーブル内"
                  },
                  "REGEXテーブル内": {
                    "label": "REGEXテーブル内"
                  },
                  "日付": {
                    "label": "日付"
                  }
                },
                "val": "含む"
              },
              "date_condition": {
                "type": "select",
                "label": "日付オペレータの選択",
                "required": true,
                "show_if": "this.condition == 'DATE'",
                "options": {
                  "<": {
                    "label": "前"
                  },
                  "<=": {
                    "label": "前または等しい"
                  },
                  "=": {
                    "label": "等しい"
                  },
                  ">": {
                    "label": "以降"
                  },
                  ">=": {
                    "label": "以降または等しい"
                  }
                },
                "val": "="
              },
              "case_sensitive": {
                "type": "select",
                "label": "ケース・センシティブの選択",
                "required": true,
                "show_if": "this.condition == 'CONTAINS' || this.condition == 'CONTAINED IN STRING' || this.condition == '=' || this.condition == 'STARTS WITH' || this.condition == 'ENDS WITH' || this.condition == 'REGEX MATCH' || this.condition == 'NOT REGEX MATCH' || this.condition == 'IN REGEX TABLE' || this.condition == 'NOT IN REGEX TABLE'",
                "options": {
                  "yes": {
                    "label": "ケース・センシティブ"
                  },
                  "no": {
                    "label": "ケース・インセンシティブ"
                  }
                },
                "val": "yes"
              },
              "date_value": {
                "type": "date",
                "label": "日付の選択",
                "show_if": "this.condition=='DATE'",
                "val": "",
                "required": true
              },
              "value": {
                "type": "text",
                "label": "値の入力",
                "show_if": "this.condition != 'DATE' && this.condition != 'EMPTY' && this.condition != 'NOT EMPTY' && this.condition != 'BLANK' && this.condition != 'NOT BLANK' && this.condition != 'IS NULL' && this.condition != 'IS NOT NULL' && this.condition != 'CONTAINED IN TABLE' && this.condition != 'NOT CONTAINED IN TABLE' && this.condition != 'IN REGEX TABLE' && this.condition != 'NOT IN REGEX TABLE'",
                "val": "",
                "required": true
              },
              "compare_table": {
                "type": "select",
                "label": "テーブルの選択",
                "bind": "TABLES",
                "empty": "",
                "show_if": "this.condition == 'CONTAINED IN TABLE' || this.condition == 'NOT CONTAINED IN TABLE' || this.condition == 'IN REGEX TABLE' || this.condition == 'NOT IN REGEX TABLE'",
                "required": true
              },
              "compare_column": {
                "type": "select",
                "label": "列の選択",
                "table": "this.compare_table",
                "bind": "COLUMNS",
                "show_if": "this.condition == 'CONTAINED IN TABLE' || this.condition == 'NOT CONTAINED IN TABLE' || this.condition == 'IN REGEX TABLE' || this.condition == 'NOT IN REGEX TABLE'",
                "required": true
              },
              "delete": {
                "type": "button",
                "label": "削除",
                "function": "DELETE_ROW"
              }
            }
          },
          "options.apply_to_record": {
            "type": "checkbox",
            "label": "レコードに適用",
            "val": "true",
            "isOn": "true",
            "isOff": "false",
            "style": "margin-top: 10px",
            "breakLine": true
          },
          "apply_to_group_box": {
            "type": "fieldset",
            "label": "グループに適用",
            "show_if": "options.apply_to_record=='false'",
            "repeat": false,
            "fields": {
              "options.apply_to_group_columns": {
                "type": "select",
                "label": "グループ列の選択",
                "required": true,
                "width": "100%",
                "table": "input_table",
                "multiple": true,
                "bind": "COLUMNS",
                "breakLine": true
              }
            }
          }
        }
      },
      "options.target_cells": {
        "type": "fieldset",
        "label": "追加された行（複数可）の定義",
        "repeat": true,
        "fields": {
          "column": {
            "type": "select",
            "label": "列の選択",
            "table": "input_table",
            "bind": "COLUMNS",
            "required": true
          },
          "formula_type": {
            "type": "select",
            "label": "数式タイプ",
            "required": true,
            "options": {
              "copy_selected": {
                "label": "選択されたコピー"
              },
              "static_value": {
                "label": "静的値"
              },
              "numeric_formula": {
                "label": "数式"
              },
              "text_formula": {
                "label": "テキスト数式"
              }
            },
            "val": "copy_selected"
          },
          "static_value": {
            "type": "text",
            "label": "値",
            "show_if": "this.formula_type=='static_value'",
            "val": ""
          },
          "numeric_formula": {
            "type": "text",
            "label": "数式（例：[Column1] + 2 * ([Row-1:Column3] - [Column1])",
            "show_if": "this.formula_type=='numeric_formula'",
            "val": "",
            "style": "width: 100%"
          },
          "rounding": {
            "type": "select",
            "label": "四捨五入",
            "show_if": "this.formula_type=='numeric_formula'",
            "required": true,
            "options": {
              "no_rounding": {
                "label": "四捨五入無し"
              },
              "0": {
                "label": "0桁"
              },
              "1": {
                "label": "1桁"
              },
              "2": {
                "label": "2桁"
              },
              "3": {
                "label": "3桁"
              },
              "4": {
                "label": "4桁"
              },
              "5": {
                "label": "5桁"
              },
              "6": {
                "label": "6桁"
              },
              "7": {
                "label": "7桁"
              },
              "8": {
                "label": "8桁"
              },
              "9": {
                "label": "9桁"
              }
            },
            "val": "no_rounding"
          },
          "text_formula": {
            "type": "text",
            "label": "Formula, e.g. [firstName] + ' + [lastName] + ' + [Row-1:Column3]",
            "show_if": "this.formula_type=='text_formula'",
            "val": "",
            "style": "width: 100%"
          },
          "use_multi_row": {
            "type": "checkbox",
            "label": "複数行オプション",
            "show_if": "options.position!='add_row_after_each_group' && (this.formula_type=='numeric_formula' || this.formula_type=='text_formula')",
            "val": "false",
            "isOn": "true",
            "isOff": "false"
          },
          "if_row_not_exist": {
            "type": "select",
            "label": "参照された行が存在しない場合",
            "show_if": "options.position!='add_row_after_each_group' && this.use_multi_row=='true' && (this.formula_type=='numeric_formula' || this.formula_type=='text_formula')",
            "required": true,
            "options": {
              "empty": {
                "label": "空または0"
              },
              "closest": {
                "label": "最も近い行の値"
              }
            },
            "val": "empty",
            "breakLine": true
          },
          "delete": {
            "type": "button",
            "label": "削除",
            "function": "DELETE_ROW"
          }
        },
        "breakLine": true
      },
      "options.set_date_column": {
        "type": "checkbox",
        "label": "選択した日付列にタイムシフトを適用する",
        "val": "false",
        "isOn": "true",
        "isOff": "false",
        "style": "margin-top: 20px",
        "breakLine": true
      },
      "options.date_column": {
        "type": "select",
        "label": "日付列の選択",
        "show_if": "options.set_date_column=='true'",
        "table": "input_table",
        "bind": "COLUMNS",
        "required": true
      },
      "options.period_shift.number": {
        "type": "text",
        "label": "期間シフト",
        "show_if": "options.set_date_column=='true'",
        "required": true,
        "val": "0"
      },
      "options.period_shift.period": {
        "type": "select",
        "label": "周期の選択",
        "show_if": "options.set_date_column=='true'",
        "options": {
          "day": {
            "label": "日"
          },
          "week": {
            "label": "週"
          },
          "month": {
            "label": "月"
          },
          "year": {
            "label": "年"
          }
        },
        "val": "month"
      }
    }
  }
}