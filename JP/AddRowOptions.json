    "type": "fieldset", 
    "label": "オプションの選択", 
    "repeat": false, 
    "fields": {
      "options.position": {
        "type": "select", 
        "label": "位置の選択", 
        "required": true, 
        "show_if": "table_type=='header_only'", 
        "options": {
          "add_rows_below_selected_row": {
            "label": "下に行を追加"
          }, 
          "add_rows_above_selected_row": {
            "label": "上に行を追加"
          }, 
          "add_rows_at_table_end": {
            "label": "テーブルの最後に行を追加"
          }, 
          "add_single_row_at_table_end": {
            "label": "テーブルの最後に1行追加"
          }, 
          "add_row_after_each_group": {
            "label": "各グループの後に行を追加"
          }
        }, 
        "val": "add_rows_below_selected_row", 
        "breakLine": true
      }, 
      "options.group_column": {
        "type": "select", 
        "label": "列のグループ", 
        "show_if": "table_type=='header_only' && options.position=='add_row_after_each_group'", 
        "table": "input_table", 
        "bind": "COLUMNS", 
        "required": true, 
        "breakLine": true
      }, 
      "options.use_filter": {
        "type": "checkbox", 
        "label": "フィルターの使用", 
        "show_if": "table_type=='header_only' && (options.position=='add_rows_below_selected_row' || options.position=='add_rows_above_selected_row' || options.position=='add_rows_at_table_end')", 
        "val": "false", 
        "isOn": "true", 
        "isOff": "false", 
        "breakLine": true, 
        "style": "margin-top: 10px"
      }, 
      "filter_box": {
        "type": "fieldset", 
        "label": "フィルターオプション", 
        "show_if": "table_type=='header_only' && options.use_filter=='true' && (options.position=='add_rows_below_selected_row' || options.position=='add_rows_above_selected_row' || options.position=='add_rows_at_table_end')", 
        "repeat": false, 
        "fields": {
          "options.filter": {
            "type": "fieldset", 
            "label": "フィルター条件の追加", 
            "repeat": true, 
            "fields": {
              "initial_op": {
                "type": "select", 
                "label": "論理オペレータの選択", 
                "required": true, 
                "show_if": "$index == 0", 
                "options": {
                  "if": {
                    "label": "IF"
                  }, 
                  "if_not": {
                    "label": "IF NOT"
                  }
                }, 
                "val": "if"
              }, 
              "op": {
                "type": "select", 
                "label": "論理オペレータの選択", 
                "required": true, 
                "show_if": "$index > 0", 
                "options": {
                  "and": {
                    "label": "AND"
                  }, 
                  "or": {
                    "label": "OR"
                  }, 
                  "and_not": {
                    "label": "AND NOT"
                  }, 
                  "or_not": {
                    "label": "OR NOT"
                  }
                }, 
                "val": "and"
              }, 
              "column": {
                "type": "select", 
                "label": "列の選択", 
                "table": "input_table", 
                "bind": "COLUMNS", 
                "required": true
              }, 
              "condition": {
                "type": "select", 
                "label": "オペレータの選択", 
                "required": true, 
                "options": {
                  "含む": {
                    "label": "含む"
                  }, 
                  "CONTAINED IN STRING": {
                    "label": "次の中に含む"
                  }, 
                  "次で開始": {
                    "label": "次で開始"
                  }, 
                  "次で終了": {
                    "label": "次で終了"
                  }, 
                  "=": {
                    "label": "等しい"
                  }, 
                  ">": {
                    "label": "超"
                  }, 
                  ">=": {
                    "label": "以上"
                  }, 
                  "<": {
                    "label": "より小さい"
                  }, 
                  "<=": {
                    "label": "より小さいまたは等しい"
                  }, 
                  "EMPTY": {
                    "label": "空です"
                  }, 
                  "BLANK": {
                    "label": "空白です"
                  }, 
                  "無効です": {
                    "label": "無効です"
                  }, 
                  "REGEXの一致": {
                    "label": "REGEXの一致"
                  }, 
                  "CONTAINED IN TABLE": {
                    "label": "テーブル内"
                  }, 
                  "REGEXテーブル内": {
                    "label": "REGEXテーブル内"
                  }, 
                  "日付": {
                    "label": "日付"
                  }
                }, 
                "val": "含む"
              }, 
              "date_condition": {
                "type": "select", 
                "label": "日付オペレータの選択", 
                "required": true, 
                "show_if": "this.condition == 'DATE'", 
                "options": {
                  "<": {
                    "label": "前"
                  }, 
                  "<=": {
                    "label": "前または等しい"
                  }, 
                  "=": {
                    "label": "等しい"
                  }, 
                  ">": {
                    "label": "以降"
                  }, 
                  ">=": {
                    "label": "以降または等しい"
                  }
                }, 
                "val": "="
              }, 
              "case_sensitive": {
                "type": "select", 
                "label": "ケース・センシティブの選択", 
                "required": true, 
                "show_if": "this.condition == 'CONTAINS' || this.condition == 'CONTAINED IN STRING' || this.condition == '=' || this.condition == 'STARTS WITH' || this.condition == 'ENDS WITH' || this.condition == 'REGEX MATCH' || this.condition == 'NOT REGEX MATCH' || this.condition == 'IN REGEX TABLE' || this.condition == 'NOT IN REGEX TABLE'", 
                "options": {
                  "yes": {
                    "label": "ケース・センシティブ"
                  }, 
                  "no": {
                    "label": "ケース・インセンシティブ"
                  }
                }, 
                "val": "yes"
              }, 
              "date_value": {
                "type": "date", 
                "label": "日付の選択", 
                "show_if": "this.condition=='DATE'", 
                "val": "1/31/2020", 
                "required": true
              }, 
              "value": {
                "type": "text", 
                "label": "値の入力", 
                "show_if": "this.condition != 'DATE' && this.condition != 'EMPTY' && this.condition != 'NOT EMPTY' && this.condition != 'BLANK' && this.condition != 'NOT BLANK' && this.condition != 'IS NULL' && this.condition != 'IS NOT NULL' && this.condition != 'CONTAINED IN TABLE' && this.condition != 'NOT CONTAINED IN TABLE' && this.condition != 'IN REGEX TABLE' && this.condition != 'NOT IN REGEX TABLE'", 
                "val": "", 
                "required": true
              }, 
              "compare_table": {
                "type": "select", 
                "label": "テーブルの選択", 
                "bind": "TABLES", 
                "empty": "", 
                "show_if": "this.condition == 'CONTAINED IN TABLE' || this.condition == 'NOT CONTAINED IN TABLE' || this.condition == 'IN REGEX TABLE' || this.condition == 'NOT IN REGEX TABLE'", 
                "required": true
              }, 
              "compare_column": {
                "type": "select", 
                "label": "列の選択", 
                "table": "this.compare_table", 
                "bind": "COLUMNS", 
                "show_if": "this.condition == 'CONTAINED IN TABLE' || this.condition == 'NOT CONTAINED IN TABLE' || this.condition == 'IN REGEX TABLE' || this.condition == 'NOT IN REGEX TABLE'", 
                "required": true
              }, 
              "delete": {
                "type": "button", 
                "label": "削除", 
                "function": "DELETE_ROW"
              }
