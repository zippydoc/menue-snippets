{
  "options": {
    "type": "fieldset",
    "label": "クリーニングオプションの選択",
    "repeat": false,
    "fields": {
      "replace_null": {
        "type": "fieldset",
        "label": "空のレコード",
        "repeat": false,
        "fields": {
          "replace_null": {
            "type": "checkbox",
            "label": "空のレコード",
            "val": "false",
            "isOn": "true",
            "isOff": "false",
            "breakLine": true
          },
          "replace_null_target": {
            "type": "select",
            "label": "置換",
            "show_if": "replace_null=='true'",
            "options": {
              "empty": {
                "label": "空"
              },
              "blank": {
                "label": "空白"
              },
              "null": {
                "label": "NULL"
              }
            },
            "val": "empty",
            "breakLine": true
          },
          "replace_null_by": {
            "type": "radio",
            "show_if": "replace_null=='true'",
            "val": "by_blank",
            "values": {
              "by_blank": "By Blank",
              "by_zero": "By Zero"
            },
            "breakLine": true
          }
        }
      },
      "remove_whitespace": {
        "type": "fieldset",
        "label": "空白の削除",
        "repeat": false,
        "fields": {
          "remove_characters.all_whitespace": {
            "type": "checkbox",
            "label": "全ての空白",
            "val": "false",
            "isOn": "true",
            "isOff": "false",
            "breakLine": true
          },
          "remove_characters.leading_whitespace": {
            "type": "checkbox",
            "label": "先頭の空白",
            "show_if": "remove_characters.all_whitespace=='false'",
            "val": "false",
            "isOn": "true",
            "isOff": "false",
            "breakLine": true
          },
          "remove_characters.trailing_whitespace": {
            "type": "checkbox",
            "label": "末尾空白",
            "show_if": "remove_characters.all_whitespace=='false'",
            "val": "false",
            "isOn": "true",
            "isOff": "false",
            "breakLine": true
          },
          "remove_characters.whitespace": {
            "type": "checkbox",
            "label": "スペース",
            "show_if": "remove_characters.all_whitespace=='false'",
            "val": "false",
            "isOn": "true",
            "isOff": "false",
            "breakLine": true
          },
          "remove_characters.tabs": {
            "type": "checkbox",
            "label": "タブ",
            "show_if": "remove_characters.all_whitespace=='false'",
            "val": "false",
            "isOn": "true",
            "isOff": "false",
            "breakLine": true
          },
          "remove_characters.line_breaks": {
            "type": "checkbox",
            "label": "改行",
            "show_if": "remove_characters.all_whitespace=='false'",
            "val": "false",
            "isOn": "true",
            "isOff": "false",
            "breakLine": true
          },
          "remove_characters.duplicate_whitespace": {
            "type": "checkbox",
            "label": "空白の重複",
            "show_if": "remove_characters.all_whitespace=='false'",
            "val": "false",
            "isOn": "true",
            "isOff": "false",
            "breakLine": true
          }
        }
      },
      "remove_duplicates": {
        "type": "fieldset",
        "label": "重複の削除",
        "repeat": false,
        "fields": {
          "remove_duplicates.cell": {
            "type": "checkbox",
            "label": "セル内の重複",
            "val": "false",
            "isOn": "true",
            "isOff": "false",
            "breakLine": true
          },
          "remove_duplicates.delimiter": {
            "type": "text",
            "label": "区切り文字",
            "show_if": "remove_duplicates.cell=='true'",
            "val": ";",
            "required": true
          },
          "remove_duplicates.include_whitespace": {
            "type": "checkbox",
            "label": "(空白を含む)",
            "show_if": "remove_duplicates.cell=='true'",
            "val": "true",
            "isOn": "true",
            "isOff": "false",
            "breakLine": true
          },
          "remove_duplicates.rows": {
            "type": "checkbox",
            "label": "行の重複",
            "val": "false",
            "isOn": "true",
            "isOff": "false",
            "breakLine": true
          }
        }
      },
      "remove_characters": {
        "type": "fieldset",
        "label": "文字の削除",
        "repeat": false,
        "fields": {
          "remove_characters.letters": {
            "type": "checkbox",
            "label": "文字",
            "val": "false",
            "isOn": "true",
            "isOff": "false",
            "breakLine": true
          },
          "remove_characters.numbers": {
            "type": "checkbox",
            "label": "数字",
            "val": "false",
            "isOn": "true",
            "isOff": "false",
            "breakLine": true
          },
          "remove_characters.punctuation": {
            "type": "checkbox",
            "label": "句読点",
            "val": "false",
            "isOn": "true",
            "isOff": "false",
            "breakLine": true
          }
        }
      },
      "modify_case": {
        "type": "fieldset",
        "label": "ケースの修正",
        "repeat": false,
        "fields": {
          "modify_case": {
            "type": "checkbox",
            "label": "ケースの修正",
            "val": "false",
            "isOn": "true",
            "isOff": "false",
            "breakLine": true
          },
          "modify_case_by": {
            "type": "radio",
            "show_if": "modify_case=='true'",
            "val": "upper",
            "values": {
              "upper": "to Upper Case",
              "lower": "to Lower Case",
              "title": "to Title Case"
            },
            "breakLine": true
          }
        }
      }
    }
  }
}