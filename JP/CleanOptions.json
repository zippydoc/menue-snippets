{
  "options": {
    "type": "fieldset",
    "label": "クリーニングオプションの選択",
    "repeat": false,
    "fields": {
      "replace_null": {
        "type": "fieldset",
        "label": "空のレコード",
        "repeat": false,
        "fields": {
          "replace_null": {
            "type": "checkbox",
            "label": "空のレコード",
            "val": "false",
            "isOn": "true",
            "isOff": "false",
            "breakLine": true
          },
          "replace_null_target": {
            "type": "select",
            "label": "置換",
            "show_if": "replace_null=='true'",
            "options": {
              "empty": {
                "label": "空"
              },
              "blank": {
                "label": "空白"
              },
              "null": {
                "label": "NULL"
              }
            },
            "val": "empty",
            "breakLine": true
          },
          "replace_null_by": {
            "type": "radio",
            "show_if": "replace_null=='true'",
            "val": "by_blank",
            "values": {
              "by_blank": "By Blank",
              "by_zero": "By Zero"
            },
            "breakLine": true
          }
        }
      },
      "remove_whitespace": {
        "type": "fieldset",
        "label": "空白の削除",
        "repeat": false,
        "fields": {
          "remove_characters.all_whitespace": {
            "type": "checkbox",
            "label": "全ての空白",
            "val": "false",
            "isOn": "true",
            "isOff": "false",
            "breakLine": true
          },
          "remove_characters.leading_whitespace": {
            "type": "checkbox",
            "label": "先頭の空白",
            "show_if": "remove_characters.all_whitespace=='false'",
            "val": "false",
            "isOn": "true",
            "isOff": "false",
            "breakLine": true
          },
          "remove_characters.trailing_whitespace": {
            "type": "checkbox",
            "label": "末尾空白",
            "show_if": "remove_characters.all_whitespace=='false'",
            "val": "false",
            "isOn": "true",
            "isOff": "false",
            "breakLine": true
          },
          "remove_characters.whitespace": {
            "type": "checkbox",
            "label": "スペース",
            "show_if": "remove_characters.all_whitespace=='false'",
            "val": "false",
            "isOn": "true",
            "isOff": "false",
            "breakLine": true
          },
          "remove_characters.tabs": {
            "type": "checkbox",
            "label": "タブ",
            "show_if": "remove_characters.all_whitespace=='false'",
            "val": "false",
            "isOn": "true",
            "isOff": "false",
            "breakLine": true
          },
          "remove_characters.line_breaks": {
            "type": "checkbox",
            "label": "改行",
            "show_if": "remove_characters.all_whitespace=='false'",
            "val": "false",
            "isOn": "true",
            "isOff": "false",
            "breakLine": true
          },
          "remove_characters.duplicate_whitespace": {
            "type": "checkbox",
            "label": "空白の重複",
            "show_if": "remove_characters.all_whitespace=='false'",
            "val": "false",
            "isOn": "true",
            "isOff": "false",
            "breakLine": true
          }
        }
      },
      "remove_duplicates": {
        "type": "fieldset",
        "label": "重複の削除",
        "repeat": false,
        "fields": {
          "remove_duplicates.cell": {
            "type": "checkbox",
            "label": "セル内の重複",
            "val": "false",
            "isOn": "true",
            "isOff": "false",
            "breakLine": true
          },
          "remove_duplicates.delimiter": {
            "type": "text",
            "label": "区切り文字",
            "show_if": "remove_duplicates.cell=='true'",
            "val": ";",
            "required": true
          },
          "remove_duplicates.include_whitespace": {
            "type": "checkbox",
            "label": "(空白を含む)",
            "show_if": "remove_duplicates.cell=='true'",
            "val": "true",
            "isOn": "true",
            "isOff": "false",
            "breakLine": true
          },
          "remove_duplicates.rows": {
            "type": "checkbox",
            "label": "行の重複",
            "val": "false",
            "isOn": "true",
            "isOff": "false",
            "breakLine": true
          }
        }
      },
      "remove_characters": {
        "type": "fieldset",
        "label": "文字の削除",
        "repeat": false,
        "fields": {
          "remove_characters.letters": {
            "type": "checkbox",
            "label": "文字",
            "val": "false",
            "isOn": "true",
            "isOff": "false",
            "breakLine": true
          },
          "remove_characters.numbers": {
            "type": "checkbox",
            "label": "数字",
            "val": "false",
            "isOn": "true",
            "isOff": "false",
            "breakLine": true
          },
          "remove_characters.punctuation": {
            "type": "checkbox",
            "label": "句読点",
            "val": "false",
            "isOn": "true",
            "isOff": "false",
            "breakLine": true
          },
          "remove_characters.trailing_zero": {
            "type": "checkbox",
            "label": "Trailing 0",
            "val": "false",
            "isOn": "true",
            "isOff": "false",
            "breakLine": true
          },
          "remove_characters.custom": {
            "type": "checkbox",
            "label": "To-be-delted Characters",
            "val": "false",
            "isOn": "true",
            "isOff": "false",
            "breakLine": true
          },
          "remove_characters.custom_chars": {
            "type": "text",
            "label": "to be deleted characters",
            "show_if": "remove_characters.custom=='true'",
            "val": "",
            "required": true
          },
          "remove_characters.custom_mode": {
            "type": "select",
            "label": "ポジション",
            "show_if": "remove_characters.custom=='true'",
            "options": {
              "all": {
                "label": "all"
              },
              "leading only": {
                "label": "leading only"
              },
              "trailing only": {
                "label": "trailing only"
              }
            },
            "val": "all"
          },
          "remove_characters.custom_delimiter": {
            "type": "select",
            "label": "区切り文字",
            "show_if": "remove_characters.custom=='true'",
            "options": {
              ",": {
                "label": ","
              },
              ";": {
                "label": ";"
              },
              ".": {
                "label": "."
              }
            },
            "val": ","
          }
        }
      },
      "modify_case": {
        "type": "fieldset",
        "label": "ケースの修正",
        "repeat": false,
        "fields": {
          "modify_case": {
            "type": "checkbox",
            "label": "ケースの修正",
            "val": "false",
            "isOn": "true",
            "isOff": "false",
            "breakLine": true
          },
          "modify_case_by": {
            "type": "radio",
            "show_if": "modify_case=='true'",
            "val": "upper",
            "values": {
              "upper": "to Upper Case",
              "lower": "to Lower Case",
              "title": "to Title Case"
            },
            "breakLine": true
          }
        }
      },
      "rounding": {
        "type": "fieldset",
        "label": "四捨五入",
        "repeat": false,
        "fields": {
          "is_rounding": {
            "type": "checkbox",
            "label": "四捨五入",
            "val": "false",
            "isOn": "true",
            "isOff": "false",
            "breakLine": true
          },
          "rounding": {
            "type": "select",
            "label": "四捨五入",
            "show_if": "is_rounding=='true'",
            "required": true,
            "options": {
              "no_rounding": {
                "label": "四捨五入無し"
              },
              "0": {
                "label": "0桁"
              },
              "1": {
                "label": "1桁"
              },
              "2": {
                "label": "2桁"
              },
              "3": {
                "label": "3桁"
              },
              "4": {
                "label": "4桁"
              },
              "5": {
                "label": "5桁"
              },
              "6": {
                "label": "6桁"
              },
              "7": {
                "label": "7桁"
              },
              "8": {
                "label": "8桁"
              },
              "9": {
                "label": "9桁"
              }
            },
            "val": "no_rounding"
          }
        }
      },
      "options.use_filter": {
        "type": "checkbox",
        "label": "フィルターの使用",
        "val": "false",
        "isOn": "true",
        "isOff": "false",
        "style": "margin-top: 10px",
        "breakLine": true
      },
      "filter_box": {
        "type": "fieldset",
        "label": "フィルターオプション",
        "show_if": "options.use_filter=='true'",
        "repeat": false,
        "fields": {
          "options.filter": {
            "type": "fieldset",
            "label": "フィルター条件の追加",
            "repeat": true,
            "fields": {
              "initial_op": {
                "type": "select",
                "label": "論理オペレータの選択",
                "required": true,
                "show_if" :  "$index == 0",
                "options": {
                  "if": {
                    "label": "IF"
                  },
                  "if_not": {
                    "label": "IF NOT"
                  }
                },
                "val": "if"
              },
              "op": {
                "type": "select",
                "label": "論理オペレータの選択",
                "required": true,
                "show_if" :  "$index > 0",
                "options": {
                  "and": {
                    "label": "AND"
                  },
                  "or": {
                    "label": "OR"
                  },
                  "and_not": {
                    "label": "AND NOT"
                  },
                  "or_not": {
                    "label": "OR NOT"
                  }
                },
                "val": "and"
              },
              "column": {
                "type": "select",
                "label": "列の選択",
                "table": "table_id",
                "bind": "COLUMNS",
                "required": true
              },
              "condition": {
                "type": "select",
                "label": "オペレータの選択",
                "required": true,
                "options": {
                  "CONTAINS": {
                    "label": "含む"
                  },
                  "CONTAINED IN STRING": {
                    "label": "次の中に含む"
                  },
                  "STARTS WITH": {
                    "label": "次で開始"
                  },
                  "ENDS WITH": {
                    "label": "次で終了"
                  },
                  "=": {
                    "label": "等しい"
                  },
                  ">": {
                    "label": "超"
                  },
                  ">=": {
                    "label": "以上"
                  },
                  "<": {
                    "label": "より小さい"
                  },
                  "<=": {
                    "label": "より小さいまたは等しい"
                  },
                  "EMPTY": {
                    "label": "空です"
                  },
                  "BLANK": {
                    "label": "空白です"
                  },
                  "IS NULL": {
                    "label": "無効です"
                  },
                  "IS TRUE": {
                    "label": "IS TRUE"
                  },
                  "IS FALSE": {
                    "label": "IS FALSE"
                  },
                  "REGEX MATCH": {
                    "label": "REGEXの一致"
                  },
                  "CONTAINED IN TABLE": {
                    "label": "テーブル内"
                  },
                  "IN REGEX TABLE": {
                    "label": "REGEXテーブル内"
                  },
                  "DATE": {
                    "label": "日付"
                  }
                },
                "val": "CONTAINS"
              },
              "date_condition": {
                "type": "select",
                "label": "日付オペレータの選択",
                "required": true,
                "show_if": "this.condition == 'DATE'",
                "options": {
                  "<": {
                    "label": "前"
                  },
                  "<=": {
                    "label": "前または等しい"
                  },
                  "=": {
                    "label": "等しい"
                  },
                  ">": {
                    "label": "以降"
                  },
                  ">=": {
                    "label": "以降または等しい"
                  }
                },
                "val": "="
              },
              "compare_with": {
                "type": "select",
                "label": "Compare with",
                "required": true,
                "show_if": "this.condition=='CONTAINS' || this.condition=='CONTAINED IN STRING' || this.condition=='STARTS WITH' || this.condition=='ENDS WITH' || this.condition=='=' || this.condition=='>' || this.condition=='>=' || this.condition=='<' || this.condition=='<='",
                "options": {
                  "custom_value": {
                    "label": "Custom value"
                  },
                  "column_value": {
                    "label": "Column value"
                  }
                },
                "val": "custom_value"
              },
              "self_compare_column": {
                "type": "select",
                "label": "Select Compare Column",
                "table": "table_id",
                "bind": "COLUMNS",
                "show_if": "this.compare_with=='column_value' && (this.condition=='CONTAINS' || this.condition=='CONTAINED IN STRING' || this.condition=='STARTS WITH' || this.condition=='ENDS WITH' || this.condition=='=' || this.condition=='>' || this.condition=='>=' || this.condition=='<' || this.condition=='<=')",
                "required": true
              },
              "date_value": {
                "type": "date",
                "label": "日付の選択",
                "show_if": "this.condition=='DATE'",
                "val": "",
                "required": true
              },
              "value": {
                "type": "text",
                "label": "値の入力",
                "show_if": "this.compare_with=='custom_value' && this.condition != 'DATE' && this.condition != 'EMPTY' && this.condition != 'NOT EMPTY' && this.condition != 'BLANK' && this.condition != 'NOT BLANK' && this.condition != 'IS NULL' && this.condition != 'IS NOT NULL' && this.condition != 'IS TRUE' && this.condition != 'IS FALSE' && this.condition != 'CONTAINED IN TABLE' && this.condition != 'NOT CONTAINED IN TABLE' && this.condition != 'IN REGEX TABLE' && this.condition != 'NOT IN REGEX TABLE'",
                "val": "",
                "required": true
              },
              "case_sensitive": {
                "type": "select",
                "label": "ケース・センシティブの選択",
                "required": true,
                "show_if": "this.condition == 'CONTAINS' || this.condition == 'CONTAINED IN STRING' || this.condition == '=' || this.condition == 'STARTS WITH' || this.condition == 'ENDS WITH' || this.condition == 'REGEX MATCH' || this.condition == 'NOT REGEX MATCH' || this.condition == 'IN REGEX TABLE' || this.condition == 'NOT IN REGEX TABLE'",
                "options": {
                  "yes": {
                    "label": "ケース・センシティブ"
                  },
                  "no": {
                    "label": "ケース・インセンシティブ"
                  }
                },
                "val": "yes"
              },
              "compare_table": {
                "type": "select",
                "label": "テーブルの選択",
                "bind": "TABLES",
                "empty": "",
                "show_if": "this.condition == 'CONTAINED IN TABLE' || this.condition == 'NOT CONTAINED IN TABLE' || this.condition == 'IN REGEX TABLE' || this.condition == 'NOT IN REGEX TABLE'",
                "required": true
              },
              "compare_column": {
                "type": "select",
                "label": "列の選択",
                "table": "this.compare_table",
                "bind": "COLUMNS",
                "show_if": "this.condition == 'CONTAINED IN TABLE' || this.condition == 'NOT CONTAINED IN TABLE' || this.condition == 'IN REGEX TABLE' || this.condition == 'NOT IN REGEX TABLE'",
                "required": true
              },
              "delete": {
                "type": "button",
                "label": "削除",
                "function": "DELETE_ROW"
              }
            }
          },
          "options.apply_to_record": {
            "type": "checkbox",
            "label": "レコードに適用",
            "val": "true",
            "isOn": "true",
            "isOff": "false",
            "style": "margin-top: 10px",
            "breakLine": true
          },
          "apply_to_group_box": {
            "type": "fieldset",
            "label": "グループに適用",
            "show_if": "options.apply_to_record=='false'",
            "repeat": false,
            "fields": {
              "options.apply_to_group_columns": {
                "type": "select",
                "label": "グループ列の選択",
                "required": true,
                "width": "100%",
                "table": "table_id",
                "multiple": true,
                "bind": "COLUMNS",
                "breakLine": true
              }
            }
          }
        }
      }
    }
  }
}