{
  "options": {
    "type": "fieldset",
    "label": "オプション",
    "repeat": false,
    "fields": {
      "compare_set": {
        "type": "fieldset",
        "label": "パターンの選択",
        "repeat": true,
        "fields": {
          "pattern": {
            "type": "select",
            "label": "テーブルパターンタイプ",
            "options": {
              "all_col": {
                "label": "全ての列アイデンティティ"
              },
              "two_col": {
                "label": "二列アイデンティティ"
              }
            },
            "val": "all_col",
            "required": true
          },
          "compare_type": {
            "type": "select",
            "label": "セルパターンの種類",
            "options": {
              "equal": {
                "label": "等しい"
              },
              "contain": {
                "label": "含む"
              },
              "start_with": {
                "label": "次で開始"
              },
              "end_with": {
                "label": "次で終了"
              },
              "reg_exp": {
                "label": "正規表現"
              }
            },
            "val": "equal",
            "required": true
          },
          "case_sensitive": {
            "type": "select",
            "label": "大文字と小文字の区別",
            "required": true,
            "options": {
              "yes": {
                "label": "ケース・センシティブ"
              },
              "no": {
                "label": "ケース・インセンシティブ"
              }
            },
            "val": "no"
          },
          "expression": {
            "type": "text",
            "label": "表現",
            "show_if": "this.compare_type != 'equal'",
            "required": true,
            "val": ""
          },
          "delete": {
            "show_if": "this.field_name != ''",
            "type": "button",
            "label": "削除",
            "function": "DELETE_ROW"
          }
        },
        "breakLine": true
      },
      "target": {
        "type": "select",
        "label": "対象",
        "options": {
          "to_table": {
            "label": "テーブルに適用"
          },
          "to_rows": {
            "label": "行ごと"
          }
        },
        "val": "to_table",
        "required": true
      },
      "match_type": {
        "type": "select",
        "show_if": "target != 'to_table'",
        "label": "一致タイプ",
        "options": {
          "match": {
            "label": "一致のみ"
          },
          "nonmatch": {
            "label": "不一致のみ"
          },
          "all": {
            "label": "全て"
          }
        },
        "val": "match",
        "required": true,
        "breakLine": true
      },
      "create_sql": {
        "type": "checkbox",
        "label": "SQLの詳細を表示",
        "val": "false",
        "isOn": "true",
        "isOff": "false"
      }
    }
  }
}