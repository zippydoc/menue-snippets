{
  "options": {
    "type": "fieldset",
    "label": "オプションの選択",
    "repeat": false,
    "fields": {
      "options.convert_type": {
        "type": "select",
        "label": "変換タイプ",
        "required": true,
        "options": {
          "complete_date": {
            "label": "完成日"
          },
          "expand_date": {
            "label": "日付の展開"
          },
          "convert_date": {
            "label": "日付の変換"
          },
          "convert_date_range": {
            "label": "日付範囲の変換"
          }
        },
        "val": "complete_date"
      },
      "options.date_convert_type": {
        "type": "select",
        "label": "変換先",
        "show_if": "options.convert_type=='convert_date'",
        "required": true,
        "options": {
          "begin_of_month": {
            "label": "月初め"
          },
          "end_of_month": {
            "label": "月末"
          },
          "hy_year": {
            "label": "半年"
          },
          "qrt_year": {
            "label": "四半期"
          },
          "month_year": {
            "label": "月 年"
          },
          "week_year": {
            "label": "週 年"
          }
        },
        "val": "begin_of_month"
      },
      "options.date_range_convert_type": {
        "type": "select",
        "label": "変換先",
        "show_if": "options.convert_type=='convert_date_range'",
        "required": true,
        "options": {
          "begin_of_month": {
            "label": "月初め"
          },
          "end_of_month": {
            "label": "月末"
          },
          "begin_of_range": {
            "label": "範囲の始まり"
          },
          "end_of_range": {
            "label": "範囲の終わり"
          },
          "begin_of_quarter": {
            "label": "四半期の初め"
          },
          "end_of_quarter": {
            "label": "四半期末"
          }
        },
        "val": "begin_of_month",
        "breakLine": true
      },
      "options.output_in_new_column": {
        "type": "checkbox",
        "label": "新しい列の出力",
        "val": "false",
        "isOn": "true",
        "isOff": "false"
      },
      "options.new_field_name": {
        "type": "text",
        "label": "新しい列名",
        "show_if": "options.output_in_new_column=='true'",
        "val": "GeneratedDate",
        "required": true,
        "minLength": 1,
        "maxLength": 255,
        "breakLine": true
      }
    }
  }
}