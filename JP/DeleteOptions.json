{
  "placeholder": {
    "breakLine": true
  },
  "delete_options": {
    "type": "select",
    "label": "オプションの削除",
    "required": true,
    "options": {
      "del_table": {
        "label": "テーブルの削除"
      },
      "del_columns": {
        "label": "選択された列の削除"
      },
      "del_rows": {
        "label": "選択された行の削除"
      }
    },
    "val": "del_table"
  },
  "apply_columns": {
    "type": "select",
    "label": "削除する列の選択",
    "show_if": "delete_options=='del_columns'",
    "required": true,
    "table": "input_table",
    "multiple": true,
    "bind": "COLUMNS"
  },
  "apply_rows": {
    "type": "select",
    "label": "削除する行の選択",
    "show_if": "delete_options=='del_rows'",
    "required": true,
    "table": "input_table",
    "multiple": true,
    "bind": "LABELS"
  }
}