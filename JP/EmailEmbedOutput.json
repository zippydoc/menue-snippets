{
  "embed_output_frame": {
    "type": "fieldset",
    "label": "Define Embed(Attach) Table Name",
    "show_if": "options.add_table=='true'",
    "repeat": false,
    "fields": {
      "embed_output.custom_text": {
        "type": "text",
        "label": "出力テーブル名",
        "show_if": "embed_output.compose_table_name=='false'",
        "minLength": 1,
        "val": "",
        "width": "100%",
        "breakLine": true
      },
      "embed_output.custom_options": {
        "type": "checkbox",
        "label": "出力オプション",
        "val": "false",
        "isOn": "true",
        "isOff": "false",
        "breakLine": true
      },
      "embed_output.compose_table_name": {
        "type": "checkbox",
        "label": "テーブル名の構成",
        "show_if": "embed_output.custom_options=='true'",
        "val": "false",
        "isOn": "true",
        "isOff": "false",
        "breakLine": true
      },
      "embed_output_compose_table_name_frame": {
        "type": "fieldset",
        "label": "出力テーブル名の構成",
        "show_if": "embed_output.custom_options=='true' && embed_output.compose_table_name=='true'",
        "repeat": false,
        "fields": {
          "embed_output.prefix_rule": {
            "type": "select",
            "label": "プレフィックス",
            "val": "empty",
            "options": {
              "empty": {
                "label": "-"
              },
              "script_name": {
                "label": "タスク名"
              },
              "date_string_type1": {
                "label": "日付:  dd_mm_yyyy"
              },
              "date_string_type2": {
                "label": "日付:  yyyy_mm_dd"
              },
              "time_string": {
                "label": "時間: 時_分_秒"
              },
              "datetime_string": {
                "label": "日付_時間"
              },
              "custom": {
                "label": "カスタム"
              }
            }
          },
          "embed_output.prefix_text": {
            "type": "text",
            "label": "プレフィックステキスト",
            "show_if": "embed_output.prefix_rule=='custom'",
            "minLength": 1,
            "maxLength": 31,
            "val": ""
          },
          "embed_output.core_delimiter": {
            "type": "text",
            "label": "区切り文字",
            "show_if": "embed_output.prefix_rule!='empty'",
            "val": "_",
            "width": "50px"
          },
          "embed_output.core_rule": {
            "type": "select",
            "label": "列名",
            "val": "input_table_name",
            "options": {
              "input_table_name": {
                "label": "テーブル名の入力"
              },
              "script_name": {
                "label": "タスク名"
              },
              "custom": {
                "label": "カスタム"
              }
            }
          },
          "embed_output.core_text": {
            "type": "text",
            "label": "メインテーブル名",
            "show_if": "embed_output.core_rule=='custom'",
            "required": true,
            "minLength": 1,
            "maxLength": 31,
            "val": ""
          },
          "embed_output.suffix_delimiter": {
            "type": "text",
            "label": "区切り文字",
            "val": "_",
            "width": "50px"
          },
          "embed_output.suffix_rule": {
            "type": "select",
            "label": "サフィックス",
            "val": "script_name",
            "options": {
              "empty": {
                "label": "-"
              },
              "script_name": {
                "label": "タスク名"
              },
              "date_string_type1": {
                "label": "日付:  dd_mm_yyyy"
              },
              "date_string_type2": {
                "label": "日付:  yyyy_mm_dd"
              },
              "time_string": {
                "label": "時間: 時_分_秒"
              },
              "datetime_string": {
                "label": "日付_時間"
              },
              "custom": {
                "label": "カスタム"
              }
            }
          },
          "embed_output.suffix_text": {
            "type": "text",
            "label": "サフィックステキスト",
            "show_if": "embed_output.suffix_rule=='custom'",
            "minLength": 1,
            "maxLength": 31,
            "val": "",
            "breakLine": true
          },
          "embed_output.timezone": {
            "type": "select",
            "label": "タイムゾーン",
            "show_if": "embed_output.prefix_rule=='date_string_type1' || embed_output.prefix_rule=='date_string_type2' || embed_output.prefix_rule=='time_string' || embed_output.prefix_rule=='datetime_string' || embed_output.suffix_rule=='date_string_type1' || embed_output.suffix_rule=='date_string_type2' || embed_output.suffix_rule=='time_string' || embed_output.suffix_rule=='datetime_string'",
            "val": "Europe/Berlin",
            "required": true,
            "options": {
              "America/Adak": {
                "label": "アダック、ガンビアー"
              },
              "Australia/Adelaide": {
                "label": "アデレード、ブロークンヒル、ダーウィン"
              },
              "America/Anchorage": {
                "label": "Anchorage, Juneau, Sitka"
              },
              "Pacific/Apia": {
                "label": "アピア、アサウ、ムリファヌア、アフェガ、レウルモエガ"
              },
              "Pacific/Enderbury": {
                "label": "アピア、フィジー、トンガタプ"
              },
              "Antarctica/McMurdo": {
                "label": "オークランド ケーシー デービス"
              },
              "Pacific/Auckland": {
                "label": "オークランド、ウェリントン、クライストチャーチ"
              },
              "Asia/Bangkok": {
                "label": "バンコク、ジャカルタ、ノボシビルスク"
              },
              "Europe/Berlin": {
                "label": "ベルリン、パリ、アムステルダム"
              },
              "Australia/Perth": {
                "label": "ブルネイ, シンガポール, パース"
              },
              "Atlantic/Cape_Verde": {
                "label": "カーボベルデ,プライア,ミンデロ,サンタマリア"
              },
              "America/Belize": {
                "label": "コスタリカ、エルサルバドル、デンバー"
              },
              "Antarctica/Vostok": {
                "label": "ダッカ、ビシュケク、オムスク"
              },
              "Europe/Astrakhan": {
                "label": "ドバイ、バクー、トビリシ"
              },
              "Australia/Eucla": {
                "label": "ユークラ"
              },
              "Pacific/Honolulu": {
                "label": "ホノルル、ラロトンガ、タヒチ"
              },
              "Asia/Kabul": {
                "label": "カブール カンダハール マザール・エ・シャリーフ ヘラート ジャラーバード"
              },
              "Asia/Aqtau": {
                "label": "カラチ、タシケント、モルジブ"
              },
              "Asia/Kathmandu": {
                "label": "Kathmandu, Pokhara, Pātan, Biratnagar"
              },
              "Pacific/Kiritimati": {
                "label": "キリティマティ"
              },
              "Asia/Colombo": {
                "label": "コルカタ、コロンボ、ジャフナ、ネゴンボ"
              },
              "Europe/Guernsey": {
                "label": "ロンドン、ダブリン、リスボン"
              },
              "Australia/Lord_Howe": {
                "label": "ロード・ハウ"
              },
              "America/Creston": {
                "label": "ロスアンゼルス バンクーバー エルモシヨ"
              },
              "Pacific/Marquesas": {
                "label": "マルケサス島 タイオハエ"
              },
              "America/Cayman": {
                "label": "メキシコシティ, パナマ, シカゴ"
              },
              "Pacific/Niue": {
                "label": "ミッドウェイ、ニウエ、パゴパゴ"
              },
              "Europe/Mariehamn": {
                "label": "モスクワ、イスタンブール、カタール"
              },
              "Asia/Kolkata": {
                "label": "ムンバイ、デリー、ベンガルール、コルカタ、チェンナイ"
              },
              "America/Miquelon": {
                "label": "ノロンハ、ヌーク、ミクロン島"
              },
              "America/Danmarkshavn": {
                "label": "レイキャビク、ダカール、カサブランカ"
              },
              "Pacific/Bougainville": {
                "label": "サハリン、ノーフォーク、ブーゲンビル"
              },
              "America/Araguaina": {
                "label": "サンパウロ、ブエノスアイレス、モンテビデオ"
              },
              "America/St_Johns": {
                "label": "St.Johns, Mount Pearl, Corner Brook"
              },
              "Asia/Tehran": {
                "label": "テヘラン、マシュハド、イスファハン、カラジ、タブリーズ"
              },
              "Asia/Tokyo": {
                "label": "東京、ソウル、ジャヤプラ"
              },
              "America/New_York": {
                "label": "トロント、サンティアゴ、ニューヨーク"
              },
              "Australia/Sydney": {
                "label": "ウラジオストク、シドニー、ブリスベン"
              },
