{
  "output": {
    "type": "fieldset",
    "label": "出力テーブルの定義",
    "show_if": "options.task_type=='write_series'",
    "repeat": false,
    "fields": {
      "output.custom_text": {
        "type": "text",
        "label": "出力テーブル名",
        "show_if": "output.custom_options=='false' || (output.custom_options=='true' && output.compose_table_name=='false')",
        "minLength": 1,
        "val": "",
        "width": "100%",
        "breakLine": true
      },
      "output.custom_options": {
        "type": "checkbox",
        "label": "出力オプション",
        "val": "false",
        "isOn": "true",
        "isOff": "false",
        "breakLine": true
      },
      "output.compose_table_name": {
        "type": "checkbox",
        "label": "テーブル名の構成",
        "show_if": "output.custom_options=='true'",
        "val": "false",
        "isOn": "true",
        "isOff": "false",
        "breakLine": true
      },
      "compose_table_name_frame": {
        "type": "fieldset",
        "label": "出力テーブル名の構成",
        "show_if": "output.custom_options=='true' && output.compose_table_name=='true'",
        "repeat": false,
        "fields": {
          "output.prefix_rule": {
            "type": "select",
            "label": "プレフィックス",
            "show_if": "output.compose_table_name=='true'",
            "val": "empty",
            "options": {
              "empty": {
                "label": "-"
              },
              "script_name": {
                "label": "タスク名"
              },
              "date_string_type1": {
                "label": "日付:  dd_mm_yyyy"
              },
              "date_string_type2": {
                "label": "日付:  yyyy_mm_dd"
              },
              "time_string": {
                "label": "時間: 時_分_秒"
              },
              "datetime_string": {
                "label": "日付_時間"
              },
              "custom": {
                "label": "カスタム"
              }
            }
          },
          "output.prefix_text": {
            "type": "text",
            "label": "プレフィックステキスト",
            "show_if": "output.compose_table_name=='true' && output.prefix_rule=='custom'",
            "minLength": 1,
            "maxLength": 31,
            "val": ""
          },
          "output.core_delimiter": {
            "type": "text",
            "label": "区切り文字",
            "show_if": "output.compose_table_name=='true' && output.prefix_rule!='empty'",
            "val": "_",
            "width": "50px"
          },
          "output.core_rule": {
            "type": "select",
            "label": "列名",
            "show_if": "output.compose_table_name=='true'",
            "val": "input_table_name",
            "options": {
              "input_table_name": {
                "label": "テーブル名の入力"
              },
              "script_name": {
                "label": "タスク名"
              },
              "custom": {
                "label": "カスタム"
              }
            }
          },
          "output.core_text": {
            "type": "text",
            "label": "メインテーブル名",
            "show_if": "output.compose_table_name=='true' && output.core_rule=='custom'",
            "required": true,
            "minLength": 1,
            "maxLength": 31,
            "val": ""
          },
          "output.suffix_delimiter": {
            "type": "text",
            "label": "区切り文字",
            "show_if": "output.compose_table_name=='true'",
            "val": "_",
            "width": "50px"
          },
          "output.suffix_rule": {
            "type": "select",
            "label": "サフィックス",
            "show_if": "output.compose_table_name=='true'",
            "val": "script_name",
            "options": {
              "empty": {
                "label": "-"
              },
              "script_name": {
                "label": "タスク名"
              },
              "date_string_type1": {
                "label": "日付:  dd_mm_yyyy"
              },
              "date_string_type2": {
                "label": "日付:  yyyy_mm_dd"
              },
              "time_string": {
                "label": "時間: 時_分_秒"
              },
              "datetime_string": {
                "label": "日付_時間"
              },
              "custom": {
                "label": "カスタム"
              }
            }
          },
          "output.suffix_text": {
            "type": "text",
            "label": "サフィックステキスト",
            "show_if": "output.compose_table_name=='true' && output.suffix_rule=='custom'",
            "minLength": 1,
