{
  "options": {
    "type": "fieldset",
    "ラベル": "オプションの選択",
    "repeat": false,
    "fields": {
      "options.type": {
        "type": "radio",
        "val": "expand",
        "values": {
          "expand": "拡張",
          "compress": "Compress"
        },
        "breakLine": true
      },
      "options.column_to_expand": {
        "type": "select",
        "ラベル": "展開する列,
        "show_if": "options.type=='expand'",
        "table": "input_table",
        "bind": "COLUMNS",
        "required": true
      },
      "options.column_to_group": {
        "type": "select",
        "ラベル": "グループ化するカラム",
        "show_if": "options.type=='compress'",
        "table": "input_table",
        "bind": "COLUMNS",
        "required": true
      },
      "options.column_to_compress": {
        "type": "select",
        "ラベル": "圧縮する列",
        "show_if": "options.type=='compress'",
        "table": "input_table",
        "bind": "COLUMNS",
        "required": true
      },
      "options.delimiter": {
        "type": "text",
        "ラベル": "区切り文字",
        "val": ";",
        "trim": "false",
        "required": true
      },
      "options.new_col_name": {
        "type": "text",
        "ラベル": "新しい列名",
        "show_if": "options.type=='expand' && options.expand__output_in_new_column=='true'",
        "val": "ラベル",
        "required": true,
        "breakLine": true
      },
      "options.expand__output_in_new_column": {
        "type": "checkbox",
        "ラベル": "新しいカラムに出力",
        "show_if": "options.type=='expand'",
        "val": "false",
        "isOn": "true",
        "isOff": "false",
        "breakLine": true
      },
      "options.create_new_table": {
        "type": "checkbox",
        "ラベル": "新しいテーブルとして出力",
        "val": "true",
        "isOn": "true",
        "isOff": "false",
        "breakLine": true
      }
    }
  }
}