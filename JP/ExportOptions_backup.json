{
  "options": {
    "type": "fieldset", 
    "label": "オプションの選択", 
    "repeat": false, 
    "fields": {
      "options.to": {
        "type": "select", 
        "label": "タイプ", 
        "options": {
          "database": {
            "label": "データベースへのエクスポート"
          }, 
          "file": {
            "label": "ファイルへのエクスポート"
          }, 
          "mytask": {
            "label": "MyTaskのフォーマット"
          }
        }, 
        "val": "database", 
        "required": true
      }, 
      "options.export_series": {
        "type": "checkbox", 
        "label": "シリーズのエクスポート", 
        "show_if": "options.to=='file'", 
        "val": "false", 
        "isOn": "true", 
        "isOff": "false", 
        "breakLine": true
      }, 
      "options.file_name_column": {
        "type": "select", 
        "label": "ファイル名列の選択", 
        "show_if": "options.to=='file' && options.export_series=='true'", 
        "table": "input_table", 
        "bind": "COLUMNS", 
        "required": true, 
        "breakLine": true
      }, 
      "postgresql_db_credential_box": {
        "type": "fieldset", 
        "label": "データベース接続証明書", 
        "show_if": "options.to=='database'", 
        "repeat": false, 
        "fields": {
          "options.dbms": {
            "type": "select", 
            "label": "DBMSの選択", 
            "options": {
              "postgresql": {
                "label": "PostgreSQL"
              }, 
              "mysql": {
                "label": "MySQL"
              }, 
              "sql_server": {
                "label": "MS SQL Server"
              }, 
              "oracle": {
                "label": "Oracle"
              }
            }, 
            "val": "postgresql", 
            "required": true, 
            "breakLine": true
          }, 
          "options.postgresql_credential.user": {
            "type": "text", 
            "label": "ユーザー", 
            "show_if": "options.dbms=='postgresql'", 
            "required": true, 
            "width": "100%", 
            "breakLine": true
          }, 
          "options.postgresql_credential.password": {
            "type": "text", 
            "label": "パスワード", 
            "show_if": "options.dbms=='postgresql'", 
            "required": true, 
            "width": "100%", 
            "breakLine": true
          }, 
          "options.postgresql_credential.host": {
            "type": "text", 
            "label": "ホスト", 
            "show_if": "options.dbms=='postgresql'", 
            "required": true, 
            "width": "100%", 
            "breakLine": true
          }, 
          "options.postgresql_credential.database": {
            "type": "text", 
            "label": "データベース", 
            "show_if": "options.dbms=='postgresql'", 
            "required": true, 
            "width": "100%", 
            "breakLine": true
          }
        }
      }, 
      "options.schema_name_to_export": {
        "type": "text", 
        "label": "エクスポート先のスキーマ名", 
        "show_if": "options.to=='database' && options.dbms=='postgresql'", 
        "required": true, 
        "val": "public", 
        "width": "100%", 
        "breakLine": true
      }, 
      "options.webservice": {
        "type": "select", 
        "label": "ウェブサービスの選択", 
        "show_if": "options.to=='file' || options.to=='mytask'", 
        "options": {
          "github": {
            "label": "Github"
          }
        }, 
        "val": "github", 
        "required": true
      }, 
      "options.export_format": {
        "type": "select", 
        "label": "フォーマットのエクスポート", 
        "show_if": "options.to=='file'", 
        "options": {
          "csv_file": {
            "label": "Csvファイル(複数可)"
          }, 
          "word_by_word": {
            "label": "単語ごと"
          }, 
          "line_by_line": {
            "label": "行ごと"
          }
        }, 
        "val": "csv_file", 
        "required": true, 
        "breakLine": true
      }, 
      "options.github_token": {
        "type": "text", 
        "label": "書き込み権限を持つgithubパーソナルアクセストークンを指定:", 
        "show_if": "options.to=='file' || options.to=='mytask'", 
        "required": true, 
        "width": "100%", 
        "breakLine": true
      }, 
      "options.github_url": {
        "type": "text", 
        "label": "リンクを入力 例：https://github.com/zippydoc/test", 
        "show_if": "options.to=='file' || options.to=='mytask'", 
        "val": "https://github.com/zippydoc/public-tables", 
        "required": true, 
        "width": "100%", 
        "breakLine": true
      }, 
      "options.if_duplicated": {
        "type": "select", 
        "label": "重複している場合", 
        "val": "drop_if_exist", 
        "options": {
          "drop_if_exist": {
            "label": "作成またはドロップ（既存の場合）"
          }, 
          "error_if_exist": {
            "label": "新規作成（既存の場合はエラーにする）"
          }, 
          "error_if_not_exist": {
            "label": "ドロップ（存在しない場合はエラーにする）"
          }, 
          "append": {
            "label": "追加"
          }
        }
      }, 
      "options.export_task_name": {
        "type": "text", 
        "label": "タスク名の入力", 
        "show_if": "options.to=='mytask'", 
        "required": true, 
        "val": "Flow6_Task1_AddColumns", 
        "width": "100%", 
        "breakLine": true
      }
    }
  }
}