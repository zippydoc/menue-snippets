{
  "options": {
    "type": "fieldset",
    "label": "オプションの指定",
    "repeat": false,
    "fields": {
      "fields_to_impute": {
        "type": "select",
        "label": "検索する列の選択",
        "required": true,
        "table": "input_table",
        "multiple": true,
        "bind": "COLUMNS",
        "width": "100%",
        "breakLine": true
      },
      "incoming_value_to_replace.type": {
        "type": "select",
        "label": "入力値",
        "options": {
          "user_specified_value": {
            "label": "等しい"
          },
          "begins_with": {
            "label": "次で開始"
          },
          "ends_with": {
            "label": "次で終了"
          },
          "empty": {
            "label": "空"
          },
          "regex": {
            "label": "正規表現"
          },
          "replacement_table": {
            "label": "代替テーブル"
          }
        },
        "val": "user_specified_value",
        "style": "min-width: 190px"
      },
      "incoming_value_to_replace.value": {
        "type": "text",
        "label": "置換する入力値",
        "required": true,
        "show_if": "incoming_value_to_replace.type=='user_specified_value' || incoming_value_to_replace.type=='begins_with' || incoming_value_to_replace.type=='ends_with'",
        "val": "",
        "style": "min-width: 170px"
      },
      "incoming_value_to_replace.regex": {
        "type": "text",
        "label": "正規表現",
        "required": true,
        "show_if": "incoming_value_to_replace.type=='regex'",
        "val": "(.*),(.*),(.*)\s(\d{5})(-\d{4})?",
        "style": "min-width: 200px"
      },
      "replace_table.table_id": {
        "type": "select",
        "label": "代替テーブル",
        "show_if": "incoming_value_to_replace.type=='replacement_table'",
        "bind": "TABLES",
        "required": true
      },
      "replace_table.find_column": {
        "type": "select",
        "label": "検索列の選択",
        "show_if": "incoming_value_to_replace.type=='replacement_table'",
        "table": "replace_table.table_id",
        "bind": "COLUMNS",
        "required": true
      },
      "replace_table.replace_with_column": {
        "type": "select",
        "label": "列での置換を選択",
        "show_if": "incoming_value_to_replace.type=='replacement_table'",
        "table": "replace_table.table_id",
        "bind": "COLUMNS",
        "required": true
      },
      "incoming_value_to_replace.partial_match": {
        "type": "select",
        "label": "部分一致",
        "show_if": "incoming_value_to_replace.type=='user_specified_value' || incoming_value_to_replace.type=='replacement_table'",
        "options": {
          "full_match": {
            "label": "Full match"
          },
          "full_and_partial_match": {
            "label": "Full and partial match"
          },
          "partial_match_only": {
            "label": "Partial match only"
          }
        },
        "val": "full_and_partial_match"
      },
      "replace_with.value_type": {
        "type": "select",
        "label": "次に置換",
        "show_if": "incoming_value_to_replace.type!='regex' && incoming_value_to_replace.type!='replacement_table'",
        "options": {
          "user_specified_value": {
            "label": "カスタム値"
          },
          "average": {
            "label": "列の平均値"
          },
          "median": {
            "label": "列の中央値"
          },
          "mode": {
            "label": "列のモード"
          },
          "sum": {
            "label": "列の合計"
          },
          "table": {
            "label": "列テーブル"
          }
        },
        "val": "user_specified_value",
        "style": "min-width: 190px"
      },
      "replace_with.value": {
        "type": "text",
        "label": "価値に置換",
        "show_if": "incoming_value_to_replace.type!='regex' && replace_with.value_type=='user_specified_value' && incoming_value_to_replace.type!='replacement_table'",
        "val": ""
      },
      "replace_with.regex_value": {
        "type": "text",
        "label": "価値に置換",
        "show_if": "incoming_value_to_replace.type=='regex' && incoming_value_to_replace.type!='replacement_table'",
        "val": "\4: \2, \3"
      },
      "replace_with.table": {
        "type": "select",
        "label": "テーブルの選択",
        "show_if": "incoming_value_to_replace.type!='regex' && replace_with.value_type=='table' && incoming_value_to_replace.type!='replacement_table'",
        "bind": "TABLES",
        "required": true
      },
      "replace_with.column": {
        "type": "select",
        "label": "列の選択",
        "show_if": "incoming_value_to_replace.type!='regex' && replace_with.value_type=='table' && incoming_value_to_replace.type!='replacement_table'",
        "table": "replace_with.table",
        "bind": "COLUMNS",
        "required": true,
        "breakLine": true
      },
      "replacement_table_notice": {
        "type": "text",
        "label": "",
        "show_if": "incoming_value_to_replace.type=='replacement_table'",
        "val": "Line-break will be ignored in replacement table",
        "disabled": true,
        "width": "100%"
      },
      "use_filter": {
        "type": "checkbox",
        "label": "フィルターの使用",
        "val": "false",
        "isOn": "true",
        "isOff": "false",
        "breakLine": true
      },
      "filter_options_box": {
        "type": "fieldset",
        "label": "フィルターオプション",
        "show_if": "use_filter=='true'",
        "repeat": false,
        "fields": {
          "filter": {
            "type": "fieldset",
            "label": "フィルター条件の追加",
            "show_if": "use_filter=='true'",
            "repeat": true,
            "fields": {
              "initial_op": {
                "type": "select",
                "label": "論理オペレータの選択",
                "required": true,
                "show_if" :  "$index == 0",
                "options": {
                  "if": {
                    "label": "IF"
                  },
                  "if_not": {
                    "label": "IF NOT"
                  }
                },
                "val": "if"
              },
              "op": {
                "type": "select",
                "label": "論理オペレータの選択",
                "required": true,
                "show_if" :  "$index > 0",
                "options": {
                  "and": {
                    "label": "AND"
                  },
                  "or": {
                    "label": "OR"
                  },
                  "and_not": {
                    "label": "AND NOT"
                  },
                  "or_not": {
                    "label": "OR NOT"
                  }
                },
                "val": "and"
              },
              "column": {
                "type": "select",
                "label": "列の選択",
                "table": "input_table",
                "bind": "COLUMNS",
                "required": true
              },
              "condition": {
                "type": "select",
                "label": "オペレータの選択",
                "required": true,
                "options": {
                  "含む": {
                    "label": "含む"
                  },
                  "CONTAINED IN STRING": {
                    "label": "次の中に含む"
                  },
                  "次で開始": {
                    "label": "次で開始"
                  },
                  "次で終了": {
                    "label": "次で終了"
                  },
                  "=": {
                    "label": "等しい"
                  },
                  ">": {
                    "label": "超"
                  },
                  ">=": {
                    "label": "以上"
                  },
                  "<": {
                    "label": "より小さい"
                  },
                  "<=": {
                    "label": "より小さいまたは等しい"
                  },
                  "EMPTY": {
                    "label": "空です"
                  },
                  "BLANK": {
                    "label": "空白です"
                  },
                  "無効です": {
                    "label": "無効です"
                  },
                  "REGEXの一致": {
                    "label": "REGEXの一致"
                  },
                  "CONTAINED IN TABLE": {
                    "label": "テーブル内"
                  },
                  "REGEXテーブル内": {
                    "label": "REGEXテーブル内"
                  },
                  "日付": {
                    "label": "日付"
                  }
                },
                "val": "含む"
              },
              "date_condition": {
                "type": "select",
                "label": "日付オペレータの選択",
                "required": true,
                "show_if": "this.condition == 'DATE'",
                "options": {
                  "<": {
                    "label": "前"
                  },
                  "<=": {
                    "label": "前または等しい"
                  },
                  "=": {
                    "label": "等しい"
                  },
                  ">": {
                    "label": "以降"
                  },
                  ">=": {
                    "label": "以降または等しい"
                  }
                },
                "val": "="
              },
              "case_sensitive": {
                "type": "select",
                "label": "ケース・センシティブの選択",
                "required": true,
                "show_if": "this.condition == 'CONTAINS' || this.condition == 'CONTAINED IN STRING' || this.condition == '=' || this.condition == 'STARTS WITH' || this.condition == 'ENDS WITH' || this.condition == 'REGEX MATCH' || this.condition == 'NOT REGEX MATCH' || this.condition == 'IN REGEX TABLE' || this.condition == 'NOT IN REGEX TABLE'",
                "options": {
                  "yes": {
                    "label": "ケース・センシティブ"
                  },
                  "no": {
                    "label": "ケース・インセンシティブ"
                  }
                },
                "val": "yes"
              },
              "date_value": {
                "type": "date",
                "label": "日付の選択",
                "show_if": "this.condition=='DATE'",
                "val": "1/31/2020",
                "required": true
              },
              "value": {
                "type": "text",
                "label": "値の入力",
                "label": "論理オペレータの選択",
                "required": true,
                "show_if" :  "$index == 0",
                "options": {
                "show_if": "this.condition != 'DATE' && this.condition != 'EMPTY' && this.condition != 'NOT EMPTY' && this.condition != 'BLANK' && this.condition != 'NOT BLANK' && this.condition != 'IS NULL' && this.condition != 'IS NOT NULL' && this.condition != 'CONTAINED IN TABLE' && this.condition != 'NOT CONTAINED IN TABLE' && this.condition != 'IN REGEX TABLE' && this.condition != 'NOT IN REGEX TABLE'",
                "val": "",
                "required": true
              },
              "compare_table": {
                "type": "select",
                "label": "テーブルの選択",
                "bind": "TABLES",
                "empty": "",
                "show_if": "this.condition == 'CONTAINED IN TABLE' || this.condition == 'NOT CONTAINED IN TABLE' || this.condition == 'IN REGEX TABLE' || this.condition == 'NOT IN REGEX TABLE'",
                "required": true
              },
              "compare_column": {
                "type": "select",
                "label": "列の選択",
                "table": "this.compare_table",
                "bind": "COLUMNS",
                "show_if": "this.condition == 'CONTAINED IN TABLE' || this.condition == 'NOT CONTAINED IN TABLE' || this.condition == 'IN REGEX TABLE' || this.condition == 'NOT IN REGEX TABLE'",
                "required": true
              },
              "delete": {
                "type": "button",
                "label": "削除",
                "function": "DELETE_ROW"
              }
            }
          },
          "apply_to_record": {
            "type": "checkbox",
            "label": "レコードに適用",
            "val": "true",
            "isOn": "true",
            "isOff": "false",
            "style": "margin-top: 10px",
            "breakLine": true
          },
          "apply_to_group_box": {
            "type": "fieldset",
            "label": "グループに適用",
            "show_if": "apply_to_record=='false'",
            "repeat": false,
            "fields": {
              "apply_to_group_columns": {
                "type": "select",
                "label": "グループ列の選択",
                "required": true,
                "width": "100%",
                "table": "input_table",
                "multiple": true,
                "bind": "COLUMNS",
                "breakLine": true
              }
            }
          }
        }
      },
      "use_replace_table_filter": {
        "type": "checkbox",
        "label": "Use Filter on "次に置換" Table",
        "show_if": "incoming_value_to_replace.type=='replacement_table' || (incoming_value_to_replace.type!='regex' && replace_with.value_type=='table')",
        "val": "false",
        "isOn": "true",
        "isOff": "false",
        "style": "margin-top: 10px",
        "breakLine": true
      },
      "replace_table_filter_option_box": {
        "type": "fieldset",
        "label": ""次に置換" Table Filter Options",
        "show_if": "use_replace_table_filter=='true' && (incoming_value_to_replace.type=='replacement_table' || (incoming_value_to_replace.type!='regex' && replace_with.value_type=='table'))",
        "repeat": false,
        "fields": {
          "replace_table_filter_option.filter": {
            "type": "fieldset",
            "label": "Add Filter Conditions on "次に置換" Table",
            "repeat": true,
            "fields": {
              "initial_op": {
                "type": "select",
                  "if": {
                    "label": "IF"
                  },
                  "if_not": {
                    "label": "IF NOT"
                  }
                },
                "val": "if"
              },
              "op": {
                "type": "select",
                "label": "論理オペレータの選択",
                "required": true,
                "show_if" :  "$index > 0",
                "options": {
                  "and": {
                    "label": "AND"
                  },
                  "or": {
                    "label": "OR"
                  },
                  "and_not": {
                    "label": "AND NOT"
                  },
                  "or_not": {
                    "label": "OR NOT"
                  }
                },
                "val": "and"
              },
              "column": {
                "type": "select",
                "label": "列の選択",
                "table": "replace_with.table",
                "bind": "COLUMNS",
                "required": true
              },
              "condition": {
                "type": "select",
                "label": "オペレータの選択",
                "required": true,
                "options": {
                  "含む": {
                    "label": "含む"
                  },
                  "CONTAINED IN STRING": {
                    "label": "次の中に含む"
                  },
                  "次で開始": {
                    "label": "次で開始"
                  },
                  "次で終了": {
                    "label": "次で終了"
                  },
                  "=": {
                    "label": "等しい"
                  },
                  ">": {
                    "label": "超"
                  },
                  ">=": {
                    "label": "以上"
                  },
                  "<": {
                    "label": "より小さい"
                  },
                  "<=": {
                    "label": "より小さいまたは等しい"
                  },
                  "EMPTY": {
                    "label": "空です"
                  },
                  "BLANK": {
                    "label": "空白です"
                  },
                  "無効です": {
                    "label": "無効です"
                  },
                  "REGEXの一致": {
                    "label": "REGEXの一致"
                  },
                  "CONTAINED IN TABLE": {
                    "label": "テーブル内"
                  },
                  "REGEXテーブル内": {
                    "label": "REGEXテーブル内"
                  },
                  "日付": {
                    "label": "日付"
                  }
                },
                "val": "含む"
              },
              "date_condition": {
                "type": "select",
                "label": "日付オペレータの選択",
                "required": true,
                "show_if": "this.condition == 'DATE'",
                "options": {
                  "<": {
                    "label": "前"
                  },
                  "<=": {
                    "label": "前または等しい"
                  },
                  "=": {
                    "label": "等しい"
                  },
                  ">": {
                    "label": "以降"
                  },
                  ">=": {
                    "label": "以降または等しい"
                  }
                },
                "val": "="
              },
              "case_sensitive": {
                "type": "select",
                "label": "ケース・センシティブの選択",
                "required": true,
                "show_if": "this.condition == 'CONTAINS' || this.condition == 'CONTAINED IN STRING' || this.condition == '=' || this.condition == 'STARTS WITH' || this.condition == 'ENDS WITH' || this.condition == 'REGEX MATCH' || this.condition == 'NOT REGEX MATCH' || this.condition == 'IN REGEX TABLE' || this.condition == 'NOT IN REGEX TABLE'",
                "options": {
                  "yes": {
                    "label": "ケース・センシティブ"
                  },
                  "no": {
                    "label": "ケース・インセンシティブ"
                  }
                },
                "val": "yes"
              },
              "date_value": {
                "type": "date",
                "label": "日付の選択",
                "show_if": "this.condition=='DATE'",
                "val": "1/31/2020",
                "required": true
              },
              "value": {
                "type": "text",
                "label": "値の入力",
                "show_if": "this.condition != 'DATE' && this.condition != 'EMPTY' && this.condition != 'NOT EMPTY' && this.condition != 'BLANK' && this.condition != 'NOT BLANK' && this.condition != 'IS NULL' && this.condition != 'IS NOT NULL' && this.condition != 'CONTAINED IN TABLE' && this.condition != 'NOT CONTAINED IN TABLE' && this.condition != 'IN REGEX TABLE' && this.condition != 'NOT IN REGEX TABLE'",
                "val": "",
                "required": true
              },
              "compare_table": {
                "type": "select",
                "label": "テーブルの選択",
                "bind": "TABLES",
                "empty": "",
                "show_if": "this.condition == 'CONTAINED IN TABLE' || this.condition == 'NOT CONTAINED IN TABLE' || this.condition == 'IN REGEX TABLE' || this.condition == 'NOT IN REGEX TABLE'",
                "required": true
              },
              "compare_column": {
                "type": "select",
                "label": "列の選択",
                "table": "this.compare_table",
                "bind": "COLUMNS",
                "show_if": "this.condition == 'CONTAINED IN TABLE' || this.condition == 'NOT CONTAINED IN TABLE' || this.condition == 'IN REGEX TABLE' || this.condition == 'NOT IN REGEX TABLE'",
                "required": true
              },
              "delete": {
                "type": "button",
                "label": "削除",
                "function": "DELETE_ROW"
              }
            }
          },
          "replace_table_filter_option.apply_to_record": {
            "type": "checkbox",
            "label": "Apply to Record on "次に置換" Table",
            "val": "true",
            "isOn": "true",
            "isOff": "false",
            "style": "margin-top: 10px",
            "breakLine": true
          },
          "replace_table_filter_option_apply_to_group_box": {
            "type": "fieldset",
            "label": "Apply to Group on "次に置換" Table",
            "show_if": "replace_table_filter_option.apply_to_record=='false' && (incoming_value_to_replace.type!='replacement_table' && incoming_value_to_replace.type!='regex' && replace_with.value_type=='table')",
            "repeat": false,
            "fields": {
              "replace_table_filter_option.apply_to_group_columns": {
                "type": "select",
                "label": "グループ列の選択",
                "required": true,
                "width": "100%",
                "table": "replace_with.table",
                "multiple": true,
                "bind": "COLUMNS",
                "breakLine": true
              }
            }
          },
          "replacement_table_filter_option_apply_to_group_box": {
            "type": "fieldset",
            "label": "Apply to Group on "次に置換" Table",
            "show_if": "replace_table_filter_option.apply_to_record=='false' && incoming_value_to_replace.type=='replacement_table'",
            "repeat": false,
            "fields": {
              "replacement_table_filter_option.apply_to_group_columns": {
                "type": "select",
                "label": "グループ列の選択",
                "required": true,
                "width": "100%",
                "table": "replace_table.table_id",
                "multiple": true,
                "bind": "COLUMNS",
                "breakLine": true
              }
            }
          }
        }
      }
    }
  }
}