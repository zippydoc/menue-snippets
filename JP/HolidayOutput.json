{
  "output": {
    "type": "fieldset",
    "label": "出力テーブルの定義",
    "repeat": false,
    "fields": {
      "output.custom_text": {
        "type": "text",
        "label": "出力テーブル名",
        "show_if": "output.custom_options=='false' || (output.custom_options=='true' && output.compose_table_name=='false')",
        "minLength": 1,
        "val": "",
        "width": "100%",
        "breakLine": true
      },
      "output.custom_options": {
        "type": "checkbox",
        "label": "出力オプション",
        "val": "false",
        "isOn": "true",
        "isOff": "false",
        "breakLine": true
      },
      "output.select_output_columns": {
        "type": "checkbox",
        "label": "出力列の選択",
        "show_if": "options.is_generate_holiday_table=='add_column' && output.custom_options=='true'",
        "val": "false",
        "isOn": "true",
        "isOff": "false"
      },
      "output_columns": {
        "type": "fieldset",
        "label": "Select OutputColumns",
        "show_if": "options.is_generate_holiday_table=='add_column' && output.custom_options=='true' && output.select_output_columns=='true'",
        "repeat": true,
        "fields": {
          "field_name": {
            "type": "select",
            "label": "列の選択",
            "table": "input_table",
            "bind": "COLUMNS",
            "required": true
          },
          "order": {
            "type": "select",
            "label": "注文",
            "options": {
              "-": {
                "label": "-"
              },
              "asc": {
                "label": "昇順"
              },
              "desc": {
                "label": "降順"
              }
            },
            "val": "-"
          },
          "is_rename": {
            "type": "checkbox",
            "label": "名前の変更",
            "val": "false",
            "isOn": "true",
            "isOff": "false"
          },
          "rename": {
            "show_if": "this.is_rename == 'true'",
            "type": "text",
            "required": true,
            "minLength": 1,
            "maxLength": 255,
            "label": "新しい列名",
            "style": "margin-left: 10px"
          },
          "format__category": {
            "type": "select",
            "label": "フォーマット",
            "required": true,
            "options": {
              "standard": {
                "label": "標準"
              },
              "number": {
                "label": "数"
              },
              "percentage": {
                "label": "割合"
              },
              "text": {
                "label": "テキスト"
              },
              "date": {
                "label": "日付"
              },
              "currency": {
                "label": "通貨"
              }
            },
            "val": "standard"
          },
          "format__rounding": {
            "type": "select",
            "label": "四捨五入",
            "show_if": "this.format__category=='number' || this.format__category=='percentage' || this.format__category=='currency'",
            "required": true,
            "options": {
              "no_rounding": {
                "label": "四捨五入無し"
              },
              "0": {
                "label": "0桁"
              },
              "1": {
                "label": "1桁"
              },
              "2": {
                "label": "2桁"
              },
              "3": {
                "label": "3桁"
              },
              "4": {
                "label": "4桁"
              },
              "5": {
                "label": "5桁"
              },
              "6": {
                "label": "6桁"
              },
              "7": {
                "label": "7桁"
              },
              "8": {
                "label": "8桁"
              },
              "9": {
                "label": "9桁"
              }
            },
            "val": "2"
          },
          "format__use_1000_separator": {
            "type": "checkbox",
            "label": "Use 1000 Separator(.)",
            "show_if": "this.format__category=='number' || this.format__category=='percentage' || this.format__category=='currency'",
            "val": "true",
            "isOn": "true",
            "isOff": "false"
          },
          "format__currency_symbol": {
            "type": "select",
            "label": "シンボル",
            "show_if": "this.format__category=='currency'",
            "required": true,
            "options": {
              "dollar": {
                "label": "$"
              },
              "euro": {
                "label": "€"
              },
              "pound": {
                "label": "£"
              }
            },
            "val": "dollar"
          },
          "format__date_format": {
            "type": "select",
            "label": "タイプ",
            "show_if": "this.format__category=='date'",
            "required": true,
            "options": {
              "mm/dd/yyyy": {
                "label": "3/14/2012"
              },
              "D, M dd, Y": {
                "label": "水曜日、3月14日、2012"
              },
              "yyyy-mm-dd": {
                "label": "2012-03-14"
              }
            },
            "val": "mm/dd/yyyy",
            "breakLine": true
          },
          "delete": {
            "type": "button",
            "label": "削除",
            "function": "DELETE_ROW"
          }
        },
        "breakLine": true,
        "style": "margin-bottom: 20px"
      },
      "output.compose_table_name": {
        "type": "checkbox",
        "label": "テーブル名の構成",
        "show_if": "output.custom_options=='true'",
        "val": "false",
        "isOn": "true",
        "isOff": "false",
        "breakLine": true
      },
      "compose_table_name_frame": {
        "type": "fieldset",
        "label": "出力テーブル名の構成",
        "show_if": "output.custom_options=='true' && output.compose_table_name=='true'",
        "repeat": false,
        "fields": {
          "output.prefix_rule": {
            "type": "select",
            "label": "プレフィックス",
            "show_if": "output.compose_table_name=='true'",
            "val": "empty",
            "options": {
              "empty": {
                "label": "-"
              },
              "script_name": {
                "label": "タスク名"
              },
              "date_string_type1": {
                "label": "日付:  dd_mm_yyyy"
              },
              "date_string_type2": {
                "label": "日付:  yyyy_mm_dd"
              },
              "time_string": {
                "label": "時間: 時_分_秒"
              },
              "datetime_string": {
                "label": "日付_時間"
              },
              "custom": {
                "label": "カスタム"
              }
            }
          },
          "output.prefix_text": {
            "type": "text",
            "label": "プレフィックステキスト",
            "show_if": "output.compose_table_name=='true' && output.prefix_rule=='custom'",
            "minLength": 1,
            "maxLength": 31,
            "val": ""
          },
          "output.core_delimiter": {
            "type": "text",
            "label": "区切り文字",
            "show_if": "output.compose_table_name=='true' && output.prefix_rule!='empty'",
            "val": "_",
            "width": "50px"
          },
          "output.generate_core_rule": {
            "type": "select",
            "label": "列名",
            "show_if": "output.compose_table_name=='true' && options.is_generate_holiday_table=='generate_table'",
            "val": "script_name",
            "options": {
              "script_name": {
                "label": "タスク名"
              },
              "custom": {
                "label": "カスタム"
              }
            }
          },
          "output.core_rule": {
            "type": "select",
            "label": "列名",
            "show_if": "output.compose_table_name=='true' && options.is_generate_holiday_table=='add_column'",
            "val": "input_table_name",
            "options": {
              "input_table_name": {
                "label": "テーブル名の入力"
              },
              "script_name": {
                "label": "タスク名"
              },
              "custom": {
                "label": "カスタム"
              }
            }
          },
          "output.core_text": {
            "type": "text",
            "label": "メインテーブル名",
            "show_if": "output.compose_table_name=='true' && output.core_rule=='custom'",
            "required": true,
            "minLength": 1,
            "maxLength": 31,
            "val": ""
          },
          "output.suffix_delimiter": {
            "type": "text",
            "label": "区切り文字",
            "show_if": "output.compose_table_name=='true'",
            "val": "_",
            "width": "50px"
          },
          "output.suffix_rule": {
            "type": "select",
            "label": "サフィックス",
            "show_if": "output.compose_table_name=='true'",
            "val": "script_name",
            "options": {
              "empty": {
                "label": "-"
              },
              "script_name": {
                "label": "タスク名"
              },
              "date_string_type1": {
                "label": "日付:  dd_mm_yyyy"
              },
              "date_string_type2": {
                "label": "日付:  yyyy_mm_dd"
              },
              "time_string": {
                "label": "時間: 時_分_秒"
              },
              "datetime_string": {
                "label": "日付_時間"
              },
              "custom": {
                "label": "カスタム"
              }
            }
          },
          "output.suffix_text": {
            "type": "text",
            "label": "サフィックステキスト",
            "show_if": "output.compose_table_name=='true' && output.suffix_rule=='custom'",
            "minLength": 1,
            "maxLength": 31,
            "val": "",
            "breakLine": true
          },
          "output.timezone": {
            "type": "select",
            "label": "タイムゾーン",
            "show_if": "output.compose_table_name=='true' && (output.prefix_rule=='date_string_type1' || output.prefix_rule=='date_string_type2' || output.prefix_rule=='time_string' || output.prefix_rule=='datetime_string' || output.suffix_rule=='date_string_type1' || output.suffix_rule=='date_string_type2' || output.suffix_rule=='time_string' || output.suffix_rule=='datetime_string')",
            "val": "ヨーロッパ/ベルリン",
            "required": true,
            "options": {
              "America/Adak": {
                "label": "アダック、ガンビアー"
              },
              "Australia/Adelaide": {
                "label": "Adelaide, Broken_Hill, Darwin"
              },
              "America/Anchorage": {
                "label": "Anchorage, Juneau, Sitka"
              },
              "Pacific/Apia": {
                "label": "アピア、アサウ、ムリファヌア、アフェガ、レウルモエガ"
              },
              "Pacific/Enderbury": {
                "label": "アピア、フィジー、トンガタプ"
              },
              "Antarctica/McMurdo": {
                "label": "オークランド ケーシー デービス"
              },
              "Pacific/Auckland": {
                "label": "オークランド、ウェリントン、クライストチャーチ"
              },
              "Asia/Bangkok": {
                "label": "バンコク、ジャカルタ、ノボシビルスク
              },
              "ヨーロッパ/ベルリン": {
                "label": "ベルリン、パリ、アムステルダム
              },
              "Australia/Perth": {
                "label": "ブルネイ, シンガポール, パース"
              },
              "Atlantic/Cape_Verde": {
                "label": "Cape_Verde,Praia, Mindelo, Santa Maria"
              },
              "America/Belize": {
                "label": "Costa_Rica, El_Salvador, Denver"
              },
              "Antarctica/Vostok": {
                "label": "ダッカ、ビシュケク、オムスク"
              },
              "Europe/Astrakhan": {
                "label": "ドバイ、バクー、トビリシ
              },
              "Australia/Eucla": {
                "label": "ユークラ
              },
              "Pacific/Honolulu": {
                "label": "ホノルル、ラロトンガ、タヒチ"
              },
              "Asia/Kabul": {
                "label": "カブール カンダハール マザール・エ・シャリーフ ヘラート ジャラーバード
              },
              "Asia/Aqtau": {
                "label": "カラチ、タシケント、モルジブ
              },
              "Asia/Kathmandu": {
                "label": "Kathmandu,Pokhara, Pātan, Biratnagar"
              },
              "Pacific/Kiritimati": {
                "label": "キリティマティ"
              },
              "Asia/Colombo": {
                "label": "Kolkata, Colombo,Jaffna, Negombo"
              },
              "Europe/Guernsey": {
                "label": "London. Dublin, Lisbon"
              },
              "Australia/Lord_Howe": {
                "label": "Lord_Howe"
              },
              "America/Creston": {
                "label": "Los_Angeles, Vancouver, Hermosillo"
              },
              "Pacific/Marquesas": {
                "label": "Marquesas,Taiohae"
              },
              "America/Cayman": {
                "label": "Mexico_City, Panama, Chicago"
              },
              "Pacific/Niue": {
                "label": "Midway, Niue, Pago_Pago"
              },
              "Europe/Mariehamn": {
                "label": "モスクワ、イスタンブール、カタール
              },
              "Asia/Kolkata": {
                "label": "ムンバイ、デリー、ベンガルール、コルカタ、チェンナイ
              },
              "America/Miquelon": {
                "label": "ノロンハ、ヌーク、ミクロン島"
              },
              "America/Danmarkshavn": {
                "label": "レイキャビク、ダカール、カサブランカ
              },
              "Pacific/Bougainville": {
                "label": "サハリン、ノーフォーク、ブーゲンビル"
              },
              "America/Araguaina": {
                "label": "Sao_Paulo, Buenos_Aires, Montevideo"
              },
              "America/St_Johns": {
                "label": "St_Johns,Mount Pearl, Corner Brook"
              },
              "Asia/Tehran": {
                "label": "テヘラン、マシュハド、イスファハン、カラジ、タブリーズ"
              },
              "Asia/Tokyo": {
                "label": "東京、ソウル、ジャヤプラ"
              },
              "America/New_York": {
                "label": "Toronto, Santiago, New_York"
              },
              "Australia/Sydney": {
                "label": "ウラジオストク、シドニー、ブリスベン
              },
              "Asia/Yangon": {
                "label": "Yangon, Cocos,Mandalay, Nay Pyi Taw, Mawlamyine"
              },
              "Indian/Cocos": {
                "label": "Yangon, Cocos,West Island"
              }
            },
            "breakLine": true
          }
        }
      }
    }
  }
}