{
  "options": {
    "type": "fieldset",
    "label": "オプションの選択",
    "repeat": false,
    "fields": {
      "options.from": {
        "type": "hidden",
        "val": "web"
      },
      "options.webservice": {
        "type": "hidden",
        "val": "dropbox"
      },
      "options.output_format": {
        "type": "select",
        "label": "選択タイプ",
        "options": {
          "csv_table": {
            "label": "CSVテーブル"
          },
          "line_by_line": {
            "label": "行ごと"
          },
          "word_by_word": {
            "label": "テキスト"
          }
        },
        "val": "csv_table",
        "required": true
      },
      "options.word_pattern_type": {
        "type": "select",
        "label": "単語パターン",
        "show_if": "options.output_format=='word_by_word' && options.specify_word_pattern=='true'",
        "options": {
          "regex": {
            "label": "正規表現"
          }
        },
        "val": "regex",
        "required": true
      },
      "options.word_pattern_regex": {
        "type": "text",
        "label": "正規表現",
        "show_if": "options.output_format=='word_by_word' && options.specify_word_pattern=='true' && options.word_pattern_type=='regex'",
        "required": true,
        "val": "[a-z][1-9]",
        "breakLine": true
      },
      "options.column_setting": {
        "type": "checkbox",
        "label": "カラムの設定",
        "show_if": "options.output_format!='file_list'",
        "val": "false",
        "isOn": "true",
        "isOff": "false",
        "breakLine": true
      },
      "column_setting_frame": {
        "type": "fieldset",
        "label": "カラムの設定",
        "show_if": "options.output_format!='file_list' && options.column_setting=='true'",
        "repeat": false,
        "fields": {
          "options.add_row_column": {
            "type": "checkbox",
            "label": "行列の追加",
            "show_if": "options.output_format=='word_by_word'",
            "val": "true",
            "isOn": "true",
            "isOff": "false",
            "breakLine": true
          },
          "options.add_word_count_column": {
            "type": "checkbox",
            "label": "Word Count カラムの追加",
            "show_if": "options.output_format=='word_by_word'",
            "val": "false",
            "isOn": "true",
            "isOff": "false",
            "breakLine": true
          },
          "options.add_word_nonword_column": {
            "type": "checkbox",
            "label": "Word & Non-Word カラムの追加",
            "show_if": "options.output_format=='word_by_word'",
            "val": "false",
            "isOn": "true",
            "isOff": "false",
            "breakLine": true
          },
          "options.add_section_column": {
            "type": "checkbox",
            "label": "セクション列の追加",
            "show_if": "options.output_format=='word_by_word'",
            "val": "false",
            "isOn": "true",
            "isOff": "false",
            "breakLine": true
          },
          "section_column_types_frame": {
            "type": "fieldset",
            "label": "セクションのカラムタイプ",
            "show_if": "options.output_format=='word_by_word' && options.add_section_column=='true'",
            "repeat": false,
            "fields": {
              "options.section_columns.add_paragraph_section_column": {
                "type": "checkbox",
                "label": "段落",
                "val": "false",
                "isOn": "true",
                "isOff": "false",
                "breakLine": true
              },
              "options.section_columns.add_custom_section_column": {
                "type": "checkbox",
                "label": "カスタムセクション区切り文字",
                "val": "false",
                "isOn": "true",
                "isOff": "false"
              },
              "options.section_columns.custom_delimiter": {
                "type": "text",
                "label": "区切り文字（1文字）",
                "show_if": "options.section_columns.add_custom_section_column=='true'",
                "maxLength": 1,
                "required": true,
                "breakLine": true
              },
              "options.section_columns.add_brackets_punctuation_section_column": {
                "type": "checkbox",
                "label": "括弧類と句読点",
                "val": "false",
                "isOn": "true",
                "isOff": "false"
              },
              "options.section_columns.brackets_punctuation_section_column_type": {
                "type": "select",
                "label": "括弧の種類",
                "show_if": "options.section_columns.add_brackets_punctuation_section_column=='true'",
                "options": {
                  "nesting_structure": {
                    "label": "入れ子構造"
                  },
                  "inner_bracket": {
                    "label": "インナーブラケット"
                  },
                  "brackets_only": {
                    "label": "Brackets Only"
                  },
                  "show_all": {
                    "label": "全てを表示"
                  }
                },
                "val": "show_all",
                "required": true,
                "breakLine": true
              }
            }
          },
          "options.add_filename_column": {
            "type": "checkbox",
            "label": "ファイル名の列を追加",
            "val": "false",
            "isOn": "true",
            "isOff": "false",
            "style": "margin-top: 10px",
            "breakLine": true
          },
          "options.add_filepath_column": {
            "type": "checkbox",
            "label": "ファイルパスのカラムを追加",
            "val": "false",
            "isOn": "true",
            "isOff": "false",
            "style": "margin-top: 10px",
            "breakLine": true
          }
        }
      },
      "options.is_cloud_dir": {
        "type": "hidden",
        "val": "false"
      },
      "if_dir_frame": {
        "type": "fieldset",
        "label": "ディレクトリインポートオプション",
        "show_if": "options.output_format!='file_list' && options.is_cloud_dir=='true'",
        "repeat": false,
        "fields": {
          "options.unify_in_one_table": {
            "type": "checkbox",
            "label": "一つのテーブルで統一",
            "val": "true",
            "isOn": "true",
            "isOff": "false",
            "breakLine": true
          },
          "options.suffix": {
            "type": "select",
            "label": "出力テーブルのサフィックス",
            "show_if": "options.unify_in_one_table=='false'",
            "options": {
              "number": {
                "label": "数"
              },
              "filename": {
                "label": "ファイル名"
              }
            },
            "val": "number",
            "required": true,
            "breakLine": true
          },
          "options.include_subdir": {
            "type": "checkbox",
            "label": "サブディレクトリを含む",
            "show_if": "options.output_format!='file_list' && options.is_cloud_dir=='true'",
            "val": "false",
            "isOn": "true",
            "isOff": "false",
            "breakLine": true,
            "style": "margin-top: 10px"
          }
        }
      },
      "options.file_list.include_subdir": {
        "type": "checkbox",
        "label": "サブディレクトリを含む",
        "show_if": "options.output_format=='file_list'",
        "val": "true",
        "isOn": "true",
        "isOff": "false",
        "breakLine": true,
        "style": "margin-top: 10px"
      },
      "options.file_list.add_change_date_column": {
        "type": "checkbox",
        "label": "変更日付列の追加",
        "show_if": "options.output_format=='file_list'",
        "val": "false",
        "isOn": "true",
        "isOff": "false",
        "breakLine": true
      }
    }
  },
  "import_link_token_frame": {
    "type": "fieldset",
    "label": "インポートリンク",
    "repeat": false,
    "fields": {
      "options.link_csv_file": {
        "type": "text",
        "label": "リンクを入力 例： https://github.com/zippydoc/test/blob/main/example.csv",
        "show_if": "options.webservice=='github' && options.is_cloud_dir=='false' && options.output_format=='csv_table'",
        "required": true,
        "width": "100%",
        "breakLine": true
      },
      "options.link_text_file": {
        "type": "text",
        "label": "Enter link e.g. https://github.com/zippydoc/test/blob/main/example.txt(json, csv, etc)",
        "show_if": "options.webservice=='github' && options.is_cloud_dir=='false' && options.output_format!='csv_table' && options.output_format!='file_list'",
        "required": true,
        "width": "100%",
        "breakLine": true
      },
      "options.one_drive.file_path": {
        "type": "text",
        "label": "ファイルのパスを入力してください 例：ZippyDoc/Subdir1/test.txt",
        "show_if": "options.webservice=='ms_one_drive' && options.output_format!='file_list' && options.is_cloud_dir=='false'",
        "required": true,
        "width": "100%",
        "breakLine": true
      },
      "options.dropbox.file_path": {
        "type": "text",
        "label": "ファイルのパスを入力してください 例：ZippyDoc/Subdir1/test.txt",
        "show_if": "options.webservice=='dropbox' && options.output_format!='file_list' && options.is_cloud_dir=='false'",
        "required": true,
        "width": "100%",
        "breakLine": true
      },
      "options.dir_path_filter": {
        "type": "fieldset",
        "label": "インポートパスとファイルフィルター",
        "show_if": "options.output_format=='file_list' || options.is_cloud_dir=='true'",
        "repeat": true,
        "fields": {
          "github__link_directory": {
            "type": "text",
            "label": "リンクを入力 例：https://github.com/zippydoc/test",
            "show_if": "options.webservice=='github'",
            "required": true,
            "width": "100%",
            "breakLine": true
          },
          "one_drive__dir_path": {
            "type": "text",
            "label": "ディレクトリへのパスを入力 例：ZippyDoc/Subdir1",
            "show_if": "options.webservice=='ms_one_drive'",
            "width": "100%",
            "breakLine": true
          },
          "dropbox__dir_path": {
            "type": "text",
            "label": "ディレクトリへのパスを入力 例：ZippyDoc/Subdir1",
            "show_if": "options.webservice=='dropbox'",
            "width": "100%",
            "breakLine": true
          },
          "google_drive__dir_path": {
            "type": "text",
            "label": "ディレクトリへのパスを入力",
            "show_if": "options.webservice=='google_drive'",
            "width": "100%",
            "breakLine": true
          },
          "dir_filter__filter_type": {
            "type": "select",
            "label": "フィルタータイプ",
            "options": {
              "all": {
                "label": "全て"
              },
              "contains": {
                "label": "含む"
              },
              "starts_with": {
                "label": "次で開始"
              },
              "ends_with": {
                "label": "次で終了"
              },
              "regex": {
                "label": "正規表現"
              }
            },
            "val": "all",
            "required": true
          },
          "dir_filter__filter_string": {
            "type": "text",
            "label": "フィルター",
            "show_if": "this.dir_filter__filter_type!='all'",
            "required": true
          },
          "delete": {
            "type": "button",
            "label": "削除",
            "function": "DELETE_ROW"
          }
        }
      },
      "options.is_github_private": {
        "type": "checkbox",
        "label": "Github リポジトリはプライベート",
        "show_if": "options.webservice=='github'",
        "val": "false",
        "isOn": "true",
        "isOff": "false",
        "style": "margin-top: 10px",
        "breakLine": true
      },
      "options.github_token": {
        "type": "text",
        "label": "Github個人用アクセストークン",
        "show_if": "options.webservice=='github' && options.is_github_private=='true'",
        "required": true,
        "width": "100%",
        "breakLine": true
      },
      "options.one_drive.access_token": {
        "type": "text",
        "label": "ワンドライブアクセストークン",
        "show_if": "options.webservice=='ms_one_drive'",
        "required": true,
        "width": "100%",
        "breakLine": true
      },
      "options.dropbox.auth_code": {
        "type": "text",
        "label": "Dropbox認証コード",
        "show_if": "options.webservice=='dropbox'",
        "required": true,
        "width": "100%",
        "breakLine": true
      },
      "options.google_drive.access_token": {
        "type": "text",
        "label": "Googleドライブアクセストークン",
        "show_if": "options.webservice=='google_drive'",
        "required": true,
        "width": "100%",
        "breakLine": true
      }
    }
  }
}