{
  "options": {
    "type": "fieldset",
    "label": "オプションの選択",
    "repeat": false,
    "fields": {
      "options.from": {
        "type": "select",
        "label": "データソース",
        "options": {
          "web": {
            "label": "ウェブサービスからのインポート"
          },
          "database": {
            "label": "データベースからのインポート"
          }
        },
        "val": "web",
        "required": true,
        "breakLine": true
      },
      "postgresql_db_credential_box": {
        "type": "fieldset",
        "label": "データベース接続証明書",
        "show_if": "options.from=='database'",
        "repeat": false,
        "fields": {
          "options.dbms": {
            "type": "select",
            "label": "DBMSの選択",
            "options": {
              "postgresql": {
                "label": "PostgreSQL"
              },
              "mysql": {
                "label": "MySQL (近日公開...)"
              },
              "sql_server": {
                "label": "MS SQL Server (近日公開...)"
              },
              "oracle": {
                "label": "Oracle (近日公開...)"
              }
            },
            "val": "postgresql",
            "required": true,
            "breakLine": true
          },
          "options.postgresql_credential.user": {
            "type": "text",
            "label": "ユーザー",
            "show_if": "options.dbms=='postgresql'",
            "required": true,
            "width": "100%",
            "breakLine": true
          },
          "options.postgresql_credential.password": {
            "type": "text",
            "label": "パスワード",
            "show_if": "options.dbms=='postgresql'",
            "required": true,
            "width": "100%",
            "breakLine": true
          },
          "options.postgresql_credential.host": {
            "type": "text",
            "label": "ホスト",
            "show_if": "options.dbms=='postgresql'",
            "required": true,
            "width": "100%",
            "breakLine": true
          },
          "options.postgresql_credential.database": {
            "type": "text",
            "label": "データベース",
            "show_if": "options.dbms=='postgresql'",
            "required": true,
            "width": "100%",
            "breakLine": true
          }
        }
      },
      "options.schema_name_to_import": {
        "type": "text",
        "label": "インポートするテーブルを含むスキーマ名",
        "show_if": "options.from=='database' && options.dbms=='postgresql'",
        "required": true,
        "val": "public",
        "width": "100%",
        "breakLine": true
      },
      "options.table_name_to_import": {
        "type": "text",
        "label": "インポートするテーブル名",
        "show_if": "options.from=='database' && options.dbms=='postgresql'",
        "required": true,
        "width": "100%",
        "breakLine": true
      },
      "options.webservice": {
        "type": "select",
        "label": "ウェブサービスの選択",
        "show_if": "options.from=='web'",
        "options": {
          "github": {
            "label": "Github"
          },
          "ms_one_drive": {
            "label": "Microsoft OneDrive"
          },
          "dropbox": {
            "label": "Dropbox"
          },
          "google_drive": {
            "label": "Googleドライブ（Coming soon）"
          }
        },
        "val": "github",
        "required": true,
        "breakLine": true
      },
      "options.output_format": {
        "type": "select",
        "label": "選択タイプ",
        "show_if": "options.from=='web'",
        "options": {
          "csv_table": {
            "label": "CSVテーブル"
          },
          "line_by_line": {
            "label": "行ごと"
          },
          "word_by_word": {
            "label": "単語ごと"
          },
          "file_list": {
            "label": "ファイルリスト"
          }
        },
        "val": "csv_table",
        "required": true
      },
      "options.word_pattern_type": {
        "type": "select",
        "label": "単語パターン",
        "show_if": "options.from=='web' && options.output_format=='word_by_word' && options.specify_word_pattern=='true'",
        "options": {
          "regex": {
            "label": "正規表現"
          }
        },
        "val": "regex",
        "required": true
      },
      "options.word_pattern_regex": {
        "type": "text",
        "label": "正規表現",
        "show_if": "options.from=='web' && options.output_format=='word_by_word' && options.specify_word_pattern=='true' && options.word_pattern_type=='regex'",
        "required": true,
        "val": "[a-z][1-9]",
        "breakLine": true
      },
      "options.column_setting": {
        "type": "checkbox",
        "label": "カラムの設定",
        "show_if": "options.from=='web' && options.output_format!='file_list'",
        "val": "false",
        "isOn": "true",
        "isOff": "false",
        "breakLine": true
      },
      "column_setting_frame": {
        "type": "fieldset",
        "label": "カラムの設定",
        "show_if": "options.from=='web' && options.output_format!='file_list' && options.column_setting=='true'",
        "repeat": false,
        "fields": {
          "options.add_row_column": {
            "type": "checkbox",
            "label": "行列の追加",
            "show_if": "options.from=='web' && options.output_format=='word_by_word'",
            "val": "true",
            "isOn": "true",
            "isOff": "false",
            "breakLine": true
          },
          "options.add_word_count_column": {
            "type": "checkbox",
            "label": "Word Count カラムの追加",
            "show_if": "options.from=='web' && options.output_format=='word_by_word'",
            "val": "false",
            "isOn": "true",
            "isOff": "false",
            "breakLine": true
          },
          "options.add_word_nonword_column": {
            "type": "checkbox",
            "label": "Word & Non-Word カラムの追加",
            "show_if": "options.from=='web' && options.output_format=='word_by_word'",
            "val": "false",
            "isOn": "true",
            "isOff": "false",
            "breakLine": true
          },
          "options.add_section_column": {
            "type": "checkbox",
            "label": "セクション列の追加",
            "show_if": "options.from=='web' && options.output_format=='word_by_word'",
            "val": "true",
            "isOn": "true",
            "isOff": "false",
            "breakLine": true
          },
          "section_column_types_frame": {
            "type": "fieldset",
            "label": "セクションのカラムタイプ",
            "show_if": "options.from=='web' && options.output_format=='word_by_word' && options.add_section_column=='true'",
            "repeat": false,
            "fields": {
              "options.section_columns.add_paragraph_section_column": {
                "type": "checkbox",
                "label": "段落",
                "val": "false",
                "isOn": "true",
                "isOff": "false",
                "breakLine": true
              },
              "options.section_columns.add_custom_section_column": {
                "type": "checkbox",
                "label": "カスタムセクション区切り文字",
                "val": "false",
                "isOn": "true",
                "isOff": "false"
              },
              "options.section_columns.custom_delimiter": {
                "type": "text",
                "label": "区切り文字（1文字）",
                "show_if": "options.section_columns.add_custom_section_column=='true'",
                "maxLength": 1,
                "required": true,
                "breakLine": true
              },
              "options.section_columns.add_brackets_punctuation_section_column": {
                "type": "checkbox",
                "label": "括弧類と句読点",
                "val": "false",
                "isOn": "true",
                "isOff": "false"
              },
              "options.section_columns.brackets_punctuation_section_column_type": {
                "type": "select",
                "label": "括弧の種類",
                "show_if": "options.section_columns.add_brackets_punctuation_section_column=='true'",
                "options": {
                  "nesting_structure": {
                    "label": "入れ子構造"
                  },
                  "inner_bracket": {
                    "label": "インナーブラケット"
                  },
                  "show_all": {
                    "label": "全てを表示"
                  }
                },
                "val": "nesting_structure",
                "required": true,
                "breakLine": true
              }
            }
          },
          "options.add_filename_column": {
            "type": "checkbox",
            "label": "ファイル名の列を追加",
            "val": "false",
            "isOn": "true",
            "isOff": "false",
            "style": "margin-top: 10px",
            "breakLine": true
          }
        }
      },
      "options.is_cloud_dir": {
        "type": "checkbox",
        "label": "ディレクトリのインポート",
        "show_if": "options.from=='web' && options.output_format!='file_list'",
        "val": "false",
        "isOn": "true",
        "isOff": "false",
        "style": "margin-top: 10px",
        "breakLine": true
      },
      "options.use_filename_as_output_table_name": {
        "type": "checkbox",
        "label": "テーブル名にファイル名を使用",
        "show_if": "options.from=='web' && options.output_format!='file_list' && options.is_cloud_dir=='false'",
        "val": "false",
        "isOn": "true",
        "isOff": "false",
        "breakLine": true
      },
      "options.unify_in_one_table": {
        "type": "checkbox",
        "label": "一つのテーブルで統一",
        "show_if": "options.from=='web' && options.output_format!='file_list' && options.is_cloud_dir=='true'",
        "val": "true",
        "isOn": "true",
        "isOff": "false"
      },
      "options.suffix": {
        "type": "select",
        "label": "サフィックス",
        "show_if": "options.from=='web' && options.output_format!='file_list' && options.is_cloud_dir=='true' && options.unify_in_one_table=='false'",
        "options": {
          "number": {
            "label": "数"
          },
          "filename": {
            "label": "ファイル名"
          }
        },
        "val": "number",
        "required": true,
        "breakLine": true
      },
      "options.is_github_private": {
        "type": "checkbox",
        "label": "リポジトリは非公開です",
        "show_if": "options.from=='web' && options.webservice=='github'",
        "val": "false",
        "isOn": "true",
        "isOff": "false",
        "breakLine": true
      },
      "options.include_subdir": {
        "type": "checkbox",
        "label": "サブディレクトリを含む",
        "show_if": "options.from=='web' && options.output_format=='file_list'",
        "val": "false",
        "isOn": "true",
        "isOff": "false",
        "breakLine": true
      },
      "options.file_list.add_change_date_column": {
        "type": "checkbox",
        "label": "変更日付列の追加",
        "show_if": "options.from=='web' && options.output_format=='file_list'",
        "val": "false",
        "isOn": "true",
        "isOff": "false",
        "breakLine": true
      },
      "options.github_token": {
        "type": "text",
        "label": "Github パーソナルアクセストークンの指定",
        "show_if": "options.from=='web' && options.webservice=='github' && options.is_github_private=='true'",
        "required": true,
        "width": "100%",
        "breakLine": true
      },
      "options.link_directory": {
        "type": "text",
        "label": "リンクを入力 例：https://github.com/zippydoc/test",
        "show_if": "options.from=='web' && options.webservice=='github' && options.output_format!='file_list' && options.is_cloud_dir=='true'",
        "required": true,
        "width": "100%",
        "breakLine": true
      },
      "options.link_directory_4_file_list": {
        "type": "text",
        "label": "リンクを入力 例：https://github.com/zippydoc/test",
        "show_if": "options.from=='web' && options.webservice=='github' && options.output_format=='file_list'",
        "required": true,
        "width": "100%",
        "breakLine": true
      },
      "options.link_file": {
        "type": "text",
        "label": "リンクを入力 例： https://github.com/zippydoc/test/blob/main/example.csv",
        "show_if": "options.from=='web' && options.webservice=='github' && options.is_cloud_dir=='false' && options.output_format=='csv_table'",
        "required": true,
        "width": "100%",
        "breakLine": true
      },
      "options.link_text_file": {
        "type": "text",
        "label": "Enter link e.g. https://github.com/zippydoc/test/blob/main/example.txt(json, csv, etc)",
        "show_if": "options.from=='web' && options.webservice=='github' && options.is_cloud_dir=='false' && options.output_format!='csv_table' && options.output_format!='file_list'",
        "required": true,
        "width": "100%",
        "breakLine": true
      },
      "options.one_drive.access_token": {
        "type": "text",
        "label": "OAuthアクセストークン",
        "show_if": "options.from=='web' && options.webservice=='ms_one_drive' && options.is_cloud_dir=='false'",
        "required": true,
        "width": "100%",
        "breakLine": true
      },
      "options.one_drive.file_path": {
        "type": "text",
        "label": "ファイルのパスを入力してください 例：ZippyDoc/Subdir1/test.txt",
        "show_if": "options.from=='web' && options.webservice=='ms_one_drive' && options.output_format!='file_list' && options.is_cloud_dir=='false'",
        "required": true,
        "width": "100%",
        "breakLine": true
      },
      "options.one_drive.dir_path": {
        "type": "text",
        "label": "ディレクトリへのパスを入力 例：ZippyDoc/Subdir1",
        "show_if": "options.from=='web' && options.webservice=='ms_one_drive' && (options.output_format=='file_list' || options.is_cloud_dir=='true')",
        "width": "100%",
        "breakLine": true
      },
      "options.dropbox.auth_code": {
        "type": "text",
        "label": "Dropbox認証コード",
        "show_if": "options.from=='web' && options.webservice=='dropbox' && options.is_cloud_dir=='false'",
        "required": true,
        "width": "100%",
        "breakLine": true
      },
      "options.dropbox.file_path": {
        "type": "text",
        "label": "ファイルのパスを入力してください 例：ZippyDoc/Subdir1/test.txt",
        "show_if": "options.from=='web' && options.webservice=='dropbox' && options.output_format!='file_list' && options.is_cloud_dir=='false'",
        "required": true,
        "width": "100%",
        "breakLine": true
      },
      "options.dropbox.dir_path": {
        "type": "text",
        "label": "ディレクトリへのパスを入力 例：ZippyDoc/Subdir1",
        "show_if": "options.from=='web' && options.webservice=='dropbox' && (options.output_format=='file_list' || options.is_cloud_dir=='true')",
        "width": "100%",
        "breakLine": true
      },
      "dir_filter_frame": {
        "type": "fieldset",
        "label": "ディレクトリ内のファイルをフィルタリング",
        "show_if": "options.from=='web' && options.output_format!='file_list' && options.is_cloud_dir=='true'",
        "repeat": false,
        "fields": {
          "options.dir_filter.filter_type": {
            "type": "select",
            "label": "フィルタータイプ",
            "options": {
              "all": {
                "label": "全て"
              },
              "contains": {
                "label": "含む"
              },
              "starts_with": {
                "label": "次で開始"
              },
              "ends_with": {
                "label": "次で終了"
              },
              "regex": {
                "label": "正規表現"
              }
            },
            "val": "all",
            "required": true
          },
          "options.dir_filter.filter_string": {
            "type": "text",
            "label": "文字列の入力",
            "show_if": "options.dir_filter.filter_type!='all'",
            "required": true,
            "width": "100%"
          }
        }
      }
    }
  }
}