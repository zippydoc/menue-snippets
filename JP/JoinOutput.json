{
  "output.output_table_types": {
    "type": "select",
    "label": "出力テーブルタイプ(複数可)",
    "required": true,
    "options": {
      "inner": {
        "label": "内部統合テーブル"
      },
      "left": {
        "label": "左結合テーブル"
      },
      "right": {
        "label": "右結合テーブル"
      }
    },
    "multiple": true,
    "val": ["inner"]
  },
  "output.standard_ending": {
    "type": "checkbox",
    "label": "標準ファイル拡張子",
    "val": "true",
    "isOn": "true",
    "isOff": "false",
    "breakLine": true
  },
  "output.inner_ending": {
    "show_if": "output.standard_ending == 'false'",
    "type": "text",
    "label": "内部結合テーブルのファイル拡張子",
    "val": "_j",
    "style": "margin-left: 10px"
  },
  "output.left_ending": {
    "show_if": "output.standard_ending == 'false'",
    "type": "text",
    "label": "左端",
    "val": "_l",
    "style": "margin-left: 10px"
  },
  "output.right_ending": {
    "show_if": "output.standard_ending == 'false'",
    "type": "text",
    "label": "右端",
    "val": "_r",
    "style": "margin-left: 10px",
    "breakLine": true
  }
}