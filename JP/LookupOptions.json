{
  "options": {
    "type": "fieldset",
    "label": "オプションの選択",
    "repeat": false,
    "fields": {
      "options.task_type": {
        "type": "select",
        "label": "タスクタイプ",
        "required": true,
        "options": {
          "generate_lookup_table": {
            "label": "ルックアップテーブルの生成"
          },
          "apply_lookup_table": {
            "label": "ルックアップテーブルの適用"
          },
          "apply_regex_table": {
            "label": "Regexテーブルの適用"
          },
          "apply_lookup_table_to_cells": {
            "label": "セルにルックアップテーブルを適用"
          },
          "apply_regex_table_to_cells": {
            "label": "セルにRegexテーブルを適用"
          }
        },
        "val": "generate_lookup_table"
      },
      "options.pattern_type": {
        "type": "select",
        "label": "パターンタイプ",
        "show_if": "options.task_type==''generate_lookup_table'' || options.task_type==''apply_lookup_table'' || options.task_type==''apply_lookup_table_to_cells''",
        "required": true,
        "options": {
          "duplicate_pattern": {
            "label": "単語パターン"
          },
          "number_range": {
            "label": "範囲(例：10 < x <= 100)"
          }
        },
        "val": "duplicate_pattern"
      },
      "duplicate_option_box": {
        "type": "fieldset",
        "label": "パターンオプションの重複",
        "show_if": "options.task_type==''label_duplicates'' || (options.task_type==''generate_lookup_table'' && options.pattern_type==''duplicate_pattern'')",
        "repeat": false,
        "fields": {
          "options.minimum_number_of_matching_characters": {
            "type": "text",
            "label": "一致する文字の最小数",
            "val": "3",
            "required": true,
            "style": "min-width: 240px"
          },
          "options.minimum_number_of_duplicates": {
            "type": "text",
            "label": "重複の最小数",
            "val": "2",
            "required": true,
            "style": "min-width: 240px",
            "breakLine": true
          }
        }
      },
      "number_range_option_box": {
        "type": "fieldset",
        "label": "オプションの数範囲",
        "show_if": "options.task_type==''generate_lookup_table'' && options.pattern_type==''number_range''",
        "repeat": false,
        "fields": {
          "options.number_range.range_type": {
            "type": "select",
            "label": "範囲タイプ",
            "required": true,
            "options": {
              "equal_interval": {
                "label": "N 等間隔"
              },
              "equal_range": {
                "label": "等しい範囲"
              },
              "custom_range": {
                "label": "カスタム範囲"
              }
            },
            "val": "equal_interval"
          },
          "options.number_range.n_value": {
            "type": "text",
            "label": "N",
            "show_if": "options.number_range.range_type==''equal_interval''",
            "val": "3",
            "required": true
          },
          "options.number_range.range_value": {
            "type": "text",
            "label": "範囲",
            "show_if": "options.number_range.range_type==''equal_range''",
            "val": "10",
            "required": true
          },
          "options.number_range.custom_ranges": {
            "type": "fieldset",
            "label": "カスタム範囲パターンの追加",
            "show_if": "options.number_range.range_type==''custom_range''",
            "repeat": true,
            "fields": {
              "pattern": {
                "type": "text",
                "label": "パターン",
                "val": "0<=x<10",
                "required": true
              },
              "label": {
                "type": "text",
                "label": "ラベル",
                "val": "positive10",
                "required": true
              },
              "delete": {
                "type": "button",
                "label": "削除",
                "function": "DELETE_ROW"
              }
            }
          }
        },
        "breakLine": true
      },
      "options.lookup_table_type": {
        "type": "select",
        "label": "Lookup Table Type",
        "show_if": "options.task_type==''apply_lookup_table'' || options.task_type==''apply_regex_table'' || options.task_type==''apply_lookup_table_to_cells'' || options.task_type==''apply_regex_table_to_cells''",
        "required": true,
        "options": {
          "select_lookup_table": {
            "label": "Select Lookup Table"
          },
          "custom_lookup_table": {
            "label": "Custom Lookup Table"
          }
        },
        "val": "select_lookup_table"
      },
      "lookup_table_frame": {
        "type": "fieldset",
        "label": "",
        "show_if": "options.task_type==''apply_lookup_table'' || options.task_type==''apply_regex_table'' || options.task_type==''apply_lookup_table_to_cells'' || options.task_type==''apply_regex_table_to_cells''",
        "repeat": false,
        "fields": {
          "options.lookup_table_id": {
            "type": "select",
            "label": "ルックアップテーブル",
            "show_if": "options.lookup_table_type==''select_lookup_table''",
            "bind": "TABLES",
            "empty": "",
            "required": true,
            "breakLine": true
          },
          "options.custom_lookup_table": {
            "type": "fieldset",
            "label": "Custom Lookup Table",
            "show_if": "options.lookup_table_type==''custom_lookup_table''",
            "repeat": true,
            "fields": {
              "lookup": {
                "type": "text",
                "label": "ルックアップ",
                "val": "",
                "required": true
              },
              "label": {
                "type": "text",
                "label": "ラベル",
                "val": "",
                "required": true
              },
              "delete": {
                "type": "button",
                "label": "削除",
                "function": "DELETE_ROW"
              }
            }
          },
          "options.partial_match": {
            "type": "checkbox",
            "label": "部分一致",
            "show_if": "options.pattern_type==''duplicate_pattern'' && (options.task_type==''apply_lookup_table'' || options.task_type==''apply_lookup_table_to_cells'')",
            "val": "false",
            "isOn": "true",
            "isOff": "false",
            "style": "margin-top: 10px",
            "breakLine": true
          },
          "options.case_sensitive": {
            "type": "checkbox",
            "label": "ケース・センシティブ",
            "show_if": "options.pattern_type==''duplicate_pattern'' && (options.task_type==''apply_lookup_table'' || options.task_type==''apply_lookup_table_to_cells'')",
            "val": "false",
            "isOn": "true",
            "isOff": "false",
            "breakLine": true
          },
          "options.use_delimiter": {
            "type": "checkbox",
            "label": "カスタム区切り文字",
            "show_if": "options.task_type==''apply_lookup_table'' || options.task_type==''apply_lookup_table_to_cells''",
            "val": "false",
            "isOn": "true",
            "isOff": "false"
          },
          "options.delimiter": {
            "type": "text",
            "label": "区切り文字の入力",
            "show_if": "options.use_delimiter==''true'' && (options.task_type==''apply_lookup_table'' || options.task_type==''apply_lookup_table_to_cells'')",
            "val": ";",
            "required": true,
            "breakLine": true
          }
        }
      },
      "options.input_table.use_filter": {
        "type": "checkbox",
        "label": "入力テーブルにフィルターを使用",
        "show_if": "options.task_type==''apply_lookup_table'' || options.task_type==''apply_regex_table'' || options.task_type==''apply_lookup_table_to_cells'' || options.task_type==''apply_regex_table_to_cells''",
        "val": "false",
        "isOn": "true",
        "isOff": "false",
        "style": "margin-top: 10px",
        "breakLine": true
      },
      "input_table_filter_box": {
        "type": "fieldset",
        "label": "テーブルフィルターオプションの入力",
        "show_if": "options.input_table.use_filter==''true'' && (options.task_type==''apply_lookup_table'' || options.task_type==''apply_regex_table'' || options.task_type==''apply_lookup_table_to_cells'' || options.task_type==''apply_regex_table_to_cells'')",
        "repeat": false,
        "fields": {
          "options.input_table.filter": {
            "type": "fieldset",
            "label": "入力テーブルのフィルター条件を追加",
            "repeat": true,
            "fields": {
              "initial_op": {
                "type": "select",
                "label": "論理オペレータの選択",
                "required": true,
                "show_if": "$index == 0",
                "options": {
                  "if": {
                    "label": "IF"
                  },
                  "if_not": {
                    "label": "IF NOT"
                  }
                },
                "val": "if"
              },
              "op": {
                "type": "select",
                "label": "論理オペレータの選択",
                "required": true,
                "show_if": "$index > 0",
                "options": {
                  "and": {
                    "label": "AND"
                  },
                  "or": {
                    "label": "OR"
                  },
                  "and_not": {
                    "label": "AND NOT"
                  },
                  "or_not": {
                    "label": "OR NOT"
                  }
                },
                "val": "and"
              },
              "column": {
                "type": "select",
                "label": "列の選択",
                "table": "input_table",
                "bind": "COLUMNS",
                "required": true
              },
              "condition": {
                "type": "select",
                "label": "オペレータの選択",
                "required": true,
                "options": {
                  "含む": {
                    "label": "含む"
                  },
                  "CONTAINED IN STRING": {
                    "label": "次の中に含む"
                  },
                  "次で開始": {
                    "label": "次で開始"
                  },
                  "次で終了": {
                    "label": "次で終了"
                  },
                  "=": {
                    "label": "等しい"
                  },
                  ">": {
                    "label": "超"
                  },
                  ">=": {
                    "label": "以上"
                  },
                  "<": {
                    "label": "より小さい"
                  },
                  "<=": {
                    "label": "より小さいまたは等しい"
                  },
                  "EMPTY": {
                    "label": "空です"
                  },
                  "BLANK": {
                    "label": "空白です"
                  },
                  "無効です": {
                    "label": "無効です"
                  },
                  "REGEXの一致": {
                    "label": "REGEXの一致"
                  },
                  "CONTAINED IN TABLE": {
                    "label": "テーブル内"
                  },
                  "REGEXテーブル内": {
                    "label": "REGEXテーブル内"
                  },
                  "日付": {
                    "label": "日付"
                  }
                },
                "val": "含む"
              },
              "date_condition": {
                "type": "select",
                "label": "日付オペレータの選択",
                "required": true,
                "show_if": "this.condition == ''DATE''",
                "options": {
                  "<": {
                    "label": "前"
                  },
                  "<=": {
                    "label": "前または等しい"
                  },
                  "=": {
                    "label": "等しい"
                  },
                  ">": {
                    "label": "以降"
                  },
                  ">=": {
                    "label": "以降または等しい"
                  }
                },
                "val": "="
              },
              "case_sensitive": {
                "type": "select",
                "label": "ケース・センシティブの選択",
                "required": true,
                "show_if": "this.condition == ''CONTAINS'' || this.condition == ''CONTAINED IN STRING'' || this.condition == ''='' || this.condition == ''STARTS WITH'' || this.condition == ''ENDS WITH'' || this.condition == ''REGEX MATCH'' || this.condition == ''NOT REGEX MATCH'' || this.condition == ''IN REGEX TABLE'' || this.condition == ''NOT IN REGEX TABLE''",
                "options": {
                  "yes": {
                    "label": "ケース・センシティブ"
                  },
                  "no": {
                    "label": "ケース・インセンシティブ"
                  }
                },
                "val": "yes"
              },
              "date_value": {
                "type": "date",
                "label": "日付の選択",
                "show_if": "this.condition==''DATE''",
                "val": "1/31/2020",
                "required": true
              },
              "value": {
                "type": "text",
                "label": "値の入力",
                "show_if": "this.condition != ''DATE'' && this.condition != ''EMPTY'' && this.condition != ''NOT EMPTY'' && this.condition != ''BLANK'' && this.condition != ''NOT BLANK'' && this.condition != ''IS NULL'' && this.condition != ''IS NOT NULL'' && this.condition != ''CONTAINED IN TABLE'' && this.condition != ''NOT CONTAINED IN TABLE'' && this.condition != ''IN REGEX TABLE'' && this.condition != ''NOT IN REGEX TABLE''",
                "val": "",
                "required": true
              },
              "compare_table": {
                "type": "select",
                "label": "テーブルの選択",
                "bind": "TABLES",
                "empty": "",
                "show_if": "this.condition == ''CONTAINED IN TABLE'' || this.condition == ''NOT CONTAINED IN TABLE'' || this.condition == ''IN REGEX TABLE'' || this.condition == ''NOT IN REGEX TABLE''",
                "required": true
              },
              "compare_column": {
                "type": "select",
                "label": "列の選択",
                "table": "this.compare_table",
                "bind": "COLUMNS",
                "show_if": "this.condition == ''CONTAINED IN TABLE'' || this.condition == ''NOT CONTAINED IN TABLE'' || this.condition == ''IN REGEX TABLE'' || this.condition == ''NOT IN REGEX TABLE''",
                "required": true
              },
              "delete": {
                "type": "button",
                "label": "削除",
                "function": "DELETE_ROW"
              }
            }
          },
          "options.input_table.apply_to_record": {
            "type": "checkbox",
            "label": "入力テーブルフィルターのレコードに適用",
            "val": "true",
            "isOn": "true",
            "isOff": "false",
            "style": "margin-top: 10px",
            "breakLine": true
          },
          "input_table_apply_to_group_box": {
            "type": "fieldset",
            "label": "入力テーブルのグループに適用",
            "show_if": "options.input_table.apply_to_record==''false''",
            "repeat": false,
            "fields": {
