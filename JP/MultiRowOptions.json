
    "type": "fieldset", {
  "options_box": {
    "label": "オプション", 
    "repeat": false, 
    "fields": {
      "options.task": {
        "type": "hidden", 
        "val": "multi_row"
      }, 
      "options.add_columns": {
        "type": "fieldset", 
        "label": "新しい列の追加", 
        "repeat": true, 
        "fields": {
          "new_column_name": {
            "type": "text", 
            "label": "新しい列名", 
            "val": "", 
            "breakLine": true
          }, 
          "multi_row_functions__function": {
            "type": "select", 
            "label": "機能", 
            "required": true, 
            "options": {
              "trailing_row_n_average": {
                "label": "末尾行-N平均"
              }, 
              "index_from_row_n": {
                "label": "N行目からの指数"
              }, 
              "absolute_growth": {
                "label": "絶対成長"
              }, 
              "relative_growth": {
                "label": "相対的成長"
              }, 
              "compound_growth": {
                "label": "複合成長"
              }
            }, 
            "val": "trailing_row_n_average"
          }, 
          "multi_row_functions__sel_column": {
            "type": "select", 
            "label": "列", 
            "show_if": "this.multi_row_functions__function=='trailing_row_n_average' || this.multi_row_functions__function=='index_from_row_n' || this.multi_row_functions__function=='absolute_growth' || this.multi_row_functions__function=='relative_growth' || this.multi_row_functions__function=='compound_growth'", 
            "table": "input_table", 
            "bind": "COLUMNS", 
            "required": true
          }, 
          "multi_row_functions__n_value": {
            "type": "text", 
            "label": "N (正の整数)", 
            "show_if": "this.multi_row_functions__function=='trailing_row_n_average' || this.multi_row_functions__function=='index_from_row_n' || this.multi_row_functions__function=='compound_growth'", 
            "val": "1", 
            "required": true, 
            "breakLine": true
          }, 
          "format__category": {
            "type": "select", 
            "label": "フォーマット", 
            "required": true, 
            "options": {
              "standard": {
                "label": "標準"
              }, 
              "number": {
                "label": "数"
              }, 
              "percentage": {
                "label": "割合"
              }, 
              "text": {
                "label": "テキスト"
              }, 
              "date": {
                "label": "日付"
              }, 
              "currency": {
                "label": "通貨"
              }
            }, 
            "val": "standard"
          }, 
          "format__rounding": {
            "type": "select", 
            "label": "四捨五入", 
            "show_if": "this.format__category=='number' || this.format__category=='percentage' || this.format__category=='currency'", 
            "required": true, 
            "options": {
              "no_rounding": {
                "label": "四捨五入無し"
              }, 
              "0": {
                "label": "0桁"
              }, 
              "1": {
                "label": "1桁"
              }, 
              "2": {
                "label": "2桁"
              }, 
              "3": {
                "label": "3桁"
              }, 
              "4": {
                "label": "4桁"
              }, 
              "5": {
                "label": "5桁"
              }, 
              "6": {
                "label": "6桁"
              }, 
              "7": {
                "label": "7桁"
              }, 
              "8": {
                "label": "8桁"
              }, 
              "9": {
                "label": "9桁"
              }
            }, 
            "val": "2"
          }, 
          "format__use_1000_separator": {
            "type": "checkbox", 
            "label": "1000セパレータ(.)を使用", 
            "show_if": "this.format__category=='number' || this.format__category=='percentage' || this.format__category=='currency'", 
            "val": "true", 
            "isOn": "true", 
            "isOff": "false"
          }, 
          "format__currency_symbol": {
            "type": "select", 
            "label": "シンボル", 
            "show_if": "this.format__category=='currency'", 
            "required": true, 
            "options": {
              "dollar": {
                "label": "$"
              }, 
              "euro": {
                "label": "€"
              }, 
              "pound": {
                "label": "£"
              }
            }, 
            "val": "dollar"
          }, 
          "format__date_format": {
            "type": "select", 
            "label": "タイプ", 
            "show_if": "this.format__category=='date'", 
            "required": true, 
            "options": {
              "mm/dd/yyyy": {
                "label": "3/14/2012"
              }, 
              "D,  M dd,  Y": {
                "label": "2012年3月14日（水）"
              }, 
              "yyyy-mm-dd": {
                "label": "2012-03-14"
              }
            }, 
            "val": "mm/dd/yyyy", 
            "breakLine": true
          }, 
          "apply_to_group__enabled": {
            "type": "checkbox", 
            "label": "グループに適用", 
            "show_if": "this.task_type!='static_value'", 
            "val": "false", 
            "isOn": "true", 
            "isOff": "false"
          }, 
          "apply_to_group__group_columns": {
            "type": "select", 
            "label": "列(複数可)のグループ", 
            "show_if": "this.apply_to_group__enabled == 'true' && this.task_type!='static_value'", 
            "table": "input_table", 
            "bind": "COLUMNS", 
            "multiple": true, 
            "required": true
          }, 
          "delete": {
            "type": "button", 
            "label": "削除", 
            "function": "DELETE_ROW"
          }
        }
      }, 
      "options.use_filter": {
        "type": "checkbox", 
        "label": "フィルターの使用", 
        "val": "false", 
        "isOn": "true", 
        "isOff": "false", 
        "style": "margin-top: 10px", 
        "breakLine": true
      }, 
      "filter_box": {
        "type": "fieldset", 
        "label": "フィルターオプション", 
        "show_if": "options.use_filter=='true'", 
        "repeat": false, 
        "fields": {
          "options.filter": {
            "type": "fieldset", 
            "label": "フィルター条件の追加", 
            "repeat": true, 
            "fields": {
              "initial_op": {
                "type": "select", 
                "label": "論理オペレータの選択", 
                "required": true, 
                "show_if" :  "$index == 0", 
                "options": {
                  "if": {
                    "label": "IF"
                  }, 
                  "if_not": {
                    "label": "IF NOT"
                  }
                }, 
                "val": "if"
              }, 
              "op": {
                "type": "select", 
                "label": "論理オペレータの選択", 
                "required": true, 
                "show_if" :  "$index > 0", 
                "options": {
                  "and": {
                    "label": "AND"
                  }, 
                  "or": {
                    "label": "OR"
                  }, 
                  "and_not": {
                    "label": "AND NOT"
                  }, 
                  "or_not": {
                    "label": "OR NOT"
                  }
                }, 
                "val": "and"
              }, 
              "column": {
                "type": "select", 
                "label": "列の選択", 
                "table": "input_table", 
                "bind": "COLUMNS", 
                "required": true
              }, 
              "condition": {
                "type": "select", 
                "label": "オペレータの選択", 
                "required": true, 
                "options": {
                  "含む": {
                    "label": "含む"
                  }, 
                  "CONTAINED IN STRING": {
                    "label": "次の中に含む"
                  }, 
                  "次で開始": {
                    "label": "次で開始"
                  }, 
                  "次で終了": {
                    "label": "次で終了"
                  }, 
                  "=": {
                    "label": "等しい"
                  }, 
                  ">": {
                    "label": "超"
                  }, 
                  ">=": {
                    "label": "以上"
                  }, 
                  "<": {
                    "label": "より小さい"
                  }, 
                  "<=": {
                    "label": "より小さいまたは等しい"
                  }, 
                  "EMPTY": {
                    "label": "空です"
                  }, 
                  "BLANK": {
                    "label": "空白です"
                  }, 
                  "無効です": {
                    "label": "無効です"
                  }, 
                  "REGEXの一致": {
                    "label": "REGEXの一致"
                  }, 
                  "CONTAINED IN TABLE": {
                    "label": "テーブル内"
                  }, 
                  "REGEXテーブル内": {
                    "label": "REGEXテーブル内"
                  }, 
                  "日付": {
                    "label": "日付"
                  }
                }, 
                "val": "含む"
              }, 
              "date_condition": {
                "type": "select", 
                "label": "日付オペレータの選択", 
                "required": true, 
                "show_if": "this.condition == 'DATE'", 
                "options": {
                  "<": {
                    "label": "前"
                  }, 
                  "<=": {
                    "label": "前または等しい"
                  }, 
                  "=": {
                    "label": "等しい"
                  }, 
                  ">": {
                    "label": "以降"
                  }, 
                  ">=": {
                    "label": "以降または等しい"
                  }
                }, 
                "val": "="
              }, 
              "case_sensitive": {
                "type": "select", 
                "label": "ケース・センシティブの選択", 
                "required": true, 
                "show_if": "this.condition == 'CONTAINS' || this.condition == 'CONTAINED IN STRING' || this.condition == '=' || this.condition == 'STARTS WITH' || this.condition == 'ENDS WITH' || this.condition == 'REGEX MATCH' || this.condition == 'NOT REGEX MATCH' || this.condition == 'IN REGEX TABLE' || this.condition == 'NOT IN REGEX TABLE'", 
                "options": {
                  "yes": {
                    "label": "ケース・センシティブ"
                  }, 
                  "no": {
                    "label": "ケース・インセンシティブ"
                  }
                }, 
                "val": "yes"
              }, 
              "date_value": {
                "type": "date", 
                "label": "日付の選択", 
                "show_if": "this.condition=='DATE'", 
                "val": "1/31/2020", 
                "required": true
              }, 
              "value": {
                "type": "text", 
                "label": "値の入力", 
                "show_if": "this.condition != 'DATE' && this.condition != 'EMPTY' && this.condition != 'NOT EMPTY' && this.condition != 'BLANK' && this.condition != 'NOT BLANK' && this.condition != 'IS NULL' && this.condition != 'IS NOT NULL' && this.condition != 'CONTAINED IN TABLE' && this.condition != 'NOT CONTAINED IN TABLE' && this.condition != 'IN REGEX TABLE' && this.condition != 'NOT IN REGEX TABLE'", 
                "val": "", 
                "required": true
              }, 
              "compare_table": {
                "type": "select", 
                "label": "テーブルの選択", 
                "bind": "TABLES", 
                "empty": "", 
                "show_if": "this.condition == 'CONTAINED IN TABLE' || this.condition == 'NOT CONTAINED IN TABLE' || this.condition == 'IN REGEX TABLE' || this.condition == 'NOT IN REGEX TABLE'", 
                "required": true
              }, 
              "compare_column": {
                "type": "select", 
                "label": "列の選択", 
                "table": "this.compare_table", 
                "bind": "COLUMNS", 
                "show_if": "this.condition == 'CONTAINED IN TABLE' || this.condition == 'NOT CONTAINED IN TABLE' || this.condition == 'IN REGEX TABLE' || this.condition == 'NOT IN REGEX TABLE'", 
                "required": true
              }, 
              "delete": {
                "type": "button", 
                "label": "削除", 
                "function": "DELETE_ROW"
              }
            }
          }, 
          "options.apply_to_record": {
            "type": "checkbox", 
            "label": "レコードに適用", 
            "val": "true", 
            "isOn": "true", 
            "isOff": "false", 
            "style": "margin-top: 10px", 
            "breakLine": true
          }, 
          "apply_to_group_box": {
            "type": "fieldset", 
            "label": "グループに適用", 
            "show_if": "options.apply_to_record=='false'", 
            "repeat": false, 
            "fields": {
              "options.apply_to_group_columns": {
                "type": "select", 
                "label": "グループ列の選択", 
                "required": true, 
                "width": "100%", 
                "table": "input_table", 
                "multiple": true, 
                "bind": "COLUMNS", 
                "breakLine": true
              }
            }
          }
        }
      }
    }
  }
}