{
  "options": {
    "type": "fieldset",
    "label": "オプションの選択",
    "repeat": false,
    "fields": {
      "type": {
        "type": "select",
        "label": "選択タイプ",
        "val": "first_n",
        "options": {
          "first_n": {
            "label": "最初のN"
          },
          "last_n": {
            "label": "最後のN"
          },
          "custom_selection": {
            "label": "カスタムセレクション"
          },
          "skip_first_n": {
            "label": "最初のNをスキップ"
          },
          "skip_last_n": {
            "label": "最後のNをスキップ"
          },
          "first_n_percent": {
            "label": "最初のN%"
          },
          "last_n_percent": {
            "label": "最後のN%"
          },
          "random_n_records": {
            "label": "ランダムN"
          },
          "random_n_percent": {
            "label": "ランダムN%"
          },
          "random_choice_1_n": {
            "label": "ランダムチョイス1:N"
          },
          "every_nth_record": {
            "label": "Nレコード毎"
          }
        },
        "required": true,
        "width": "100%",
        "breakLine": true
      },
      "n": {
        "type": "text",
        "label": "入力 N",
        "show_if": "type!='custom_selection'",
        "val": "1",
        "required": true,
        "width": "100%",
        "breakLine": true
      },
      "ranges": {
        "type": "text",
        "label": "範囲の入力",
        "show_if": "type=='custom_selection'",
        "val": "-2; 4-6; 9; 12+",
        "required": true,
        "width": "100%",
        "breakLine": true
      },
      "set_offset": {
        "type": "checkbox",
        "label": "オフセット",
        "show_if": "type=='every_nth_record'",
        "val": "false",
        "isOn": "true",
        "isOff": "false"
      },
      "offset": {
        "type": "text",
        "label": "オフセット",
        "show_if": "type=='every_nth_record' && set_offset=='true'",
        "val": "1",
        "required": true,
        "breakLine": true
      },
      "group_by_column": {
        "type": "checkbox",
        "label": "列別グループ",
        "val": "false",
        "isOn": "true",
        "isOff": "false",
        "breakLine": true
      },
      "columns_to_group_box": {
        "type": "fieldset",
        "label": "列別グループ",
        "show_if": "group_by_column=='true'",
        "repeat": false,
        "fields": {
          "columns_to_group": {
            "type": "select",
            "label": "グループ列の選択",
            "required": true,
            "width": "100%",
            "table": "table_id",
            "multiple": true,
            "bind": "COLUMNS",
            "breakLine": true
          }
        }
      }
    }
  }
}