{
  "output.add_period": {
    "type": "checkbox",
    "ラベル": "周期の追加",
    "show_if": "options.type=='process' || options.type=='custom'",
    "val": "false",
    "isOn": "true",
    "isOff": "false",
    "style": "margin-top: 10px",
    "breakLine": true
  },
  "output.period": {
    "type": "select",
    "ラベル": "周期",
    "show_if": "(options.type=='process' || options.type=='custom') && output.add_period=='true'",
    "multiple": true,
    "options": {
      "year": {
        "ラベル": "年"
      },
      "half-year": {
        "ラベル": "半年"
      },
      "quarter": {
        "ラベル": "四半期"
      },
      "month": {
        "ラベル": "月"
      },
      "week": {
        "ラベル": "週"
      }
    },
    "val": "",
    "required": true
  },
  "output.period_concatenate": {
    "type": "checkbox",
    "ラベル": "連結",
    "show_if": "(options.type=='process' || options.type=='custom') && output.add_period=='true'",
    "val": "false",
    "isOn": "true",
    "isOff": "false",
    "breakLine": true
  }
}