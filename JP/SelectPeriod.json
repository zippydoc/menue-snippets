{
  "output.add_period": {
    "type": "checkbox", 
    "label": "周期の追加", 
    "show_if": "options.type=='process'", 
    "val": "false", 
    "isOn": "true", 
    "isOff": "false", 
    "style": "margin-top: 10px", 
    "breakLine": true
  }, 
  "output.period": {
    "type": "select", 
    "label": "周期", 
    "show_if": "options.type=='process' && output.add_period=='true'", 
    "multiple": true, 
    "options": {
      "year": {
        "label": "年"
      }, 
      "half-year": {
        "label": "半年"
      }, 
      "quarter": {
        "label": "四半期"
      }, 
      "month": {
        "label": "月"
      }, 
      "week": {
        "label": "週"
      }
    }, 
    "val": "", 
    "required": true
  }, 
  "output.period_concatenate": {
    "type": "checkbox", 
    "label": "連結", 
    "show_if": "options.type=='process' && output.add_period=='true'", 
    "val": "false", 
    "isOn": "true", 
    "isOff": "false"
  }
}