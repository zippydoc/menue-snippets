{
  "actions": {
    "type": "fieldset",
    "label": "アクションの追加",
    "repeat": true,
    "fields": {
      "columns": {
        "type": "select",
        "label": "Column(s)",
        "table": "table_id",
		"assign_selected_value_to":"this.output_column_name",
        "bind": "COLUMNS",
        "multiple": true,
        "required": true
      },
      "action": {
        "type": "select",
        "label": "アクション",
        "options": {
          "group_by": {
            "label": "グループ別"
          },
          "count": {
            "label": "計算"
          },
          "sum": {
            "label": "合計"
          },
          "first": {
            "label": "最初"
          },
          "last": {
            "label": "最後"
          },
          "shortest": {
            "label": "最短"
          },
          "longest": {
            "label": "最長"
          },
          "concatenate": {
            "label": "連結"
          },
          "mathematics": {
            "label": "数学"
          }
        },
        "val": "group_by"
      },
      "count_action_category": {
        "type": "select",
        "label": "アクションタイプ",
        "show_if": "this.action=='count'",
        "options": {
          "all": {
            "label": "全て"
          },
          "distinct": {
            "label": "区別"
          },
          "empty_or_zero": {
            "label": "空または0"
          },
          "non_empty_or_zero": {
            "label": "空または0以外"
          }
        },
        "val": "all"
      },
      "action_type1": {
        "type": "select",
        "label": "アクションサブタイプ",
        "show_if": "this.action=='count' && (this.count_action_category=='all' || this.count_action_category=='distinct')",
        "options": {
          "-": {
            "label": "-"
          },
          "non_zero": {
            "label": "0以外"
          },
          "non_empty": {
            "label": "空以外"
          },
          "non_null": {
            "label": "無効以外"
          },
          "non_blank": {
            "label": "空白以外"
          }
        },
        "val": "-"
      },
      "action_type2": {
        "type": "select",
        "label": "アクションサブタイプ",
        "show_if": "this.action=='count' && this.count_action_category=='empty_or_zero'",
        "options": {
          "zero": {
            "label": "0"
          },
          "empty": {
            "label": "空(無効 & 空欄)"
          },
          "null": {
            "label": "Null"
          },
          "blank": {
            "label": "空白"
          }
        },
        "val": "zero"
      },
      "action_type3": {
        "type": "select",
        "label": "アクションサブタイプ",
        "show_if": "this.action=='count' && this.count_action_category=='non_empty_or_zero'",
        "options": {
          "non_zero": {
            "label": "0以外"
          },
          "non_empty": {
            "label": "空以外"
          },
          "non_null": {
            "label": "無効以外"
          },
          "non_blank": {
            "label": "空白以外"
          }
        },
        "val": "non_zero"
      },
      "mathematics_action_category": {
        "type": "select",
        "label": "アクションタイプ",
        "show_if": "this.action=='mathematics'",
        "options": {
          "min": {
            "label": "最小"
          },
          "max": {
            "label": "最大"
          },
          "average": {
            "label": "平均"
          },
          "median": {
            "label": "中央値"
          },
          "standard_deviation": {
            "label": "標準偏差"
          },
          "variance": {
            "label": "分散"
          }
        },
        "val": "min"
      },
      "action_type4": {
        "type": "select",
        "label": "アクションサブタイプ",
        "show_if": "this.action=='mathematics'",
        "options": {
          "include_zero": {
            "label": "0を含む"
          },
          "exclude_zero": {
            "label": "0を除く"
          }
        },
        "val": "include_zero"
      },
      "concatenate_separator": {
        "type": "text",
        "label": "セパレータ",
        "show_if": "this.action=='concatenate'",
        "val": ","
      },
      "output_column_name": {
        "type": "text",
        "label": "列名の出力",
        "val": ""
      },
      "rounding": {
        "type": "select",
        "label": "四捨五入",
        "show_if": "this.action=='sum' || this.action=='mathematics'",
        "required": true,
        "options": {
          "no_rounding": {
            "label": "四捨五入無し"
          },
          "0": {
            "label": "0桁"
          },
          "1": {
            "label": "1桁"
          },
          "2": {
            "label": "2桁"
          },
          "3": {
            "label": "3桁"
          },
          "4": {
            "label": "4桁"
          },
          "5": {
            "label": "5桁"
          },
          "6": {
            "label": "6桁"
          },
          "7": {
            "label": "7桁"
          },
          "8": {
            "label": "8桁"
          },
          "9": {
            "label": "9桁"
          }
        },
        "val": "no_rounding"
      },
      "delete": {
        "type": "button",
        "label": "削除",
        "function": "DELETE_ROW"
      }
    }
  },
  "info1": {
    "type": "textarea",
    "label": "",
    "val": "If more than one columns selected, Output Column Name input value should be used as suffix",
    "disabled": true,
    "style": "width: 100%"
  }
}