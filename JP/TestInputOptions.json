{
  "options": {
    "type": "fieldset",
    "label": "テーブルヘッダとラベルの結果",
    "repeat": false,
    "fields": {
      "has_header": {
        "type": "button",
        "label": "ヘッダーあり！",
        "show_if": "$input_table.numOfHeader >= 1"
      },
      "has_label": {
        "type": "button",
        "label": "ラベルがある,
        "show_if": "$input_table.numOfLabel >= 1"
      },
      "has_both": {
        "type": "button",
        "label": "両方あり",
        "show_if": "$input_table.numOfHeader >= 1 && $input_table.numOfLabel >= 1"
      },
      "has_both2": {
        "type": "text",
        "label": "両方を持つ2",
        "show_if": "$input_table.numOfHeader>=1 && $input_table.numOfLabel>=1",
        "val": "both"
      },
      "header_only": {
        "type": "button",
        "label": "ヘッダのみ!",
        "show_if": "$input_table.numOfHeader >= 1 && $input_table.numOfLabel < 1"
      },
      "label_only": {
        "type": "button",
        "label": "ラベルのみ",
        "show_if": "$input_table.numOfHeader < 1 && $input_table.numOfLabel >= 1"
      },
      "no_header": {
        "type": "button",
        "label": "ヘッダなし",
        "show_if": "$input_table.numOfHeader < 1"
      },
      "no_label": {
        "type": "button",
        "label": "ラベルなし,
        "show_if": "$input_table.numOfLabel < 1"
      }
    }
  }
}