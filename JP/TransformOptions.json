{
  "options": {
    "type": "fieldset",
    "ラベル": "オプションの選択",
    "repeat": false,
    "fields": {
      "options.type": {
        "type": "radio",
        "val": "transform_group",
        "show_if": "false",
        "values": {
          "transform_group": "Transform Group"
        },
        "breakLine": true
      },
      "options.group_label_direction": {
        "type": "select",
        "ラベル": "方向転換",
        "required": true,
        "options": {
          "group_to_label": {
            "ラベル": "ラベルへのグループ"
          },
          "label_to_group": {
            "ラベル": "グループへのラベル"
          }
        },
        "val": "group_to_label"
      },
      "options.group_column": {
        "type": "select",
        "ラベル": "列のグループ",
        "show_if": "options.group_label_direction=='group_to_label'",
        "table": "input_table",
        "bind": "COLUMNS",
        "required": true
      },
      "options.group_column_name": {
        "type": "text",
        "ラベル": "列名のグループ",
        "show_if": "options.group_label_direction=='label_to_group'",
        "val": "Group",
        "required": true
      }
    }
  }
}