{
  "output.output_table_types": {
    "type": "select", 
    "label": "出力テーブルタイプ(複数可)", 
    "required": true, 
    "options": {
      "unique": {
        "label": "ユニークテーブル"
      }, 
      "duplicates": {
        "label": "重複テーブル"
      }
    }, 
    "multiple": true, 
    "val": ["unique"]
  }, 
  "output.standard_ending": {
    "type": "checkbox", 
    "label": "標準ファイル拡張子", 
    "val": "true", 
    "isOn": "true", 
    "isOff": "false", 
    "breakLine": true
  }, 
  "output.unique_ending": {
    "show_if": "output.standard_ending == 'false'", 
    "type": "text", 
    "label": "ユニーク出力ファイル拡張子", 
    "val": "_u", 
    "style": "width: 160px; margin-left: 10px"
  }, 
  "output.duplicates_ending": {
    "show_if": "output.standard_ending == 'false'", 
    "type": "text", 
    "label": "重複出力のファイル拡張子", 
    "val": "_d", 
    "style": "width: 160px; margin-left: 10px", 
    "breakLine": true
  }
}