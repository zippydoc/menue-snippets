{
  "output.standard_ending": {
    "type": "checkbox",
    "label": "Standard Ending",
    "val": "true",
    "isOn": "true",
    "isOff": "false",
    "breakLine": true
  },
  "output.inner_ending": {
    "show_if": "output.standard_ending == 'false'",
    "type": "text",
    "required": true,
    "label": "Inner Ending",
    "val": "_j",
    "style": "margin-left: 10px"
  },
  "output.left_ending": {
    "show_if": "output.standard_ending == 'false'",
    "type": "text",
    "required": true,
    "label": "Left Ending",
    "val": "_l",
    "style": "margin-left: 10px"
  },
  "output.right_ending": {
    "show_if": "output.standard_ending == 'false'",
    "type": "text",
    "required": true,
    "label": "Right Ending",
    "val": "_r",
    "style": "margin-left: 10px",
    "breakLine": true
  },
  "output.output_table_types": {
    "type": "select",
    "label": "Output Table Type(s)",
    "required": true,
    "options": {
      "inner": {
        "label": "Inner Join"
      },
      "left": {
        "label": "Left Join"
      },
      "right": {
        "label": "Right Join"
      }
    },
    "multiple": true,
    "val": "inner"
  }
}