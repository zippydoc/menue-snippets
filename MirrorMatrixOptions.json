{
  "options": {
    "type": "fieldset",
    "label": "Select Options",
    "show_if": "$input_table.numOfHeader >= 1",
    "repeat": false,
    "fields": {
      "transpose_type": {
        "type": "hidden",
        "val": "mirror_matrix"
      },
      "mirror_matrix.convert_header_to_label": {
        "type": "checkbox",
        "label": "Convert header to label",
        "show_if": "transpose_type=='mirror_matrix'",
        "val": "true",
        "isOn": "true",
        "isOff": "false",
        "breakLine": true
      }
    }
  }
}