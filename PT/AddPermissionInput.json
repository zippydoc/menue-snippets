{
  "input": {
    "type": "fieldset",
    "etiqueta": "Selecionar Tabela de Entrada",
    "repeat": false,
    "fields": {
      "input.table_or_flow": {
        "type": "select",
        "etiqueta": "Tipo",
        "required": true,
        "options": {
          "set_table_permissions": {
            "etiqueta": "Definir permissões de mesa"
          },
          "set_flow_permissions": {
            "etiqueta": "Definir permissões de fluxo"
          }
        },
        "val": "set_table_permissions"
      },
      "input.table_id": {
        "type": "select",
        "etiqueta": "Selecionar Tabela",
        "show_if": "input.table_or_flow=='set_table_permissions'",
        "bind": "TABLES",
        "empty": "",
        "required": true
      }
    }
  }
}