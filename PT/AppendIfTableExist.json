{
  "output.if_table_exist.type": {
    "type": "select",
    "label": "Se existir",
    "val": "drop_if_exist",
    "options": {
      "drop_if_exist": {
        "label": "Criar ou Descartar (se já existir)"
      },
      "error_if_exist": {
        "label": "Criar Novo (gera erro se existir)"
      },
      "error_if_not_exist": {
        "label": "Descartar (gera erro se não existir)"
      },
      "append_all": {
        "label": "Acrescentar tudo"
      },
      "append_not_existing": {
        "label": "Anexar não existente"
      }
    }
  },
  "output.if_table_exist.append_not_existing_cols": {
    "type": "select",
    "label": "Selecionar Coluna(s)",
    "show_if": "output.if_table_exist.type=='append_not_existing'",
    "table": "target_table.table_id",
    "bind": "COLUMNS",
    "multiple": true,
    "required": true,
    "breakLine": true
  },
  "append_not_existing_info1": {
    "type": "text",
    "label": "",
    "show_if": "output.if_table_exist.type=='append_not_existing'",
    "val": "Appends rows only if the value of the selected column is new in the existing table.",
    "disabled": true,
    "style": "width: 100%"
  },
  "append_not_existing_info2": {
    "type": "text",
    "label": "",
    "show_if": "output.if_table_exist.type=='append_not_existing'",
    "val": "It is assumed that the selected column is found in the output and existing table",
    "disabled": true,
    "style": "width: 100%"
  }
}