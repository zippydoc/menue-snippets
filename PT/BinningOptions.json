{
  "options": {
    "type": "fieldset",
    "label": "Selecionar Opções de Discretização",
    "repeat": false,
    "fields": {
      "number_of_tiles": {
        "type": "number",
        "label": "Número de Grupos",
        "val": "2",
        "required": true,
        "breakLine": true
      },
      "binning_type": {
        "type": "select",
        "label": "Tipo de Discretização",
        "options": {
          "equal_records": {
            "label": "Discretização Simples"
          },
          "equal_intervals": {
            "label": "Igualar Intervalo de Discretização"
          }
        },
        "val": "equal_records"
      },
      "equal_records_order": {
        "type": "select",
        "label": "Encomendar",
        "show_if": "binning_type=='equal_records'",
        "options": {
          "keep_current": {
            "label": "Manter Atual"
          },
          "asc": {
            "label": "Ascendente"
          },
          "desc": {
            "label": "Descendente"
          }
        },
        "val": "keep_current"
      },
      "equal_intervals_order": {
        "type": "select",
        "label": "Encomendar",
        "show_if": "binning_type=='equal_intervals'",
        "options": {
          "asc": {
            "label": "Ascendente"
          },
          "desc": {
            "label": "Descendente"
          }
        },
        "val": "asc",
        "breakLine": true
      }
    },
    "breakLine": true
  }
}