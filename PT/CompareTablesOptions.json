{
  "options": {
    "type": "fieldset",
    "label": "Opções",
    "repeat": false,
    "fields": {
      "compare_set": {
        "type": "fieldset",
        "label": "Selecionar Padrões",
        "repeat": true,
        "fields": {
          "pattern": {
            "type": "select",
            "label": "Tipo de Padrão de Tabela",
            "options": {
              "all_col": {
                "label": "Todas as colunas idênticas"
              },
              "two_col": {
                "label": "Duas colunas idênticas"
              }
            },
            "val": "all_col",
            "required": true
          },
          "compare_type": {
            "type": "select",
            "label": "Tipo de Padrão de Célula",
            "options": {
              "equal": {
                "label": "Igual"
              },
              "contain": {
                "label": "Contém"
              },
              "start_with": {
                "label": "Inicia com"
              },
              "end_with": {
                "label": "Termina Com"
              },
              "reg_exp": {
                "label": "Expressão Regular"
              }
            },
            "val": "equal",
            "required": true
          },
          "case_sensitive": {
            "type": "select",
            "label": "Sensível a Maiúsculas e Minúsculas",
            "required": true,
            "options": {
              "yes": {
                "label": "Sensível a Maiúsculas e Minúsculas"
              },
              "no": {
                "label": "Insensível a Maiúsculas e Minúsculas"
              }
            },
            "val": "no"
          },
          "expression": {
            "type": "text",
            "label": "Expressão",
            "show_if": "this.compare_type != 'equal'",
            "required": true,
            "val": ""
          },
          "delete": {
            "show_if": "this.field_name != ''",
            "type": "button",
            "label": "Apagar",
            "function": "DELETE_ROW"
          }
        },
        "breakLine": true
      },
      "target": {
        "type": "select",
        "label": "Objetivo",
        "options": {
          "to_table": {
            "label": "Aplicar à tabela"
          },
          "to_rows": {
            "label": "Linha a Linha"
          }
        },
        "val": "to_table",
        "required": true
      },
      "match_type": {
        "type": "select",
        "show_if": "target != 'to_table'",
        "label": "Tipo de Correspondência",
        "options": {
          "match": {
            "label": "Apenas Correspondências"
          },
          "nonmatch": {
            "label": "Apenas Não Correspondências"
          },
          "all": {
            "label": "Todos"
          }
        },
        "val": "match",
        "required": true,
        "breakLine": true
      },
      "create_sql": {
        "type": "checkbox",
        "label": "Mostrar Detalhes de SQL",
        "val": "false",
        "isOn": "true",
        "isOff": "false"
      }
    }
  }
}