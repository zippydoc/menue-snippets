{
  "input": {
    "type": "fieldset",
    "label": "Opções de Consolidação",
    "repeat": true,
    "fields": {
      "new_position": {
        "type": "text",
        "label": "Nova Posição",
        "val": "",
        "placeholder": "Text",
        "required": true,
        "breakLine": true
      },
      "table_id": {
        "type": "select",
        "label": "Selecionar Tabela",
        "bind": "TABLES",
        "empty": "",
        "required": true
      },
      "rows": {
        "type": "select",
        "label": "Selecção de filas",
        "required": true,
        "table": "this.table_id",
        "multiple": true,
        "bind": "LABELS"
      },
      "multiplier": {
        "type": "select",
        "label": "Multiplicador",
        "required": true,
        "options": {
          "positive": {
            "label": "Guardar sinal"
          },
          "negative": {
            "label": "Sinal de troca"
          }
        },
        "val": "positive",
        "breakLine": true
      },
      "format__category": {
        "type": "select",
        "label": "Formato de exibição",
        "required": true,
        "options": {
          "standard": {
            "label": "Padrão"
          },
          "number": {
            "label": "Número"
          },
          "percentage": {
            "label": "Percentagem"
          },
          "text": {
            "label": "Texto"
          },
          "date": {
            "label": "Data"
          },
          "currency": {
            "label": "Moeda"
          }
        },
        "val": "standard"
      },
      "format__rounding": {
        "type": "select",
        "label": "Lugares decimais para exibição",
        "show_if": "this.format__category=='number' || this.format__category=='percentage' || this.format__category=='currency'",
        "required": true,
        "options": {
          "no_rounding": {
            "label": "Arredondamento de números"
          },
          "0": {
            "label": "0 dígitos"
          },
          "1": {
            "label": "1 dígito"
          },
          "2": {
            "label": "2 dígitos"
          },
          "3": {
            "label": "3 dígitos"
          },
          "4": {
            "label": "4 dígitos"
          },
          "5": {
            "label": "5 dígitos"
          },
          "6": {
            "label": "6 dígitos"
          },
          "7": {
            "label": "7 dígitos"
          },
          "8": {
            "label": "8 dígitos"
          },
          "9": {
            "label": "9 dígitos"
          }
        },
        "val": "2"
      },
      "format__use_1000_separator": {
        "type": "checkbox",
        "label": "Usar o Separador 1000(.)",
        "show_if": "this.format__category=='number' || this.format__category=='percentage' || this.format__category=='currency'",
        "val": "true",
        "isOn": "true",
        "isOff": "false"
      },
      "format__currency_symbol": {
        "type": "select",
        "label": "Símbolo",
        "show_if": "this.format__category=='currency'",
        "required": true,
        "options": {
          "dollar": {
            "label": "$"
          },
          "euro": {
            "label": "€"
          },
          "pound": {
            "label": "£"
          }
        },
        "val": "dollar"
      },
      "format__date_format": {
        "type": "select",
        "label": "Tipo",
        "show_if": "this.format__category=='date'",
        "required": true,
        "options": {
          "mm/dd/yyyy": {
            "label": "3/14/2012"
          },
          "D, M dd, Y": {
            "label": "Quarta-feira, 14 de Março de 2012"
          },
          "yyyy-mm-dd": {
            "label": "2012-03-14"
          },
          "m.dd.yyyy": {
            "label": "3.14.2012"
          },
          "m.dd.yy": {
            "label": "3.14.12"
          },
          "dd.m.yyyy": {
            "label": "14.3.2012"
          },
          "dd.m.yy": {
            "label": "14.3.12"
          },
          "M dd, Y": {
            "label": "14 de Março de 2012"
          },
          "dd-mm-yyyy": {
            "label": "14-03-2012"
          },
          "mm-dd-yyyy": {
            "label": "03-14-2012"
          },
          "dd-mm-yy": {
            "label": "14-03-12"
          },
          "mm-dd-yy": {
            "label": "03-14-12"
          }
        },
        "val": "mm/dd/yyyy",
        "breakLine": true
      },
      "delete": {
        "type": "button",
        "label": "Apagar",
        "function": "DELETE_ROW"
      }
    }
  }
}