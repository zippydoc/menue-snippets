{
  "input": {
    "type": "fieldset",
    "etiqueta": "Consolidate Options",
    "repeat": true,
    "fields": {
      "new_position": {
        "type": "text",
        "etiqueta": "New Position",
        "val": "",
        "placeholder": "Texto",
        "required": true
      },
      "input_table": {
        "type": "select",
        "etiqueta": "Selecionar Tabela",
        "bind": "TABLES",
        "empty": "",
        "required": true,
        "breakLine": true
      },
      "multiple_selection": {
        "type": "select",
        "etiqueta": "Multiple Selection",
        "required": true,
        "table": "this.input_table",
        "multiple": true,
        "bind": "LABELS",
        "breakLine": true
      },
      "format__category": {
        "type": "select",
        "etiqueta": "Formato",
        "required": true,
        "options": {
          "standard": {
            "etiqueta": "Padrão"
          },
          "number": {
            "etiqueta": "Número"
          },
          "percentage": {
            "etiqueta": "Percentagem"
          },
          "text": {
            "etiqueta": "Texto"
          },
          "date": {
            "etiqueta": "Data"
          },
          "currency": {
            "etiqueta": "Moeda"
          }
        },
        "val": "standard"
      },
      "format__rounding": {
        "type": "select",
        "etiqueta": "Arredondamento",
        "show_if": "this.format__category=='number' || this.format__category=='percentage' || this.format__category=='currency'",
        "required": true,
        "options": {
          "no_rounding": {
            "etiqueta": "Arredondamento de números"
          },
          "0": {
            "etiqueta": "0 dígitos"
          },
          "1": {
            "etiqueta": "1 dígito"
          },
          "2": {
            "etiqueta": "2 dígitos"
          },
          "3": {
            "etiqueta": "3 dígitos"
          },
          "4": {
            "etiqueta": "4 dígitos"
          },
          "5": {
            "etiqueta": "5 dígitos"
          },
          "6": {
            "etiqueta": "6 dígitos"
          },
          "7": {
            "etiqueta": "7 dígitos"
          },
          "8": {
            "etiqueta": "8 dígitos"
          },
          "9": {
            "etiqueta": "9 dígitos"
          }
        },
        "val": "2"
      },
      "format__use_1000_separator": {
        "type": "checkbox",
        "etiqueta": "Use 1000 Separator(.)",
        "show_if": "this.format__category=='number' || this.format__category=='percentage' || this.format__category=='currency'",
        "val": "true",
        "isOn": "true",
        "isOff": "false"
      },
      "format__currency_symbol": {
        "type": "select",
        "etiqueta": "Símbolo",
        "show_if": "this.format__category=='currency'",
        "required": true,
        "options": {
          "dollar": {
            "etiqueta": "$"
          },
          "euro": {
            "etiqueta": "€"
          },
          "pound": {
            "etiqueta": "£"
          }
        },
        "val": "dollar"
      },
      "format__date_format": {
        "type": "select",
        "etiqueta": "Tipo",
        "show_if": "this.format__category=='date'",
        "required": true,
        "options": {
          "mm/dd/yyyy": {
            "etiqueta": "3/14/2012"
          },
          "D, M dd, Y": {
            "etiqueta": "Quarta-feira, 14 de Março de 2012"
          },
          "yyyy-mm-dd": {
            "etiqueta": "2012-03-14"
          }
        },
        "val": "mm/dd/yyyy",
        "breakLine": true
      }
    }
  }
}