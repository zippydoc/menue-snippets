{
  "options_frame": {
    "type": "fieldset",
    "label": "Opções",
    "repeat": false,
    "fields": {
      "options.group_by": {
        "type": "select",
        "label": "Agrupar por",
        "required": true,
        "options": {
          "Region": {
            "label": "Região"
          },
          "Income Group": {
            "label": "Grupo de rendimento"
          }
        },
        "val": "Region",
        "breakLine": true
      },
      "options.stat_var_name": {
        "type": "select",
        "label": "Dados Estatísticos",
        "required": true,
        "options": {
          "UnemploymentRate_Person": {
            "label": "Taxa de Desemprego"
          },
          "Count_Person_InLaborForce": {
            "label": "Em População Trabalhista"
          },
          "Count_Person_15To64Years_InLaborForce_AsFractionOf_Count_Person_15To64Years": {
            "label": "15 - 64 Anos, em Força de Trabalho População Per Capita 15 a 64 Anos"
          },
          "Count_Person_15To64Years_Female_InLaborForce_AsFractionOf_Count_Person_15To64Years_Female": {
            "label": "15 - 64 Anos, em Força de Trabalho, População Feminina Per Capita 15 a 64 Anos Feminina"
          },
          "Count_Person_15To64Years_Male_InLaborForce_AsFractionOf_Count_Person_15To64Years_Male": {
            "label": "15 - 64 Anos, em Força de Trabalho, População Masculina Per Capita 15 a 64 Anos Masculina"
          },
          "Count_Person_15OrMoreYears_InLaborForce_Female_AsFractionOf_Count_Person_InLaborForce": {
            "label": "15 Anos ou Mais, em Força de Trabalho, População Feminina Per Capita em Força de Trabalho"
          }
        },
        "val": "UnemploymentRate_Person"
      },
      "options.per_capita": {
        "type": "checkbox",
        "label": "Per capita",
        "show_if": "false",
        "val": "false",
        "isOn": "true",
        "isOff": "false",
        "breakLine": true
      },
      "options.all_countries": {
        "type": "checkbox",
        "label": "Todos os países",
        "val": "true",
        "isOn": "true",
        "isOff": "false",
        "breakLine": true
      },
      "options.output_as_chart": {
        "type": "checkbox",
        "label": "Produzir um gráfico",
        "val": "true",
        "isOn": "true",
        "isOff": "false"
      },
      "options.graph_type": {
        "type": "select",
        "label": "Tipo de gráfico",
        "show_if": "options.output_as_chart=='true'",
        "required": true,
        "options": {
          "zoomable_sunburst": {
            "label": "Sunburst"
          },
          "treemap": {
            "label": "Treemap"
          }
        },
        "val": "zoomable_sunburst",
        "breakLine": true
      }
    }
  }
}