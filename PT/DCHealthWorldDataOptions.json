{
  "options_frame": {
    "type": "fieldset",
    "label": "Opções",
    "repeat": false,
    "fields": {
      "options.group_by": {
        "type": "select",
        "label": "Agrupar por",
        "required": true,
        "options": {
          "Region": {
            "label": "Região"
          },
          "Income Group": {
            "label": "Grupo de rendimento"
          }
        },
        "val": "Region",
        "breakLine": true
      },
      "options.stat_var_name": {
        "type": "select",
        "label": "Dados Estatísticos",
        "required": true,
        "options": {
          "FertilityRate_Person_Female": {
            "label": "Taxa de fertilidade"
          },
          "LifeExpectancy_Person": {
            "label": "Expectativa de vida"
          },
          "Count_BirthEvent_LiveBirth_AsFractionOf_Count_Person": {
            "label": "Número de nascimentos vivos ocorridos durante o ano, Per Capita"
          },
          "Count_Person_20To79Years_Diabetes_AsFractionOf_Count_Person_20To79Years": {
            "label": "Percentagem de pessoas de 20-79 anos que têm diabetes tipo 1 ou tipo 2"
          },
          "Count_Death": {
            "label": "Evento de Contagem da Mortalidade"
          },
          "Count_Person_15OrMoreYears_Smoking_AsFractionOf_Count_Person_15OrMoreYears": {
            "label": "15 Anos ou Mais, População Fumadora Per Capita 15 ou Mais Anos"
          },
          "Count_Person_Upto4Years_Overweight_AsFractionOf_Count_Person_Upto4Years": {
            "label": "4 Anos ou Menos, Sobrepeso da População Per Capita até 4 Anos"
          }
        },
        "val": "FertilityRate_Person_Female"
      },
      "options.per_capita": {
        "type": "checkbox",
        "label": "Per capita",
        "show_if": "false",
        "val": "false",
        "isOn": "true",
        "isOff": "false",
        "breakLine": true
      },
      "options.all_countries": {
        "type": "checkbox",
        "label": "Todos os países",
        "val": "true",
        "isOn": "true",
        "isOff": "false",
        "breakLine": true
      },
      "options.output_as_chart": {
        "type": "checkbox",
        "label": "Produzir um gráfico",
        "val": "true",
        "isOn": "true",
        "isOff": "false"
      },
      "options.graph_type": {
        "type": "select",
        "label": "Tipo de gráfico",
        "show_if": "options.output_as_chart=='true'",
        "required": true,
        "options": {
          "zoomable_sunburst": {
            "label": "Sunburst"
          },
          "treemap": {
            "label": "Treemap"
          }
        },
        "val": "zoomable_sunburst",
        "breakLine": true
      }
    }
  }
}