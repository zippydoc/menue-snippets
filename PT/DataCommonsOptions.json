{
  "options_frame": {
    "type": "fieldset",
    "label": "Opções",
    "repeat": false,
    "fields": {
      "options.category": {
        "type": "select",
        "label": "Categoria de dados",
        "required": true,
        "options": {
          "health": {
            "label": "Saúde"
          },
          "demographics": {
            "label": "Demográficos"
          },
          "economic": {
            "label": "Económica"
          }
        },
        "val": "health"
      },
      "options.health__stat_var": {
        "type": "select",
        "label": "Dados Estatísticos",
        "show_if": "options.category=='health'",
        "required": true,
        "options": {
          "CumulativeCount_Vaccine_COVID_19_Administered": {
            "label": "Contagem acumulada de COVID_19, Vaccine Administed"
          },
          "IncrementalCount_Vaccine_COVID_19_Administered": {
            "label": "Contagem Incremental de COVID_19, Vacina Administrada"
          },
          "Count_BirthEvent_LiveBirth_AsFractionOf_Count_Person": {
            "label": "Contar_BirthEvent_LiveBirth_AsFractionOf_Count_Person"
          }
        },
        "val": "CumulativeCount_Vaccine_COVID_19_Administered"
      },
      "options.demographics__stat_var": {
        "type": "select",
        "label": "Dados Estatísticos",
        "show_if": "options.category=='demographics'",
        "required": true,
        "options": {
          "Count_Person": {
            "label": "População"
          },
          "GrowthRate_Count_Person": {
            "label": "Taxa de Crescimento Populacional"
          },
          "Count_Person_PerArea": {
            "label": "Densidade Populacional"
          }
        },
        "val": "Count_Person"
      },
      "options.economic__stat_var": {
        "type": "select",
        "label": "Dados Estatísticos",
        "show_if": "options.category=='economic'",
        "required": true,
        "options": {
          "Amount_EconomicActivity_GrossDomesticProduction_Nominal": {
