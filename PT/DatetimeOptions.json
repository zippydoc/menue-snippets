{
  "options": {
    "type": "fieldset",
    "label": "Opções de Seleção",
    "repeat": false,
    "fields": {
      "options.convert_type": {
        "type": "select",
        "label": "Tipo de Conversão",
        "required": true,
        "options": {
          "complete_date": {
            "label": "Completar data"
          },
          "expand_date": {
            "label": "Expandir data"
          },
          "convert_date": {
            "label": "Converter data"
          },
          "convert_date_range": {
            "label": "Converter intervalo de datas"
          }
        },
        "val": "complete_date"
      },
      "options.date_convert_type": {
        "type": "select",
        "label": "Converta para",
        "show_if": "options.convert_type=='convert_date'",
        "required": true,
        "options": {
          "begin_of_month": {
            "label": "Início de Mês"
          },
          "end_of_month": {
            "label": "Fim de Mês"
          },
          "hy_year": {
            "label": "Semestre"
          },
          "qrt_year": {
            "label": "Trimestre Ano"
          },
          "month_year": {
            "label": "Mês Ano"
          },
          "week_year": {
            "label": "Semana Ano"
          }
        },
        "val": "begin_of_month"
      },
      "options.date_range_convert_type": {
        "type": "select",
        "label": "Converta para",
        "show_if": "options.convert_type=='convert_date_range'",
        "required": true,
        "options": {
          "begin_of_month": {
            "label": "Início de Mês"
          },
          "end_of_month": {
            "label": "Fim de Mês"
          },
          "begin_of_range": {
            "label": "Início de lntervalo"
          },
          "end_of_range": {
            "label": "Fim de Intervalo"
          },
          "begin_of_quarter": {
            "label": "Início de Trimestre"
          },
          "end_of_quarter": {
            "label": "Fim de Trimestre"
          }
        },
        "val": "begin_of_month",
        "breakLine": true
      },
      "options.output_in_new_column": {
        "type": "checkbox",
        "label": "Mostrar em Nova Coluna",
        "val": "false",
        "isOn": "true",
        "isOff": "false"
      },
      "options.new_field_name": {
        "type": "text",
        "label": "Novo nome de coluna",
        "show_if": "options.output_in_new_column=='true'",
        "val": "GeneratedDate",
        "required": true,
        "minLength": 1,
        "maxLength": 255,
        "breakLine": true
      }
    }
  }
}