{
  "placeholder": {
    "breakLine": true
  },
  "delete_options": {
    "type": "select",
    "etiqueta": "Eliminar opções",
    "required": true,
    "options": {
      "del_table": {
        "etiqueta": "Apagar tabela"
      },
      "del_columns": {
        "etiqueta": "Eliminar Colunas Seleccionadas"
      },
      "del_rows": {
        "etiqueta": "Eliminar Linhas Seleccionadas"
      }
    },
    "val": "del_table"
  },
  "apply_columns": {
    "type": "select",
    "etiqueta": "Seleccione as colunas a eliminar",
    "show_if": "delete_options=='del_columns'",
    "required": true,
    "table": "input_table",
    "multiple": true,
    "bind": "COLUMNS"
  },
  "apply_rows": {
    "type": "select",
    "etiqueta": "Seleccionar Linhas a Apagar",
    "show_if": "delete_options=='del_rows'",
    "required": true,
    "table": "input_table",
    "multiple": true,
    "bind": "LABELS"
  }
}