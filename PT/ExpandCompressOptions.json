{
  "options": {
    "type": "fieldset",
    "label": "Select Options",
    "repeat": false,
    "fields": {
      "options.type": {
        "type": "radio",
        "val": "expand",
        "values": {
          "expand": "Expand",
          "compress": "Compress"
        },
        "breakLine": true
      },
      "options.column_to_expand": {
        "type": "select",
        "label": "Column to expand",
        "show_if": "options.type==''expand''",
        "table": "input_table",
        "bind": "COLUMNS",
        "required": true
      },
      "options.column_to_group": {
        "type": "select",
        "label": "Column to group",
        "show_if": "options.type==''compress''",
        "table": "input_table",
        "bind": "COLUMNS",
        "required": true
      },
      "options.column_to_compress": {
        "type": "select",
        "label": "Column to compress",
        "show_if": "options.type==''compress''",
        "table": "input_table",
        "bind": "COLUMNS",
        "required": true
      },
      "options.delimiter": {
        "type": "text",
        "label": "Delimiter",
        "val": ";",
        "trim": "false",
        "required": true
      },
      "options.new_col_name": {
        "type": "text",
        "label": "New Column Name",
        "show_if": "options.type==''expand'' && options.expand__output_in_new_column==''true''",
        "val": "label",
        "required": true,
        "breakLine": true
      },
      "options.expand__output_in_new_column": {
        "type": "checkbox",
        "label": "Output in new column",
        "show_if": "options.type==''expand''",
        "val": "false",
        "isOn": "true",
        "isOff": "false",
        "breakLine": true
      },
      "options.create_new_table": {
        "type": "checkbox",
        "label": "Output as new table",
        "val": "true",
        "isOn": "true",
        "isOff": "false",
        "breakLine": true
      }
    }
  }
}