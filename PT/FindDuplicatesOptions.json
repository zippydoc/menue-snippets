        "repeat": false,
        "fields": {
          "options.input_table.filter": {
            "type": "fieldset",
            "etiqueta": "Adicionar Condições de Filtro para Tabela de Entrada",
            "repeat": true,
            "fields": {
              "initial_op": {
                "type": "select",
                "etiqueta": "Selecionar Operador Lógico",
                "required": true,
                "show_if": "$index == 0",
                "options": {
                  "if": {
                    "etiqueta": "IF"
                  },
                  "if_not": {
                    "etiqueta": "IF NOT"
                  }
                },
                "val": "if"
              },
              "op": {
                "type": "select",
                "etiqueta": "Selecionar Operador Lógico",
                "required": true,
                "show_if": "$index > 0",
                "options": {
                  "and": {
                    "etiqueta": "AND"
                  },
                  "or": {
                    "etiqueta": "OR"
                  },
                  "and_not": {
                    "etiqueta": "AND NOT"
                  },
                  "or_not": {
                    "etiqueta": "OR NOT"
                  }
                },
                "val": "and"
              },
              "column": {
                "type": "select",
                "etiqueta": "Selecionar Coluna",
                "table": "input_table",
                "bind": "COLUMNS",
                "required": true
              },
              "condition": {
                "type": "select",
                "etiqueta": "Selecionar Operador",
                "required": true,
                "options": {
                  "CONTÉM": {
                    "etiqueta": "CONTÉM"
                  },
                  "CONTAINED IN STRING": {
                    "etiqueta": "CONTIDO EM"
                  },
                  "INICIA COM": {
                    "etiqueta": "INICIA COM"
                  },
                  "TERMINA COM": {
                    "etiqueta": "TERMINA COM"
                  },
                  "=": {
                    "etiqueta": "IGUAL"
                  },
                  ">": {
                    "etiqueta": "MAIOR QUE"
                  },
                  ">=": {
                    "etiqueta": "MAIOR OU IGUAL"
                  },
                  "<": {
                    "etiqueta": "MENOR QUE"
                  },
                  "<=": {
                    "etiqueta": "MENOR OU IGUAL"
                  },
                  "EMPTY": {
                    "etiqueta": "ESTÁ VAZIO"
                  },
                  "BLANK": {
                    "etiqueta": "ESTÁ EM BRANCO"
                  },
                  "IS NULL": {
                    "etiqueta": "IS NULL"
                  },
                  "CORRESPONDÊNCIA COM EXPRESSÃO REGULAR": {
                    "etiqueta": "CORRESPONDÊNCIA COM EXPRESSÃO REGULAR"
                  },
                  "CONTAINED IN TABLE": {
                    "etiqueta": "NA TABELA"
                  },
                  "NA TABELA DE EXPRESSÕES REGULARES": {
                    "etiqueta": "NA TABELA DE EXPRESSÕES REGULARES"
                  },
                  "DATA": {
                    "etiqueta": "DATA"
                  }
                },
                "val": "CONTÉM"
              },
              "date_condition": {
                "type": "select",
                "etiqueta": "Selecionar Operador de Data",
                "required": true,
                "show_if": "this.condition == 'DATE'",
                "options": {
                  "<": {
                    "etiqueta": "ANTERIOR"
                  },
                  "<=": {
                    "etiqueta": "ANTERIOR OU IGUAL"
                  },
                  "=": {
                    "etiqueta": "IGUAL"
                  },
                  ">": {
                    "etiqueta": "DEPOIS"
                  },
                  ">=": {
                    "etiqueta": "DEPOIS OU IGUAL"
                  }
                },
                "val": "="
              },
              "case_sensitive": {
                "type": "select",
                "etiqueta": "Selecionar Sensibilidade a Maiúsculas e Minúsculasl",
                "required": true,
                "show_if": "this.condition == 'CONTAINS' || this.condition == 'CONTAINED IN STRING' || this.condition == '=' || this.condition == 'STARTS WITH' || this.condition == 'ENDS WITH' || this.condition == 'REGEX MATCH' || this.condition == 'NOT REGEX MATCH' || this.condition == 'IN REGEX TABLE' || this.condition == 'NOT IN REGEX TABLE'",
                "options": {
                  "yes": {
                    "etiqueta": "Sensível a Maiúsculas e Minúsculas"
                  },
                  "no": {
                    "etiqueta": "Insensível a Maiúsculas e Minúsculas"
                  }
                },
                "val": "yes"
              },
              "date_value": {
                "type": "date",
                "etiqueta": "Selecionar Data",
                "show_if": "this.condition=='DATE'",
                "val": "",
                "required": true
              },
              "value": {
                "type": "text",
                "etiqueta": "Introduzir Valor",
                "show_if": "this.condition != 'DATE' && this.condition != 'EMPTY' && this.condition != 'NOT EMPTY' && this.condition != 'BLANK' && this.condition != 'NOT BLANK' && this.condition != 'IS NULL' && this.condition != 'IS NOT NULL' && this.condition != 'CONTAINED IN TABLE' && this.condition != 'NOT CONTAINED IN TABLE' && this.condition != 'IN REGEX TABLE' && this.condition != 'NOT IN REGEX TABLE'",
                "val": "",
                "required": true
              },
              "compare_table": {
                "type": "select",
                "etiqueta": "Selecionar Tabela",
                "bind": "TABLES",
                "empty": "",
                "show_if": "this.condition == 'CONTAINED IN TABLE' || this.condition == 'NOT CONTAINED IN TABLE' || this.condition == 'IN REGEX TABLE' || this.condition == 'NOT IN REGEX TABLE'",
                "required": true
              },
              "compare_column": {
                "type": "select",
                "etiqueta": "Selecionar Coluna",
                "table": "this.compare_table",
                "bind": "COLUMNS",
                "show_if": "this.condition == 'CONTAINED IN TABLE' || this.condition == 'NOT CONTAINED IN TABLE' || this.condition == 'IN REGEX TABLE' || this.condition == 'NOT IN REGEX TABLE'",
                "required": true
              },
              "delete": {
                "type": "button",
                "etiqueta": "Apagar",
                "function": "DELETE_ROW"
              }
            }
