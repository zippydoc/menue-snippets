{
  "options": {
    "type": "fieldset",
    "etiqueta": "Opções de Seleção",
    "repeat": false,
    "fields": {
      "options.task": {
        "type": "hidden",
        "val": "find_duplicates"
      },
      "options.task_type": {
        "type": "hidden",
        "val": "label_duplicates"
      },
      "options.pattern_type": {
        "type": "select",
        "etiqueta": "Tipo de Padrão",
        "show_if": "options.task_type=='generate_lookup_table' || options.task_type=='apply_lookup_table' || options.task_type=='apply_lookup_table_to_cells'",
        "required": true,
        "options": {
          "duplicate_pattern": {
            "etiqueta": "Padrão de Palavras"
          },
          "number_range": {
            "etiqueta": "Intervalos (ex. 10 < x <= 100)"
          }
        },
        "val": "duplicate_pattern"
      },
      "duplicate_option_box": {
        "type": "fieldset",
        "etiqueta": "Opção de Duplicar Padrões",
        "show_if": "options.task_type=='label_duplicates' || (options.task_type=='generate_lookup_table' && options.pattern_type=='duplicate_pattern')",
        "repeat": false,
        "fields": {
          "options.minimum_number_of_matching_characters": {
            "type": "text",
            "etiqueta": "Número Mínimo de Caracteres Correspondentes",
            "val": "3",
            "required": true,
            "style": "min-width: 240px"
          },
          "options.minimum_number_of_duplicates": {
            "type": "text",
            "etiqueta": "Número Mínimo de Duplicados",
            "val": "2",
            "required": true,
            "style": "min-width: 240px",
            "breakLine": true
          }
        }
      },
      "number_range_option_box": {
        "type": "fieldset",
        "etiqueta": "Opção de Intervalos Numéricos",
        "show_if": "options.task_type=='generate_lookup_table' && options.pattern_type=='number_range'",
        "repeat": false,
        "fields": {
          "options.number_range.range_type": {
            "type": "select",
            "etiqueta": "Tipo de Intervalo",
            "required": true,
            "options": {
              "equal_interval": {
                "etiqueta": "N Intervalos Iguais"
              },
              "equal_range": {
                "etiqueta": "Igualar Intervalo"
              },
              "custom_range": {
                "etiqueta": "Personalizar Intervalo"
              }
            },
            "val": "equal_interval"
          },
          "options.number_range.n_value": {
            "type": "text",
            "etiqueta": "N",
            "show_if": "options.number_range.range_type=='equal_interval'",
            "val": "3",
            "required": true
          },
          "options.number_range.range_value": {
            "type": "text",
            "etiqueta": "Intervalo",
            "show_if": "options.number_range.range_type=='equal_range'",
            "val": "10",
            "required": true
          },
          "options.number_range.custom_ranges": {
            "type": "fieldset",
            "etiqueta": "Adicionar Intervalo Padrão Personalizado",
            "show_if": "options.number_range.range_type=='custom_range'",
            "repeat": true,
            "fields": {
              "pattern": {
                "type": "text",
                "etiqueta": "Padrão",
                "val": "0<=x<10",
                "required": true
              },
              "etiqueta": {
                "type": "text",
                "etiqueta": "Rotular",
                "val": "positive10",
                "required": true
              },
              "delete": {
                "type": "button",
                "etiqueta": "Apagar",
                "function": "DELETE_ROW"
              }
            }
          }
        },
        "breakLine": true
      },
      "options.lookup_table_id": {
        "type": "select",
        "etiqueta": "Tabela de Pesquisa",
        "show_if": "options.task_type=='apply_lookup_table' || options.task_type=='apply_regex_table' || options.task_type=='apply_lookup_table_to_cells' || options.task_type=='apply_regex_table_to_cells'",
        "bind": "TABLES",
        "empty": "",
        "required": true
      },
      "options.partial_match": {
        "type": "checkbox",
        "etiqueta": "Correspondência Parcial",
        "show_if": "options.pattern_type=='duplicate_pattern' && (options.task_type=='apply_lookup_table' || options.task_type=='apply_lookup_table_to_cells')",
        "val": "true",
        "isOn": "true",
        "isOff": "false"
      },
      "options.case_sensitive": {
        "type": "checkbox",
        "etiqueta": "Sensível a Maiúsculas e Minúsculas",
        "show_if": "options.pattern_type=='duplicate_pattern' && (options.task_type=='apply_lookup_table' || options.task_type=='apply_lookup_table_to_cells')",
        "val": "false",
        "isOn": "true",
        "isOff": "false",
        "breakLine": true
      },
      "options.use_delimiter": {
        "type": "checkbox",
        "etiqueta": "Personalizar Delimitador",
        "show_if": "options.task_type=='apply_lookup_table' || options.task_type=='apply_lookup_table_to_cells'",
        "val": "false",
        "isOn": "true",
        "isOff": "false"
      },
      "options.delimiter": {
        "type": "text",
        "etiqueta": "Introduzir Delimitador",
        "show_if": "options.use_delimiter=='true' && (options.task_type=='apply_lookup_table' || options.task_type=='apply_lookup_table_to_cells')",
        "val": ";",
        "required": true,
        "breakLine": true
      },
      "options.input_table.use_filter": {
        "type": "checkbox",
        "etiqueta": "Utilizar Filtro para Tabela de Entrada",
        "show_if": "options.task_type=='apply_lookup_table' || options.task_type=='apply_regex_table' || options.task_type=='apply_lookup_table_to_cells' || options.task_type=='apply_regex_table_to_cells'",
        "val": "false",
        "isOn": "true",
        "isOff": "false",
        "breakLine": true
      },
      "input_table_filter_box": {
        "type": "fieldset",
        "etiqueta": "Introduzir Opções de Filtro da Tabela",
