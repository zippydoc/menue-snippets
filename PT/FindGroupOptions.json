{
  "options": {
    "type": "fieldset",
    "label": "Encontrar opções de grupo",
    "repeat": false,
    "fields": {
      "group_column": {
        "type": "select",
        "label": "Select Group Column",
        "required": true,
        "table": "table_id",
        "bind": "COLUMNS",
        "multiple": false
      },
      "item_column": {
        "type": "select",
        "label": "Select Item Column",
        "required": true,
        "table": "table_id",
        "bind": "COLUMNS",
        "multiple": false,
        "breakLine": true
      },
      "min_group_count": {
        "type": "number",
        "label": "Contagem do grupo min",
        "required": true,
        "val": "3"
      },
      "min_group_size": {
        "type": "number",
        "label": "Tamanho mínimo do grupo",
        "required": true,
        "val": "1",
        "breakLine": true
      },
      "ordered": {
        "type": "select",
        "label": "Encomendado / Não encomendado",
        "required": true,
        "options": {
          "ordered": {
            "label": "Encomendado"
          },
          "unordered": {
            "label": "Sem ordem"
          }
        },
        "val": "unordered"
      },
      "item_order": {
        "type": "select",
        "label": "Encomendar",
        "required": true,
        "options": {
          "no": {
            "label": "-"
          },
          "ascending": {
            "label": "Ascendente"
          },
          "descending": {
            "label": "Descendente"
          }
        },
        "val": "no",
        "breakLine": true,
        "show_if": "ordered == 'unordered'"
      },
      "output_type": {
        "type": "select",
        "label": "Output Type (Tipo de saída)",
        "required": true,
        "options": {
          "item_group_list": {
            "label": "Lista de grupos encontrados"
          },
          "item_group_column": {
            "label": "Atribuir grupos encontrados"
          }
        },
        "val": "item_group_list"
      }
    }
  }
}