              },
              "Europe/Astrakhan": {
                "label": "Dubai, Baku, Tbilisi"
              },
              "Australia/Eucla": {
                "label": "Eucla"
              },
              "Pacific/Honolulu": {
                "label": "Honolulu, Rarotonga, Tahiti"
              },
              "Asia/Kabul": {
                "label": "Cabul, Kandahār, Mazār-e Sharīf, Herāt, Jalālābād"
              },
              "Asia/Aqtau": {
                "label": "Karachi, Tashkent, Maldivas"
