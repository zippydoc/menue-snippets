              },
              "Pacific/Kiritimati": {
                "label": "Kiritimati"
              },
              "Asia/Colombo": {
                "label": "Kolkata, Colombo, Jaffna, Negombo"
              },
              "Europe/Guernsey": {
                "label": "Londres. Dublin, Lisboa"
              },
              "Australia/Lord_Howe": {
                "label": "Lord_Howe"
              },
              "America/Creston": {
                "label": "Los_Angeles, Vancouver, Hermosillo"
              },
              "Pacific/Marquesas": {
                "label": "Marquesas,Taiohae"
