              },
              "Asia/Tehran": {
                "etiqueta": "Teerão, Mashhad, Isfahan, Karaj, Tabriz"
              },
              "Asia/Tokyo": {
                "etiqueta": "Tokyo, Seul, Jayapura"
              },
              "America/New_York": {
                "etiqueta": "Toronto, Santiago, New_York"
              },
              "Australia/Sydney": {
                "etiqueta": "Vladivostok, Sydney, Brisbane"
              },
              "Asia/Yangon": {
                "etiqueta": "Yangon, Cocos,Mandalay, Nay Pyi Taw, Mawlamyine"
              },
