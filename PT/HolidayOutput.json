              "Asia/Kathmandu": {
                "etiqueta": "Kathmandu,Pokhara, Pātan, Biratnagar"
              },
              "Pacific/Kiritimati": {
                "etiqueta": "Kiritimati"
              },
              "Asia/Colombo": {
                "etiqueta": "Kolkata, Colombo,Jaffna, Negombo"
              },
              "Europe/Guernsey": {
                "etiqueta": "London. Dublin, Lisbon"
              },
              "Australia/Lord_Howe": {
                "etiqueta": "Lord_Howe"
              },
              "America/Creston": {
                "etiqueta": "Los_Angeles, Vancouver, Hermosillo"
              },
              "Pacific/Marquesas": {
                "etiqueta": "Marquesas,Taiohae"
              },
              "America/Cayman": {
                "etiqueta": "Mexico_City, Panama, Chicago"
              },
              "Pacific/Niue": {
                "etiqueta": "Midway, Niue, Pago_Pago"
              },
              "Europe/Mariehamn": {
                "etiqueta": "Moscovo, Istambul, Qatar"
              },
              "Asia/Kolkata": {
