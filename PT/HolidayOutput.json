              "Pacific/Honolulu": {
                "etiqueta": "Honolulu, Rarotonga, Tahiti"
              },
              "Asia/Kabul": {
                "etiqueta": "Cabul, Kandahār, Mazār-e Sharīf, Herāt, Jalālābād"
              },
              "Asia/Aqtau": {
