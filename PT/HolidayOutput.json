{
  "output": {
    "type": "fieldset",
    "etiqueta": "Definir Tabela de Saída",
    "repeat": false,
    "fields": {
      "output.custom_text": {
        "type": "text",
        "etiqueta": "Mostrar Nome da Tabela",
        "show_if": "output.custom_options=='false' || (output.custom_options=='true' && output.compose_table_name=='false')",
        "minLength": 1,
        "val": "",
        "width": "100%",
        "breakLine": true
      },
      "output.custom_options": {
        "type": "checkbox",
        "etiqueta": "Mostrar Opções",
        "val": "false",
        "isOn": "true",
        "isOff": "false",
        "breakLine": true
      },
      "output.select_output_columns": {
        "type": "checkbox",
        "etiqueta": "Seleccionar colunas de saída",
        "show_if": "options.is_generate_holiday_table=='add_column' && output.custom_options=='true'",
        "val": "false",
        "isOn": "true",
        "isOff": "false"
      },
      "output_columns": {
        "type": "fieldset",
        "etiqueta": "Select OutputColumns",
        "show_if": "options.is_generate_holiday_table=='add_column' && output.custom_options=='true' && output.select_output_columns=='true'",
        "repeat": true,
        "fields": {
          "field_name": {
            "type": "select",
            "etiqueta": "Selecionar Coluna",
            "table": "input_table",
            "bind": "COLUMNS",
            "required": true
          },
          "order": {
            "type": "select",
            "etiqueta": "Encomendar",
            "options": {
              "-": {
                "etiqueta": "-"
              },
              "asc": {
                "etiqueta": "Ascendente"
              },
              "desc": {
                "etiqueta": "Descendente"
              }
            },
            "val": "-"
          },
          "is_rename": {
            "type": "checkbox",
            "etiqueta": "Mudar Nome",
            "val": "false",
            "isOn": "true",
            "isOff": "false"
          },
          "rename": {
            "show_if": "this.is_rename == 'true'",
            "type": "text",
            "required": true,
            "minLength": 1,
            "maxLength": 255,
            "etiqueta": "Novo Nome de Coluna",
            "style": "margin-left: 10px"
          },
          "format__category": {
            "type": "select",
            "etiqueta": "Formato",
            "required": true,
            "options": {
              "standard": {
                "etiqueta": "Padrão"
              },
              "number": {
                "etiqueta": "Número"
              },
              "percentage": {
                "etiqueta": "Percentagem"
              },
              "text": {
                "etiqueta": "Texto"
              },
              "date": {
                "etiqueta": "Data"
              },
              "currency": {
                "etiqueta": "Moeda"
              }
            },
            "val": "standard"
          },
          "format__rounding": {
            "type": "select",
            "etiqueta": "Arredondamento",
            "show_if": "this.format__category=='number' || this.format__category=='percentage' || this.format__category=='currency'",
            "required": true,
            "options": {
              "no_rounding": {
                "etiqueta": "Arredondamento de números"
              },
              "0": {
                "etiqueta": "0 dígitos"
              },
              "1": {
                "etiqueta": "1 dígito"
              },
              "2": {
                "etiqueta": "2 dígitos"
              },
              "3": {
                "etiqueta": "3 dígitos"
              },
              "4": {
                "etiqueta": "4 dígitos"
              },
              "5": {
                "etiqueta": "5 dígitos"
              },
              "6": {
                "etiqueta": "6 dígitos"
              },
              "7": {
                "etiqueta": "7 dígitos"
              },
              "8": {
                "etiqueta": "8 dígitos"
              },
              "9": {
                "etiqueta": "9 dígitos"
              }
            },
            "val": "2"
          },
          "format__use_1000_separator": {
            "type": "checkbox",
            "etiqueta": "Use 1000 Separator(.)",
            "show_if": "this.format__category=='number' || this.format__category=='percentage' || this.format__category=='currency'",
            "val": "true",
            "isOn": "true",
            "isOff": "false"
          },
          "format__currency_symbol": {
            "type": "select",
            "etiqueta": "Símbolo",
            "show_if": "this.format__category=='currency'",
            "required": true,
            "options": {
              "dollar": {
                "etiqueta": "$"
              },
              "euro": {
                "etiqueta": "€"
              },
              "pound": {
                "etiqueta": "£"
              }
            },
            "val": "dollar"
          },
          "format__date_format": {
            "type": "select",
            "etiqueta": "Tipo",
            "show_if": "this.format__category=='date'",
            "required": true,
            "options": {
              "mm/dd/yyyy": {
                "etiqueta": "3/14/2012"
              },
              "D, M dd, Y": {
                "etiqueta": "Quarta-feira, 14 de Março de 2012"
              },
              "yyyy-mm-dd": {
                "etiqueta": "2012-03-14"
              }
            },
            "val": "mm/dd/yyyy",
            "breakLine": true
          },
          "delete": {
            "type": "button",
            "etiqueta": "Apagar",
            "function": "DELETE_ROW"
          }
        },
        "breakLine": true,
        "style": "margin-bottom: 20px"
      },
      "output.compose_table_name": {
        "type": "checkbox",
        "etiqueta": "Compôr Nome de Tabela",
        "show_if": "output.custom_options=='true'",
        "val": "false",
        "isOn": "true",
        "isOff": "false",
        "breakLine": true
      },
      "compose_table_name_frame": {
        "type": "fieldset",
        "etiqueta": "Compôr Nome de Tabela de Saída",
        "show_if": "output.custom_options=='true' && output.compose_table_name=='true'",
        "repeat": false,
        "fields": {
          "output.prefix_rule": {
            "type": "select",
            "etiqueta": "Prefixo",
            "show_if": "output.compose_table_name=='true'",
            "val": "empty",
            "options": {
              "empty": {
                "etiqueta": "-"
              },
              "script_name": {
                "etiqueta": "Nome da Tarefa"
              },
              "date_string_type1": {
                "etiqueta": "Data: dd_mm_aaaa"
              },
              "date_string_type2": {
                "etiqueta": "Data: aaaa_mm_dd"
              },
              "time_string": {
                "etiqueta": "Hora: h_min_seg"
              },
              "datetime_string": {
                "etiqueta": "Data_Hora"
              },
              "custom": {
                "etiqueta": "Personalizar"
              }
            }
          },
          "output.prefix_text": {
            "type": "text",
            "etiqueta": "Adicionar Prefixo ao Texto",
            "show_if": "output.compose_table_name=='true' && output.prefix_rule=='custom'",
            "minLength": 1,
            "maxLength": 31,
            "val": ""
          },
          "output.core_delimiter": {
            "type": "text",
            "etiqueta": "Delimitador",
            "show_if": "output.compose_table_name=='true' && output.prefix_rule!='empty'",
            "val": "_",
            "width": "50px"
          },
          "output.generate_core_rule": {
            "type": "select",
            "etiqueta": "Nome da Tabela",
            "show_if": "output.compose_table_name=='true' && options.is_generate_holiday_table=='generate_table'",
            "val": "script_name",
            "options": {
              "script_name": {
                "etiqueta": "Nome da Tarefa"
              },
              "custom": {
                "etiqueta": "Personalizar"
              }
            }
          },
          "output.core_rule": {
            "type": "select",
            "etiqueta": "Nome da Tabela",
            "show_if": "output.compose_table_name=='true' && options.is_generate_holiday_table=='add_column'",
            "val": "input_table_name",
            "options": {
              "input_table_name": {
                "etiqueta": "Introduzir Nome da Tabela"
              },
              "script_name": {
                "etiqueta": "Nome da Tarefa"
              },
              "custom": {
                "etiqueta": "Personalizar"
              }
            }
          },
          "output.core_text": {
            "type": "text",
            "etiqueta": "Nome da Tabela Principal",
            "show_if": "output.compose_table_name=='true' && output.core_rule=='custom'",
            "required": true,
            "minLength": 1,
            "maxLength": 31,
            "val": ""
          },
          "output.suffix_delimiter": {
            "type": "text",
            "etiqueta": "Delimitador",
            "show_if": "output.compose_table_name=='true'",
            "val": "_",
            "width": "50px"
          },
          "output.suffix_rule": {
            "type": "select",
            "etiqueta": "Sufixo",
            "show_if": "output.compose_table_name=='true'",
            "val": "script_name",
            "options": {
              "empty": {
                "etiqueta": "-"
              },
              "script_name": {
                "etiqueta": "Nome da Tarefa"
              },
              "date_string_type1": {
                "etiqueta": "Data: dd_mm_aaaa"
              },
              "date_string_type2": {
                "etiqueta": "Data: aaaa_mm_dd"
              },
              "time_string": {
                "etiqueta": "Hora: h_min_seg"
              },
              "datetime_string": {
                "etiqueta": "Data_Hora"
              },
              "custom": {
                "etiqueta": "Personalizar"
              }
            }
          },
          "output.suffix_text": {
            "type": "text",
            "etiqueta": "Juntar Sufixo ao Texto",
            "show_if": "output.compose_table_name=='true' && output.suffix_rule=='custom'",
            "minLength": 1,
            "maxLength": 31,
            "val": "",
            "breakLine": true
          },
          "output.timezone": {
            "type": "select",
            "etiqueta": "Fuso horário",
            "show_if": "output.compose_table_name=='true' && (output.prefix_rule=='date_string_type1' || output.prefix_rule=='date_string_type2' || output.prefix_rule=='time_string' || output.prefix_rule=='datetime_string' || output.suffix_rule=='date_string_type1' || output.suffix_rule=='date_string_type2' || output.suffix_rule=='time_string' || output.suffix_rule=='datetime_string')",
            "val": "Europa/Berlim",
            "required": true,
            "options": {
              "America/Adak": {
                "etiqueta": "Adak, Gambier"
              },
              "Australia/Adelaide": {
                "etiqueta": "Adelaide, Broken_Hill, Darwin"
              },
              "America/Anchorage": {
                "etiqueta": "Anchorage, Juneau, Sitka"
              },
              "Pacific/Apia": {
                "etiqueta": "Apia, Asau, Mulifanua, Afega, Leulumoega"
              },
              "Pacific/Enderbury": {
                "etiqueta": "Apia, Fiji, Tongatapu"
              },
              "Antarctica/McMurdo": {
                "etiqueta": "Auckland, Casey, Davis"
              },
              "Pacific/Auckland": {
