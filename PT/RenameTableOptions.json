{
  "options": {
    "type": "fieldset", 
    "label": "Opções", 
    "repeat": false, 
    "fields": {
      "rename_columns": {
        "type": "checkbox", 
        "label": "Mudar Nome de Colunas", 
        "val": "false", 
        "isOn": "true", 
        "isOff": "false"
      }, 
      "columns": {
        "type": "fieldset", 
        "label": "Selecionar Colunas", 
        "show_if": "rename_columns=='true'", 
        "repeat": true, 
        "fields": {
          "field_name": {
            "type": "select", 
            "label": "Selecionar Coluna", 
            "table": "input_table", 
            "bind": "COLUMNS", 
            "required": true
          }, 
          "rename": {
            "show_if": "this.field_name != ''", 
            "type": "text", 
            "required": true, 
            "minLength": 1, 
            "maxLength": 255, 
            "label": "Mudar nome para"
          }, 
          "delete": {
            "show_if": "this.field_name != ''", 
            "type": "button", 
            "label": "Eliminar", 
            "function": "DELETE_ROW"
          }
        }, 
        "breakLine": true
      }, 
      "rename_rows": {
        "type": "checkbox", 
        "label": "Mudar Nome de Rótulos", 
        "val": "false", 
        "isOn": "true", 
        "isOff": "false", 
        "style": "margin-top: 10px"
      }, 
      "rows": {
        "type": "fieldset", 
        "label": "Selecionar Rótulos", 
        "show_if": "rename_rows=='true'", 
        "repeat": true, 
        "fields": {
          "label_name": {
            "type": "select", 
            "label": "Selecionar Rótulo", 
            "table": "input_table", 
            "bind": "LABELS", 
            "required": true
          }, 
          "rename": {
            "show_if": "this.label_name != ''", 
            "type": "text", 
            "required": true, 
            "minLength": 1, 
            "maxLength": 255, 
            "label": "Mudar nome para"
          }, 
          "delete": {
            "show_if": "this.label_name != ''", 
            "type": "button", 
            "label": "Eliminar", 
            "function": "DELETE_ROW"
          }
        }, 
        "breakLine": true
      }
    }
  }
}