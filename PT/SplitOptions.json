{
  "options": {
    "type": "fieldset", 
    "label": "Seleccione Opções", 
    "repeat": false, 
    "fields": {
      "split_method": {
        "type": "select", 
        "label": "Método Split", 
        "required": true, 
        "options": {
          "standard": {
            "label": "Norma"
          }, 
          "regex": {
            "label": "RegEx"
          }
        }, 
        "val": "standard"
      }, 
      "split_type": {
        "type": "select", 
        "label": "Tipo de divisão", 
        "required": true, 
        "options": {
          "split_to_columns": {
            "label": "Dividir em Colunas"
          }, 
          "split_to_rows": {
            "label": "Dividir em filas"
          }
        }, 
        "val": "split_to_columns", 
        "breakLine": true
      }, 
      "standard_delimiter": {
        "type": "select", 
        "label": "Delimitador Dividido", 
        "show_if": "split_method=='standard'", 
        "required": true, 
        "options": {
          "tabs": {
            "label": "Tabs"
          }, 
          "space": {
            "label": "Espaço"
          }, 
          "whitespace": {
            "label": "Espaço Branco"
          }, 
          "linebreak": {
            "label": "Quebra de linha"
          }, 
          "punctuation": {
            "label": "Pontuação"
          }, 
          "punctuation_whitespace": {
            "label": "Pontuação e Espaço Branco"
          }, 
          "custom": {
            "label": "Personalizado"
          }
        }, 
        "val": "punctuation"
      }, 
      "delimiter": {
        "type": "text", 
        "label": "Delimitador Dividido", 
        "show_if": "split_method=='standard' && standard_delimiter=='custom'", 
        "val": ", ,\
        trim": "false", 
        "required": true, 
        "breakLine": true
      }, 
      "format": {
        "type": "fieldset", 
        "label": "Formato a Partir para Partir", 
        "show_if": "split_method=='regex'", 
        "repeat": false, 
        "fields": {
          "format_to_convert.regular_expression": {
            "type": "text", 
            "label": "Parse Expressão regular ( ex: ([a-z]+)\\\\s([a-z]+) )", 
            "val": "([a-z]+)\\\\s([a-z]+)", 
            "required": true, 
            "width": "100%", 
            "trim": "false", 
            "breakLine": true
          }, 
          "format_to_convert.case_insensitive": {
            "type": "checkbox", 
            "label": "Insensível a maiúsculas e minúsculas", 
            "val": "true", 
            "isOn": "true", 
            "isOff": "false", 
            "breakLine": true
          }
        }
      }
    }
  }
}
