{
  "options": {
    "type": "fieldset",
    "etiqueta": "Cabeçalho de mesa e resultado de etiqueta" "Sem cabeçalho!,
    "repeat": false,
    "fields": {
      "has_header": {
        "type": "button",
        "etiqueta": "Tem cabeçalho!",
        "show_if": "$input_table.numOfHeader >= 1"
      },
      "has_label": {
        "type": "button",
        "etiqueta": "Tem etiqueta!",
        "show_if": "$input_table.numOfLabel >= 1"
      },
      "has_both": {
        "type": "button",
        "etiqueta": "Tem ambos!",
        "show_if": "$input_table.numOfHeader >= 1 && $input_table.numOfLabel >= 1"
      },
      "has_both2": {
        "type": "text",
        "etiqueta": "Tem ambos2!",
        "show_if": "$input_table.numOfHeader>=1 && $input_table.numOfLabel>=1",
        "val": "both"
      },
      "header_only": {
        "type": "button",
        "etiqueta": "Apenas cabeçalho!" "Tem cabeçalho!",
        "show_if": "$input_table.numOfHeader >= 1 && $input_table.numOfLabel < 1"
      },
      "label_only": {
        "type": "button",
        "etiqueta": "Só etiqueta!" "Tem apenas rótulo!",
        "show_if": "$input_table.numOfHeader < 1 && $input_table.numOfLabel >= 1"
      },
      "no_header": {
        "type": "button",
        "etiqueta": "Sem cabeçalho!" "Não tem cabeçalho!",
        "show_if": "$input_table.numOfHeader < 1"
      },
      "no_label": {
        "type": "button",
        "etiqueta": "Sem rótulo!" "Sem rótulo!",
        "show_if": "$input_table.numOfLabel < 1"
      }
    }
  }
}