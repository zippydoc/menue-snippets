{
  "output.output_table_types": {
    "type": "select", 
    "label": "Mostrar Tipo(s) de Tabela", 
    "required": true, 
    "options": {
      "unique": {
        "label": "Tabela Única"
      }, 
      "duplicates": {
        "label": "Tabela de Duplicados"
      }
    }, 
    "multiple": true, 
    "val": ["unique"]
  }, 
  "output.standard_ending": {
    "type": "checkbox", 
    "label": "Padrão Final", 
    "val": "true", 
    "isOn": "true", 
    "isOff": "false", 
    "breakLine": true
  }, 
  "output.unique_ending": {
    "show_if": "output.standard_ending == 'false'", 
    "type": "text", 
    "label": "Fim de Saída Único", 
    "val": "_u", 
    "style": "width: 160px; margin-left: 10px"
  }, 
  "output.duplicates_ending": {
    "show_if": "output.standard_ending == 'false'", 
    "type": "text", 
    "label": "Duplica o Final de Saída", 
    "val": "_d", 
    "style": "width: 160px; margin-left: 10px", 
    "breakLine": true
  }
}