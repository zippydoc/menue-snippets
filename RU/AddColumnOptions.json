{
  "options_box": {
    "type": "fieldset",
    "label": "Опции",
    "repeat": false,
    "fields": {
      "options.add_columns": {
        "type": "fieldset",
        "label": "Добавить новые столбцы",
        "repeat": true,
        "fields": {
          "new_column_name": {
            "type": "text",
            "label": "Название нового столбца",
            "val": "",
            "breakLine": true
          },
          "task_type": {
            "type": "select",
            "label": "Тип задачи",
            "required": true,
            "options": {
              "static_value": {
                "label": "Статическое значение"
              },
              "numeric_formula": {
                "label": "Числовая формула"
              },
              "text_formula": {
                "label": "Текстовая формула"
              },
              "multi_column_functions": {
                "label": "Несколько столбцов"
              },
              "multi_row_functions": {
                "label": "Несколько строк"
              }
            },
            "val": "multi_column_functions",
            "breakLine": true
          },
          "multi_column_functions__function": {
            "type": "select",
            "label": "Функция",
            "show_if": "this.task_type==''multi_column_functions''",
            "required": true,
            "options": {
              "sum": {
                "label": "Сумма"
              },
              "average": {
                "label": "Средний"
              },
              "median": {
                "label": "Медиана"
              },
              "max": {
                "label": "Макс"
              },
              "min": {
                "label": "Мин"
              },
              "standard_deviation": {
                "label": "Стандартное отклонение"
              },
              "product": {
                "label": "Продукт"
              },
              "quota": {
                "label": "Квота"
              },
              "absolute_growth": {
                "label": "Абсолютный рост"
              },
              "relative_growth": {
                "label": "Относительный рост"
              },
              "compound_growth": {
                "label": "Рост соединений"
              }
            },
            "val": "sum"
          },
          "multi_column_functions__sel_columns": {
            "type": "select",
            "label": "Выбрать столбец(цы)",
            "show_if": "this.task_type==''multi_column_functions'' && (this.multi_column_functions__function==''sum'' || this.multi_column_functions__function==''average'' || this.multi_column_functions__function==''median'' || this.multi_column_functions__function==''max'' || this.multi_column_functions__function==''min'' || this.multi_column_functions__function==''standard_deviation'' || this.multi_column_functions__function==''product'')",
            "table": "input_table",
            "bind": "COLUMNS",
            "multiple": true,
            "required": true
          },
          "multi_column_functions__column_a": {
            "type": "select",
            "label": "Выбрать столбец А",
            "show_if": "this.task_type==''multi_column_functions'' && (this.multi_column_functions__function==''quota'' || this.multi_column_functions__function==''absolute_growth'' || this.multi_column_functions__function==''relative_growth'' || this.multi_column_functions__function==''compound_growth'')",
            "table": "input_table",
            "bind": "COLUMNS",
            "required": true
          },
          "multi_column_functions__column_b": {
            "type": "select",
            "label": "Выбрать столбец B",
            "show_if": "this.task_type==''multi_column_functions'' && (this.multi_column_functions__function==''quota'' || this.multi_column_functions__function==''absolute_growth'' || this.multi_column_functions__function==''relative_growth'' || this.multi_column_functions__function==''compound_growth'')",
            "table": "input_table",
            "bind": "COLUMNS",
            "required": true
          },
          "multi_column_functions__n_value": {
            "type": "text",
            "label": "N (целое положительное число)",
            "show_if": "this.task_type==''multi_column_functions'' && this.multi_column_functions__function==''compound_growth''",
            "val": "1",
            "required": true
          },
          "multi_row_functions__function": {
            "type": "select",
            "label": "Функция",
            "show_if": "this.task_type==''multi_row_functions''",
            "required": true,
            "options": {
              "trailing_row_n_average": {
                "label": "Конечная средняя строка-N"
              },
              "index_from_row_n": {
                "label": "Индекс из стоки-N"
              },
              "absolute_growth": {
                "label": "Абсолютный рост"
              },
              "relative_growth": {
                "label": "Относительный рост"
              },
              "compound_growth": {
                "label": "Рост соединений"
              }
            },
            "val": "trailing_row_n_average"
          },
          "multi_row_functions__sel_column": {
            "type": "select",
            "label": "Столбец",
            "show_if": "this.task_type==''multi_row_functions'' && (this.multi_row_functions__function==''trailing_row_n_average'' || this.multi_row_functions__function==''index_from_row_n'' || this.multi_row_functions__function==''absolute_growth'' || this.multi_row_functions__function==''relative_growth'' || this.multi_row_functions__function==''compound_growth'')",
            "table": "input_table",
            "bind": "COLUMNS",
            "required": true
          },
          "multi_row_functions__n_value": {
            "type": "text",
            "label": "N (целое положительное число)",
            "show_if": "this.task_type==''multi_row_functions'' && (this.multi_row_functions__function==''trailing_row_n_average'' || this.multi_row_functions__function==''index_from_row_n'' || this.multi_row_functions__function==''compound_growth'')",
            "val": "1",
            "required": true,
            "breakLine": true
          },
          "static_value": {
            "type": "text",
            "label": "Добавить значение",
            "show_if": "this.task_type==''static_value''",
            "val": "",
            "width": "100%"
          },
          "numeric_formula": {
            "type": "text",
            "label": "Input Formula, e.g. [Column1] + 2 * ([Row-1:Column3] - [Column1])",
            "show_if": "this.task_type==''numeric_formula''",
            "val": "",
            "width": "100%"
          },
          "text_formula": {
            "type": "text",
            "label": "Input Formula, e.g. [firstName] + '' '' + [lastName] + '' '' + [Row-1:Column3]",
            "show_if": "this.task_type==''text_formula''",
            "val": "",
            "width": "100%"
          },
          "use_multi_row": {
            "type": "checkbox",
            "label": "Настройки нескольких строк",
            "show_if": "this.task_type==''numeric_formula'' || this.task_type==''text_formula''",
            "val": "false",
            "isOn": "true",
            "isOff": "false"
          },
          "if_row_not_exist": {
            "type": "select",
            "label": "Если упомянутой строки не существует",
            "show_if": "this.use_multi_row==''true'' && (this.task_type==''numeric_formula'' || this.task_type==''text_formula'')",
            "required": true,
            "options": {
              "empty": {
                "label": "Пусто или 0"
              },
              "closest": {
                "label": "Значение ближайшей строки"
              }
            },
            "val": "empty",
            "breakLine": true
          },
          "format__category": {
            "type": "select",
            "label": "Формат",
            "required": true,
            "options": {
              "standard": {
                "label": "Стандартный"
              },
              "number": {
                "label": "Число"
              },
              "percentage": {
                "label": "Процент"
              },
              "text": {
                "label": "Текст"
              },
              "date": {
                "label": "Дата"
              },
              "currency": {
                "label": "Валюта"
              }
            },
            "val": "standard"
          },
          "format__rounding": {
            "type": "select",
            "label": "Округление",
            "show_if": "this.format__category==''number'' || this.format__category==''percentage'' || this.format__category==''currency''",
            "required": true,
            "options": {
              "no_rounding": {
                "label": "Округление чисел"
              },
              "0": {
                "label": "0 цифр"
              },
              "1": {
                "label": "1 цифра"
              },
              "2": {
                "label": "2 цифры"
              },
              "3": {
                "label": "3 цифры"
              },
              "4": {
                "label": "4 цифры"
              },
              "5": {
                "label": "5 цифр"
              },
              "6": {
                "label": "6 цифр"
              },
              "7": {
                "label": "7 цифр"
              },
              "8": {
                "label": "8 цифр"
              },
              "9": {
                "label": "9 цифр"
              }
            },
            "val": "2"
          },
          "format__use_1000_separator": {
            "type": "checkbox",
            "label": "Используйте сепаратор 1000(.)",
            "show_if": "this.format__category==''number'' || this.format__category==''percentage'' || this.format__category==''currency''",
            "val": "true",
            "isOn": "true",
            "isOff": "false"
          },
          "format__currency_symbol": {
            "type": "select",
            "label": "Символ",
            "show_if": "this.format__category==''currency''",
            "required": true,
            "options": {
              "dollar": {
                "label": "$"
              },
              "euro": {
                "label": "€"
              },
              "pound": {
                "label": "£"
              }
            },
            "val": "dollar"
          },
          "format__date_format": {
            "type": "select",
            "label": "Тип",
            "show_if": "this.format__category==''date''",
            "required": true,
            "options": {
              "mm/dd/yyyy": {
                "label": "3/14/2012"
              },
              "D, M dd, Y": {
                "label": "Wednesday, March 14, 2012"
              },
              "yyyy-mm-dd": {
                "label": "2012-03-14"
              }
            },
            "val": "mm/dd/yyyy",
            "breakLine": true
          },
          "group_counter__inc_dec_curly_bracket": {
            "type": "checkbox",
            "label": "Увеличение/уменьшение с помощью {}",
            "show_if": "this.task_type==''group_counter''",
            "val": "false",
            "isOn": "true",
            "isOff": "false",
            "breakLine": true
          },
          "group_counter__inc_dec_square_bracket": {
            "type": "checkbox",
            "label": "Увеличение/уменьшение с помощью []",
            "show_if": "this.task_type==''group_counter''",
            "val": "false",
            "isOn": "true",
            "isOff": "false",
            "breakLine": true
          },
          "group_counter__inc_dec_parenthesis": {
            "type": "checkbox",
            "label": "Увеличение/уменьшение с помощью ()",
            "show_if": "this.task_type==''group_counter''",
            "val": "false",
            "isOn": "true",
            "isOff": "false",
            "breakLine": true
          },
          "group_counter__custom": {
            "type": "checkbox",
            "label": "Пользовательский счетчик группы",
            "show_if": "this.task_type==''group_counter''",
            "val": "false",
            "isOn": "true",
            "isOff": "false",
            "breakLine": true
          },
          "group_counter__column": {
            "type": "select",
            "label": "Столбец",
            "show_if": "this.task_type==''group_counter'' && this.group_counter__custom==''true''",
            "table": "input_table",
            "bind": "COLUMNS",
            "required": true
          },
          "group_counter__type": {
            "type": "select",
            "label": "Когда",
            "show_if": "this.task_type==''group_counter'' && this.group_counter__custom==''true''",
            "required": true,
            "options": {
              "starts_with": {
                "label": "НАЧИНАЕТСЯ С"
              },
              "ends_with": {
                "label": "ЗАКАНЧИВАЕТСЯ С"
              },
              "starts_with_regex": {
                "label": "НАЧИНАЕТСЯ С РЕГУЛЯРНОГО ВЫРАЖЕНИЯ"
              },
              "ends_with_regex": {
                "label": "ЗАКАНЧИВАЕТСЯ С РЕГУЛЯРНЫМ ВЫРАЖЕНИЕМ"
              }
            },
            "val": "starts_with"
          },
          "group_counter__value": {
            "type": "text",
            "label": "Добавить значение",
            "show_if": "this.task_type==''group_counter'' && this.group_counter__custom==''true''",
            "val": ".",
            "breakLine": true
          },
          "group_counter__label": {
            "type": "text",
            "label": "Добавить метку",
            "show_if": "this.task_type==''group_counter'' && this.group_counter__custom==''true''",
            "val": "sentence",
            "breakLine": true
          },
          "group_counter__if_end_of_string_start_labeling_with_next_record": {
            "type": "checkbox",
            "label": "Если конец строки отмечается со следующей записи.",
            "show_if": "this.task_type==''group_counter'' && this.group_counter__custom==''true'' && (this.group_counter__type==''starts_with'' || this.group_counter__type==''starts_with_regex'')",
            "val": "true",
            "isOn": "true",
            "isOff": "false"
          },
          "group_counter__if_begin_of_string_start_labeling_with_previous_record": {
            "type": "checkbox",
            "label": "Если начало строки отмечается с предыдущей записи.",
            "show_if": "this.task_type==''group_counter'' && this.group_counter__custom==''true'' && (this.group_counter__type==''ends_with'' || this.group_counter__type==''ends_with_regex'')",
            "val": "true",
            "isOn": "true",
            "isOff": "false"
          },
          "group_counter__always_include": {
            "type": "checkbox",
            "label": "Всегда включать",
            "show_if": "this.task_type==''group_counter'' && this.group_counter__custom==''true''",
            "val": "true",
            "isOn": "true",
            "isOff": "false"
          },
          "apply_to_group__enabled": {
            "type": "checkbox",
            "label": "Применить к группе",
            "show_if": "this.task_type!=''static_value''",
            "val": "false",
            "isOn": "true",
            "isOff": "false"
          },
          "apply_to_group__group_columns": {
            "type": "select",
            "label": "Групповой столбец(цы)",
            "show_if": "this.apply_to_group__enabled == ''true'' && this.task_type!=''static_value''",
            "table": "input_table",
            "bind": "COLUMNS",
            "multiple": true,
            "required": true
          },
          "delete": {
            "type": "button",
            "label": "Удалить",
            "function": "DELETE_ROW"
          }
        }
      },
      "options.use_filter": {
        "type": "checkbox",
        "label": "Использовать фильтр",
        "val": "false",
        "isOn": "true",
        "isOff": "false",
        "style": "margin-top: 10px",
        "breakLine": true
      },
      "filter_box": {
        "type": "fieldset",
        "label": "Настройки фильтра",
        "show_if": "options.use_filter==''true''",
        "repeat": false,
        "fields": {
          "options.filter": {
            "type": "fieldset",
            "label": "Добавить условия фильтрации",
            "repeat": true,
            "fields": {
              "initial_op": {
                "type": "select",
                "label": "Выбрать оператора логики",
                "required": true,
                "show_if" :  "$index == 0",
                "options": {
                  "if": {
                    "label": "ЕСЛИ"
                  },
                  "if_not": {
                    "label": "ЕСЛИ НЕ"
                  }
                },
                "val": "if"
              },
              "op": {
                "type": "select",
                "label": "Выбрать оператора логики",
                "required": true,
                "show_if" :  "$index > 0",
                "options": {
                  "and": {
                    "label": "И"
                  },
                  "or": {
                    "label": "ИЛИ"
                  },
                  "and_not": {
                    "label": "И НЕ"
                  },
                  "or_not": {
                    "label": "ИЛИ НЕ"
                  }
                },
                "val": "and"
              },
              "column": {
                "type": "select",
                "label": "Выбрать столбец",
                "table": "input_table",
                "bind": "COLUMNS",
                "required": true
              },
              "condition": {
                "type": "select",
                "label": "Выбрать оператора",
                "required": true,
                "options": {
                  "СОДЕРЖИТ": {
                    "label": "СОДЕРЖИТ"
                  },
                  "CONTAINED IN STRING": {
                    "label": "СОДЕРЖАЛОСЬ В"
                  },
                  "НАЧИНАЕТСЯ С": {
                    "label": "НАЧИНАЕТСЯ С"
                  },
                  "ЗАКАНЧИВАЕТСЯ С": {
                    "label": "ЗАКАНЧИВАЕТСЯ С"
                  },
                  "=": {
                    "label": "Равный"
                  },
                  ">": {
                    "label": "БОЛЬШЕ ЧЕМ"
                  },
                  ">=": {
                    "label": "БОЛЬШИЙ ИЛИ РАВНЫЙ"
                  },
                  "<": {
                    "label": "МЕНЬШЕ ЧЕМ"
                  },
                  "<=": {
                    "label": "МЕНЬШИЙ ИЛИ РАВНЫЙ"
                  },
                  "EMPTY": {
                    "label": "ПУСТО"
                  },
                  "BLANK": {
                    "label": "ПУСТО"
                  },
                  "НОЛЬ": {
                    "label": "НОЛЬ"
                  },
                  "Совпадение регулярного выражения": {
                    "label": "Совпадение регулярного выражения"
                  },
                  "CONTAINED IN TABLE": {
                    "label": "В ТАБЛИЦЕ"
                  },
                  "В ТАБЛИЦЕ РЕГУЛЯРНЫХ ВЫРАЖЕНИЙ": {
                    "label": "В ТАБЛИЦЕ РЕГУЛЯРНЫХ ВЫРАЖЕНИЙ"
                  },
                  "ДАТА": {
                    "label": "ДАТА"
                  }
                },
                "val": "СОДЕРЖИТ"
              },
              "date_condition": {
                "type": "select",
                "label": "Выбрать оператора даты",
                "required": true,
                "show_if": "this.condition == ''DATE''",
                "options": {
                  "<": {
                    "label": "ПРЕДЫДУЩИЙ"
                  },
                  "<=": {
                    "label": "ПРЕДЫДУЩИЙ ИЛИ РАВНЫЙ"
                  },
                  "=": {
                    "label": "Равный"
                  },
                  ">": {
                    "label": "ПОСЛЕ"
                  },
                  ">=": {
                    "label": "ПОСЛЕ ИЛИ РАВНО"
                  }
                },
                "val": "="
              },
              "case_sensitive": {
                "type": "select",
                "label": "Выбрать с учетом регистра",
                "required": true,
                "show_if": "this.condition == ''CONTAINS'' || this.condition == ''CONTAINED IN STRING'' || this.condition == ''='' || this.condition == ''STARTS WITH'' || this.condition == ''ENDS WITH'' || this.condition == ''REGEX MATCH'' || this.condition == ''NOT REGEX MATCH'' || this.condition == ''IN REGEX TABLE'' || this.condition == ''NOT IN REGEX TABLE''",
                "options": {
                  "yes": {
                    "label": "С учетом регистра"
                  },
                  "no": {
                    "label": "Без учета регистра"
                  }
                },
                "val": "yes"
              },
              "date_value": {
                "type": "date",
                "label": "Выбрать дату",
                "show_if": "this.condition==''DATE''",
                "val": "1/31/2020",
                "required": true
              },
              "value": {
                "type": "text",
                "label": "Добавить значение",
                "show_if": "this.condition != ''DATE'' && this.condition != ''EMPTY'' && this.condition != ''NOT EMPTY'' && this.condition != ''BLANK'' && this.condition != ''NOT BLANK'' && this.condition != ''IS NULL'' && this.condition != ''IS NOT NULL'' && this.condition != ''CONTAINED IN TABLE'' && this.condition != ''NOT CONTAINED IN TABLE'' && this.condition != ''IN REGEX TABLE'' && this.condition != ''NOT IN REGEX TABLE''",
                "val": "",
                "required": true
              },
              "compare_table": {
                "type": "select",
                "label": "Выбрать таблицу",
                "bind": "TABLES",
                "empty": "",
                "show_if": "this.condition == ''CONTAINED IN TABLE'' || this.condition == ''NOT CONTAINED IN TABLE'' || this.condition == ''IN REGEX TABLE'' || this.condition == ''NOT IN REGEX TABLE''",
                "required": true
              },
              "compare_column": {
                "type": "select",
                "label": "Выбрать столбец",
                "table": "this.compare_table",
                "bind": "COLUMNS",
                "show_if": "this.condition == ''CONTAINED IN TABLE'' || this.condition == ''NOT CONTAINED IN TABLE'' || this.condition == ''IN REGEX TABLE'' || this.condition == ''NOT IN REGEX TABLE''",
                "required": true
              },
              "delete": {
                "type": "button",
                "label": "Удалить",
                "function": "DELETE_ROW"
              }
            }
          },
          "options.apply_to_record": {
            "type": "checkbox",
            "label": "Применить к регистру",
            "val": "true",
            "isOn": "true",
            "isOff": "false",
            "style": "margin-top: 10px",
            "breakLine": true
          },
          "apply_to_group_box": {
            "type": "fieldset",
            "label": "Применить к группе",
            "show_if": "options.apply_to_record==''false''",
            "repeat": false,
            "fields": {
              "options.apply_to_group_columns": {
                "type": "select",
                "label": "Выбрать групповые столбцы",
                "required": true,
                "width": "100%",
                "table": "input_table",
                "multiple": true,
                "bind": "COLUMNS",
                "breakLine": true
              }
            }
          }
        }
      }
    }
  }
}