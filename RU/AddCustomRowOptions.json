{
  "options": {
    "type": "fieldset",
    "label": "Выбрать параметры",
    "repeat": false,
    "fields": {
      "options.position": {
        "type": "select",
        "label": "Выбрать позицию",
        "required": true,
        "options": {
          "add_rows_below_selected_row": {
            "label": "Добавить строки под выбранными строками"
          },
          "add_rows_above_selected_row": {
            "label": "Добавить строки над выбранными строками"
          },
          "add_rows_at_table_end": {
            "label": "Добавить строки в конце таблицы"
          },
          "add_single_row_at_table_end": {
            "label": "Добавить одну строку в конце таблицы"
          },
          "add_row_after_each_group": {
            "label": "Добавлять строку после каждой группы"
          }
        },
        "val": "add_rows_below_selected_row",
        "breakLine": true
      },
      "options.group_column": {
        "type": "select",
        "label": "Групповой столбец",
        "show_if": "options.position==''add_row_after_each_group''",
        "table": "input_table",
        "bind": "COLUMNS",
        "required": true,
        "breakLine": true
      },
      "options.use_filter": {
        "type": "select",
        "label": "Выбрать тип",
        "show_if": "options.position==''add_rows_below_selected_row'' || options.position==''add_rows_above_selected_row'' || options.position==''add_rows_at_table_end''",
        "required": true,
        "options": {
          "false": {
            "label": "Add one row for each table row"
          },
          "true": {
            "label": "Add one row for each specified row"
          }
        },
        "val": "false",
        "breakLine": true
      },
      "filter_box": {
        "type": "fieldset",
        "label": "Настройки фильтра",
        "show_if": "options.use_filter==''true'' && (options.position==''add_rows_below_selected_row'' || options.position==''add_rows_above_selected_row'' || options.position==''add_rows_at_table_end'')",
        "repeat": false,
        "fields": {
          "options.filter": {
            "type": "fieldset",
            "label": "Добавить условия фильтрации",
            "repeat": true,
            "fields": {
              "initial_op": {
                "type": "select",
                "label": "Выбрать оператора логики",
                "required": true,
                "show_if": "$index == 0",
                "options": {
                  "if": {
                    "label": "ЕСЛИ"
                  },
                  "if_not": {
                    "label": "ЕСЛИ НЕ"
                  }
                },
                "val": "if"
              },
              "op": {
                "type": "select",
                "label": "Выбрать оператора логики",
                "required": true,
                "show_if": "$index > 0",
                "options": {
                  "and": {
                    "label": "И"
                  },
                  "or": {
                    "label": "ИЛИ"
                  },
                  "and_not": {
                    "label": "И НЕ"
                  },
                  "or_not": {
                    "label": "ИЛИ НЕ"
                  }
                },
                "val": "and"
              },
              "column": {
                "type": "select",
                "label": "Выбрать столбец",
                "table": "input_table",
                "bind": "COLUMNS",
                "required": true
              },
              "condition": {
                "type": "select",
                "label": "Выбрать оператора",
                "required": true,
                "options": {
                  "СОДЕРЖИТ": {
                    "label": "СОДЕРЖИТ"
                  },
                  "CONTAINED IN STRING": {
                    "label": "СОДЕРЖАЛОСЬ В"
                  },
                  "НАЧИНАЕТСЯ С": {
                    "label": "НАЧИНАЕТСЯ С"
                  },
                  "ЗАКАНЧИВАЕТСЯ С": {
                    "label": "ЗАКАНЧИВАЕТСЯ С"
                  },
                  "=": {
                    "label": "Равный"
                  },
                  ">": {
                    "label": "БОЛЬШЕ ЧЕМ"
                  },
                  ">=": {
                    "label": "БОЛЬШИЙ ИЛИ РАВНЫЙ"
                  },
                  "<": {
                    "label": "МЕНЬШЕ ЧЕМ"
                  },
                  "<=": {
                    "label": "МЕНЬШИЙ ИЛИ РАВНЫЙ"
                  },
                  "EMPTY": {
                    "label": "ПУСТО"
                  },
                  "BLANK": {
                    "label": "ПУСТО"
                  },
                  "НОЛЬ": {
                    "label": "НОЛЬ"
                  },
                  "Совпадение регулярного выражения": {
                    "label": "Совпадение регулярного выражения"
                  },
                  "CONTAINED IN TABLE": {
                    "label": "В ТАБЛИЦЕ"
                  },
                  "В ТАБЛИЦЕ РЕГУЛЯРНЫХ ВЫРАЖЕНИЙ": {
                    "label": "В ТАБЛИЦЕ РЕГУЛЯРНЫХ ВЫРАЖЕНИЙ"
                  },
                  "ДАТА": {
                    "label": "ДАТА"
                  }
                },
                "val": "СОДЕРЖИТ"
              },
              "date_condition": {
                "type": "select",
                "label": "Выбрать оператора даты",
                "required": true,
                "show_if": "this.condition == ''DATE''",
                "options": {
                  "<": {
                    "label": "ПРЕДЫДУЩИЙ"
                  },
                  "<=": {
                    "label": "ПРЕДЫДУЩИЙ ИЛИ РАВНЫЙ"
                  },
                  "=": {
                    "label": "Равный"
                  },
                  ">": {
                    "label": "ПОСЛЕ"
                  },
                  ">=": {
                    "label": "ПОСЛЕ ИЛИ РАВНО"
                  }
                },
                "val": "="
              },
              "case_sensitive": {
                "type": "select",
                "label": "Выбрать с учетом регистра",
                "required": true,
                "show_if": "this.condition == ''CONTAINS'' || this.condition == ''CONTAINED IN STRING'' || this.condition == ''='' || this.condition == ''STARTS WITH'' || this.condition == ''ENDS WITH'' || this.condition == ''REGEX MATCH'' || this.condition == ''NOT REGEX MATCH'' || this.condition == ''IN REGEX TABLE'' || this.condition == ''NOT IN REGEX TABLE''",
                "options": {
                  "yes": {
                    "label": "С учетом регистра"
                  },
                  "no": {
                    "label": "Без учета регистра"
                  }
                },
                "val": "yes"
              },
              "date_value": {
                "type": "date",
                "label": "Выбрать дату",
                "show_if": "this.condition==''DATE''",
                "val": "1/31/2020",
                "required": true
              },
              "value": {
                "type": "text",
                "label": "Добавить значение",
                "show_if": "this.condition != ''DATE'' && this.condition != ''EMPTY'' && this.condition != ''NOT EMPTY'' && this.condition != ''BLANK'' && this.condition != ''NOT BLANK'' && this.condition != ''IS NULL'' && this.condition != ''IS NOT NULL'' && this.condition != ''CONTAINED IN TABLE'' && this.condition != ''NOT CONTAINED IN TABLE'' && this.condition != ''IN REGEX TABLE'' && this.condition != ''NOT IN REGEX TABLE''",
                "val": "",
                "required": true
              },
              "compare_table": {
                "type": "select",
                "label": "Выбрать таблицу",
                "bind": "TABLES",
                "empty": "",
                "show_if": "this.condition == ''CONTAINED IN TABLE'' || this.condition == ''NOT CONTAINED IN TABLE'' || this.condition == ''IN REGEX TABLE'' || this.condition == ''NOT IN REGEX TABLE''",
                "required": true
              },
              "compare_column": {
                "type": "select",
                "label": "Выбрать столбец",
                "table": "this.compare_table",
                "bind": "COLUMNS",
                "show_if": "this.condition == ''CONTAINED IN TABLE'' || this.condition == ''NOT CONTAINED IN TABLE'' || this.condition == ''IN REGEX TABLE'' || this.condition == ''NOT IN REGEX TABLE''",
                "required": true
              },
              "delete": {
                "type": "button",
                "label": "Удалить",
                "function": "DELETE_ROW"
              }
            }
          },
          "options.apply_to_record": {
            "type": "checkbox",
            "label": "Применить к регистру",
            "val": "true",
            "isOn": "true",
            "isOff": "false",
            "style": "margin-top: 10px",
            "breakLine": true
          },
          "apply_to_group_box": {
            "type": "fieldset",
            "label": "Применить к группе",
            "show_if": "options.apply_to_record==''false''",
            "repeat": false,
            "fields": {
              "options.apply_to_group_columns": {
                "type": "select",
                "label": "Выбрать групповые столбцы",
                "required": true,
                "width": "100%",
                "table": "input_table",
                "multiple": true,
                "bind": "COLUMNS",
                "breakLine": true
              }
            }
          }
        }
      },
      "options.target_cells": {
        "type": "fieldset",
        "label": "Define added Row(s)",
        "repeat": true,
        "fields": {
          "column": {
            "type": "select",
            "label": "Выбрать столбец",
            "table": "input_table",
            "bind": "COLUMNS",
            "required": true
          },
          "formula_type": {
            "type": "select",
            "label": "Тип формулы",
            "required": true,
            "options": {
              "copy_selected": {
                "label": "Копировать выбранное"
              },
              "static_value": {
                "label": "Статическое значение"
              },
              "numeric_formula": {
                "label": "Числовая формула"
              },
              "text_formula": {
                "label": "Текстовая формула"
              }
            },
            "val": "copy_selected"
          },
          "static_value": {
            "type": "text",
            "label": "Значение",
            "show_if": "this.formula_type==''static_value''",
            "val": ""
          },
          "numeric_formula": {
            "type": "text",
            "label": "Formula, e.g. [Column1] + 2 * ([Row-1:Column3] - [Column1])",
            "show_if": "this.formula_type==''numeric_formula''",
            "val": "",
            "style": "width: 100%"
          },
          "rounding": {
            "type": "select",
            "label": "Округление",
            "show_if": "this.formula_type==''numeric_formula''",
            "required": true,
            "options": {
              "no_rounding": {
                "label": "Округление чисел"
              },
              "0": {
                "label": "0 цифр"
              },
              "1": {
                "label": "1 цифра"
              },
              "2": {
                "label": "2 цифры"
              },
              "3": {
                "label": "3 цифры"
              },
              "4": {
                "label": "4 цифры"
              },
              "5": {
                "label": "5 цифр"
              },
              "6": {
                "label": "6 цифр"
              },
              "7": {
                "label": "7 цифр"
              },
              "8": {
                "label": "8 цифр"
              },
              "9": {
                "label": "9 цифр"
              }
            },
            "val": "no_rounding"
          },
          "text_formula": {
            "type": "text",
            "label": "Formula, e.g. [firstName] + '' '' + [lastName] + '' '' + [Row-1:Column3]",
            "show_if": "this.formula_type==''text_formula''",
            "val": "",
            "style": "width: 100%"
          },
          "use_multi_row": {
            "type": "checkbox",
            "label": "Настройки нескольких строк",
            "show_if": "options.position!=''add_row_after_each_group'' && (this.formula_type==''numeric_formula'' || this.formula_type==''text_formula'')",
            "val": "false",
            "isOn": "true",
            "isOff": "false"
          },
          "if_row_not_exist": {
            "type": "select",
            "label": "Если упомянутой строки не существует",
            "show_if": "options.position!=''add_row_after_each_group'' && this.use_multi_row==''true'' && (this.formula_type==''numeric_formula'' || this.formula_type==''text_formula'')",
            "required": true,
            "options": {
              "empty": {
                "label": "Пусто или 0"
              },
              "closest": {
                "label": "Значение ближайшей строки"
              }
            },
            "val": "empty",
            "breakLine": true
          },
          "delete": {
            "type": "button",
            "label": "Удалить",
            "function": "DELETE_ROW"
          }
        },
        "breakLine": true
      },
      "options.set_date_column": {
        "type": "checkbox",
        "label": "Apply time-shift to a selected date column",
        "val": "false",
        "isOn": "true",
        "isOff": "false",
        "style": "margin-top: 20px",
        "breakLine": true
      },
      "options.date_column": {
        "type": "select",
        "label": "Выбрать столбец с датой",
        "show_if": "options.set_date_column==''true''",
        "table": "input_table",
        "bind": "COLUMNS",
        "required": true
      },
      "options.period_shift.number": {
        "type": "text",
        "label": "Сдвиг периода",
        "show_if": "options.set_date_column==''true''",
        "required": true,
        "val": "0"
      },
      "options.period_shift.period": {
        "type": "select",
        "label": "Выбрать период",
        "show_if": "options.set_date_column==''true''",
        "options": {
          "day": {
            "label": "Дни"
          },
          "week": {
            "label": "Недели"
          },
          "month": {
            "label": "Месяцы"
          },
          "year": {
            "label": "Годы"
          }
        },
        "val": "month"
      }
    }
  }
}