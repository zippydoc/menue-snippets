{
  "options": {
    "type": "fieldset",
    "label": "Выбрать параметры",
    "repeat": false,
    "fields": {
      "target_table.table_id": {
        "type": "select",
        "label": "Целевая таблица",
        "bind": "TABLES",
        "required": true
      },
      "options.value_columns": {
        "type": "select",
        "label": "Значение столбца",
        "table": "target_table.table_id",
        "bind": "COLUMNS",
        "multiple": true,
        "required": true
      },
      "options.mapping_column": {
        "type": "select",
        "label": "Сопоставление таблицы",
        "table": "target_table.table_id",
        "bind": "COLUMNS",
        "breakLine": true
      },
      "options.autofill_empty_cells": {
        "type": "checkbox",
        "label": "Автозаполнение пустых ячеек",
        "val": "true",
        "isOn": "true",
        "isOff": "false"
      },
      "options.operation": {
        "type": "select",
        "label": "Операция",
        "show_if": "options.autofill_empty_cells=='false'",
        "required": true,
        "options": {
          "replace": {
            "label": "Заменить существующее содержание"
          },
          "add": {
            "label": "Добавить в существующее содержимое"
          },
          "average": {
            "label": "Среднее значение с существующим содержимым"
          }
        },
        "val": "replace",
        "breakLine": true
      },
      "options.use_filter": {
        "type": "checkbox",
        "label": "Использовать фильтр",
        "val": "false",
        "isOn": "true",
        "isOff": "false",
        "breakLine": true
      },
      "filter_box": {
        "type": "fieldset",
        "label": "Настройки фильтра",
        "show_if": "options.use_filter=='true'",
        "repeat": false,
        "fields": {
          "options.filter": {
            "type": "fieldset",
            "label": "Добавить условия фильтрации",
            "repeat": true,
            "fields": {
              "initial_op": {
                "type": "select",
                "label": "Выбрать оператора логики",
                "required": true,
                "show_if": "$index == 0",
                "options": {
                  "if": {
                    "label": "ЕСЛИ"
                  },
                  "if_not": {
                    "label": "ЕСЛИ НЕ"
                  }
                },
                "val": "if"
              },
              "op": {
                "type": "select",
                "label": "Выбрать оператора логики",
                "required": true,
                "show_if": "$index > 0",
                "options": {
                  "and": {
                    "label": "И"
                  },
                  "or": {
                    "label": "ИЛИ"
                  },
                  "and_not": {
                    "label": "И НЕ"
                  },
                  "or_not": {
                    "label": "ИЛИ НЕ"
                  }
                },
                "val": "and"
              },
              "column": {
                "type": "select",
                "label": "Выбрать столбец",
                "table": "target_table.table_id",
                "bind": "COLUMNS",
                "required": true
              },
              "condition": {
                "type": "select",
                "label": "Выбрать оператора",
                "required": true,
                "options": {
                  "CONTAINS": {
                    "label": "СОДЕРЖИТ"
                  },
                  "CONTAINED IN STRING": {
                    "label": "СОДЕРЖАЛОСЬ В"
                  },
                  "STARTS WITH": {
                    "label": "НАЧИНАЕТСЯ С"
                  },
                  "ENDS WITH": {
                    "label": "ЗАКАНЧИВАЕТСЯ С"
                  },
                  "=": {
                    "label": "Равный"
                  },
                  ">": {
                    "label": "БОЛЬШЕ ЧЕМ"
                  },
                  ">=": {
                    "label": "БОЛЬШИЙ ИЛИ РАВНЫЙ"
                  },
                  "<": {
                    "label": "МЕНЬШЕ ЧЕМ"
                  },
                  "<=": {
                    "label": "МЕНЬШИЙ ИЛИ РАВНЫЙ"
                  },
                  "EMPTY": {
                    "label": "ПУСТО"
                  },
                  "BLANK": {
                    "label": "ПУСТО"
                  },
                  "IS NULL": {
                    "label": "НОЛЬ"
                  },
                  "IS TRUE": {
                    "label": "IS TRUE"
                  },
                  "IS FALSE": {
                    "label": "IS FALSE"
                  },
                  "REGEX MATCH": {
                    "label": "Совпадение регулярного выражения"
                  },
                  "CONTAINED IN TABLE": {
                    "label": "В ТАБЛИЦЕ"
                  },
                  "IN REGEX TABLE": {
                    "label": "В ТАБЛИЦЕ РЕГУЛЯРНЫХ ВЫРАЖЕНИЙ"
                  },
                  "DATE": {
                    "label": "ДАТА"
                  }
                },
                "val": "CONTAINS"
              },
              "date_condition": {
                "type": "select",
                "label": "Выбрать оператора даты",
                "required": true,
                "show_if": "this.condition == 'DATE'",
                "options": {
                  "<": {
                    "label": "ПРЕДЫДУЩИЙ"
                  },
                  "<=": {
                    "label": "ПРЕДЫДУЩИЙ ИЛИ РАВНЫЙ"
                  },
                  "=": {
                    "label": "Равный"
                  },
                  ">": {
                    "label": "ПОСЛЕ"
                  },
                  ">=": {
                    "label": "ПОСЛЕ ИЛИ РАВНО"
                  }
                },
                "val": "="
              },
              "compare_with": {
                "type": "select",
                "label": "Compare with",
                "required": true,
                "show_if": "this.condition=='CONTAINS' || this.condition=='CONTAINED IN STRING' || this.condition=='STARTS WITH' || this.condition=='ENDS WITH' || this.condition=='=' || this.condition=='>' || this.condition=='>=' || this.condition=='<' || this.condition=='<='",
                "options": {
                  "custom_value": {
                    "label": "Custom value"
                  },
                  "column_value": {
                    "label": "Column value"
                  }
                },
                "val": "custom_value"
              },
              "self_compare_column": {
                "type": "select",
                "label": "Select Compare Column",
                "table": "table_id",
                "bind": "COLUMNS",
                "show_if": "this.compare_with=='column_value' && (this.condition=='CONTAINS' || this.condition=='CONTAINED IN STRING' || this.condition=='STARTS WITH' || this.condition=='ENDS WITH' || this.condition=='=' || this.condition=='>' || this.condition=='>=' || this.condition=='<' || this.condition=='<=')",
                "required": true
              },
              "date_value": {
                "type": "date",
                "label": "Выбрать дату",
                "show_if": "this.condition=='DATE'",
                "val": "",
                "required": true
              },
              "value": {
                "type": "text",
                "label": "Добавить значение",
                "show_if": "this.compare_with=='custom_value' && this.condition != 'DATE' && this.condition != 'EMPTY' && this.condition != 'NOT EMPTY' && this.condition != 'BLANK' && this.condition != 'NOT BLANK' && this.condition != 'IS NULL' && this.condition != 'IS NOT NULL' && this.condition != 'IS TRUE' && this.condition != 'IS FALSE' && this.condition != 'CONTAINED IN TABLE' && this.condition != 'NOT CONTAINED IN TABLE' && this.condition != 'IN REGEX TABLE' && this.condition != 'NOT IN REGEX TABLE'",
                "val": "",
                "required": true
              },
              "case_sensitive": {
                "type": "select",
                "label": "Выбрать с учетом регистра",
                "required": true,
                "show_if": "this.condition == 'CONTAINS' || this.condition == 'CONTAINED IN STRING' || this.condition == '=' || this.condition == 'STARTS WITH' || this.condition == 'ENDS WITH' || this.condition == 'REGEX MATCH' || this.condition == 'NOT REGEX MATCH' || this.condition == 'IN REGEX TABLE' || this.condition == 'NOT IN REGEX TABLE'",
                "options": {
                  "yes": {
                    "label": "С учетом регистра"
                  },
                  "no": {
                    "label": "Без учета регистра"
                  }
                },
                "val": "yes"
              },
              "compare_table": {
                "type": "select",
                "label": "Выбрать таблицу",
                "bind": "TABLES",
                "empty": "",
                "show_if": "this.condition == 'CONTAINED IN TABLE' || this.condition == 'NOT CONTAINED IN TABLE' || this.condition == 'IN REGEX TABLE' || this.condition == 'NOT IN REGEX TABLE'",
                "required": true
              },
              "compare_column": {
                "type": "select",
                "label": "Выбрать столбец",
                "table": "this.compare_table",
                "bind": "COLUMNS",
                "show_if": "this.condition == 'CONTAINED IN TABLE' || this.condition == 'NOT CONTAINED IN TABLE' || this.condition == 'IN REGEX TABLE' || this.condition == 'NOT IN REGEX TABLE'",
                "required": true
              },
              "delete": {
                "type": "button",
                "label": "Удалить",
                "function": "DELETE_ROW"
              }
            }
          },
          "options.apply_to_record": {
            "type": "checkbox",
            "label": "Применить к регистру",
            "val": "true",
            "isOn": "true",
            "isOff": "false",
            "style": "margin-top: 10px",
            "breakLine": true
          },
          "apply_to_group_box": {
            "type": "fieldset",
            "label": "Применить к группе",
            "show_if": "options.apply_to_record=='false'",
            "repeat": false,
            "fields": {
              "options.apply_to_group_columns": {
                "type": "select",
                "label": "Выбрать групповые столбцы",
                "required": true,
                "width": "100%",
                "table": "target_table.table_id",
                "multiple": true,
                "bind": "COLUMNS",
                "breakLine": true
              }
            }
          }
        }
      }
    }
  }
}