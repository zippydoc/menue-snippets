          "keep_current": {
            "label": "Обновлять"
          },
          "asc": {
            "label": "Восходящий"
          },
          "desc": {
            "label": "Поступающий"
          }
        },
        "val": "keep_current"
      },
      "equal_intervals_order": {
        "type": "select",
        "label": "Заказать",
        "show_if": "binning_type=='equal_intervals'",
        "options": {
          "asc": {
            "label": "Восходящий"
          },
          "desc": {
            "label": "Поступающий"
          }
        },
        "val": "asc",
        "breakLine": true
      }
    },
    "breakLine": true
  }
