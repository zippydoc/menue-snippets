{
  "options": {
    "type": "fieldset",
    "label": "Выбрать параметры очистки",
    "repeat": false,
    "fields": {
      "replace_null": {
        "type": "fieldset",
        "label": "Пустые записи",
        "repeat": false,
        "fields": {
          "replace_null": {
            "type": "checkbox",
            "label": "Пустые записи",
            "val": "false",
            "isOn": "true",
            "isOff": "false",
            "breakLine": true
          },
          "replace_null_target": {
            "type": "select",
            "label": "Заменить",
            "show_if": "replace_null=='true'",
            "options": {
              "empty": {
                "label": "Пусто"
              },
              "blank": {
                "label": "Пусто"
              },
              "null": {
                "label": "НОЛЬ"
              }
            },
            "val": "empty",
            "breakLine": true
          },
          "replace_null_by": {
            "type": "radio",
            "show_if": "replace_null=='true'",
            "val": "by_blank",
            "values": {
              "by_blank": "By Blank",
              "by_zero": "By Zero"
            },
            "breakLine": true
          }
        }
      },
      "remove_whitespace": {
        "type": "fieldset",
        "label": "Удалить пробелы",
        "repeat": false,
        "fields": {
          "remove_characters.all_whitespace": {
            "type": "checkbox",
            "label": "Все пробелы",
            "val": "false",
            "isOn": "true",
            "isOff": "false",
            "breakLine": true
          },
          "remove_characters.leading_whitespace": {
            "type": "checkbox",
            "label": "Ведущий пробел",
            "show_if": "remove_characters.all_whitespace=='false'",
            "val": "false",
            "isOn": "true",
            "isOff": "false",
            "breakLine": true
          },
          "remove_characters.trailing_whitespace": {
            "type": "checkbox",
            "label": "Конечный пробел",
            "show_if": "remove_characters.all_whitespace=='false'",
            "val": "false",
            "isOn": "true",
            "isOff": "false",
            "breakLine": true
          },
          "remove_characters.whitespace": {
            "type": "checkbox",
            "label": "Пробел",
            "show_if": "remove_characters.all_whitespace=='false'",
            "val": "false",
            "isOn": "true",
            "isOff": "false",
            "breakLine": true
          },
          "remove_characters.tabs": {
            "type": "checkbox",
            "label": "Вкладки",
            "show_if": "remove_characters.all_whitespace=='false'",
            "val": "false",
            "isOn": "true",
            "isOff": "false",
            "breakLine": true
          },
          "remove_characters.line_breaks": {
            "type": "checkbox",
            "label": "Разрывы строк",
            "show_if": "remove_characters.all_whitespace=='false'",
            "val": "false",
            "isOn": "true",
            "isOff": "false",
            "breakLine": true
          },
          "remove_characters.duplicate_whitespace": {
            "type": "checkbox",
            "label": "Дублировать пробел",
            "show_if": "remove_characters.all_whitespace=='false'",
            "val": "false",
            "isOn": "true",
            "isOff": "false",
            "breakLine": true
          }
        }
      },
      "remove_duplicates": {
        "type": "fieldset",
        "label": "Удалить дубликаты",
        "repeat": false,
        "fields": {
          "remove_duplicates.cell": {
            "type": "checkbox",
            "label": "Дублировать в ячейке",
            "val": "false",
            "isOn": "true",
            "isOff": "false",
            "breakLine": true
          },
          "remove_duplicates.delimiter": {
            "type": "text",
            "label": "Разделитель",
            "show_if": "remove_duplicates.cell=='true'",
            "val": ";",
            "required": true
          },
          "remove_duplicates.include_whitespace": {
            "type": "checkbox",
            "label": "(включить пробелы)",
            "show_if": "remove_duplicates.cell=='true'",
            "val": "true",
            "isOn": "true",
            "isOff": "false",
            "breakLine": true
          },
          "remove_duplicates.rows": {
            "type": "checkbox",
            "label": "Дублировать строки",
            "val": "false",
            "isOn": "true",
            "isOff": "false",
            "breakLine": true
          }
        }
      },
      "remove_characters": {
        "type": "fieldset",
        "label": "Удалить символы",
        "repeat": false,
        "fields": {
          "remove_characters.letters": {
            "type": "checkbox",
            "label": "Письма",
            "val": "false",
            "isOn": "true",
            "isOff": "false",
            "breakLine": true
          },
          "remove_characters.numbers": {
            "type": "checkbox",
            "label": "Числа",
            "val": "false",
            "isOn": "true",
            "isOff": "false",
            "breakLine": true
          },
          "remove_characters.punctuation": {
            "type": "checkbox",
            "label": "Пунктуация",
            "val": "false",
            "isOn": "true",
            "isOff": "false",
            "breakLine": true
          },
          "remove_characters.trailing_zero": {
            "type": "checkbox",
            "label": "Trailing 0",
            "val": "false",
            "isOn": "true",
            "isOff": "false",
            "breakLine": true
          },
          "remove_characters.custom": {
            "type": "checkbox",
            "label": "To-be-delted Characters",
            "val": "false",
            "isOn": "true",
            "isOff": "false",
            "breakLine": true
          },
          "remove_characters.custom_chars": {
            "type": "text",
            "label": "to be deleted characters",
            "show_if": "remove_characters.custom=='true'",
            "val": "",
            "required": true
          },
          "remove_characters.custom_mode": {
            "type": "select",
            "label": "Позиция",
            "show_if": "remove_characters.custom=='true'",
            "options": {
              "all": {
                "label": "all"
              },
              "leading only": {
                "label": "leading only"
              },
              "trailing only": {
                "label": "trailing only"
              }
            },
            "val": "all"
          },
          "remove_characters.custom_delimiter": {
            "type": "select",
            "label": "Разделитель",
            "show_if": "remove_characters.custom=='true'",
            "options": {
              ",": {
                "label": ","
              },
              ";": {
                "label": ";"
              },
              ".": {
                "label": "."
              }
            },
            "val": ","
          }
        }
      },
      "modify_case": {
        "type": "fieldset",
        "label": "Изменить регистр",
        "repeat": false,
        "fields": {
          "modify_case": {
            "type": "checkbox",
            "label": "Изменить регистр",
            "val": "false",
            "isOn": "true",
            "isOff": "false",
            "breakLine": true
          },
          "modify_case_by": {
            "type": "radio",
            "show_if": "modify_case=='true'",
            "val": "upper",
            "values": {
              "upper": "to Upper Case",
              "lower": "to Lower Case",
              "title": "to Title Case"
            },
            "breakLine": true
          }
        }
      },
      "rounding": {
        "type": "fieldset",
        "label": "Округление",
        "repeat": false,
        "fields": {
          "is_rounding": {
            "type": "checkbox",
            "label": "Округление",
            "val": "false",
            "isOn": "true",
            "isOff": "false",
            "breakLine": true
          },
          "rounding": {
            "type": "select",
            "label": "Округление",
            "show_if": "is_rounding=='true'",
            "required": true,
            "options": {
              "no_rounding": {
                "label": "Округление чисел"
              },
              "0": {
                "label": "0 цифр"
              },
              "1": {
                "label": "1 цифра"
              },
              "2": {
                "label": "2 цифры"
              },
              "3": {
                "label": "3 цифры"
              },
              "4": {
                "label": "4 цифры"
              },
              "5": {
                "label": "5 цифр"
              },
              "6": {
                "label": "6 цифр"
              },
              "7": {
                "label": "7 цифр"
              },
              "8": {
                "label": "8 цифр"
              },
              "9": {
                "label": "9 цифр"
              }
            },
            "val": "no_rounding"
          }
        }
      },
      "options.use_filter": {
        "type": "checkbox",
        "label": "Использовать фильтр",
        "val": "false",
        "isOn": "true",
        "isOff": "false",
        "style": "margin-top: 10px",
        "breakLine": true
      },
      "filter_box": {
        "type": "fieldset",
        "label": "Настройки фильтра",
        "show_if": "options.use_filter=='true'",
        "repeat": false,
        "fields": {
          "options.filter": {
            "type": "fieldset",
            "label": "Добавить условия фильтрации",
            "repeat": true,
            "fields": {
              "initial_op": {
                "type": "select",
                "label": "Выбрать оператора логики",
                "required": true,
                "show_if" :  "$index == 0",
                "options": {
                  "if": {
                    "label": "ЕСЛИ"
                  },
                  "if_not": {
                    "label": "ЕСЛИ НЕ"
                  }
                },
                "val": "if"
              },
              "op": {
                "type": "select",
                "label": "Выбрать оператора логики",
                "required": true,
                "show_if" :  "$index > 0",
                "options": {
                  "and": {
                    "label": "И"
                  },
                  "or": {
                    "label": "ИЛИ"
                  },
                  "and_not": {
                    "label": "И НЕ"
                  },
                  "or_not": {
                    "label": "ИЛИ НЕ"
                  }
                },
                "val": "and"
              },
              "column": {
                "type": "select",
                "label": "Выбрать столбец",
                "table": "table_id",
                "bind": "COLUMNS",
                "required": true
              },
              "condition": {
                "type": "select",
                "label": "Выбрать оператора",
                "required": true,
                "options": {
                  "CONTAINS": {
                    "label": "СОДЕРЖИТ"
                  },
                  "CONTAINED IN STRING": {
                    "label": "СОДЕРЖАЛОСЬ В"
                  },
                  "STARTS WITH": {
                    "label": "НАЧИНАЕТСЯ С"
                  },
                  "ENDS WITH": {
                    "label": "ЗАКАНЧИВАЕТСЯ С"
                  },
                  "=": {
                    "label": "Равный"
                  },
                  ">": {
                    "label": "БОЛЬШЕ ЧЕМ"
                  },
                  ">=": {
                    "label": "БОЛЬШИЙ ИЛИ РАВНЫЙ"
                  },
                  "<": {
                    "label": "МЕНЬШЕ ЧЕМ"
                  },
                  "<=": {
                    "label": "МЕНЬШИЙ ИЛИ РАВНЫЙ"
                  },
                  "EMPTY": {
                    "label": "ПУСТО"
                  },
                  "BLANK": {
                    "label": "ПУСТО"
                  },
                  "IS NULL": {
                    "label": "НОЛЬ"
                  },
                  "IS TRUE": {
                    "label": "IS TRUE"
                  },
                  "IS FALSE": {
                    "label": "IS FALSE"
                  },
                  "REGEX MATCH": {
                    "label": "Совпадение регулярного выражения"
                  },
                  "CONTAINED IN TABLE": {
                    "label": "В ТАБЛИЦЕ"
                  },
                  "IN REGEX TABLE": {
                    "label": "В ТАБЛИЦЕ РЕГУЛЯРНЫХ ВЫРАЖЕНИЙ"
                  },
                  "DATE": {
                    "label": "ДАТА"
                  }
                },
                "val": "CONTAINS"
              },
              "date_condition": {
                "type": "select",
                "label": "Выбрать оператора даты",
                "required": true,
                "show_if": "this.condition == 'DATE'",
                "options": {
                  "<": {
                    "label": "ПРЕДЫДУЩИЙ"
                  },
                  "<=": {
                    "label": "ПРЕДЫДУЩИЙ ИЛИ РАВНЫЙ"
                  },
                  "=": {
                    "label": "Равный"
                  },
                  ">": {
                    "label": "ПОСЛЕ"
                  },
                  ">=": {
                    "label": "ПОСЛЕ ИЛИ РАВНО"
                  }
                },
                "val": "="
              },
              "compare_with": {
                "type": "select",
                "label": "Compare with",
                "required": true,
                "show_if": "this.condition=='CONTAINS' || this.condition=='CONTAINED IN STRING' || this.condition=='STARTS WITH' || this.condition=='ENDS WITH' || this.condition=='=' || this.condition=='>' || this.condition=='>=' || this.condition=='<' || this.condition=='<='",
                "options": {
                  "custom_value": {
                    "label": "Custom value"
                  },
                  "column_value": {
                    "label": "Column value"
                  }
                },
                "val": "custom_value"
              },
              "self_compare_column": {
                "type": "select",
                "label": "Select Compare Column",
                "table": "table_id",
                "bind": "COLUMNS",
                "show_if": "this.compare_with=='column_value' && (this.condition=='CONTAINS' || this.condition=='CONTAINED IN STRING' || this.condition=='STARTS WITH' || this.condition=='ENDS WITH' || this.condition=='=' || this.condition=='>' || this.condition=='>=' || this.condition=='<' || this.condition=='<=')",
                "required": true
              },
              "date_value": {
                "type": "date",
                "label": "Выбрать дату",
                "show_if": "this.condition=='DATE'",
                "val": "",
                "required": true
              },
              "value": {
                "type": "text",
                "label": "Добавить значение",
                "show_if": "this.compare_with=='custom_value' && this.condition != 'DATE' && this.condition != 'EMPTY' && this.condition != 'NOT EMPTY' && this.condition != 'BLANK' && this.condition != 'NOT BLANK' && this.condition != 'IS NULL' && this.condition != 'IS NOT NULL' && this.condition != 'IS TRUE' && this.condition != 'IS FALSE' && this.condition != 'CONTAINED IN TABLE' && this.condition != 'NOT CONTAINED IN TABLE' && this.condition != 'IN REGEX TABLE' && this.condition != 'NOT IN REGEX TABLE'",
                "val": "",
                "required": true
              },
              "case_sensitive": {
                "type": "select",
                "label": "Выбрать с учетом регистра",
                "required": true,
                "show_if": "this.condition == 'CONTAINS' || this.condition == 'CONTAINED IN STRING' || this.condition == '=' || this.condition == 'STARTS WITH' || this.condition == 'ENDS WITH' || this.condition == 'REGEX MATCH' || this.condition == 'NOT REGEX MATCH' || this.condition == 'IN REGEX TABLE' || this.condition == 'NOT IN REGEX TABLE'",
                "options": {
                  "yes": {
                    "label": "С учетом регистра"
                  },
                  "no": {
                    "label": "Без учета регистра"
                  }
                },
                "val": "yes"
              },
              "compare_table": {
                "type": "select",
                "label": "Выбрать таблицу",
                "bind": "TABLES",
                "empty": "",
                "show_if": "this.condition == 'CONTAINED IN TABLE' || this.condition == 'NOT CONTAINED IN TABLE' || this.condition == 'IN REGEX TABLE' || this.condition == 'NOT IN REGEX TABLE'",
                "required": true
              },
              "compare_column": {
                "type": "select",
                "label": "Выбрать столбец",
                "table": "this.compare_table",
                "bind": "COLUMNS",
                "show_if": "this.condition == 'CONTAINED IN TABLE' || this.condition == 'NOT CONTAINED IN TABLE' || this.condition == 'IN REGEX TABLE' || this.condition == 'NOT IN REGEX TABLE'",
                "required": true
              },
              "delete": {
                "type": "button",
                "label": "Удалить",
                "function": "DELETE_ROW"
              }
            }
          },
          "options.apply_to_record": {
            "type": "checkbox",
            "label": "Применить к регистру",
            "val": "true",
            "isOn": "true",
            "isOff": "false",
            "style": "margin-top: 10px",
            "breakLine": true
          },
          "apply_to_group_box": {
            "type": "fieldset",
            "label": "Применить к группе",
            "show_if": "options.apply_to_record=='false'",
            "repeat": false,
            "fields": {
              "options.apply_to_group_columns": {
                "type": "select",
                "label": "Выбрать групповые столбцы",
                "required": true,
                "width": "100%",
                "table": "table_id",
                "multiple": true,
                "bind": "COLUMNS",
                "breakLine": true
              }
            }
          }
        }
      }
    }
  }
}