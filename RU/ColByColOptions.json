          "add_rows_below_selected_row": {
            "Метка": "Добавьте строки ниже"
          },
          "add_rows_above_selected_row": {
            "Метка": "Добавьте строки выше"
          },
          "add_rows_at_table_end": {
            "Метка": "Добавить строки в конце таблицы"
          },
          "add_single_row_at_table_end": {
            "Метка": "Добавить одну строку в конце таблицы"
          },
          "add_row_after_each_group": {
            "Метка": "Добавлять строку после каждой группы"
          }
        },
        "val": "add_rows_below_selected_row",
        "breakLine": true
      },
      "options.group_column": {
        "type": "select",
        "Метка": "Групповой столбец",
        "show_if": "table_type=='header_only' && options.position=='add_row_after_each_group'",
        "table": "input_table",
        "bind": "COLUMNS",
        "required": true,
        "breakLine": true
      },
      "options.use_filter": {
        "type": "checkbox",
