{
  "input": {
    "type": "fieldset",
    "label": "Consolidate Options",
    "repeat": true,
    "fields": {
      "new_position": {
        "type": "text",
        "label": "New Position",
        "val": "",
        "placeholder": "Текст",
        "required": true
      },
      "input_table": {
        "type": "select",
        "label": "Выбрать таблицу",
        "bind": "TABLES",
        "empty": "",
        "required": true,
        "breakLine": true
      },
      "multiple_selection": {
        "type": "select",
        "label": "Multiple Selection",
        "required": true,
        "table": "this.input_table",
        "multiple": true,
        "bind": "LABELS",
        "breakLine": true
      },
      "format__category": {
        "type": "select",
        "label": "Формат",
        "required": true,
        "options": {
          "standard": {
            "label": "Стандартный"
          },
          "number": {
            "label": "Число"
          },
          "percentage": {
            "label": "Процент"
          },
          "text": {
            "label": "Текст"
          },
          "date": {
            "label": "Дата"
          },
          "currency": {
            "label": "Валюта"
          }
        },
        "val": "standard"
      },
      "format__rounding": {
        "type": "select",
        "label": "Округление",
        "show_if": "this.format__category=='number' || this.format__category=='percentage' || this.format__category=='currency'",
        "required": true,
        "options": {
          "no_rounding": {
            "label": "Округление чисел"
          },
          "0": {
            "label": "0 цифр"
          },
          "1": {
            "label": "1 цифра"
          },
          "2": {
            "label": "2 цифры"
          },
          "3": {
            "label": "3 цифры"
          },
          "4": {
            "label": "4 цифры"
          },
          "5": {
            "label": "5 цифр"
          },
          "6": {
            "label": "6 цифр"
          },
          "7": {
            "label": "7 цифр"
          },
          "8": {
            "label": "8 цифр"
          },
          "9": {
            "label": "9 цифр"
          }
        },
        "val": "2"
      },
      "format__use_1000_separator": {
        "type": "checkbox",
        "label": "Use 1000 Separator(.)",
        "show_if": "this.format__category=='number' || this.format__category=='percentage' || this.format__category=='currency'",
        "val": "true",
        "isOn": "true",
        "isOff": "false"
      },
      "format__currency_symbol": {
        "type": "select",
        "label": "Символ",
        "show_if": "this.format__category=='currency'",
        "required": true,
        "options": {
          "dollar": {
            "label": "$"
          },
          "euro": {
            "label": "€"
          },
          "pound": {
            "label": "£"
          }
        },
        "val": "dollar"
      },
      "format__date_format": {
        "type": "select",
        "label": "Тип",
        "show_if": "this.format__category=='date'",
        "required": true,
        "options": {
          "mm/dd/yyyy": {
            "label": "3/14/2012"
          },
          "D, M dd, Y": {
            "label": "Среда, 14 марта 2012 года"
          },
          "yyyy-mm-dd": {
            "label": "2012-03-14"
          }
        },
        "val": "mm/dd/yyyy",
        "breakLine": true
      }
    }
  }
}