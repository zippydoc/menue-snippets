{
  "input": {
    "type": "fieldset",
    "label": "Консолидировать варианты",
    "repeat": true,
    "fields": {
      "new_position": {
        "type": "text",
        "label": "New Position",
        "val": "",
        "placeholder": "Text",
        "required": true,
        "breakLine": true
      },
      "table_id": {
        "type": "select",
        "label": "Выбрать таблицу",
        "bind": "TABLES",
        "empty": "",
        "required": true
      },
      "rows": {
        "type": "select",
        "label": "Row Selection",
        "required": true,
        "table": "this.table_id",
        "multiple": true,
        "bind": "LABELS"
      },
      "multiplier": {
        "type": "select",
        "label": "Multiplier",
        "required": true,
        "options": {
          "positive": {
            "label": "Keep sign"
          },
          "negative": {
            "label": "Switch sign"
          }
        },
        "val": "positive",
        "breakLine": true
      },
      "format__category": {
        "type": "select",
        "label": "Формат дисплея",
        "required": true,
        "options": {
          "standard": {
            "label": "Стандартный"
          },
          "number": {
            "label": "Число"
          },
          "percentage": {
            "label": "Процент"
          },
          "text": {
            "label": "Текст"
          },
          "date": {
            "label": "Дата"
          },
          "currency": {
            "label": "Валюта"
          }
        },
        "val": "standard"
      },
      "format__rounding": {
        "type": "select",
        "label": "Десятичные знаки для отображения",
        "show_if": "this.format__category=='number' || this.format__category=='percentage' || this.format__category=='currency'",
        "required": true,
        "options": {
          "no_rounding": {
            "label": "Округление чисел"
          },
          "0": {
            "label": "0 цифр"
          },
          "1": {
            "label": "1 цифра"
          },
          "2": {
            "label": "2 цифры"
          },
          "3": {
            "label": "3 цифры"
          },
          "4": {
            "label": "4 цифры"
          },
          "5": {
            "label": "5 цифр"
          },
          "6": {
            "label": "6 цифр"
          },
          "7": {
            "label": "7 цифр"
          },
          "8": {
            "label": "8 цифр"
          },
          "9": {
            "label": "9 цифр"
          }
        },
        "val": "2"
      },
      "format__use_1000_separator": {
        "type": "checkbox",
        "label": "Use 1000 Separator(.)",
        "show_if": "this.format__category=='number' || this.format__category=='percentage' || this.format__category=='currency'",
        "val": "true",
        "isOn": "true",
        "isOff": "false"
      },
      "format__currency_symbol": {
        "type": "select",
        "label": "Символ",
        "show_if": "this.format__category=='currency'",
        "required": true,
        "options": {
          "dollar": {
            "label": "$"
          },
          "euro": {
            "label": "€"
          },
          "pound": {
            "label": "£"
          }
        },
        "val": "dollar"
      },
      "format__date_format": {
        "type": "select",
        "label": "Тип",
        "show_if": "this.format__category=='date'",
        "required": true,
        "options": {
          "mm/dd/yyyy": {
            "label": "3/14/2012"
          },
          "D, M dd, Y": {
            "label": "Среда, 14 марта 2012 года"
          },
          "yyyy-mm-dd": {
            "label": "2012-03-14"
          },
          "m.dd.yyyy": {
            "label": "3.14.2012"
          },
          "m.dd.yy": {
            "label": "3.14.12"
          },
          "dd.m.yyyy": {
            "label": "14.3.2012"
          },
          "dd.m.yy": {
            "label": "14.3.12"
          },
          "M dd, Y": {
            "label": "March 14, 2012"
          },
          "dd-mm-yyyy": {
            "label": "14-03-2012"
          },
          "mm-dd-yyyy": {
            "label": "03-14-2012"
          },
          "dd-mm-yy": {
            "label": "14-03-12"
          },
          "mm-dd-yy": {
            "label": "03-14-12"
          }
        },
        "val": "mm/dd/yyyy",
        "breakLine": true
      },
      "delete": {
        "type": "button",
        "label": "Удалить",
        "function": "DELETE_ROW"
      }
    }
  }
}