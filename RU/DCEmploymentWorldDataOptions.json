    "label": "Опции",
    "repeat": false,
    "fields": {
      "options.group_by": {
        "type": "select",
        "label": "Группировать по",
        "required": true,
        "options": {
          "Region": {
            "label": "Region"
          },
          "Income Group": {
            "label": "Income Group"
          }
        },
        "val": "Region",
        "breakLine": true
      },
      "options.stat_var_name": {
        "type": "select",
        "label": "Statistics Data",
        "required": true,
        "options": {
          "UnemploymentRate_Person": {
            "label": "Unemployment Rate"
          },
          "Count_Person_InLaborForce": {
            "label": "In Labor Force Population"
          },
          "Count_Person_15To64Years_InLaborForce_AsFractionOf_Count_Person_15To64Years": {
            "label": "15 - 64 года, в составе рабочей силы Население на душу населения 15 - 64 года"
          },
          "Count_Person_15To64Years_Female_InLaborForce_AsFractionOf_Count_Person_15To64Years_Female": {
            "label": "15 - 64 года, в рабочей силе, женское население на душу населения 15 - 64 года женщины"
          },
          "Count_Person_15To64Years_Male_InLaborForce_AsFractionOf_Count_Person_15To64Years_Male": {
            "label": "15 - 64 года, в рабочей силе, мужское население на душу населения 15 - 64 года мужчины"
          },
          "Count_Person_15OrMoreYears_InLaborForce_Female_AsFractionOf_Count_Person_InLaborForce": {
            "label": "15 лет и более, в рабочей силе, женское население на душу населения в рабочей силе"
          }
        },
        "val": "UnemploymentRate_Person"
      },
      "options.per_capita": {
        "type": "checkbox",
        "label": "Per capita",
        "show_if": "false",
        "val": "false",
        "isOn": "true",
        "isOff": "false",
        "breakLine": true
      },
      "options.all_countries": {
        "type": "checkbox",
        "label": "Все страны",
        "val": "true",
        "isOn": "true",
        "isOff": "false",
        "breakLine": true
      },
      "options.output_as_chart": {
        "type": "checkbox",
        "label": "Output a Graph",
        "val": "true",
        "isOn": "true",
        "isOff": "false"
      },
      "options.graph_type": {
        "type": "select",
        "label": "Graph Type",
        "show_if": "options.output_as_chart=='true'",
        "required": true,
        "options": {
          "zoomable_sunburst": {
            "label": "Sunburst"
          },
          "treemap": {
            "label": "Treemap"
          }
        },
        "val": "zoomable_sunburst",
        "breakLine": true
      }
