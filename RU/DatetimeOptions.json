{
  "options": {
    "type": "fieldset",
    "Метка": "Выбрать параметры",
    "repeat": false,
    "fields": {
      "options.convert_type": {
        "type": "select",
        "Метка": "Тип конвертации",
        "required": true,
        "options": {
          "complete_date": {
            "Метка": "Полная дата"
          },
          "expand_date": {
            "Метка": "Расширить дату"
          },
          "convert_date": {
            "Метка": "Конвертировать дату"
          },
          "convert_date_range": {
            "Метка": "Конвертировать диапазон дат"
          }
        },
        "val": "complete_date"
      },
      "options.date_convert_type": {
        "type": "select",
        "Метка": "Конвертировать в",
        "show_if": "options.convert_type=='convert_date'",
        "required": true,
        "options": {
          "begin_of_month": {
            "Метка": "Начало месяца"
          },
          "end_of_month": {
            "Метка": "Конец месяца"
          },
          "hy_year": {
            "Метка": "Полгода"
          },
          "qrt_year": {
            "Метка": "Квартальный год"
          },
          "month_year": {
            "Метка": "Месяц Год"
          },
          "week_year": {
            "Метка": "Неделя Год"
          }
        },
        "val": "begin_of_month"
      },
      "options.date_range_convert_type": {
        "type": "select",
        "Метка": "Конвертировать в",
        "show_if": "options.convert_type=='convert_date_range'",
        "required": true,
        "options": {
          "begin_of_month": {
            "Метка": "Начало месяца"
          },
          "end_of_month": {
            "Метка": "Конец месяца"
          },
          "begin_of_range": {
            "Метка": "Начало диапазона"
          },
          "end_of_range": {
            "Метка": "Конец диапазона"
          },
          "begin_of_quarter": {
            "Метка": "Начало квартала"
          },
          "end_of_quarter": {
            "Метка": "Конец квартала"
          }
        },
        "val": "begin_of_month",
        "breakLine": true
      },
      "options.output_in_new_column": {
        "type": "checkbox",
        "Метка": "Выход в новом столбце",
        "val": "false",
        "isOn": "true",
        "isOff": "false"
      },
      "options.new_field_name": {
        "type": "text",
        "Метка": "Название нового столбца",
        "show_if": "options.output_in_new_column=='true'",
        "val": "GeneratedDate",
        "required": true,
        "minLength": 1,
        "maxLength": 255,
        "breakLine": true
      }
    }
  }
}