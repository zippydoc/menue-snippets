{
  "placeholder": {
    "breakLine": true
  },
  "delete_options": {
    "type": "select",
    "label": "Удалить параметры",
    "required": true,
    "options": {
      "del_table": {
        "label": "Удалить таблицу"
      },
      "del_columns": {
        "label": "Удалить выбранные столбцы"
      },
      "del_rows": {
        "label": "Удалить выделенные строки"
      }
    },
    "val": "del_table"
  },
  "apply_columns": {
    "type": "select",
    "label": "Выберите столбцы для удаления",
    "show_if": "delete_options=='del_columns'",
    "required": true,
    "table": "table_id",
    "multiple": true,
    "bind": "COLUMNS"
  },
  "apply_rows": {
    "type": "select",
    "label": "Выбрать строки для удаления",
    "show_if": "delete_options=='del_rows'",
    "required": true,
    "table": "table_id",
    "multiple": true,
    "bind": "LABELS"
  }
}