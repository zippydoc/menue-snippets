{
  "placeholder": {
    "breakLine": true
  },
  "delete_options": {
    "type": "select",
    "Метка": "Удалить параметры",
    "required": true,
    "options": {
      "del_table": {
        "Метка": "Удалить таблицу"
      },
      "del_columns": {
        "Метка": "Удалить выбранные столбцы"
      },
      "del_rows": {
        "Метка": "Удалить выделенные строки"
      }
    },
    "val": "del_table"
  },
  "apply_columns": {
    "type": "select",
    "Метка": "Выберите столбцы для удаления",
    "show_if": "delete_options=='del_columns'",
    "required": true,
    "table": "input_table",
    "multiple": true,
    "bind": "COLUMNS"
  },
  "apply_rows": {
    "type": "select",
    "Метка": "Выбрать строки для удаления",
    "show_if": "delete_options=='del_rows'",
    "required": true,
    "table": "input_table",
    "multiple": true,
    "bind": "LABELS"
  }
}