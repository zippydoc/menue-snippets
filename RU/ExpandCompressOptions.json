{
  "options": {
    "type": "fieldset",
    "Метка": "Выбрать параметры",
    "repeat": false,
    "fields": {
      "options.type": {
        "type": "radio",
        "val": "expand",
        "values": {
          "expand": "Расширять или сжимать",
          "compress": "Compress"
        },
        "breakLine": true
      },
      "options.column_to_expand": {
        "type": "select",
        "Метка": "Колонка для расширения",
        "show_if": "options.type=='expand'",
        "table": "input_table",
        "bind": "COLUMNS",
        "required": true
      },
      "options.column_to_group": {
        "type": "select",
        "Метка": "Колонка для группировки",
        "show_if": "options.type=='compress'",
        "table": "input_table",
        "bind": "COLUMNS",
        "required": true
      },
      "options.column_to_compress": {
        "type": "select",
        "Метка": "Колонка для сжатия",
        "show_if": "options.type=='compress'",
        "table": "input_table",
        "bind": "COLUMNS",
        "required": true
      },
      "options.delimiter": {
        "type": "text",
        "Метка": "Разделитель",
        "val": ";",
        "trim": "false",
        "required": true
      },
      "options.new_col_name": {
        "type": "text",
        "Метка": "Название нового столбца",
        "show_if": "options.type=='expand' && options.expand__output_in_new_column=='true'",
        "val": "Метка",
        "required": true,
        "breakLine": true
      },
      "options.expand__output_in_new_column": {
        "type": "checkbox",
        "Метка": "Вывести в новый столбец",
        "show_if": "options.type=='expand'",
        "val": "false",
        "isOn": "true",
        "isOff": "false",
        "breakLine": true
      },
      "options.create_new_table": {
        "type": "checkbox",
        "Метка": "Вывести как новую таблицу",
        "val": "true",
        "isOn": "true",
        "isOff": "false",
        "breakLine": true
      }
    }
  }
}