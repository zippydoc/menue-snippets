            "label": "Символ", 
            "show_if": "this.format__category=='currency'", 
            "required": true, 
            "options": {
              "dollar": {
                "label": "$"
              }, 
              "euro": {
                "label": "€"
              }, 
              "pound": {
                "label": "£"
