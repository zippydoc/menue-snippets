              "percentage": {
                "label": "Процент"
              }, 
              "text": {
                "label": "Текст"
              }, 
              "date": {
                "label": "Дата"
              }, 
              "currency": {
                "label": "Валюта"
              }
            }, 
            "val": "standard"
          }, 
          "format__rounding": {
            "type": "select", 
            "label": "Округление", 
            "show_if": "this.format__category=='number' || this.format__category=='percentage' || this.format__category=='currency'", 
            "required": true, 
            "options": {
              "no_rounding": {
                "label": "Округление чисел"
              }, 
              "0": {
                "label": "0 цифр"
              }, 
              "1": {
                "label": "1 цифра"
              }, 
              "2": {
                "label": "2 цифры"
              }, 
              "3": {
                "label": "3 цифры"
              }, 
              "4": {
                "label": "4 цифры"
              }, 
              "5": {
                "label": "5 цифр"
              }, 
              "6": {
                "label": "6 цифр"
              }, 
              "7": {
                "label": "7 цифр"
              }, 
              "8": {
                "label": "8 цифр"
              }, 
              "9": {
                "label": "9 цифр"
              }
