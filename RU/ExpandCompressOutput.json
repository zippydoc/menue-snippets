            "label": "Переименовать", 
            "val": "false", 
            "isOn": "true", 
            "isOff": "false"
          }, 
          "rename": {
            "show_if": "this.is_rename == 'true'", 
            "type": "text", 
            "required": true, 
            "minLength": 1, 
            "maxLength": 255, 
            "label": "Название нового столбца", 
            "style": "margin-left: 10px"
          }, 
          "format__category": {
            "type": "select", 
            "label": "Формат", 
            "required": true, 
            "options": {
              "standard": {
                "label": "Стандартный"
              }, 
              "number": {
                "label": "Число"
