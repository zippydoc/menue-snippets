        "val": "", 
        "width": "100%", 
        "breakLine": true
      }, 
      "output.select_output_columns": {
        "type": "checkbox", 
        "label": "Выбрать выходные столбцы", 
        "val": "false", 
        "isOn": "true", 
        "isOff": "false"
      }, 
