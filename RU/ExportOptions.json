{
  "options": {
    "type": "fieldset",
    "label": "Выбрать параметры",
    "repeat": false,
    "fields": {
      "options.to": {
        "type": "select",
        "label": "Тип",
        "options": {
          "database": {
            "label": "Экспорт в базу данных"
          },
          "file": {
            "label": "Экспорт в файл"
          }
        },
        "val": "database",
        "required": true
      },
      "options.export_series": {
        "type": "checkbox",
        "label": "Экспорт серий",
        "show_if": "options.to=='file'",
        "val": "false",
        "isOn": "true",
        "isOff": "false",
        "breakLine": true
      },
      "options.file_name_column": {
        "type": "select",
        "label": "Выбрать столбец с названием файла",
        "show_if": "options.to=='file' && options.export_series=='true'",
        "table": "input_table",
        "bind": "COLUMNS",
        "required": true,
        "breakLine": true
      },
      "postgresql_db_credential_box": {
        "type": "fieldset",
        "label": "Учетные данные для подключения к базе данных",
        "show_if": "options.to=='database'",
        "repeat": false,
        "fields": {
          "options.dbms": {
            "type": "select",
            "label": "Выбрать СУБД",
            "options": {
              "postgresql": {
                "label": "PostgreSQL"
              },
              "mysql": {
                "label": "MySQL"
              },
              "sql_server": {
                "label": "MS SQL Сервер"
              },
              "oracle": {
                "label": "Oracle"
              }
            },
            "val": "postgresql",
            "required": true,
            "breakLine": true
          },
          "options.postgresql_credential.user": {
            "type": "text",
            "label": "Пользователь",
            "show_if": "options.dbms=='postgresql'",
            "required": true,
            "width": "100%",
            "breakLine": true
          },
          "options.postgresql_credential.password": {
            "type": "text",
            "label": "Пароль",
            "show_if": "options.dbms=='postgresql'",
            "required": true,
            "width": "100%",
            "breakLine": true
          },
          "options.postgresql_credential.host": {
            "type": "text",
            "label": "Хост",
            "show_if": "options.dbms=='postgresql'",
            "required": true,
            "width": "100%",
            "breakLine": true
          },
          "options.postgresql_credential.database": {
            "type": "text",
            "label": "База данных",
            "show_if": "options.dbms=='postgresql'",
            "required": true,
            "width": "100%",
            "breakLine": true
          }
        }
      },
      "options.schema_name_to_export": {
        "type": "text",
        "label": "Название схемы для экспорта",
        "show_if": "options.to=='database' && options.dbms=='postgresql'",
        "required": true,
        "val": "public",
        "width": "100%",
        "breakLine": true
      },
      "options.webservice": {
        "type": "select",
        "label": "Выбрать Веб-сервис",
        "show_if": "options.to=='file' || options.to=='mytask'",
        "options": {
          "github": {
            "label": "Github"
          },
          "dropbox": {
            "label": "Dropbox"
          },
          "ms_one_drive": {
            "label": "Microsoft OneDrive"
          },
          "google_drive": {
            "label": "Google Drive"
          }
        },
        "val": "github",
        "required": true
      },
      "options.export_format": {
        "type": "select",
        "label": "Формат экспорта",
        "show_if": "options.to=='file'",
        "options": {
          "csv_file": {
            "label": "Csv-файл(ы)"
          },
          "word_by_word": {
            "label": "Слово за словом"
          },
          "line_by_line": {
            "label": "Линия за линией"
          }
        },
        "val": "csv_file",
        "required": true,
        "breakLine": true
      },
      "options.github_create_pr": {
        "type": "checkbox",
        "label": "Создать пул-реквест",
        "show_if": "options.to=='file' && options.webservice=='github'",
        "val": "false",
        "isOn": "true",
        "isOff": "false",
        "breakLine": true
      },
      "options.github_url": {
        "type": "text",
        "label": "Введите ссылку, например, https://github.com/zippydoc/test.",
        "show_if": "options.to=='file' && options.webservice=='github'",
        "val": "https://github.com/zippydoc/public-tables",
        "required": true,
        "width": "100%",
        "breakLine": true
      },
      "options.export_dir": {
        "type": "text",
        "label": "Введите каталог, например, /MyZippyDocFiles/SubDir/",
        "show_if": "options.to=='file' && options.webservice!='github'",
        "val": "/",
        "required": true,
        "width": "100%",
        "breakLine": true
      },
      "options.github_token": {
        "type": "text",
        "label": "Укажите персональный токен доступа Github, имеющий право на запись:",
        "show_if": "options.to=='file' && options.webservice=='github'",
        "required": true,
        "width": "100%",
        "breakLine": true
      },
      "options.ms_one_drive_token": {
        "type": "text",
        "label": "Токен доступа к Microsoft Graph API. Вы можете получить его здесь https://ms-oauth.herokuapp.com/",
        "show_if": "options.to=='file' && options.webservice=='ms_one_drive'",
        "required": true,
        "width": "100%",
        "breakLine": true
      },
      "options.google_authorization_code": {
        "type": "text",
        "label": "Код авторизации Google OAuth с областью действия https://www.googleapis.com/auth/drive. Вы можете получить его здесь https://developers.google.com/oauthplayground/",
        "show_if": "options.to=='file' && options.webservice=='google_drive'",
        "required": true,
        "width": "100%",
        "breakLine": true
      },
      "options.dropbox_authorization_code": {
        "type": "text",
        "label": "Dropbox Authorization Code. You can get it here https://www.dropbox.com/oauth2/authorize?client_id=9i7woztjp94jrkw&response_type=code",
        "show_if": "options.to=='file' && options.webservice=='dropbox'",
        "required": true,
        "width": "100%",
        "breakLine": true
      },
      "options.if_duplicated": {
        "type": "select",
        "label": "Если дублируется",
        "show_if": "options.to=='database' || (options.to=='file' && options.webservice=='github')",
        "val": "drop_if_exist",
        "options": {
          "drop_if_exist": {
            "label": "Создать или сбросить (если уже существует)"
          },
          "error_if_exist": {
            "label": "Создать новое (убрать ошибку, если есть)"
          },
          "error_if_not_exist": {
            "label": "Отменить (убрать ошибку, если ее нет)"
          },
          "append": {
            "label": "Прикрепить"
          }
        }
      },
      "options.export_task_name": {
        "type": "text",
        "label": "Добавить название задачи",
        "show_if": "options.to=='mytask'",
        "required": true,
        "val": "Flow6_Task1_AddColumns",
        "width": "100%",
        "breakLine": true
      }
    }
  }
}