{
  "options": {
    "type": "fieldset",
    "label": "Выбрать параметры",
    "repeat": false,
    "fields": {
      "options.to": {
        "type": "hidden",
        "val": "database"
      },
      "postgresql_db_credential_box": {
        "type": "fieldset",
        "label": "Учетные данные для подключения к базе данных",
        "repeat": false,
        "fields": {
          "options.dbms": {
            "type": "select",
            "label": "Выбрать СУБД",
            "options": {
              "postgresql": {
                "label": "PostgreSQL"
              },
              "mysql": {
                "label": "MySQL"
              },
              "sql_server": {
                "label": "MS SQL Сервер"
              },
              "oracle": {
                "label": "Oracle"
              }
            },
            "val": "postgresql",
            "required": true,
            "breakLine": true
          },
          "options.host": {
            "type": "text",
            "label": "Хост",
            "required": true,
            "width": "100%",
            "breakLine": true
          },
          "options.user": {
            "type": "text",
            "label": "Пользователь",
            "required": true,
            "width": "100%",
            "breakLine": true
          },
          "options.password": {
            "type": "password",
            "label": "Пароль",
            "required": true,
            "width": "100%",
            "breakLine": true
          },
          "options.database": {
            "type": "text",
            "label": "База данных",
            "required": true,
            "width": "100%",
            "breakLine": true
          }
        }
      },
      "options.schema_name_to_export": {
        "type": "text",
        "label": "Название схемы для экспорта",
        "show_if": "options.dbms=='postgresql' || options.dbms=='sql_server' || options.dbms=='oracle'",
        "required": true,
        "val": "public",
        "width": "100%",
        "breakLine": true
      },
      "options.if_duplicated": {
        "type": "select",
        "label": "Если дублируется",
        "val": "drop_if_exist",
        "options": {
          "drop_if_exist": {
            "label": "Создать или сбросить (если уже существует)"
          },
          "error_if_exist": {
            "label": "Создать новое (убрать ошибку, если есть)"
          },
          "error_if_not_exist": {
            "label": "Отменить (убрать ошибку, если ее нет)"
          },
          "append": {
            "label": "Прикрепить"
          }
        },
        "breakLine": true
      },
      "output.output_as_new_table": {
        "type": "hidden",
        "val": "true"
      }
    }
  }
}