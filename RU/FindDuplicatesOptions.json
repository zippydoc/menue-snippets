      "options.task_type": {
        "type": "hidden", 
        "val": "label_duplicates"
      }, 
      "options.pattern_type": {
        "type": "select", 
        "label": "Тип шаблона", 
        "show_if": "options.task_type=='generate_lookup_table' || options.task_type=='apply_lookup_table' || options.task_type=='apply_lookup_table_to_cells'", 
        "required": true, 
        "options": {
          "duplicate_pattern": {
