{
  "options": {
    "type": "fieldset",
    "Метка": "Опции",
    "repeat": false,
    "fields": {
      "reformat_columns": {
        "type": "checkbox",
        "Метка": "Форматирование столбцов",
        "show_if": "false",
        "val": "false",
        "isOn": "true",
        "isOff": "false"
      },
      "columns": {
        "type": "fieldset",
        "Метка": "Выбрать строки",
        "show_if": "$input_table.numOfHeader >= 1",
        "repeat": true,
        "fields": {
          "field_name": {
            "type": "select",
            "Метка": "Выбрать столбец",
            "table": "input_table",
            "bind": "COLUMNS",
            "required": true
          },
          "format__category": {
            "type": "select",
            "Метка": "Формат",
            "required": true,
            "options": {
              "standard": {
                "Метка": "Стандартный"
              },
              "number": {
                "Метка": "Число"
              },
              "percentage": {
                "Метка": "Процент"
              },
              "text": {
                "Метка": "Текст"
              },
              "date": {
                "Метка": "Дата"
              },
              "currency": {
                "Метка": "Валюта"
              }
            },
            "val": "standard"
          },
          "format__rounding": {
            "type": "select",
            "Метка": "Округление",
            "show_if": "this.format__category=='number' || this.format__category=='percentage' || this.format__category=='currency'",
            "required": true,
            "options": {
              "no_rounding": {
                "Метка": "Округление чисел"
              },
              "0": {
                "Метка": "0 цифр"
              },
              "1": {
                "Метка": "1 цифра"
              },
              "2": {
                "Метка": "2 цифры"
              },
              "3": {
                "Метка": "3 цифры"
              },
              "4": {
                "Метка": "4 цифры"
              },
              "5": {
                "Метка": "5 цифр"
              },
              "6": {
                "Метка": "6 цифр"
              },
              "7": {
                "Метка": "7 цифр"
              },
              "8": {
                "Метка": "8 цифр"
              },
              "9": {
                "Метка": "9 цифр"
              }
            },
            "val": "2"
          },
          "format__use_1000_separator": {
            "type": "checkbox",
            "Метка": "Use 1000 Separator(.)",
            "show_if": "this.format__category=='number' || this.format__category=='percentage' || this.format__category=='currency'",
            "val": "true",
            "isOn": "true",
            "isOff": "false"
          },
          "format__currency_symbol": {
            "type": "select",
            "Метка": "Символ",
            "show_if": "this.format__category=='currency'",
            "required": true,
            "options": {
              "dollar": {
                "Метка": "$"
              },
              "euro": {
                "Метка": "€"
              },
              "pound": {
                "Метка": "£"
              }
            },
            "val": "dollar"
          },
          "format__date_format": {
            "type": "select",
            "Метка": "Тип",
            "show_if": "this.format__category=='date'",
            "required": true,
            "options": {
              "mm/dd/yyyy": {
                "Метка": "3/14/2012"
              },
              "D, M dd, Y": {
                "Метка": "Среда, 14 марта 2012 года"
              },
              "yyyy-mm-dd": {
                "Метка": "2012-03-14"
              }
            },
            "val": "mm/dd/yyyy",
            "breakLine": true
          },
          "delete": {
            "show_if": "this.field_name != ''",
            "type": "button",
            "Метка": "Удалить",
            "function": "DELETE_ROW"
          }
        },
        "breakLine": true
      },
      "reformat_rows": {
        "type": "checkbox",
        "Метка": "Форматирование меток",
        "show_if": "false",
        "val": "false",
        "isOn": "true",
        "isOff": "false",
        "style": "margin-top: 10px"
      },
      "rows": {
        "type": "fieldset",
        "Метка": "Выбрать метки",
        "show_if": "$input_table.numOfLabel >= 1",
        "repeat": true,
        "fields": {
          "label_name": {
            "type": "select",
            "Метка": "Выбрать метку",
            "table": "input_table",
            "bind": "LABELS",
            "required": true
          },
          "format__category": {
            "type": "select",
            "Метка": "Формат",
            "required": true,
            "options": {
              "standard": {
                "Метка": "Стандартный"
              },
              "number": {
                "Метка": "Число"
              },
              "percentage": {
                "Метка": "Процент"
              },
              "text": {
                "Метка": "Текст"
              },
              "date": {
                "Метка": "Дата"
              },
              "currency": {
                "Метка": "Валюта"
              }
            },
            "val": "standard"
          },
          "format__rounding": {
            "type": "select",
            "Метка": "Округление",
            "show_if": "this.format__category=='number' || this.format__category=='percentage' || this.format__category=='currency'",
            "required": true,
            "options": {
              "no_rounding": {
                "Метка": "Округление чисел"
              },
              "0": {
                "Метка": "0 цифр"
              },
              "1": {
                "Метка": "1 цифра"
              },
              "2": {
                "Метка": "2 цифры"
              },
              "3": {
                "Метка": "3 цифры"
              },
              "4": {
                "Метка": "4 цифры"
              },
              "5": {
                "Метка": "5 цифр"
              },
              "6": {
                "Метка": "6 цифр"
              },
              "7": {
                "Метка": "7 цифр"
              },
              "8": {
                "Метка": "8 цифр"
              },
              "9": {
                "Метка": "9 цифр"
              }
            },
            "val": "2"
          },
          "format__use_1000_separator": {
            "type": "checkbox",
            "Метка": "Use 1000 Separator(.)",
            "show_if": "this.format__category=='number' || this.format__category=='percentage' || this.format__category=='currency'",
            "val": "true",
            "isOn": "true",
            "isOff": "false"
          },
          "format__currency_symbol": {
            "type": "select",
            "Метка": "Символ",
            "show_if": "this.format__category=='currency'",
            "required": true,
            "options": {
              "dollar": {
                "Метка": "$"
              },
              "euro": {
                "Метка": "€"
              },
              "pound": {
                "Метка": "£"
              }
            },
            "val": "dollar"
          },
          "format__date_format": {
            "type": "select",
            "Метка": "Тип",
            "show_if": "this.format__category=='date'",
            "required": true,
            "options": {
              "mm/dd/yyyy": {
                "Метка": "3/14/2012"
              },
              "D, M dd, Y": {
                "Метка": "Среда, 14 марта 2012 года"
              },
              "yyyy-mm-dd": {
                "Метка": "2012-03-14"
              }
            },
            "val": "mm/dd/yyyy",
            "breakLine": true
          },
          "delete": {
            "show_if": "this.label_name != ''",
            "type": "button",
            "Метка": "Удалить",
            "function": "DELETE_ROW"
          }
        },
        "breakLine": true
      },
      "create_new_table": {
        "type": "checkbox",
        "Метка": "Вывести как новую таблицу",
        "val": "true",
        "isOn": "true",
        "isOff": "false",
        "breakLine": true,
        "style": "margin-top: 10px"
      }
    }
  }
}