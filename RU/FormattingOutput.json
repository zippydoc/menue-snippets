{
  "output": {
    "type": "fieldset",
    "label": "Определить выходную таблицу",
    "show_if": "create_new_table=='true'",
    "repeat": false,
    "fields": {
      "output.custom_text": {
        "type": "text",
        "label": "Вывести название таблицы",
        "show_if": "output.custom_options=='false' || (output.custom_options=='true' && output.compose_table_name=='false')",
        "minLength": 1,
        "val": "",
        "width": "100%",
        "breakLine": true
      },
      "output.custom_options": {
        "type": "checkbox",
        "label": "Настройки вывода",
        "val": "false",
        "isOn": "true",
        "isOff": "false",
        "breakLine": true
      },
      "output.compose_table_name": {
        "type": "checkbox",
        "label": "Объединить название таблицы",
        "show_if": "output.custom_options=='true'",
        "val": "false",
        "isOn": "true",
        "isOff": "false",
        "breakLine": true
      },
      "compose_table_name_frame": {
        "type": "fieldset",
        "label": "Объединить название входной таблицы",
        "show_if": "output.custom_options=='true' && output.compose_table_name=='true'",
        "repeat": false,
        "fields": {
          "output.prefix_rule": {
            "type": "select",
            "label": "Префикс",
            "show_if": "output.compose_table_name=='true'",
            "val": "empty",
            "options": {
              "empty": {
                "label": "-"
              },
              "script_name": {
                "label": "Название задачи"
              },
              "date_string_type1": {
                "label": "Дата: дд_мм_гггг"
              },
              "date_string_type2": {
                "label": "Дата: гггг_мм_дд"
              },
              "time_string": {
                "label": "Время: ч_мин_сек"
              },
              "datetime_string": {
                "label": "Дата_время"
              },
              "custom": {
                "label": "Пользовательский"
              }
            }
          },
          "output.prefix_text": {
            "type": "text",
            "label": "Текст префикса",
            "show_if": "output.compose_table_name=='true' && output.prefix_rule=='custom'",
            "minLength": 1,
            "maxLength": 31,
            "val": ""
          },
          "output.core_delimiter": {
            "type": "text",
            "label": "Разделитель",
            "show_if": "output.compose_table_name=='true' && output.prefix_rule!='empty'",
            "val": "_",
            "width": "50px"
          },
          "output.core_rule": {
            "type": "select",
            "label": "Название таблицы",
            "show_if": "output.compose_table_name=='true'",
            "val": "input_table_name",
            "options": {
              "input_table_name": {
                "label": "Добавить название таблицы"
              },
              "script_name": {
                "label": "Название задачи"
              },
              "custom": {
                "label": "Пользовательский"
              }
            }
          },
          "output.core_text": {
            "type": "text",
            "label": "Название основной таблицы",
            "show_if": "output.compose_table_name=='true' && output.core_rule=='custom'",
            "required": true,
            "minLength": 1,
            "maxLength": 31,
            "val": ""
