            "required": true,
            "minLength": 1,
            "maxLength": 31,
            "val": ""
          },
          "output.suffix_delimiter": {
            "type": "text",
            "label": "Разделитель",
            "show_if": "output.compose_table_name=='true'",
            "val": "_",
            "width": "50px"
          },
          "output.suffix_rule": {
            "type": "select",
            "label": "Суффикс",
            "show_if": "output.compose_table_name=='true'",
            "val": "script_name",
            "options": {
              "empty": {
                "label": "-"
              },
              "script_name": {
                "label": "Название задачи"
              },
              "date_string_type1": {
                "label": "Дата: дд_мм_гггг"
              },
              "date_string_type2": {
                "label": "Дата: гггг_мм_дд"
              },
              "time_string": {
                "label": "Время: ч_мин_сек"
              },
              "datetime_string": {
                "label": "Дата_время"
              },
              "custom": {
                "label": "Пользовательский"
              }
            }
          },
          "output.suffix_text": {
            "type": "text",
            "label": "Текст суффикса",
            "show_if": "output.compose_table_name=='true' && output.suffix_rule=='custom'",
            "minLength": 1,
            "maxLength": 31,
            "val": "",
            "breakLine": true
          },
          "output.timezone": {
            "type": "select",
            "label": "Часовой пояс",
