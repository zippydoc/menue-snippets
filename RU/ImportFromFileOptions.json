{
  "options": {
    "type": "fieldset", 
    "label": "Выбрать параметры", 
    "repeat": false, 
    "fields": {
      "options.from": {
        "type": "hidden", 
        "val": "web"
      }, 
      "options.webservice": {
        "type": "select", 
        "label": "Выбрать Веб-сервис", 
        "options": {
          "github": {
            "label": "Github"
          }, 
          "ms_one_drive": {
            "label": "Microsoft OneDrive"
          }, 
          "dropbox": {
            "label": "Dropbox"
          }, 
          "google_drive": {
            "label": "Google Drive (Coming soon)"
          }
        }, 
        "val": "github", 
        "required": true, 
        "breakLine": true
      }, 
      "options.output_format": {
        "type": "select", 
        "label": "Выбрать тип", 
        "options": {
          "csv_table": {
            "label": "CSV-таблица"
          }, 
          "line_by_line": {
            "label": "Линия за линией"
          }, 
          "word_by_word": {
            "label": "Слово за словом"
          }, 
          "file_list": {
            "label": "Список файлов"
          }
        }, 
        "val": "csv_table", 
        "required": true
      }, 
      "options.word_pattern_type": {
        "type": "select", 
        "label": "Словесный шаблон", 
        "show_if": "options.output_format=='word_by_word' && options.specify_word_pattern=='true'", 
        "options": {
          "regex": {
            "label": "Регулярное выражение"
          }
        }, 
        "val": "regex", 
        "required": true
      }, 
      "options.word_pattern_regex": {
        "type": "text", 
        "label": "Регулярное выражение", 
        "show_if": "options.output_format=='word_by_word' && options.specify_word_pattern=='true' && options.word_pattern_type=='regex'", 
        "required": true, 
        "val": "[a-z][1-9]", 
        "breakLine": true
      }, 
      "options.column_setting": {
        "type": "checkbox", 
        "label": "Column Settings", 
        "show_if": "options.output_format!='file_list'", 
        "val": "false", 
        "isOn": "true", 
        "isOff": "false", 
        "breakLine": true
      }, 
      "column_setting_frame": {
        "type": "fieldset", 
        "label": "Column Settings", 
        "show_if": "options.output_format!='file_list' && options.column_setting=='true'", 
        "repeat": false, 
        "fields": {
          "options.add_row_column": {
            "type": "checkbox", 
            "label": "Добавить строку столбца", 
            "show_if": "options.output_format=='word_by_word'", 
            "val": "true", 
            "isOn": "true", 
            "isOff": "false", 
            "breakLine": true
          }, 
          "options.add_word_count_column": {
            "type": "checkbox", 
            "label": "Add 'Word Count' Column", 
            "show_if": "options.output_format=='word_by_word'", 
            "val": "false", 
            "isOn": "true", 
            "isOff": "false", 
            "breakLine": true
          }, 
          "options.add_word_nonword_column": {
            "type": "checkbox", 
            "label": "Add 'Word & Non-Word' Column", 
            "show_if": "options.output_format=='word_by_word'", 
            "val": "false", 
            "isOn": "true", 
            "isOff": "false", 
            "breakLine": true
          }, 
          "options.add_section_column": {
            "type": "checkbox", 
            "label": "Добавить столбец раздела", 
            "show_if": "options.output_format=='word_by_word'", 
            "val": "true", 
            "isOn": "true", 
            "isOff": "false", 
            "breakLine": true
          }, 
          "section_column_types_frame": {
            "type": "fieldset", 
            "label": "Section Column Types", 
            "show_if": "options.output_format=='word_by_word' && options.add_section_column=='true'", 
            "repeat": false, 
            "fields": {
              "options.section_columns.add_paragraph_section_column": {
                "type": "checkbox", 
                "label": "Параграф", 
                "val": "false", 
                "isOn": "true", 
                "isOff": "false", 
                "breakLine": true
              }, 
              "options.section_columns.add_custom_section_column": {
                "type": "checkbox", 
                "label": "Пользовательский разделитель", 
                "val": "false", 
                "isOn": "true", 
                "isOff": "false"
              }, 
              "options.section_columns.custom_delimiter": {
                "type": "text", 
                "label": "Разделитель (1 символ)", 
                "show_if": "options.section_columns.add_custom_section_column=='true'", 
                "maxLength": 1, 
                "required": true, 
                "breakLine": true
              }, 
              "options.section_columns.add_brackets_punctuation_section_column": {
                "type": "checkbox", 
                "label": "Скобки и пунктуация", 
                "val": "false", 
                "isOn": "true", 
                "isOff": "false"
              }, 
              "options.section_columns.brackets_punctuation_section_column_type": {
                "type": "select", 
                "label": "Brackets Type", 
                "show_if": "options.section_columns.add_brackets_punctuation_section_column=='true'", 
                "options": {
                  "nesting_structure": {
                    "label": "Nesting Structure"
                  }, 
                  "inner_bracket": {
                    "label": "Inner Bracket"
                  }, 
                  "show_all": {
                    "label": "Показать все"
                  }
                }, 
                "val": "nesting_structure", 
                "required": true, 
                "breakLine": true
              }
            }
          }, 
          "options.add_filename_column": {
            "type": "checkbox", 
            "label": "Добавить колонку в Название Файла", 
            "val": "false", 
            "isOn": "true", 
            "isOff": "false", 
            "style": "margin-top: 10px", 
            "breakLine": true
          }
        }
      }, 
      "options.is_cloud_dir": {
        "type": "checkbox", 
        "label": "Импортировать каталог", 
        "show_if": "options.output_format!='file_list'", 
        "val": "false", 
        "isOn": "true", 
        "isOff": "false", 
        "style": "margin-top: 10px", 
        "breakLine": true
      }, 
      "options.use_filename_as_output_table_name": {
        "type": "checkbox", 
        "label": "Использовать имя файла в качестве имени таблицы", 
        "show_if": "options.output_format!='file_list' && options.is_cloud_dir=='false'", 
        "val": "false", 
        "isOn": "true", 
        "isOff": "false", 
        "breakLine": true
      }, 
      "options.unify_in_one_table": {
        "type": "checkbox", 
        "label": "Объединить в одну таблицу", 
        "show_if": "options.output_format!='file_list' && options.is_cloud_dir=='true'", 
        "val": "false", 
        "isOn": "true", 
        "isOff": "false"
      }, 
      "options.suffix": {
        "type": "select", 
        "label": "Суффикс", 
        "show_if": "options.output_format!='file_list' && options.is_cloud_dir=='true' && options.unify_in_one_table=='false'", 
        "options": {
          "number": {
            "label": "Число"
          }, 
          "filename": {
            "label": "Название файла"
          }
        }, 
        "val": "number", 
        "required": true, 
        "breakLine": true
      }, 
      "options.is_github_private": {
        "type": "checkbox", 
        "label": "Репозиторий является частным", 
        "show_if": "options.webservice=='github'", 
        "val": "false", 
        "isOn": "true", 
        "isOff": "false", 
        "breakLine": true
      }, 
      "options.include_subdir": {
        "type": "checkbox", 
        "label": "Включить подкаталоги", 
        "show_if": "options.output_format=='file_list'", 
        "val": "true", 
        "isOn": "true", 
        "isOff": "false", 
        "breakLine": true
      }, 
      "options.file_list.add_change_date_column": {
        "type": "checkbox", 
        "label": "Добавить столбец изменения даты", 
        "show_if": "options.output_format=='file_list'", 
        "val": "false", 
        "isOn": "true", 
        "isOff": "false", 
        "breakLine": true
      }, 
      "options.github_token": {
        "type": "text", 
        "label": "указать токен персонального доступа Github", 
        "show_if": "options.webservice=='github' && options.is_github_private=='true'", 
        "required": true, 
        "width": "100%", 
        "breakLine": true
      }, 
      "options.link_directory": {
        "type": "text", 
        "label": "Введите ссылку, например, https://github.com/zippydoc/test.", 
        "show_if": "options.webservice=='github' && options.output_format!='file_list' && options.is_cloud_dir=='true'", 
        "required": true, 
        "width": "100%", 
        "breakLine": true
      }, 
      "options.link_directory_4_file_list": {
        "type": "text", 
        "label": "Введите ссылку, например, https://github.com/zippydoc/test.", 
        "show_if": "options.webservice=='github' && options.output_format=='file_list'", 
        "required": true, 
        "width": "100%", 
        "breakLine": true
      }, 
      "options.link_file": {
        "type": "text", 
        "label": "Введите ссылку, например, https://github.com/zippydoc/test/blob/main/example.csv.", 
        "show_if": "options.webservice=='github' && options.is_cloud_dir=='false' && options.output_format=='csv_table'", 
        "required": true, 
        "width": "100%", 
        "breakLine": true
      }, 
      "options.link_text_file": {
        "type": "text", 
        "label": "Введите ссылку, например: https://github.com/zippydoc/test/blob/main/example.txt(json,  csv,  etc)", 
        "show_if": "options.webservice=='github' && options.is_cloud_dir=='false' && options.output_format!='csv_table' && options.output_format!='file_list'", 
        "required": true, 
        "width": "100%", 
        "breakLine": true
      }, 
      "options.one_drive.access_token": {
        "type": "text", 
        "label": "OAuth Access Token", 
        "show_if": "options.webservice=='ms_one_drive' && options.is_cloud_dir=='false'", 
        "required": true, 
        "width": "100%", 
        "breakLine": true
      }, 
      "options.one_drive.file_path": {
        "type": "text", 
        "label": "Enter path to file e.g. ZippyDoc/Subdir1/test.txt", 
        "show_if": "options.webservice=='ms_one_drive' && options.output_format!='file_list' && options.is_cloud_dir=='false'", 
        "required": true, 
        "width": "100%", 
        "breakLine": true
      }, 
      "options.one_drive.dir_path": {
        "type": "text", 
        "label": "Enter path to dir e.g. ZippyDoc/Subdir1", 
        "show_if": "options.webservice=='ms_one_drive' && (options.output_format=='file_list' || options.is_cloud_dir=='true')", 
        "width": "100%", 
        "breakLine": true
      }, 
      "options.dropbox.auth_code": {
        "type": "text", 
        "label": "Dropbox Authorization Code", 
        "show_if": "options.webservice=='dropbox' && options.is_cloud_dir=='false'", 
        "required": true, 
        "width": "100%", 
        "breakLine": true
      }, 
      "options.dropbox.file_path": {
        "type": "text", 
        "label": "Enter path to file e.g. ZippyDoc/Subdir1/test.txt", 
        "show_if": "options.webservice=='dropbox' && options.output_format!='file_list' && options.is_cloud_dir=='false'", 
        "required": true, 
        "width": "100%", 
        "breakLine": true
      }, 
      "options.dropbox.dir_path": {
        "type": "text", 
        "label": "Enter path to dir e.g. ZippyDoc/Subdir1", 
        "show_if": "options.webservice=='dropbox' && (options.output_format=='file_list' || options.is_cloud_dir=='true')", 
        "width": "100%", 
        "breakLine": true
      }, 
      "dir_filter_frame": {
        "type": "fieldset", 
        "label": "Фильтровать файлы в каталоге", 
        "show_if": "options.output_format!='file_list' && options.is_cloud_dir=='true'", 
        "repeat": false, 
        "fields": {
          "options.dir_filter.filter_type": {
            "type": "select", 
            "label": "Тип фильтра", 
            "options": {
              "all": {
                "label": "Все"
              }, 
              "contains": {
                "label": "Содержит"
              }, 
              "starts_with": {
                "label": "Начинается с"
              }, 
              "ends_with": {
                "label": "Заканчивается С"
              }, 
              "regex": {
                "label": "Регулярное выражение"
              }
            }, 
            "val": "all", 
            "required": true
          }, 
          "options.dir_filter.filter_string": {
            "type": "text", 
            "label": "Введите строку", 
            "show_if": "options.dir_filter.filter_type!='all'", 
            "required": true, 
            "width": "100%"
          }
        }
      }
    }
  }
}