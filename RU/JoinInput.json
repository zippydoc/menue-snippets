{
  "input": {
    "type": "fieldset", 
    "label": "Выберите Входную таблицу", 
    "repeat": false, 
    "fields": {
      "left_table.table_id": {
        "type": "select", 
        "label": "Выберите левую таблицу", 
        "bind": "TABLES"
      }, 
      "left_table.remove_input_table_after_processing": {
        "type": "hidden", 
        "val": "false"
      }, 
      "left_table.is_word": {
        "type": "hidden", 
        "val": "false", 
        "breakLine": true
      }, 
      "right_table.table_id": {
        "type": "select", 
        "label": "Выберите Правый стол", 
        "bind": "TABLES"
      }, 
      "right_table.remove_input_table_after_processing": {
        "type": "hidden", 
        "val": "false"
      }, 
      "right_table.is_word": {
        "type": "hidden", 
        "val": "false"
      }
    }
  }
}
