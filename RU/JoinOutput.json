{
  "output.output_table_types": {
    "type": "select",
    "Метка": "Вывести тип(ы) таблиц(ы)",
    "required": true,
    "options": {
      "inner": {
        "Метка": "Внутренняя таблица объединения"
      },
      "left": {
        "Метка": "Левая таблица объединения"
      },
      "right": {
        "Метка": "Левая таблица объединения"
      }
    },
    "multiple": true,
    "val": ["inner"]
  },
  "output.standard_ending": {
    "type": "checkbox",
    "Метка": "Стандартная обработка",
    "val": "true",
    "isOn": "true",
    "isOff": "false",
    "breakLine": true
  },
  "output.inner_ending": {
    "show_if": "output.standard_ending == 'false'",
    "type": "text",
    "Метка": "Внутренний конец",
    "val": "_j",
    "style": "margin-left: 10px"
  },
  "output.left_ending": {
    "show_if": "output.standard_ending == 'false'",
    "type": "text",
    "Метка": "Левый конец",
    "val": "_l",
    "style": "margin-left: 10px"
  },
  "output.right_ending": {
    "show_if": "output.standard_ending == 'false'",
    "type": "text",
    "Метка": "Правый конец",
    "val": "_r",
    "style": "margin-left: 10px",
    "breakLine": true
  }
}