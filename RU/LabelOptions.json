    "repeat": false, 
{
  "options": {
    "type": "fieldset", 
    "label": "Добавить ярлык", 
    "fields": {
      "add_column": {
        "type": "text", 
        "label": "Название колонки для этикетки", 
        "val": "Ярлык", 
        "required": true, 
        "width": "100%", 
        "breakLine": true
      }, 
      "add_label": {
        "type": "text", 
        "label": "Добавить этикетку", 
        "val": "label1", 
        "required": true
      }, 
      "use_custom_delimiter": {
        "type": "checkbox", 
        "label": "Пользовательский разделитель", 
        "val": "false", 
        "isOn": "true", 
        "isOff": "false"
      }, 
      "custom_delimiter": {
        "type": "text", 
        "label": "Пользовательский разделитель", 
        "show_if": "use_custom_delimiter=='true'", 
        "val": ";", 
        "required": true
      }, 
      "do_not_duplicate": {
        "type": "checkbox", 
        "label": "Не дублируйте", 
        "val": "true", 
        "isOn": "true", 
        "isOff": "false", 
        "breakLine": true
      }, 
      "filter_box": {
        "type": "fieldset", 
        "label": "Опции фильтра", 
        "repeat": false, 
        "fields": {
          "filter": {
            "type": "fieldset", 
            "label": "Выберите условия для маркировки", 
            "repeat": true, 
            "fields": {
              "initial_op": {
                "type": "select", 
                "label": "Выберите Логический оператор", 
                "required": true, 
                "show_if" :  "$index == 0", 
                "options": {
                  "if": {
                    "label": "ЕСЛИ"
                  }, 
                  "if_not": {
                    "label": "ЕСЛИ НЕ"
                  }
                }, 
                "val": "if"
              }, 
              "op": {
                "type": "select", 
                "label": "Выберите Логический оператор", 
                "show_if" :  "$index > 0", 
                "options": {
                  "and": {
                    "label": "И"
                  }, 
                  "or": {
                    "label": "ИЛИ"
                  }, 
                  "and_not": {
                    "label": "И НЕ"
                  }, 
                  "or_not": {
                    "label": "ИЛИ НЕ"
                  }
                }, 
                "val": "and"
              }, 
              "column": {
                "type": "select", 
                "label": "Выбрать колонку", 
                "table": "input_table", 
                "bind": "COLUMNS", 
                "required": true
              }, 
              "condition": {
                "type": "select", 
                "label": "Выберите оператора", 
                "required": true, 
                "options": {
                  "КОНТЕИНЫ": {
                    "label": "КОНТЕИНЫ"
                  }, 
                  "CONTAINED IN STRING": {
                    "label": "КОНТАИНА"
                  }, 
                  "СТАРТЫ С": {
                    "label": "СТАРТЫ С"
                  }, 
                  "КОНЕЦ С": {
                    "label": "КОНЕЦ С"
                  }, 
                  "=": {
                    "label": "EQUAL"
                  }, 
                  ">": {
                    "label": "БОЛЬШЕЕ"
                  }, 
                  ">=": {
                    "label": "БОЛЬШИЙ ИЛИ РАВНЫЙ"
                  }, 
                  "<": {
                    "label": "СМАЛЬШЕЕ ТАН"
                  }, 
                  "<=": {
                    "label": "МЕНЬШИЙ ИЛИ РАВНЫЙ"
                  }, 
                  "EMPTY": {
                    "label": "ЕСТЬ ВРЕМЯ"
                  }, 
                  "BLANK": {
                    "label": "БЛАНК"
                  }, 
                  "НУЛЬ": {
                    "label": "НУЛЬ"
                  }, 
                  "REGEX MATCH": {
                    "label": "REGEX MATCH"
                  }, 
                  "CONTAINED IN TABLE": {
                    "label": "В ТАБЛИЦЕ"
                  }, 
                  "В РЕГИОНАЛЬНОЙ ТАБЛИЦЕ": {
                    "label": "В РЕГИОНАЛЬНОЙ ТАБЛИЦЕ"
                  }, 
                  "ДАТА": {
                    "label": "ДАТА"
                  }
                }, 
                "val": "КОНТЕИНЫ"
              }, 
              "date_condition": {
                "type": "select", 
                "label": "Выберите Дата Оператор", 
                "required": true, 
                "show_if": "this.condition == 'DATE'", 
                "options": {
                  "<": {
                    "label": "ПРИОР"
                  }, 
                  "<=": {
                    "label": "ПРИОР ИЛИ РАБОТА"
                  }, 
                  "=": {
                    "label": "EQUAL"
                  }, 
                  ">": {
                    "label": "ПОСЛЕ"
                  }, 
                  ">=": {
                    "label": "ПОСЛЕ ИЛИ ОБОРУДОВАНИЕ"
                  }
                }, 
                "val": "="
              }, 
              "case_sensitive": {
                "type": "select", 
                "label": "Выберите Чувствительный к регистру", 
                "required": true, 
                "show_if": "this.condition == 'CONTAINS' || this.condition == 'CONTAINED IN STRING' || this.condition == '=' || this.condition == 'STARTS WITH' || this.condition == 'ENDS WITH' || this.condition == 'REGEX MATCH' || this.condition == 'NOT REGEX MATCH' || this.condition == 'IN REGEX TABLE' || this.condition == 'NOT IN REGEX TABLE'", 
                "options": {
                  "yes": {
                    "label": "Чувствительность к регистру"
                  }, 
                  "no": {
                    "label": "Корпус Нечувствительный"
                  }
                }, 
                "val": "yes"
              }, 
              "date_value": {
                "type": "date", 
                "label": "Выберите Дата", 
                "show_if": "this.condition=='DATE'", 
                "val": "1/31/2020", 
                "required": true
              }, 
              "value": {
                "type": "text", 
                "label": "Входное значение", 
                "show_if": "this.condition != 'DATE' && this.condition != 'EMPTY' && this.condition != 'NOT EMPTY' && this.condition != 'BLANK' && this.condition != 'NOT BLANK' && this.condition != 'IS NULL' && this.condition != 'IS NOT NULL' && this.condition != 'CONTAINED IN TABLE' && this.condition != 'NOT CONTAINED IN TABLE' && this.condition != 'IN REGEX TABLE' && this.condition != 'NOT IN REGEX TABLE'", 
                "val": "", 
                "required": true
              }, 
              "compare_table": {
                "type": "select", 
                "label": "Выберите таблицу", 
                "bind": "TABLES", 
                "empty": "", 
                "show_if": "this.condition == 'CONTAINED IN TABLE' || this.condition == 'NOT CONTAINED IN TABLE' || this.condition == 'IN REGEX TABLE' || this.condition == 'NOT IN REGEX TABLE'", 
                "required": true
              }, 
              "compare_column": {
                "type": "select", 
                "label": "Выбрать колонку", 
                "table": "this.compare_table", 
                "bind": "COLUMNS", 
                "show_if": "this.condition == 'CONTAINED IN TABLE' || this.condition == 'NOT CONTAINED IN TABLE' || this.condition == 'IN REGEX TABLE' || this.condition == 'NOT IN REGEX TABLE'", 
                "required": true
              }, 
              "delete": {
                "type": "button", 
                "label": "Удалить", 
                "function": "DELETE_ROW"
              }
            }
          }, 
          "apply_to_record": {
            "type": "checkbox", 
            "label": "Применить к записям", 
            "val": "true", 
            "isOn": "true", 
            "isOff": "false", 
            "style": "margin-top: 10px", 
            "breakLine": true
          }, 
          "apply_to_group_box": {
            "type": "fieldset", 
            "label": "Обратиться в Группу", 
            "show_if": "apply_to_record=='false'", 
            "repeat": false, 
            "fields": {
              "apply_to_group_columns": {
                "type": "select", 
                "label": "Выберите групповые колонки", 
                "required": true, 
                "width": "100%", 
                "table": "input_table", 
                "multiple": true, 
                "bind": "COLUMNS", 
                "breakLine": true
              }
            }
          }
        }
      }
    }
  }
}
