{
  "options_box": {
    "type": "fieldset",
    "Метка": "Опции",
    "repeat": false,
    "fields": {
      "options.task": {
        "type": "hidden",
        "val": "multi_column"
      },
      "options.add_columns": {
        "type": "fieldset",
        "Метка": "Добавить новые столбцы",
        "repeat": true,
        "fields": {
          "new_column_name": {
            "type": "text",
            "Метка": "Название нового столбца",
            "val": "",
            "breakLine": true
          },
          "multi_column_functions__function": {
            "type": "select",
            "Метка": "Функция",
            "required": true,
            "options": {
              "sum": {
                "Метка": "Сумма"
              },
              "average": {
                "Метка": "Средний"
              },
              "median": {
                "Метка": "Медиана"
              },
              "max": {
                "Метка": "Макс"
              },
              "min": {
                "Метка": "Мин"
              },
              "standard_deviation": {
                "Метка": "Стандартное отклонение"
              },
              "product": {
                "Метка": "Продукт"
              },
              "quota": {
                "Метка": "Квота"
              },
              "absolute_growth": {
                "Метка": "Абсолютный рост"
              },
              "relative_growth": {
                "Метка": "Относительный рост"
              },
              "compound_growth": {
                "Метка": "Рост соединений"
              },
              "static_value": {
                "Метка": "Статическое значение"
              },
              "numeric_formula": {
                "Метка": "Числовая формула"
              },
              "text_formula": {
                "Метка": "Текстовая формула"
              }
            },
            "val": "sum"
          },
          "multi_column_functions__sel_columns": {
            "type": "select",
            "Метка": "Выбрать столбец(цы)",
            "show_if": "this.multi_column_functions__function=='sum' || this.multi_column_functions__function=='average' || this.multi_column_functions__function=='median' || this.multi_column_functions__function=='max' || this.multi_column_functions__function=='min' || this.multi_column_functions__function=='standard_deviation' || this.multi_column_functions__function=='product'",
            "table": "input_table",
            "bind": "COLUMNS",
            "multiple": true,
            "required": true
          },
          "multi_column_functions__column_a": {
            "type": "select",
            "Метка": "Выбрать столбец А",
            "show_if": "this.multi_column_functions__function=='quota' || this.multi_column_functions__function=='absolute_growth' || this.multi_column_functions__function=='relative_growth' || this.multi_column_functions__function=='compound_growth'",
            "table": "input_table",
            "bind": "COLUMNS",
            "required": true
          },
          "multi_column_functions__column_b": {
            "type": "select",
            "Метка": "Выбрать столбец B",
            "show_if": "this.multi_column_functions__function=='quota' || this.multi_column_functions__function=='absolute_growth' || this.multi_column_functions__function=='relative_growth' || this.multi_column_functions__function=='compound_growth'",
            "table": "input_table",
            "bind": "COLUMNS",
            "required": true
          },
          "multi_column_functions__n_value": {
            "type": "text",
            "Метка": "N (целое положительное число)",
            "show_if": "this.multi_column_functions__function=='compound_growth'",
            "val": "1",
            "required": true
          },
          "static_value": {
            "type": "text",
            "Метка": "Добавить значение",
            "show_if": "this.multi_column_functions__function=='static_value'",
            "val": "",
            "width": "100%"
          },
          "numeric_formula": {
            "type": "text",
            "Метка": "Формула ввода, например, [Column1] + 2 * ([Row-1:Column3] - [Column1])",
            "show_if": "this.multi_column_functions__function=='numeric_formula'",
            "val": "",
            "width": "100%"
          },
          "text_formula": {
            "type": "text",
            "Метка": "Формула ввода, например, [FirstName] + ' ' + [LastName] + ' ' + [Row-1:Column3]".,
            "show_if": "this.multi_column_functions__function=='text_formula'",
            "val": "",
            "width": "100%"
          },
          "use_multi_row": {
            "type": "checkbox",
            "Метка": "Настройки нескольких строк",
            "show_if": "this.multi_column_functions__function=='numeric_formula' || this.multi_column_functions__function=='text_formula'",
            "val": "false",
            "isOn": "true",
            "isOff": "false"
          },
          "if_row_not_exist": {
            "type": "select",
            "Метка": "Если упомянутой строки не существует",
            "show_if": "this.use_multi_row=='true' && (this.multi_column_functions__function=='numeric_formula' || this.multi_column_functions__function=='text_formula')",
            "required": true,
            "options": {
              "empty": {
                "Метка": "Пусто или 0"
              },
              "closest": {
                "Метка": "Значение ближайшей строки"
              }
            },
            "val": "empty",
            "breakLine": true
          },
          "format__category": {
            "type": "select",
            "Метка": "Формат",
            "required": true,
            "options": {
              "standard": {
                "Метка": "Стандартный"
              },
              "number": {
                "Метка": "Число"
              },
              "percentage": {
                "Метка": "Процент"
              },
              "text": {
                "Метка": "Текст"
              },
              "date": {
                "Метка": "Дата"
              },
              "currency": {
                "Метка": "Валюта"
              }
            },
            "val": "standard"
          },
          "format__rounding": {
            "type": "select",
            "Метка": "Округление",
            "show_if": "this.format__category=='number' || this.format__category=='percentage' || this.format__category=='currency'",
            "required": true,
            "options": {
              "no_rounding": {
                "Метка": "Округление чисел"
              },
              "0": {
                "Метка": "0 цифр"
              },
              "1": {
                "Метка": "1 цифра"
              },
              "2": {
                "Метка": "2 цифры"
              },
              "3": {
                "Метка": "3 цифры"
              },
              "4": {
                "Метка": "4 цифры"
              },
              "5": {
                "Метка": "5 цифр"
              },
              "6": {
                "Метка": "6 цифр"
              },
              "7": {
                "Метка": "7 цифр"
              },
              "8": {
                "Метка": "8 цифр"
              },
              "9": {
                "Метка": "9 цифр"
              }
            },
            "val": "2"
          },
          "format__use_1000_separator": {
            "type": "checkbox",
            "Метка": "Use 1000 Separator(.)",
            "show_if": "this.format__category=='number' || this.format__category=='percentage' || this.format__category=='currency'",
            "val": "true",
            "isOn": "true",
            "isOff": "false"
          },
          "format__currency_symbol": {
            "type": "select",
            "Метка": "Символ",
            "show_if": "this.format__category=='currency'",
            "required": true,
            "options": {
              "dollar": {
                "Метка": "$"
              },
              "euro": {
                "Метка": "€"
              },
              "pound": {
                "Метка": "£"
              }
            },
            "val": "dollar"
          },
          "format__date_format": {
            "type": "select",
            "Метка": "Тип",
            "show_if": "this.format__category=='date'",
            "required": true,
            "options": {
              "mm/dd/yyyy": {
                "Метка": "3/14/2012"
              },
              "D, M dd, Y": {
                "Метка": "Среда, 14 марта 2012 года"
              },
              "yyyy-mm-dd": {
                "Метка": "2012-03-14"
              }
            },
            "val": "mm/dd/yyyy",
            "breakLine": true
          },
          "group_counter__inc_dec_curly_bracket": {
            "type": "checkbox",
            "Метка": "Увеличение/уменьшение с помощью {}",
            "show_if": "this.task_type=='group_counter'",
            "val": "false",
            "isOn": "true",
            "isOff": "false",
            "breakLine": true
          },
          "group_counter__inc_dec_square_bracket": {
            "type": "checkbox",
            "Метка": "Увеличение/уменьшение с помощью []",
            "show_if": "this.task_type=='group_counter'",
            "val": "false",
            "isOn": "true",
            "isOff": "false",
            "breakLine": true
          },
          "group_counter__inc_dec_parenthesis": {
            "type": "checkbox",
            "Метка": "Увеличение/уменьшение с помощью ()",
            "show_if": "this.task_type=='group_counter'",
            "val": "false",
            "isOn": "true",
            "isOff": "false",
            "breakLine": true
          },
          "group_counter__custom": {
            "type": "checkbox",
            "Метка": "Пользовательский счетчик группы",
            "show_if": "this.task_type=='group_counter'",
            "val": "false",
            "isOn": "true",
            "isOff": "false",
            "breakLine": true
          },
          "group_counter__column": {
            "type": "select",
            "Метка": "Столбец",
            "show_if": "this.task_type=='group_counter' && this.group_counter__custom=='true'",
            "table": "input_table",
            "bind": "COLUMNS",
            "required": true
          },
          "group_counter__type": {
            "type": "select",
            "Метка": "Когда",
            "show_if": "this.task_type=='group_counter' && this.group_counter__custom=='true'",
            "required": true,
            "options": {
              "starts_with": {
                "Метка": "НАЧИНАЕТСЯ С"
              },
              "ends_with": {
                "Метка": "ЗАКАНЧИВАЕТСЯ С"
              },
              "starts_with_regex": {
                "Метка": "НАЧИНАЕТСЯ С РЕГУЛЯРНОГО ВЫРАЖЕНИЯ"
              },
              "ends_with_regex": {
                "Метка": "ЗАКАНЧИВАЕТСЯ С РЕГУЛЯРНЫМ ВЫРАЖЕНИЕМ"
              }
            },
            "val": "starts_with"
          },
          "group_counter__value": {
            "type": "text",
            "Метка": "Добавить значение",
            "show_if": "this.task_type=='group_counter' && this.group_counter__custom=='true'",
            "val": ".",
            "breakLine": true
          },
          "group_counter__label": {
            "type": "text",
            "Метка": "Добавить метку",
            "show_if": "this.task_type=='group_counter' && this.group_counter__custom=='true'",
            "val": "sentence",
            "breakLine": true
          },
          "group_counter__if_end_of_string_start_labeling_with_next_record": {
            "type": "checkbox",
            "Метка": "Если конец строки отмечается со следующей записи.",
            "show_if": "this.task_type=='group_counter' && this.group_counter__custom=='true' && (this.group_counter__type=='starts_with' || this.group_counter__type=='starts_with_regex')",
            "val": "true",
            "isOn": "true",
            "isOff": "false"
          },
          "group_counter__if_begin_of_string_start_labeling_with_previous_record": {
            "type": "checkbox",
            "Метка": "Если начало строки отмечается с предыдущей записи.",
            "show_if": "this.task_type=='group_counter' && this.group_counter__custom=='true' && (this.group_counter__type=='ends_with' || this.group_counter__type=='ends_with_regex')",
            "val": "true",
            "isOn": "true",
            "isOff": "false"
          },
          "group_counter__always_include": {
            "type": "checkbox",
            "Метка": "Всегда включать",
            "show_if": "this.task_type=='group_counter' && this.group_counter__custom=='true'",
            "val": "true",
            "isOn": "true",
            "isOff": "false"
          },
          "apply_to_group__enabled": {
            "type": "checkbox",
            "Метка": "Применить к группе",
            "show_if": "this.multi_column_functions__function!='static_value'",
            "val": "false",
            "isOn": "true",
            "isOff": "false"
          },
          "apply_to_group__group_columns": {
            "type": "select",
            "Метка": "Групповой столбец(цы)",
            "show_if": "this.apply_to_group__enabled == 'true' && this.multi_column_functions__function!='static_value'",
            "table": "input_table",
            "bind": "COLUMNS",
            "multiple": true,
            "required": true
          },
          "delete": {
            "type": "button",
            "Метка": "Удалить",
            "function": "DELETE_ROW"
          }
        }
      },
      "options.use_filter": {
        "type": "checkbox",
        "Метка": "Использовать фильтр",
        "val": "false",
        "isOn": "true",
        "isOff": "false",
        "style": "margin-top: 10px",
        "breakLine": true
      },
      "filter_box": {
        "type": "fieldset",
        "Метка": "Настройки фильтра",
        "show_if": "options.use_filter=='true'",
        "repeat": false,
        "fields": {
          "options.filter": {
            "type": "fieldset",
            "Метка": "Добавить условия фильтрации",
            "repeat": true,
            "fields": {
              "initial_op": {
                "type": "select",
                "Метка": "Выбрать оператора логики",
                "required": true,
                "show_if" :  "$index == 0",
                "options": {
                  "if": {
                    "Метка": "ЕСЛИ"
                  },
                  "if_not": {
                    "Метка": "ЕСЛИ НЕ"
                  }
                },
                "val": "if"
              },
              "op": {
                "type": "select",
                "Метка": "Выбрать оператора логики",
                "required": true,
                "show_if" :  "$index > 0",
                "options": {
                  "and": {
                    "Метка": "И"
                  },
                  "or": {
                    "Метка": "ИЛИ"
                  },
                  "and_not": {
                    "Метка": "И НЕ"
                  },
                  "or_not": {
                    "Метка": "ИЛИ НЕ"
                  }
                },
                "val": "and"
              },
              "column": {
                "type": "select",
                "Метка": "Выбрать столбец",
                "table": "input_table",
                "bind": "COLUMNS",
                "required": true
              },
              "condition": {
                "type": "select",
                "Метка": "Выбрать оператора",
                "required": true,
                "options": {
                  "СОДЕРЖИТ": {
                    "Метка": "СОДЕРЖИТ"
                  },
                  "CONTAINED IN STRING": {
                    "Метка": "СОДЕРЖАЛОСЬ В"
                  },
                  "НАЧИНАЕТСЯ С": {
                    "Метка": "НАЧИНАЕТСЯ С"
                  },
                  "ЗАКАНЧИВАЕТСЯ С": {
                    "Метка": "ЗАКАНЧИВАЕТСЯ С"
                  },
                  "=": {
                    "Метка": "Равный"
                  },
                  ">": {
                    "Метка": "БОЛЬШЕ ЧЕМ"
                  },
                  ">=": {
                    "Метка": "БОЛЬШИЙ ИЛИ РАВНЫЙ"
                  },
                  "<": {
                    "Метка": "МЕНЬШЕ ЧЕМ"
                  },
                  "<=": {
                    "Метка": "МЕНЬШИЙ ИЛИ РАВНЫЙ"
                  },
                  "EMPTY": {
                    "Метка": "ПУСТО"
                  },
                  "BLANK": {
                    "Метка": "ПУСТО"
                  },
                  "НОЛЬ": {
                    "Метка": "НОЛЬ"
                  },
                  "Совпадение регулярного выражения": {
                    "Метка": "Совпадение регулярного выражения"
                  },
                  "CONTAINED IN TABLE": {
                    "Метка": "В ТАБЛИЦЕ"
                  },
                  "В ТАБЛИЦЕ РЕГУЛЯРНЫХ ВЫРАЖЕНИЙ": {
                    "Метка": "В ТАБЛИЦЕ РЕГУЛЯРНЫХ ВЫРАЖЕНИЙ"
                  },
                  "ДАТА": {
                    "Метка": "ДАТА"
                  }
                },
                "val": "СОДЕРЖИТ"
              },
              "date_condition": {
                "type": "select",
                "Метка": "Выбрать оператора даты",
                "required": true,
                "show_if": "this.condition == 'DATE'",
                "options": {
                  "<": {
                    "Метка": "ПРЕДЫДУЩИЙ"
                  },
                  "<=": {
                    "Метка": "ПРЕДЫДУЩИЙ ИЛИ РАВНЫЙ"
                  },
                  "=": {
                    "Метка": "Равный"
                  },
                  ">": {
                    "Метка": "ПОСЛЕ"
                  },
                  ">=": {
                    "Метка": "ПОСЛЕ ИЛИ РАВНО"
                  }
                },
                "val": "="
              },
              "case_sensitive": {
                "type": "select",
                "Метка": "Выбрать с учетом регистра",
                "required": true,
                "show_if": "this.condition == 'CONTAINS' || this.condition == 'CONTAINED IN STRING' || this.condition == '=' || this.condition == 'STARTS WITH' || this.condition == 'ENDS WITH' || this.condition == 'REGEX MATCH' || this.condition == 'NOT REGEX MATCH' || this.condition == 'IN REGEX TABLE' || this.condition == 'NOT IN REGEX TABLE'",
                "options": {
                  "yes": {
                    "Метка": "С учетом регистра"
                  },
                  "no": {
                    "Метка": "Без учета регистра"
                  }
                },
                "val": "yes"
              },
              "date_value": {
                "type": "date",
                "Метка": "Выбрать дату",
                "show_if": "this.condition=='DATE'",
                "val": "",
                "required": true
              },
              "value": {
                "type": "text",
                "Метка": "Добавить значение",
                "show_if": "this.condition != 'DATE' && this.condition != 'EMPTY' && this.condition != 'NOT EMPTY' && this.condition != 'BLANK' && this.condition != 'NOT BLANK' && this.condition != 'IS NULL' && this.condition != 'IS NOT NULL' && this.condition != 'CONTAINED IN TABLE' && this.condition != 'NOT CONTAINED IN TABLE' && this.condition != 'IN REGEX TABLE' && this.condition != 'NOT IN REGEX TABLE'",
                "val": "",
                "required": true
              },
              "compare_table": {
                "type": "select",
                "Метка": "Выбрать таблицу",
                "bind": "TABLES",
                "empty": "",
                "show_if": "this.condition == 'CONTAINED IN TABLE' || this.condition == 'NOT CONTAINED IN TABLE' || this.condition == 'IN REGEX TABLE' || this.condition == 'NOT IN REGEX TABLE'",
                "required": true
              },
              "compare_column": {
                "type": "select",
                "Метка": "Выбрать столбец",
                "table": "this.compare_table",
                "bind": "COLUMNS",
                "show_if": "this.condition == 'CONTAINED IN TABLE' || this.condition == 'NOT CONTAINED IN TABLE' || this.condition == 'IN REGEX TABLE' || this.condition == 'NOT IN REGEX TABLE'",
                "required": true
              },
              "delete": {
                "type": "button",
                "Метка": "Удалить",
                "function": "DELETE_ROW"
              }
            }
          },
          "options.apply_to_record": {
            "type": "checkbox",
            "Метка": "Применить к регистру",
            "val": "true",
            "isOn": "true",
            "isOff": "false",
            "style": "margin-top: 10px",
            "breakLine": true
          },
          "apply_to_group_box": {
            "type": "fieldset",
            "Метка": "Применить к группе",
            "show_if": "options.apply_to_record=='false'",
            "repeat": false,
            "fields": {
              "options.apply_to_group_columns": {
                "type": "select",
                "Метка": "Выбрать групповые столбцы",
                "required": true,
                "width": "100%",
                "table": "input_table",
                "multiple": true,
                "bind": "COLUMNS",
                "breakLine": true
              }
            }
          }
        }
      }
    }
  }
}