{
  "options": {
    "type": "fieldset", 
    "label": "Выбрать параметры частоты", 
    "repeat": false, 
    "fields": {
      "options.type": {
        "type": "select", 
        "label": "Тип", 
        "options": {
          "generate": {
            "label": "Создать пример частоты"
          }, 
          "process": {
            "label": "Таблица частоты процесса"
          }
        }, 
        "val": "generate", 
        "breakLine": true
      }, 
      "amount_formatting_frame": {
        "type": "fieldset", 
        "label": "Amount Column Formatting", 
        "show_if": "options.type=='generate'", 
        "repeat": false, 
        "fields": {
          "options.amount.format__category": {
            "type": "select", 
            "label": "Формат", 
            "required": true, 
            "options": {
              "standard": {
                "label": "Стандартный"
              }, 
              "number": {
                "label": "Число"
              }, 
              "percentage": {
                "label": "Процент"
              }, 
              "currency": {
                "label": "Валюта"
              }
            }, 
            "val": "currency"
          }, 
          "options.amount.format__rounding": {
            "type": "select", 
            "label": "Округление", 
            "show_if": "options.amount.format__category=='number' || options.amount.format__category=='percentage' || options.amount.format__category=='currency'", 
            "required": true, 
            "options": {
              "no_rounding": {
                "label": "Округление чисел"
              }, 
              "0": {
                "label": "0 цифр"
              }, 
              "1": {
                "label": "1 цифра"
              }, 
              "2": {
                "label": "2 цифры"
              }, 
              "3": {
                "label": "3 цифры"
              }, 
              "4": {
                "label": "4 цифры"
              }, 
              "5": {
                "label": "5 цифр"
              }, 
              "6": {
                "label": "6 цифр"
              }, 
              "7": {
                "label": "7 цифр"
              }, 
              "8": {
                "label": "8 цифр"
              }, 
              "9": {
                "label": "9 цифр"
              }
            }, 
            "val": "2"
          }, 
          "options.amount.format__use_1000_separator": {
            "type": "checkbox", 
            "label": "Используйте сепаратор 1000(.)", 
            "show_if": "options.amount.format__category=='number' || options.amount.format__category=='percentage' || options.amount.format__category=='currency'", 
            "val": "true", 
            "isOn": "true", 
            "isOff": "false"
          }, 
          "options.amount.format__currency_symbol": {
            "type": "select", 
            "label": "Символ", 
            "show_if": "options.amount.format__category=='currency'", 
            "required": true, 
            "options": {
              "dollar": {
                "label": "$"
              }, 
              "euro": {
                "label": "€"
              }, 
              "pound": {
                "label": "£"
              }
            }, 
            "val": "dollar"
          }
        }
      }, 
      "options.standard_table": {
        "type": "checkbox", 
        "label": "Стандартная таблица частоты", 
        "show_if": "options.type=='process'", 
        "val": "true", 
        "isOn": "true", 
        "isOff": "false", 
        "breakLine": true
      }, 
      "options.initial_date": {
        "type": "select", 
        "label": "Начальная дата", 
        "show_if": "options.type=='process' && options.standard_table=='false'", 
        "table": "input_table", 
        "bind": "COLUMNS", 
        "required": true
      }, 
      "options.period": {
        "type": "select", 
        "label": "Частота (например, ежемесячно, ежеквартально)", 
        "show_if": "options.type=='process' && options.standard_table=='false'", 
        "table": "input_table", 
        "bind": "COLUMNS", 
        "required": true
      }, 
      "options.final_date": {
        "type": "select", 
        "label": "Заключительная дата", 
        "show_if": "options.type=='process' && options.standard_table=='false'", 
        "table": "input_table", 
        "bind": "COLUMNS", 
        "breakLine": true
      }, 
      "options.volume": {
        "type": "select", 
        "label": "Сумма", 
        "show_if": "options.type=='process' && options.standard_table=='false'", 
        "table": "input_table", 
        "bind": "COLUMNS"
      }, 
      "options.growth": {
        "type": "select", 
        "label": "Рост", 
        "show_if": "options.type=='process' && options.standard_table=='false'", 
        "table": "input_table", 
        "bind": "COLUMNS"
      }, 
      "options.growth_period": {
        "type": "select", 
        "label": "Период роста", 
        "show_if": "options.type=='process' && options.standard_table=='false'", 
        "options": {
          "selected_period": {
            "label": "Равно частоте"
          }, 
          "week_on_week": {
            "label": "Неделя за неделей"
          }, 
          "month_on_month": {
            "label": "Месяц в месяц"
          }, 
          "quarter_on_quarter": {
            "label": "Квартал за кварталом"
          }, 
          "year_on_year": {
            "label": "Год за годом"
          }, 
          "column": {
            "label": "Выбрать столбец"
          }
        }, 
        "val": "selected_period"
      }, 
      "options.growth_period_column": {
        "type": "select", 
        "label": "Колонка периода роста", 
        "show_if": "options.type=='process' && options.standard_table=='false' && options.growth_period=='column'", 
        "table": "input_table", 
        "bind": "COLUMNS", 
        "required": true, 
        "breakLine": true
      }, 
      "options.use_growth_pattern_column": {
        "type": "checkbox", 
        "label": "Использовать шаблон роста", 
        "show_if": "options.type=='process' && options.standard_table=='false'", 
        "val": "false", 
        "isOn": "true", 
        "isOff": "false"
      }, 
      "options.growth_pattern_column": {
        "type": "select", 
        "label": "Модель роста", 
        "show_if": "options.type=='process' && options.standard_table=='false' && options.use_growth_pattern_column=='true'", 
        "table": "input_table", 
        "bind": "COLUMNS", 
        "required": true, 
        "breakLine": true
      }, 
      "options.end_date": {
        "type": "date", 
        "label": "Конечная дата таблицы", 
        "show_if": "options.type=='process'", 
        "val": "", 
        "required": true, 
        "style": "width: 300px"
      }
    }
  }
}