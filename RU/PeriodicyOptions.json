{
  "options": {
    "type": "fieldset",
    "label": "Выбрать параметры частоты",
    "repeat": false,
    "fields": {
      "options.type": {
        "type": "select",
        "label": "Тип",
        "options": {
          "generate": {
            "label": "Создать пример частоты"
          },
          "process": {
            "label": "Таблица частоты процесса"
          },
          "custom": {
            "label": "Пользовательский"
          }
        },
        "val": "custom",
        "breakLine": true
      },
      "input": {
        "type": "fieldset",
        "label": "Выбрать входную таблицу",
        "show_if": "options.type=='process'",
        "repeat": false,
        "fields": {
          "input_table": {
            "type": "select",
            "label": "Выбрать таблицу",
            "bind": "TABLES",
            "empty": "",
            "required": true
          },
          "remove_input_table_after_processing": {
            "type": "hidden",
            "val": "false"
          },
          "is_word": {
            "type": "hidden",
            "val": "false"
          }
        },
        "breakLine": true
      },
      "amount_formatting_frame": {
        "type": "fieldset",
        "label": "Amount Column Formatting",
        "show_if": "options.type=='generate'",
        "repeat": false,
        "fields": {
          "options.amount.format__category": {
            "type": "select",
            "label": "Формат",
            "required": true,
            "options": {
              "standard": {
                "label": "Стандартный"
              },
              "number": {
                "label": "Число"
              },
              "percentage": {
                "label": "Процент"
              },
              "currency": {
                "label": "Валюта"
              }
            },
            "val": "currency"
          },
          "options.amount.format__rounding": {
            "type": "select",
            "label": "Округление",
            "show_if": "options.amount.format__category=='number' || options.amount.format__category=='percentage' || options.amount.format__category=='currency'",
            "required": true,
            "options": {
              "no_rounding": {
                "label": "Округление чисел"
              },
              "0": {
                "label": "0 цифр"
              },
              "1": {
                "label": "1 цифра"
              },
              "2": {
                "label": "2 цифры"
              },
              "3": {
                "label": "3 цифры"
              },
              "4": {
                "label": "4 цифры"
              },
              "5": {
                "label": "5 цифр"
              },
              "6": {
                "label": "6 цифр"
              },
              "7": {
                "label": "7 цифр"
              },
              "8": {
                "label": "8 цифр"
              },
              "9": {
                "label": "9 цифр"
              }
            },
            "val": "2"
          },
          "options.amount.format__use_1000_separator": {
            "type": "checkbox",
            "label": "Используйте сепаратор 1000(.)",
            "show_if": "options.amount.format__category=='number' || options.amount.format__category=='percentage' || options.amount.format__category=='currency'",
            "val": "true",
            "isOn": "true",
            "isOff": "false"
          },
          "options.amount.format__currency_symbol": {
            "type": "select",
            "label": "Символ",
            "show_if": "options.amount.format__category=='currency'",
            "required": true,
            "options": {
              "dollar": {
                "label": "$"
              },
              "euro": {
                "label": "€"
              },
              "pound": {
                "label": "£"
              }
            },
            "val": "dollar"
          }
        }
      },
      "options.standard_table": {
        "type": "checkbox",
        "label": "Стандартная таблица частоты",
        "show_if": "options.type=='process'",
        "val": "true",
        "isOn": "true",
        "isOff": "false",
        "breakLine": true
      },
      "options.initial_date": {
        "type": "select",
        "label": "Начальная дата",
        "show_if": "options.type=='process' && options.standard_table=='false'",
        "table": "input_table",
        "bind": "COLUMNS",
        "required": true
      },
      "options.period": {
        "type": "select",
        "label": "Periodicy (e.g. Monthly, Quarterly)",
        "show_if": "options.type=='process' && options.standard_table=='false'",
        "table": "input_table",
        "bind": "COLUMNS",
        "required": true
      },
      "options.final_date": {
        "type": "select",
        "label": "Заключительная дата",
        "show_if": "options.type=='process' && options.standard_table=='false'",
        "table": "input_table",
        "bind": "COLUMNS",
        "breakLine": true
      },
      "options.volume": {
        "type": "select",
        "label": "Сумма",
        "show_if": "options.type=='process' && options.standard_table=='false'",
        "table": "input_table",
        "bind": "COLUMNS"
      },
      "options.growth": {
        "type": "select",
        "label": "Рост",
        "show_if": "options.type=='process' && options.standard_table=='false'",
        "table": "input_table",
        "bind": "COLUMNS"
      },
      "options.growth_period": {
        "type": "select",
        "label": "Период роста",
        "show_if": "options.type=='process' && options.standard_table=='false'",
        "options": {
          "selected_period": {
            "label": "Равно частоте"
          },
          "week_on_week": {
            "label": "Неделя за неделей"
          },
          "month_on_month": {
            "label": "Месяц в месяц"
          },
          "quarter_on_quarter": {
            "label": "Квартал за кварталом"
          },
          "year_on_year": {
            "label": "Год за годом"
          },
          "column": {
            "label": "Выбрать столбец"
          }
        },
        "val": "selected_period"
      },
      "options.growth_period_column": {
        "type": "select",
        "label": "Колонка периода роста",
        "show_if": "options.type=='process' && options.standard_table=='false' && options.growth_period=='column'",
        "table": "input_table",
        "bind": "COLUMNS",
        "required": true,
        "breakLine": true
      },
      "options.use_growth_pattern_column": {
        "type": "checkbox",
        "label": "Использовать шаблон роста",
        "show_if": "options.type=='process' && options.standard_table=='false'",
        "val": "false",
        "isOn": "true",
        "isOff": "false"
      },
      "options.growth_pattern_column": {
        "type": "select",
        "label": "Модель роста",
        "show_if": "options.type=='process' && options.standard_table=='false' && options.use_growth_pattern_column=='true'",
        "table": "input_table",
        "bind": "COLUMNS",
        "required": true,
        "breakLine": true
      },
      "options.custom.table": {
        "type": "fieldset",
        "label": "Planning Position",
        "show_if": "options.type=='custom'",
        "repeat": true,
        "fields": {
          "item": {
            "type": "text",
            "label": "Item",
            "val": "",
            "required": true
          },
          "position": {
            "type": "text",
            "label": "Позиция",
            "val": "",
            "required": true,
            "breakLine": true
          },
          "amount": {
            "type": "number",
            "label": "Сумма",
            "val": "",
            "required": true
          },
          "amount_format__category": {
            "type": "select",
            "label": "Amount Format",
            "required": true,
            "options": {
              "standard": {
                "label": "Стандартный"
              },
              "number": {
                "label": "Число"
              },
              "percentage": {
                "label": "Процент"
              },
              "text": {
                "label": "Текст"
              },
              "date": {
                "label": "Дата"
              },
              "currency": {
                "label": "Валюта"
              }
            },
            "val": "standard"
          },
          "amount_format__rounding": {
            "type": "select",
            "label": "Округление",
            "show_if": "this.amount_format__category=='number' || this.amount_format__category=='percentage' || this.amount_format__category=='currency'",
            "required": true,
            "options": {
              "no_rounding": {
                "label": "Округление чисел"
              },
              "0": {
                "label": "0 цифр"
              },
              "1": {
                "label": "1 цифра"
              },
              "2": {
                "label": "2 цифры"
              },
              "3": {
                "label": "3 цифры"
              },
              "4": {
                "label": "4 цифры"
              },
              "5": {
                "label": "5 цифр"
              },
              "6": {
                "label": "6 цифр"
              },
              "7": {
                "label": "7 цифр"
              },
              "8": {
                "label": "8 цифр"
              },
              "9": {
                "label": "9 цифр"
              }
            },
            "val": "2"
          },
          "amount_format__use_1000_separator": {
            "type": "checkbox",
            "label": "Используйте сепаратор 1000(.)",
            "show_if": "this.amount_format__category=='number' || this.amount_format__category=='percentage' || this.amount_format__category=='currency'",
            "val": "true",
            "isOn": "true",
            "isOff": "false"
          },
          "amount_format__currency_symbol": {
            "type": "select",
            "label": "Символ",
            "show_if": "this.amount_format__category=='currency'",
            "required": true,
            "options": {
              "dollar": {
                "label": "$"
              },
              "euro": {
                "label": "€"
              },
              "pound": {
                "label": "£"
              }
            },
            "val": "dollar"
          },
          "amount_format__date_format": {
            "type": "select",
            "label": "Тип",
            "show_if": "this.amount_format__category=='date'",
            "required": true,
            "options": {
              "mm/dd/yyyy": {
                "label": "3/14/2012"
              },
              "D, M dd, Y": {
                "label": "Wednesday, March 14, 2012"
              },
              "yyyy-mm-dd": {
                "label": "2012-03-14"
              }
            },
            "val": "mm/dd/yyyy",
            "breakLine": true
          },
          "initial_date": {
            "type": "date",
            "label": "Начальная дата",
            "val": "",
            "required": true
          },
          "initial_date__category": {
            "type": "hidden",
            "val": "date"
          },
          "initial_date_format__date_format": {
            "type": "select",
            "label": "Тип",
            "required": true,
            "options": {
              "mm/dd/yyyy": {
                "label": "3/14/2012"
              },
              "D, M dd, Y": {
                "label": "Wednesday, March 14, 2012"
              },
              "yyyy-mm-dd": {
                "label": "2012-03-14"
              }
            },
            "val": "mm/dd/yyyy",
            "breakLine": true
          },
          "periodicy": {
            "type": "select",
            "label": "Периодический",
            "required": true,
            "options": {
              "one_off": {
                "label": "One-Off"
              },
              "daily": {
                "label": "Daily"
              },
              "work_daily": {
                "label": "Work-Daily"
              },
              "weekly": {
                "label": "Weekly"
              },
              "monthly": {
                "label": "Monthly"
              },
              "quarterly": {
                "label": "Quarterly"
              },
              "yearly": {
                "label": "Yearly"
              }
            },
            "val": "one_off"
          },
          "growth": {
            "type": "number",
            "label": "Рост",
            "show_if": "this.periodicy!='one_off'",
            "val": "",
            "required": true
          },
          "growth_format__category": {
            "type": "select",
            "label": "Growth Format",
            "show_if": "this.periodicy!='one_off'",
            "required": true,
            "options": {
              "standard": {
                "label": "Стандартный"
              },
              "number": {
                "label": "Число"
              },
              "percentage": {
                "label": "Процент"
              },
              "text": {
                "label": "Текст"
              },
              "date": {
                "label": "Дата"
              },
              "currency": {
                "label": "Валюта"
              }
            },
            "val": "standard"
          },
          "growth_format__rounding": {
            "type": "select",
            "label": "Округление",
            "show_if": "this.periodicy!='one_off' && (this.growth_format__category=='number' || this.growth_format__category=='percentage' || this.growth_format__category=='currency')",
            "required": true,
            "options": {
              "no_rounding": {
                "label": "Округление чисел"
              },
              "0": {
                "label": "0 цифр"
              },
              "1": {
                "label": "1 цифра"
              },
              "2": {
                "label": "2 цифры"
              },
              "3": {
                "label": "3 цифры"
              },
              "4": {
                "label": "4 цифры"
              },
              "5": {
                "label": "5 цифр"
              },
              "6": {
                "label": "6 цифр"
              },
              "7": {
                "label": "7 цифр"
              },
              "8": {
                "label": "8 цифр"
              },
              "9": {
                "label": "9 цифр"
              }
            },
            "val": "2"
          },
          "growth_format__use_1000_separator": {
            "type": "checkbox",
            "label": "Используйте сепаратор 1000(.)",
            "show_if": "this.periodicy!='one_off' && (this.growth_format__category=='number' || this.growth_format__category=='percentage' || this.growth_format__category=='currency')",
            "val": "true",
            "isOn": "true",
            "isOff": "false"
          },
          "growth_format__currency_symbol": {
            "type": "select",
            "label": "Символ",
            "show_if": "this.periodicy!='one_off' && this.growth_format__category=='currency'",
            "required": true,
            "options": {
              "dollar": {
                "label": "$"
              },
              "euro": {
                "label": "€"
              },
              "pound": {
                "label": "£"
              }
            },
            "val": "dollar"
          },
          "growth_format__date_format": {
            "type": "select",
            "label": "Тип",
            "show_if": "this.periodicy!='one_off' && this.growth_format__category=='date'",
            "required": true,
            "options": {
              "mm/dd/yyyy": {
                "label": "3/14/2012"
              },
              "D, M dd, Y": {
                "label": "Wednesday, March 14, 2012"
              },
              "yyyy-mm-dd": {
                "label": "2012-03-14"
              }
            },
            "val": "mm/dd/yyyy",
            "breakLine": true
          },
          "growth_period": {
            "type": "select",
            "label": "Период роста",
            "show_if": "this.periodicy!='one_off'",
            "options": {
              "week_on_week": {
                "label": "Неделя за неделей"
              },
              "month_on_month": {
                "label": "Месяц в месяц"
              },
              "quarter_on_quarter": {
                "label": "Квартал за кварталом"
              },
              "year_on_year": {
                "label": "Год за годом"
              }
            },
            "val": "week_on_week"
          },
          "growth_pattern": {
            "type": "select",
            "label": "Модель роста",
            "show_if": "this.periodicy!='one_off'",
            "options": {
              "compounding": {
                "label": "Compounding"
              },
              "linear": {
                "label": "Линейный"
              },
              "jump_fix": {
                "label": "Jump-Fix"
              }
            },
            "val": "compounding"
          },
          "final_date": {
            "type": "date",
            "label": "Заключительная дата",
            "show_if": "this.periodicy!='one_off'",
            "val": "",
            "required": true
          },
          "final_date_format__category": {
            "type": "hidden",
            "val": "date"
          },
          "final_date_format__date_format": {
            "type": "select",
            "label": "Тип",
            "show_if": "this.periodicy!='one_off'",
            "required": true,
            "options": {
              "mm/dd/yyyy": {
                "label": "3/14/2012"
              },
              "D, M dd, Y": {
                "label": "Wednesday, March 14, 2012"
              },
              "yyyy-mm-dd": {
                "label": "2012-03-14"
              }
            },
            "val": "mm/dd/yyyy",
            "breakLine": true
          },
          "delete": {
            "type": "button",
            "label": "Удалить",
            "function": "DELETE_ROW"
          }
        }
      },
      "options.end_date": {
        "type": "date",
        "label": "Конечная дата таблицы",
        "show_if": "options.type=='process' || options.type=='custom'",
        "val": "",
        "required": true,
        "style": "width: 300px"
      }
    }
  }
}