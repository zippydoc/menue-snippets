{
  "options": {
    "type": "fieldset",
    "label": "Выбрать параметры",
    "repeat": false,
    "fields": {
      "options.task_type": {
        "type": "select",
        "label": "Тип задачи",
        "required": true,
        "options": {
          "generate_lookup_table": {
            "label": "Сгенерировать таблицу поиска"
          },
          "apply_lookup_table": {
            "label": "Применить таблицу поиска"
          },
          "apply_lookup_table_to_cells": {
            "label": "Применить таблицу поиска к ячейкам"
          }
        },
        "val": "generate_lookup_table"
      },
      "options.pattern_type": {
        "type": "hidden",
        "val": "number_range"
      },
      "duplicate_option_box": {
        "type": "fieldset",
        "label": "Возможность дублирования шаблонов",
        "show_if": "options.task_type=='label_duplicates' || (options.task_type=='generate_lookup_table' && options.pattern_type=='duplicate_pattern')",
        "repeat": false,
        "fields": {
          "options.minimum_number_of_matching_characters": {
            "type": "text",
            "label": "Минимальное количество совпадающих символов",
            "val": "3",
            "required": true,
            "style": "min-width: 240px"
          },
          "options.minimum_number_of_duplicates": {
            "type": "text",
            "label": "Минимальное число дубликатов",
            "val": "2",
            "required": true,
            "style": "min-width: 240px",
            "breakLine": true
          }
        }
      },
      "number_range_option_box": {
        "type": "fieldset",
        "label": "Настройка диапазонов чисел",
        "show_if": "options.task_type=='generate_lookup_table' && options.pattern_type=='number_range'",
        "repeat": false,
        "fields": {
          "options.number_range.range_type": {
            "type": "select",
            "label": "Тип диапазона",
            "required": true,
            "options": {
              "equal_interval": {
                "label": "N равных интервалов"
              },
              "equal_range": {
                "label": "Равный диапазон"
              },
              "custom_range": {
                "label": "Пользовательский диапазон"
              }
            },
            "val": "equal_interval"
          },
          "options.number_range.n_value": {
            "type": "text",
            "label": "N",
            "show_if": "options.number_range.range_type=='equal_interval'",
            "val": "3",
            "required": true
          },
          "options.number_range.range_value": {
            "type": "text",
            "label": "Диапазон",
            "show_if": "options.number_range.range_type=='equal_range'",
            "val": "10",
            "required": true
          },
          "options.number_range.custom_ranges": {
            "type": "fieldset",
            "label": "Добавить пользовательский диапазон",
            "show_if": "options.number_range.range_type=='custom_range'",
            "repeat": true,
            "fields": {
              "pattern": {
                "type": "text",
                "label": "Образец",
                "val": "0<=x<10",
                "required": true
              },
              "label": {
                "type": "text",
                "label": "Метка",
                "val": "positive10",
                "required": true
              },
              "delete": {
                "type": "button",
                "label": "Удалить",
                "function": "DELETE_ROW"
              }
            }
          }
        }
      },
      "options.is_custom_lookup_table": {
        "type": "checkbox",
        "label": "Пользовательская таблица поиска",
        "show_if": "options.task_type=='apply_lookup_table' || options.task_type=='apply_regex_table' || options.task_type=='apply_lookup_table_to_cells' || options.task_type=='apply_regex_table_to_cells'",
        "val": "true",
        "isOn": "true",
        "isOff": "false",
        "breakLine": true
      },
      "lookup_table_frame": {
        "type": "fieldset",
        "label": "",
        "show_if": "options.task_type=='apply_lookup_table' || options.task_type=='apply_regex_table' || options.task_type=='apply_lookup_table_to_cells' || options.task_type=='apply_regex_table_to_cells'",
        "repeat": false,
        "fields": {
          "options.lookup_table_id": {
            "type": "select",
            "label": "Таблица поиска",
            "show_if": "options.is_custom_lookup_table=='false'",
            "bind": "TABLES",
            "empty": "",
            "required": true,
            "breakLine": true
          },
          "options.custom_lookup_table": {
            "type": "fieldset",
            "label": "Пользовательская таблица поиска",
            "show_if": "options.is_custom_lookup_table=='true'",
            "repeat": true,
            "fields": {
              "lookup": {
                "type": "text",
                "label": "Посмотреть",
                "placeholder": "10 < x <= 100",
                "val": "",
                "required": true
              },
              "label": {
                "type": "text",
                "label": "Метка",
                "placeholder": "10 < x <= 100",
                "val": "",
                "required": true
              },
              "delete": {
                "type": "button",
                "label": "Удалить",
                "function": "DELETE_ROW"
              }
            }
          },
          "options.partial_match": {
            "type": "checkbox",
            "label": "Частичное совпадение",
            "show_if": "options.pattern_type=='duplicate_pattern' && (options.task_type=='apply_lookup_table' || options.task_type=='apply_lookup_table_to_cells')",
            "val": "false",
            "isOn": "true",
            "isOff": "false",
            "style": "margin-top: 10px",
            "breakLine": true
          },
          "options.case_sensitive": {
            "type": "checkbox",
            "label": "С учетом регистра",
            "show_if": "options.pattern_type=='duplicate_pattern' && (options.task_type=='apply_lookup_table' || options.task_type=='apply_lookup_table_to_cells')",
            "val": "false",
            "isOn": "true",
            "isOff": "false",
            "breakLine": true
          },
          "options.use_delimiter": {
            "type": "checkbox",
            "label": "Пользовательский разделитель",
            "show_if": "options.task_type=='apply_lookup_table' || options.task_type=='apply_lookup_table_to_cells'",
            "val": "false",
            "isOn": "true",
            "isOff": "false",
            "style": "margin-top: 10px"
          },
          "options.delimiter": {
            "type": "text",
            "label": "Добавить разделитель",
            "show_if": "options.use_delimiter=='true' && (options.task_type=='apply_lookup_table' || options.task_type=='apply_lookup_table_to_cells')",
            "val": ";",
            "required": true,
            "breakLine": true
          }
        }
      },
      "options.input_table.use_filter": {
        "type": "checkbox",
        "label": "Использовать фильтр для входной таблицы",
        "show_if": "options.task_type=='apply_lookup_table' || options.task_type=='apply_regex_table' || options.task_type=='apply_lookup_table_to_cells' || options.task_type=='apply_regex_table_to_cells'",
        "val": "false",
        "isOn": "true",
        "isOff": "false",
        "style": "margin-top: 10px",
        "breakLine": true
      },
      "input_table_filter_box": {
        "type": "fieldset",
        "label": "Добавить настройки фильтра таблицы",
        "show_if": "options.input_table.use_filter=='true' && (options.task_type=='apply_lookup_table' || options.task_type=='apply_regex_table' || options.task_type=='apply_lookup_table_to_cells' || options.task_type=='apply_regex_table_to_cells')",
        "repeat": false,
        "fields": {
          "options.input_table.filter": {
            "type": "fieldset",
            "label": "Добавить условия фильтрации для входной таблицы",
            "repeat": true,
            "fields": {
              "initial_op": {
                "type": "select",
                "label": "Выбрать оператора логики",
                "required": true,
                "show_if": "$index == 0",
                "options": {
                  "if": {
                    "label": "ЕСЛИ"
                  },
                  "if_not": {
                    "label": "ЕСЛИ НЕ"
                  }
                },
                "val": "if"
              },
              "op": {
                "type": "select",
                "label": "Выбрать оператора логики",
                "required": true,
                "show_if": "$index > 0",
                "options": {
                  "and": {
                    "label": "И"
                  },
                  "or": {
                    "label": "ИЛИ"
                  },
                  "and_not": {
                    "label": "И НЕ"
                  },
                  "or_not": {
                    "label": "ИЛИ НЕ"
                  }
                },
                "val": "and"
              },
              "column": {
                "type": "select",
                "label": "Выбрать столбец",
                "table": "table_id",
                "bind": "COLUMNS",
                "required": true
              },
              "condition": {
                "type": "select",
                "label": "Выбрать оператора",
                "required": true,
                "options": {
                  "CONTAINS": {
                    "label": "СОДЕРЖИТ"
                  },
                  "CONTAINED IN STRING": {
                    "label": "СОДЕРЖАЛОСЬ В"
                  },
                  "STARTS WITH": {
                    "label": "НАЧИНАЕТСЯ С"
                  },
                  "ENDS WITH": {
                    "label": "ЗАКАНЧИВАЕТСЯ С"
                  },
                  "=": {
                    "label": "Равный"
                  },
                  ">": {
                    "label": "БОЛЬШЕ ЧЕМ"
                  },
                  ">=": {
                    "label": "БОЛЬШИЙ ИЛИ РАВНЫЙ"
                  },
                  "<": {
                    "label": "МЕНЬШЕ ЧЕМ"
                  },
                  "<=": {
                    "label": "МЕНЬШИЙ ИЛИ РАВНЫЙ"
                  },
                  "EMPTY": {
                    "label": "ПУСТО"
                  },
                  "BLANK": {
                    "label": "ПУСТО"
                  },
                  "IS NULL": {
                    "label": "НОЛЬ"
                  },
                  "IS TRUE": {
                    "label": "IS TRUE"
                  },
                  "IS FALSE": {
                    "label": "IS FALSE"
                  },
                  "REGEX MATCH": {
                    "label": "Совпадение регулярного выражения"
                  },
                  "CONTAINED IN TABLE": {
                    "label": "В ТАБЛИЦЕ"
                  },
                  "IN REGEX TABLE": {
                    "label": "В ТАБЛИЦЕ РЕГУЛЯРНЫХ ВЫРАЖЕНИЙ"
                  },
                  "DATE": {
                    "label": "ДАТА"
                  }
                },
                "val": "CONTAINS"
              },
              "date_condition": {
                "type": "select",
                "label": "Выбрать оператора даты",
                "required": true,
                "show_if": "this.condition == 'DATE'",
                "options": {
                  "<": {
                    "label": "ПРЕДЫДУЩИЙ"
                  },
                  "<=": {
                    "label": "ПРЕДЫДУЩИЙ ИЛИ РАВНЫЙ"
                  },
                  "=": {
                    "label": "Равный"
                  },
                  ">": {
                    "label": "ПОСЛЕ"
                  },
                  ">=": {
                    "label": "ПОСЛЕ ИЛИ РАВНО"
                  }
                },
                "val": "="
              },
              "compare_with": {
                "type": "select",
                "label": "Compare with",
                "required": true,
                "show_if": "this.condition=='CONTAINS' || this.condition=='CONTAINED IN STRING' || this.condition=='STARTS WITH' || this.condition=='ENDS WITH' || this.condition=='=' || this.condition=='>' || this.condition=='>=' || this.condition=='<' || this.condition=='<='",
                "options": {
                  "custom_value": {
                    "label": "Custom value"
                  },
                  "column_value": {
                    "label": "Column value"
                  }
                },
                "val": "custom_value"
              },
              "self_compare_column": {
                "type": "select",
                "label": "Select Compare Column",
                "table": "table_id",
                "bind": "COLUMNS",
                "show_if": "this.compare_with=='column_value' && (this.condition=='CONTAINS' || this.condition=='CONTAINED IN STRING' || this.condition=='STARTS WITH' || this.condition=='ENDS WITH' || this.condition=='=' || this.condition=='>' || this.condition=='>=' || this.condition=='<' || this.condition=='<=')",
                "required": true
              },
              "date_value": {
                "type": "date",
                "label": "Выбрать дату",
                "show_if": "this.condition=='DATE'",
                "val": "",
                "required": true
              },
              "value": {
                "type": "text",
                "label": "Добавить значение",
                "show_if": "this.compare_with=='custom_value' && this.condition != 'DATE' && this.condition != 'EMPTY' && this.condition != 'NOT EMPTY' && this.condition != 'BLANK' && this.condition != 'NOT BLANK' && this.condition != 'IS NULL' && this.condition != 'IS NOT NULL' && this.condition != 'IS TRUE' && this.condition != 'IS FALSE' && this.condition != 'CONTAINED IN TABLE' && this.condition != 'NOT CONTAINED IN TABLE' && this.condition != 'IN REGEX TABLE' && this.condition != 'NOT IN REGEX TABLE'",
                "val": "",
                "required": true
              },
              "case_sensitive": {
                "type": "select",
                "label": "Выбрать с учетом регистра",
                "required": true,
                "show_if": "this.condition == 'CONTAINS' || this.condition == 'CONTAINED IN STRING' || this.condition == '=' || this.condition == 'STARTS WITH' || this.condition == 'ENDS WITH' || this.condition == 'REGEX MATCH' || this.condition == 'NOT REGEX MATCH' || this.condition == 'IN REGEX TABLE' || this.condition == 'NOT IN REGEX TABLE'",
                "options": {
                  "yes": {
                    "label": "С учетом регистра"
                  },
                  "no": {
                    "label": "Без учета регистра"
                  }
                },
                "val": "yes"
              },
              "compare_table": {
                "type": "select",
                "label": "Выбрать таблицу",
                "bind": "TABLES",
                "empty": "",
                "show_if": "this.condition == 'CONTAINED IN TABLE' || this.condition == 'NOT CONTAINED IN TABLE' || this.condition == 'IN REGEX TABLE' || this.condition == 'NOT IN REGEX TABLE'",
                "required": true
              },
              "compare_column": {
                "type": "select",
                "label": "Выбрать столбец",
                "table": "this.compare_table",
                "bind": "COLUMNS",
                "show_if": "this.condition == 'CONTAINED IN TABLE' || this.condition == 'NOT CONTAINED IN TABLE' || this.condition == 'IN REGEX TABLE' || this.condition == 'NOT IN REGEX TABLE'",
                "required": true
              },
              "delete": {
                "type": "button",
                "label": "Удалить",
                "function": "DELETE_ROW"
              }
            }
          },
          "options.input_table.apply_to_record": {
            "type": "checkbox",
            "label": "Применить к регистру для фильтра входной таблицы",
            "val": "true",
            "isOn": "true",
            "isOff": "false",
            "style": "margin-top: 10px",
            "breakLine": true
          },
          "input_table_apply_to_group_box": {
            "type": "fieldset",
            "label": "Применить к группе входной таблицы",
            "show_if": "options.input_table.apply_to_record=='false'",
