              },
              "and_not": {
                "label": "И НЕ"
              },
              "or_not": {
                "label": "ИЛИ НЕ"
              }
            },
            "val": "and"
          },
          "type": {
            "type": "select",
            "label": "Тип",
            "required": true,
            "options": {
              "Table Name": {
                "label": "Название таблицы"
              },
              "Tags": {
                "label": "Tags"
              }
            },
            "val": "Table Name"
          },
          "rows": {
            "type": "select",
            "label": "Condition",
            "show_if": "this.type == 'Tags'",
            "required": true,
            "options": {
              "All Tags": {
                "label": "All Tags"
              },
              "Some Tags": {
                "label": "Some Tags"
              }
            },
            "val": "Some Tags"
          },
          "condition": {
            "type": "select",
            "label": "Operator",
            "required": true,
            "options": {
              "CONTAINS": {
                "label": "СОДЕРЖИТ"
              },
              "CONTAINED IN STRING": {
                "label": "СОДЕРЖАЛОСЬ В"
              },
              "STARTS WITH": {
