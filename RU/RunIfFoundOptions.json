            "required": true,
            "show_if" :  "$index == 0",
            "options": {
              "if": {
                "label": "ЕСЛИ"
              },
              "if_not": {
                "label": "ЕСЛИ НЕ"
              }
            },
            "val": "if"
          },
          "op": {
            "type": "select",
            "label": "Logic Operator",
            "required": true,
            "show_if" :  "$index > 0",
            "options": {
              "and": {
