{
  "options": {
    "type": "fieldset",
    "label": "Выбрать параметры",
    "repeat": false,
    "fields": {
      "type": {
        "type": "select",
        "label": "Выбрать тип",
        "val": "first_n",
        "options": {
          "first_n": {
            "label": "Первый N"
          },
          "last_n": {
            "label": "Последний N"
          },
          "custom_selection": {
            "label": "Пользовательский подбор"
          },
          "skip_first_n": {
            "label": "Пропустить первую N"
          },
          "skip_last_n": {
            "label": "Пропустить последнюю N"
          },
          "first_n_percent": {
            "label": "Первые N%"
          },
          "last_n_percent": {
            "label": "Последние N%"
          },
          "random_n_records": {
            "label": "Случайная N"
          },
          "random_n_percent": {
            "label": "Случайный N%"
          },
          "random_choice_1_n": {
            "label": "Случайный выбор 1:N"
          },
          "every_nth_record": {
            "label": "Каждая N-ая запись"
          }
        },
        "required": true,
        "width": "100%",
        "breakLine": true
      },
      "n": {
        "type": "text",
        "label": "Добавить N",
        "show_if": "type!='custom_selection'",
        "val": "1",
        "required": true,
        "width": "100%",
        "breakLine": true
      },
      "ranges": {
        "type": "text",
        "label": "Добавить диапазоны",
        "show_if": "type=='custom_selection'",
        "val": "-2; 4-6; 9; 12+",
        "required": true,
        "width": "100%",
        "breakLine": true
      },
      "set_offset": {
        "type": "checkbox",
        "label": "Компенсация",
        "show_if": "type=='every_nth_record'",
        "val": "false",
        "isOn": "true",
        "isOff": "false"
      },
      "offset": {
        "type": "text",
        "label": "Компенсация",
        "show_if": "type=='every_nth_record' && set_offset=='true'",
        "val": "1",
        "required": true,
        "breakLine": true
      },
      "group_by_column": {
        "type": "checkbox",
        "label": "Группировка по стобцам",
        "val": "false",
        "isOn": "true",
        "isOff": "false",
        "breakLine": true
      },
      "columns_to_group_box": {
        "type": "fieldset",
        "label": "Группировка по стобцам",
        "show_if": "group_by_column=='true'",
        "repeat": false,
        "fields": {
          "columns_to_group": {
            "type": "select",
            "label": "Выбрать групповые столбцы",
            "required": true,
            "width": "100%",
            "table": "table_id",
            "multiple": true,
            "bind": "COLUMNS",
            "breakLine": true
          }
        }
      }
    }
  }
}