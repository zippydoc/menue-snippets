{
  "output.add_period": {
    "type": "checkbox",
    "label": "Добавить частоту",
    "show_if": "options.type=='process' || options.type=='custom'",
    "val": "false",
    "isOn": "true",
    "isOff": "false",
    "style": "margin-top: 10px",
    "breakLine": true
  },
  "output.period": {
    "type": "select",
    "label": "Частота",
    "show_if": "(options.type=='process' || options.type=='custom') && output.add_period=='true'",
    "multiple": true,
    "options": {
      "year": {
        "label": "Год"
      },
      "half-year": {
        "label": "Полгода"
      },
      "quarter": {
        "label": "Квартал"
      },
      "month": {
        "label": "Месяц"
      },
      "week": {
        "label": "Неделя"
      }
    },
    "val": "",
    "required": true
  },
  "output.period_concatenate": {
    "type": "checkbox",
    "label": "Обхединить",
    "show_if": "(options.type=='process' || options.type=='custom') && output.add_period=='true'",
    "val": "false",
    "isOn": "true",
    "isOff": "false",
    "breakLine": true
  }
}