{
  "actions": {
    "type": "fieldset",
    "Метка": "Добавить действия",
    "repeat": true,
    "fields": {
      "column": {
        "type": "select",
        "Метка": "Столбец",
        "table": "input_table",
		"assign_selected_value_to":"this.output_column_name",
        "bind": "COLUMNS"
      },
      "action": {
        "type": "select",
        "Метка": "Действие",
        "options": {
          "group_by": {
            "Метка": "Группировать по"
          },
          "count": {
            "Метка": "Посчитать"
          },
          "sum": {
            "Метка": "Сумма"
          },
          "first": {
            "Метка": "Первый"
          },
          "last": {
            "Метка": "Последний"
          },
          "shortest": {
            "Метка": "Кратчайший"
          },
          "longest": {
            "Метка": "Самый длинный"
          },
          "concatenate": {
            "Метка": "Обхединить"
          },
          "mathematics": {
            "Метка": "Математика"
          }
        }
      },
      "count_action_category": {
        "type": "select",
        "Метка": "Тип действия",
        "show_if": "this.action=='count'",
        "options": {
          "all": {
            "Метка": "Все"
          },
          "distinct": {
            "Метка": "Различить"
          },
          "empty_or_zero": {
            "Метка": "Пусто или 0"
          },
          "non_empty_or_zero": {
            "Метка": "Кроме пустых или 0"
          }
        },
        "val": "all"
      },
      "action_type1": {
        "type": "select",
        "Метка": "Подтип действия",
        "show_if": "this.action=='count' && (this.count_action_category=='all' || this.count_action_category=='distinct')",
        "options": {
          "-": {
            "Метка": "-"
          },
          "non_zero": {
            "Метка": "Кроме 0"
          },
          "non_empty": {
            "Метка": "Кроме пустых"
          },
          "non_null": {
            "Метка": "Кроме ноля"
          },
          "non_blank": {
            "Метка": "Кроме пустых"
          }
        },
        "val": "-"
      },
      "action_type2": {
        "type": "select",
        "Метка": "Подтип действия",
        "show_if": "this.action=='count' && this.count_action_category=='empty_or_zero'",
        "options": {
          "zero": {
            "Метка": "0"
          },
          "empty": {
            "Метка": "Пусто (Ноль и незаполненно)"
          },
          "null": {
            "Метка": "Ноль"
          },
          "blank": {
            "Метка": "Пусто"
          }
        },
        "val": "zero"
      },
      "action_type3": {
        "type": "select",
        "Метка": "Подтип действия",
        "show_if": "this.action=='count' && this.count_action_category=='non_empty_or_zero'",
        "options": {
          "non_zero": {
            "Метка": "Кроме 0"
          },
          "non_empty": {
            "Метка": "Кроме пустых"
          },
          "non_null": {
            "Метка": "Кроме ноля"
          },
          "non_blank": {
            "Метка": "Кроме пустых"
          }
        },
        "val": "non_zero"
      },
      "mathematics_action_category": {
        "type": "select",
        "Метка": "Тип действия",
        "show_if": "this.action=='mathematics'",
        "options": {
          "min": {
            "Метка": "Мин"
          },
          "max": {
            "Метка": "Макс"
          },
          "average": {
            "Метка": "Средний"
          },
          "median": {
            "Метка": "Медиана"
          },
          "standard_deviation": {
            "Метка": "Стандартное отклонение"
          },
          "variance": {
            "Метка": "Вариант"
          }
        },
        "val": "min"
      },
      "action_type4": {
        "type": "select",
        "Метка": "Подтип действия",
        "show_if": "this.action=='mathematics'",
        "options": {
          "include_zero": {
            "Метка": "Включить 0"
          },
          "exclude_zero": {
            "Метка": "Исключить 0"
          }
        },
        "val": "include_zero"
      },
      "concatenate_separator": {
        "type": "text",
        "Метка": "Сепаратор",
        "show_if": "this.action=='concatenate'",
        "val": ","
      },
      "output_column_name": {
        "type": "text",
        "Метка": "Вывести название столбца",
        "val": ""
      },
      "delete": {
        "type": "button",
        "Метка": "Удалить",
        "function": "DELETE_ROW"
      }
    }
  }
}