{
  "actions": {
    "type": "fieldset",
    "label": "Добавить действия",
    "repeat": true,
    "fields": {
      "columns": {
        "type": "select",
        "label": "Column(s)",
        "table": "table_id",
		"assign_selected_value_to":"this.output_column_name",
        "bind": "COLUMNS",
        "multiple": true,
        "required": true
      },
      "action": {
        "type": "select",
        "label": "Действие",
        "options": {
          "group_by": {
            "label": "Группировать по"
          },
          "count": {
            "label": "Посчитать"
          },
          "sum": {
            "label": "Сумма"
          },
          "first": {
            "label": "Первый"
          },
          "last": {
            "label": "Последний"
          },
          "shortest": {
            "label": "Кратчайший"
          },
          "longest": {
            "label": "Самый длинный"
          },
          "concatenate": {
            "label": "Обхединить"
          },
          "mathematics": {
            "label": "Математика"
          }
        },
        "val": "group_by"
      },
      "count_action_category": {
        "type": "select",
        "label": "Тип действия",
        "show_if": "this.action=='count'",
        "options": {
          "all": {
            "label": "Все"
          },
          "distinct": {
            "label": "Различить"
          },
          "empty_or_zero": {
            "label": "Пусто или 0"
          },
          "non_empty_or_zero": {
            "label": "Кроме пустых или 0"
          }
        },
        "val": "all"
      },
      "action_type1": {
        "type": "select",
        "label": "Подтип действия",
        "show_if": "this.action=='count' && (this.count_action_category=='all' || this.count_action_category=='distinct')",
        "options": {
          "-": {
            "label": "-"
          },
          "non_zero": {
            "label": "Кроме 0"
          },
          "non_empty": {
            "label": "Кроме пустых"
          },
          "non_null": {
            "label": "Кроме ноля"
          },
          "non_blank": {
            "label": "Кроме пустых"
          }
        },
        "val": "-"
      },
      "action_type2": {
        "type": "select",
        "label": "Подтип действия",
        "show_if": "this.action=='count' && this.count_action_category=='empty_or_zero'",
        "options": {
          "zero": {
            "label": "0"
          },
          "empty": {
            "label": "Пусто (Ноль и незаполненно)"
          },
          "null": {
            "label": "Ноль"
          },
          "blank": {
            "label": "Пусто"
          }
        },
        "val": "zero"
      },
      "action_type3": {
        "type": "select",
        "label": "Подтип действия",
        "show_if": "this.action=='count' && this.count_action_category=='non_empty_or_zero'",
        "options": {
          "non_zero": {
            "label": "Кроме 0"
          },
          "non_empty": {
            "label": "Кроме пустых"
          },
          "non_null": {
            "label": "Кроме ноля"
          },
          "non_blank": {
            "label": "Кроме пустых"
          }
        },
        "val": "non_zero"
      },
      "mathematics_action_category": {
        "type": "select",
        "label": "Тип действия",
        "show_if": "this.action=='mathematics'",
        "options": {
          "min": {
            "label": "Мин"
          },
          "max": {
            "label": "Макс"
          },
          "average": {
            "label": "Средний"
          },
          "median": {
            "label": "Медиана"
          },
          "standard_deviation": {
            "label": "Стандартное отклонение"
          },
          "variance": {
            "label": "Вариант"
          }
        },
        "val": "min"
      },
      "action_type4": {
        "type": "select",
        "label": "Подтип действия",
        "show_if": "this.action=='mathematics'",
        "options": {
          "include_zero": {
            "label": "Включить 0"
          },
          "exclude_zero": {
            "label": "Исключить 0"
          }
        },
        "val": "include_zero"
      },
      "concatenate_separator": {
        "type": "text",
        "label": "Сепаратор",
        "show_if": "this.action=='concatenate'",
        "val": ","
      },
      "output_column_name": {
        "type": "text",
        "label": "Вывести название столбца",
        "val": ""
      },
      "rounding": {
        "type": "select",
        "label": "Округление",
        "show_if": "this.action=='sum' || this.action=='mathematics'",
        "required": true,
        "options": {
          "no_rounding": {
            "label": "Округление чисел"
          },
          "0": {
            "label": "0 цифр"
          },
          "1": {
            "label": "1 цифра"
          },
          "2": {
            "label": "2 цифры"
          },
          "3": {
            "label": "3 цифры"
          },
          "4": {
            "label": "4 цифры"
          },
          "5": {
            "label": "5 цифр"
          },
          "6": {
            "label": "6 цифр"
          },
          "7": {
            "label": "7 цифр"
          },
          "8": {
            "label": "8 цифр"
          },
          "9": {
            "label": "9 цифр"
          }
        },
        "val": "no_rounding"
      },
      "delete": {
        "type": "button",
        "label": "Удалить",
        "function": "DELETE_ROW"
      }
    }
  },
  "info1": {
    "type": "textarea",
    "label": "",
    "val": "If more than one columns selected, Output Column Name input value should be used as suffix",
    "disabled": true,
    "style": "width: 100%"
  }
}