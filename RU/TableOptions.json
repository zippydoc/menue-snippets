{
  "options": {
    "type": "fieldset",
    "label": "Выбрать параметры",
    "repeat": false,
    "fields": {
      "options.type": {
        "type": "radio",
        "val": "transform_group",
        "values": {
          "transform_group": "Transform Group",
          "get_header": "Get Header",
          "get_formatting": "Get Formatting and Formulas"
        },
        "breakLine": true
      },
      "options.group_label_direction": {
        "type": "select",
        "label": "Преобразовать направление",
        "show_if": "options.type==''transform_group''",
        "required": true,
        "options": {
          "group_to_label": {
            "label": "Группировать в метки"
          },
          "label_to_group": {
            "label": "Метка группы"
          }
        },
        "val": "group_to_label"
      },
      "options.group_column": {
        "type": "select",
        "label": "Групповой столбец",
        "show_if": "options.type==''transform_group'' && options.group_label_direction==''group_to_label''",
        "table": "input_table",
        "bind": "COLUMNS",
        "required": true
      },
      "options.group_column_name": {
        "type": "text",
        "label": "Название группового столбца",
        "show_if": "options.type==''transform_group'' && options.group_label_direction==''label_to_group''",
        "val": "Group",
        "required": true
      }
    }
  }
}