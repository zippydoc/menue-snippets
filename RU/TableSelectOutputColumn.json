{
  "output.select_output_columns": {
    "type": "checkbox", 
    "label": "Выбрать выходные столбцы", 
    "show_if": "options.type=='transform_group'", 
    "val": "false", 
    "isOn": "true", 
    "isOff": "false"
  }, 
  "output_columns": {
    "type": "fieldset", 
    "label": "Select OutputColumns", 
    "show_if": "options.type=='transform_group' && output.select_output_columns=='true'", 
    "repeat": true, 
    "fields": {
      "field_name": {
        "type": "select", 
        "label": "Выбрать столбец", 
        "table": "input_table", 
        "bind": "COLUMNS", 
        "required": true
      }, 
      "is_rename": {
        "type": "checkbox", 
        "label": "Переименовать", 
        "val": "false", 
        "isOn": "true", 
        "isOff": "false"
      }, 
      "rename": {
        "show_if": "this.is_rename == 'true'", 
        "type": "text", 
        "required": true, 
        "minLength": 1, 
        "maxLength": 255, 
        "label": "Название нового столбца", 
        "style": "margin-left: 10px"
      }, 
      "delete": {
        "type": "button", 
        "label": "Удалить", 
        "function": "DELETE_ROW"
      }
    }, 
    "breakLine": true, 
    "style": "margin-bottom: 20px"
  }
}