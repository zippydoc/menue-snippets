{
  "options": {
    "type": "fieldset", 
    "label": "Выбрать параметры", 
    "repeat": false, 
    "fields": {
      "transpose_type": {
        "type": "select", 
        "label": "Тип транспонирования", 
        "options": {
          "mirror_matrix": {
            "label": "Зеркальная матрица"
          }, 
          "column_name_to_label": {
            "label": "Название столбца для метки"
          }, 
          "crosstab": {
            "label": "Кросс-таблица"
          }
        }, 
        "val": "mirror_matrix", 
        "required": true, 
        "width": "100%", 
        "breakLine": true
      }, 
      "mirror_matrix.convert_header_to_label": {
        "type": "checkbox", 
        "label": "Конвертировать заголовок в метку", 
        "show_if": "transpose_type=='mirror_matrix'", 
        "val": "true", 
        "isOn": "true", 
        "isOff": "false", 
        "breakLine": true
      }, 
      "column_name_to_label.columns_to_label": {
        "type": "select", 
        "label": "Выбрать столбцы для конвертации в метки", 
        "show_if": "transpose_type=='column_name_to_label'", 
        "table": "input_table", 
        "multiple": true, 
        "bind": "COLUMNS", 
        "required": true, 
        "width": "100%", 
        "breakLine": true
      }, 
      "column_name_to_label.label_column_name": {
        "type": "text", 
        "label": "Название столбца для столбца метки", 
        "show_if": "transpose_type=='column_name_to_label'", 
        "required": true, 
        "val": "Метка", 
        "width": "100%", 
        "breakLine": true
      }, 
      "column_name_to_label.value_column_name": {
        "type": "text", 
        "label": "Название столбца для столбца значения", 
        "show_if": "transpose_type=='column_name_to_label'", 
        "required": true, 
        "val": "Значение", 
        "width": "100%", 
        "breakLine": true
      }, 
      "crosstab.group_data_by_these_values": {
        "type": "select", 
        "label": "Группировать данные по этим значениям", 
        "show_if": "transpose_type=='crosstab'", 
        "table": "input_table", 
        "multiple": true, 
        "bind": "COLUMNS", 
        "required": true, 
        "width": "100%", 
        "breakLine": true
      }, 
      "crosstab.change_column_headers": {
        "type": "select", 
        "label": "Изменить заголовок столбца", 
        "show_if": "transpose_type=='crosstab'", 
        "table": "input_table", 
        "bind": "COLUMNS", 
        "required": true, 
        "width": "100%", 
        "breakLine": true
      }, 
      "crosstab.values_for_new_columns": {
        "type": "select", 
        "label": "Значения для новых столбцов", 
        "show_if": "transpose_type=='crosstab'", 
        "table": "input_table", 
        "bind": "COLUMNS", 
        "required": true, 
        "width": "100%", 
        "breakLine": true
      }, 
      "crosstab.method_for_aggregating_values": {
        "type": "select", 
        "label": "Метод добавления значений", 
        "show_if": "transpose_type=='crosstab'", 
        "options": {
          "Обхединить": {
            "label": "Обхединить"
          }, 
          "Первый": {
            "label": "Первый"
          }, 
          "Последний": {
            "label": "Последний"
          }, 
          "Сумма": {
            "label": "Сумма"
          }, 
          "Средний": {
            "label": "Средний"
          }, 
          "Посчитать": {
            "label": "Посчитать"
          }, 
          "Определяемые пользователем": {
            "label": "Определяемые пользователем"
          }
        }, 
        "val": "Обхединить", 
        "required": true
      }, 
      "crosstab.user_defined_str": {
        "type": "text", 
        "label": "Опрелеляемое пользователем значение", 
        "show_if": "transpose_type=='crosstab' && crosstab.method_for_aggregating_values=='User Defined'", 
        "val": "x", 
        "trim": "false", 
        "breakLine": true
      }, 
      "crosstab.separator": {
        "type": "text", 
        "label": "Сепаратор", 
        "show_if": "transpose_type=='crosstab' && method_for_aggregating_values=='Concatenate'", 
        "val": ", ,
        trim": "false", 
        "required": true, 
        "breakLine": true
      }
    }
  }
}