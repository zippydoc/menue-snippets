{
  "options_box": {
    "type": "fieldset",
    "label": "Опции",
    "repeat": false,
    "fields": {
      "output_config_frame": {
        "type": "fieldset",
        "label": "Output Configuration",
        "repeat": false,
        "fields": {
          "output.add_tag": {
            "type": "checkbox",
            "label": "Добавить тэг",
            "val": "false",
            "isOn": "true",
            "isOff": "false"
          },
          "output.tag_name": {
            "type": "text",
            "label": "Название тэга",
            "show_if": "output.add_tag=='true'",
            "required": true,
            "minLength": 1,
            "maxLength": 255,
            "val": "",
            "breakLine": true,
            "style": "margin-left: 15px"
          }
        }
      },
      "output.if_table_exist.type": {
        "type": "select",
        "label": "Если существует",
        "val": "drop_if_exist",
        "options": {
          "drop_if_exist": {
            "label": "Создать или сбросить (если уже существует)"
          },
          "error_if_exist": {
            "label": "Создать новое (убрать ошибку, если есть)"
          },
          "error_if_not_exist": {
            "label": "Отменить (убрать ошибку, если ее нет)"
          },
          "append_all": {
            "label": "Добавить все"
          },
          "append_not_existing": {
            "label": "Применить не существующие"
          },
          "append_changes": {
            "label": "Применить изменения"
          }
        }
      },
      "output.if_table_exist.append_not_existing_cols": {
        "type": "select",
        "label": "Выбрать столбец(цы)",
        "show_if": "output.if_table_exist.type=='append_not_existing'",
        "table": "input_table",
        "bind": "COLUMNS",
        "multiple": true,
        "required": true,
        "breakLine": true
      },
      "output.if_table_exist.append_changes__add_timestamp": {
        "type": "checkbox",
        "label": "Добавить метку времени",
        "show_if": "output.if_table_exist.type=='append_changes'",
        "val": "false",
        "isOn": "true",
        "isOff": "false"
      },
      "output.if_table_exist.append_changes__timestamp_colname": {
        "type": "text",
        "label": "Имя столбца временной метки",
        "show_if": "output.if_table_exist.type=='append_changes' && output.if_table_exist.append_changes__add_timestamp=='true'",
        "val": "Timestamp",
        "required": true,
        "breakLine": true
      },
      "output.if_table_exist.append_changes__add_row": {
        "type": "checkbox",
        "label": "Добавить Ряд",
        "show_if": "output.if_table_exist.type=='append_changes'",
        "val": "false",
        "isOn": "true",
        "isOff": "false"
      },
      "output.if_table_exist.append_changes__row_colname": {
        "type": "text",
        "label": "Имя столбца строки",
        "show_if": "output.if_table_exist.type=='append_changes' && output.if_table_exist.append_changes__add_row=='true'",
        "val": "Row",
        "required": true,
        "breakLine": true
      },
      "append_not_existing_info1": {
        "type": "text",
        "label": "",
        "show_if": "output.if_table_exist.type=='append_not_existing'",
        "val": "Appends rows only if the value of the",
        "disabled": true,
        "style": "width: 100%"
      },
      "append_not_existing_info2": {
        "type": "text",
        "label": "",
        "show_if": "output.if_table_exist.type=='append_not_existing'",
        "val": "selected column is new in the existing table.",
        "disabled": true,
        "style": "width: 100%"
      },
      "append_not_existing_info3": {
        "type": "text",
        "label": "",
        "show_if": "output.if_table_exist.type=='append_not_existing'",
        "val": "It is assumed that the selected column",
        "disabled": true,
        "style": "width: 100%"
      },
      "append_not_existing_info4": {
        "type": "text",
        "label": "",
        "show_if": "output.if_table_exist.type=='append_not_existing'",
        "val": "is found in the output and existing table",
        "disabled": true,
        "style": "width: 100%"
      }
    }
  }
}