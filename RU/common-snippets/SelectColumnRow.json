{
  "placeholder": {
    "breakLine": true
  }, 
  "apply_to_all_columns": {
    "type": "checkbox", 
    "label": "Все колонки", 
    "val": "true", 
    "isOn": "true", 
    "isOff": "false"
  }, 
  "apply_columns": {
    "type": "select", 
    "label": "Выберите столбцы для применения", 
    "show_if": "apply_to_all_columns=='false'", 
    "required": true, 
    "table": "input_table", 
    "multiple": true, 
    "bind": "COLUMNS", 
    "breakLine": true
  }, 
  "apply_to_all_rows": {
    "type": "checkbox", 
    "label": "All Rows", 
    "val": "true", 
    "isOn": "true", 
    "isOff": "false"
  }, 
  "apply_rows": {
    "type": "select", 
    "label": "Выберите строки для применения", 
    "show_if": "apply_to_all_rows=='false'", 
    "required": true, 
    "table": "input_table", 
    "multiple": true, 
    "bind": "LABELS", 
    "breakLine": true
  }, 
  "intersection": {
    "type": "select", 
    "label": "Отбор", 
    "show_if": "apply_to_all_columns=='false' && apply_to_all_rows=='false'", 
    "options": {
      "all": {
        "label": "Все выбранные строки и столбцы"
      }, 
      "intersection": {
        "label": "Пересечение выбранных строк и столбцов"
      }
    }, 
    "val": "intersection"
  }
}
