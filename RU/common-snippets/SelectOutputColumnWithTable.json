{
  "output.select_output_columns": {
    "type": "checkbox", 
    "label": "Выбрать исходящие столбцы",
    "val": "false", 
    "isOn": "true", 
    "isOff": "false"
  }, 
  "output_columns": {
    "type": "fieldset", 
    "label": "Выбрать исходящие столбцы",
    "show_if": "output.select_output_columns=='true'", 
    "repeat": true, 
    "fields": {
      "table_id": {
        "type": "select", 
        "label": "Выбрать таблицу",
        "bind": "TABLES", 
        "required": true
      }, 
      "field_name": {
        "type": "select", 
        "label": "Выбрать столбец",
        "table": "this.table_id", 
        "bind": "COLUMNS", 
        "required": true
      }, 
      "order": {
        "type": "select", 
        "label": "Порядок",
        "options": {
          "-": {
            "label": "-"
          }, 
          "asc": {
            "label": "Восходящее"
          }, 
          "desc": {
            "label": "Убывающий"
          }
        }, 
        "val": "-"
      }, 
      "is_rename": {
        "type": "checkbox", 
        "label": "Переименовать",
        "val": "false", 
        "isOn": "true", 
        "isOff": "false"
      }, 
      "rename": {
        "show_if": "this.is_rename == 'true'", 
        "type": "text", 
        "required": true, 
        "minLength": 1, 
        "maxLength": 255, 
        "label": "Имя нового столбца",
        "style": "margin-left: 10px"
      }, 
      "delete": {
        "type": "button", 
        "label": "Удалить",
        "function": "DELETE_ROW"
      }
    }, 
    "breakLine": true, 
    "style": "margin-bottom: 20px"
  }
}
