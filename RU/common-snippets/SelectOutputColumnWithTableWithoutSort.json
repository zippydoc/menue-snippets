{
  "output.select_output_columns": {
    "type": "checkbox", 
    "label": "Выберите Выходные колонки", 
    "val": "false", 
    "isOn": "true", 
    "isOff": "false"
  }, 
  "output_columns": {
    "type": "fieldset", 
    "label": "Выберите OutputColumns", 
    "show_if": "output.select_output_columns=='true'", 
    "repeat": true, 
    "fields": {
      "table_id": {
        "type": "select", 
        "label": "Выберите таблицу", 
        "bind": "TABLES", 
        "required": true
      }, 
      "field_name": {
        "type": "select", 
        "label": "Выбрать колонку", 
        "table": "this.table_id", 
        "bind": "COLUMNS", 
        "required": true
      }, 
      "is_rename": {
        "type": "checkbox", 
        "label": "Переименовать", 
        "val": "false", 
        "isOn": "true", 
        "isOff": "false"
      }, 
      "rename": {
        "show_if": "this.is_rename == 'true'", 
        "type": "text", 
        "required": true, 
        "minLength": 1, 
        "maxLength": 255, 
        "label": "Новая колонка Название", 
        "style": "margin-left: 10px"
      }, 
      "delete": {
        "type": "button", 
        "label": "Удалить", 
        "function": "DELETE_ROW"
      }
    }, 
    "breakLine": true, 
    "style": "margin-bottom: 20px"
  }
}
