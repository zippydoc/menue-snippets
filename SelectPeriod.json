{
  "output.add_period": {
    "type": "checkbox",
    "label": "Add Periodicy",
    "show_if": "options.type=='process'",
    "val": "false",
    "isOn": "true",
    "isOff": "false",
    "breakLine": true
  },
  "output.period": {
    "type": "select",
    "label": "Periodicy",
    "show_if": "options.type=='process' && output.add_period=='true'",
    "multiple": true,
    "options": {
      "year": {
        "label": "Year"
      },
      "half-year": {
        "label": "Half-Year"
      },
      "quarter": {
        "label": "Quarter"
      },
      "month": {
        "label": "Month"
      },
      "week": {
        "label": "Week"
      }
    },
    "val": "",
    "required": true
  },
  "output.period_concatenate": {
    "type": "checkbox",
    "label": "Concatenate",
    "show_if": "options.type=='process' && output.add_period=='true'",
    "val": "false",
    "isOn": "true",
    "isOff": "false"
  }
}