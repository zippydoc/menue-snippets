{
  "split_column_box": {
    "type": "fieldset",
    "label": "Columns to split",
    "show_if": "split_type=='split_to_columns'",
    "repeat": false,
    "fields": {
      "split_all_columns": {
        "type": "checkbox",
        "label": "All Columns",
        "val": "false",
        "isOn": "true",
        "isOff": "false",
        "breakLine": true
      },
      "column_to_parse": {
        "type": "select",
        "label": "Column to split",
        "show_if": "split_all_columns=='false'",
        "multiple": true,
        "table": "input_table",
        "bind": "COLUMNS",
        "required": true,
        "width": "100%"
      }
    }
  }
}
