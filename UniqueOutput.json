{
  "output.output_table_types": {
    "type": "select",
    "label": "Output Table Type(s)",
    "required": true,
    "options": {
      "unique": {
        "label": "Unique Table"
      },
      "duplicates": {
        "label": "Duplicates Table"
      }
    },
    "multiple": true,
    "val": ["unique"]
  },
  "output.standard_ending": {
    "type": "checkbox",
    "label": "Standard Ending",
    "val": "true",
    "isOn": "true",
    "isOff": "false",
    "breakLine": true
  },
  "output.unique_ending": {
    "show_if": "output.standard_ending == 'false'",
    "type": "text",
    "label": "Unique Output Ending",
    "val": "_u",
    "style": "width: 160px; margin-left: 10px"
  },
  "output.duplicates_ending": {
    "show_if": "output.standard_ending == 'false'",
    "type": "text",
    "label": "Duplicates Output Ending",
    "val": "_d",
    "style": "width: 160px; margin-left: 10px",
    "breakLine": true
  }
}