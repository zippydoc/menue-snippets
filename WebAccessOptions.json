{
  "options": {
    "type": "fieldset",
    "label": "Options",
    "repeat": false,
    "fields": {
      "options.type": {
        "type": "radio",
        "label": "",
        "required": true,
        "values": {
          "embed_code": "Generate Embed-Code",
          "api_key": "Generate API-KEY"
        },
        "val": "embed_code",
        "breakLine": true
      },
      "options.embed_code.framework": {
        "type": "select",
        "label": "Select Frontend Framework",
        "required": true,
        "show_if": "options.type=='embed_code'",
        "options": {
          "jquery": {
            "label": "jQuery"
          },
          "react": {
            "label": "React"
          },
          "vue": {
            "label": "Vue"
          },
          "angular": {
            "label": "Angular"
          }
        },
        "val": "if"
      }
    }
  }
}