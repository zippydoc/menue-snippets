{
  "options": {
    "type": "fieldset",
    "label": "选择选项",
    "repeat": false,
    "fields": {
      "options.position": {
        "type": "select",
        "label": "选择位置",
        "show_if": "$table_id.numOfLabel >= 1",
        "required": true,
        "options": {
          "add_rows_at_table_end": {
            "label": "在底部添加行"
          },
          "add_row_on_top": {
            "label": "在顶部添加行"
          },
          "add_rows_above_selected_row": {
            "label": "在选择上方添加行"
          },
          "add_rows_below_selected_row": {
            "label": "在选择下方添加行"
          },
          "add_row_after_each_group": {
            "label": "在各组后添加行"
          }
        },
        "val": "add_rows_at_table_end",
        "breakLine": true
      },
      "options.group_column": {
        "type": "select",
        "label": "组列",
        "show_if": "$table_id.numOfLabel < 1 && options.position=='add_row_after_each_group'",
        "table": "table_id",
        "bind": "COLUMNS",
        "required": true,
        "breakLine": true
      },
      "options.use_filter": {
        "type": "select",
        "label": "选择类型",
        "show_if": "false",
        "required": true,
        "options": {
          "false": {
            "label": "为每个表格行添加一行"
          },
          "true": {
            "label": "为每个指定的行添加一条记录"
          }
        },
        "val": "false",
        "breakLine": true,
        "style": "margin-top: 10px"
      },
      "filter_box": {
        "type": "fieldset",
        "label": "筛选选项",
        "show_if": "options.use_filter=='true' && $table_id.numOfLabel < 1",
        "repeat": false,
        "fields": {
          "options.filter": {
            "type": "fieldset",
            "label": "添加筛选条件",
            "repeat": true,
            "fields": {
              "initial_op": {
                "type": "select",
                "label": "选择逻辑运算符",
                "required": true,
                "show_if": "$index == 0",
                "options": {
                  "if": {
                    "label": "IF"
                  },
                  "if_not": {
                    "label": "如果不是"
                  }
                },
                "val": "if"
              },
              "op": {
                "type": "select",
                "label": "选择逻辑运算符",
                "required": true,
                "show_if": "$index > 0",
                "options": {
                  "and": {
                    "label": "AND"
                  },
                  "or": {
                    "label": "OR"
                  },
                  "and_not": {
                    "label": "AND NOT"
                  },
                  "or_not": {
                    "label": "OR NOT"
                  }
                },
                "val": "and"
              },
              "column": {
                "type": "select",
                "label": "选择列",
                "table": "table_id",
                "bind": "COLUMNS",
                "required": true
              },
              "condition": {
                "type": "select",
                "label": "选择运算符",
                "required": true,
                "options": {
                  "CONTAINS": {
                    "label": "包含"
                  },
                  "CONTAINED IN STRING": {
                    "label": "包含于"
                  },
                  "STARTS WITH": {
                    "label": "开始于"
                  },
                  "ENDS WITH": {
                    "label": "结尾是"
                  },
                  "=": {
                    "label": "相等"
                  },
                  ">": {
                    "label": "大于"
                  },
                  ">=": {
                    "label": "大于或等于"
                  },
                  "<": {
                    "label": "小于"
                  },
                  "<=": {
                    "label": "小于或等于"
                  },
                  "EMPTY": {
                    "label": "为空"
                  },
                  "BLANK": {
                    "label": "为空白"
                  },
                  "IS NULL": {
                    "label": "为 NULL"
                  },
                  "IS TRUE": {
                    "label": "IS TRUE"
                  },
                  "IS FALSE": {
                    "label": "IS FALSE"
                  },
                  "REGEX MATCH": {
                    "label": "正则表达式匹配"
                  },
                  "CONTAINED IN TABLE": {
                    "label": "于表格"
                  },
                  "IN REGEX TABLE": {
                    "label": "于正则表格"
                  },
                  "DATE": {
                    "label": "日期"
                  }
                },
                "val": "CONTAINS"
              },
              "date_condition": {
                "type": "select",
                "label": "选择日期运算符",
                "required": true,
                "show_if": "this.condition == 'DATE'",
                "options": {
                  "<": {
                    "label": "PRIOR"
                  },
                  "<=": {
                    "label": "先于或等于"
                  },
                  "=": {
                    "label": "相等"
                  },
                  ">": {
                    "label": "之后"
                  },
                  ">=": {
                    "label": "之后或等于"
                  }
                },
                "val": "="
              },
              "compare_with": {
                "type": "select",
                "label": "Compare with",
                "required": true,
                "show_if": "this.condition=='CONTAINS' || this.condition=='CONTAINED IN STRING' || this.condition=='STARTS WITH' || this.condition=='ENDS WITH' || this.condition=='=' || this.condition=='>' || this.condition=='>=' || this.condition=='<' || this.condition=='<='",
                "options": {
                  "custom_value": {
                    "label": "Custom value"
                  },
                  "column_value": {
                    "label": "Column value"
                  }
                },
                "val": "custom_value"
              },
              "self_compare_column": {
                "type": "select",
                "label": "Select Compare Column",
                "table": "table_id",
                "bind": "COLUMNS",
                "show_if": "this.compare_with=='column_value' && (this.condition=='CONTAINS' || this.condition=='CONTAINED IN STRING' || this.condition=='STARTS WITH' || this.condition=='ENDS WITH' || this.condition=='=' || this.condition=='>' || this.condition=='>=' || this.condition=='<' || this.condition=='<=')",
                "required": true
              },
              "date_value": {
                "type": "date",
                "label": "选择日期",
                "show_if": "this.condition=='DATE'",
                "val": "",
                "required": true
              },
              "value": {
                "type": "text",
                "label": "输入值",
                "show_if": "this.compare_with=='custom_value' && this.condition != 'DATE' && this.condition != 'EMPTY' && this.condition != 'NOT EMPTY' && this.condition != 'BLANK' && this.condition != 'NOT BLANK' && this.condition != 'IS NULL' && this.condition != 'IS NOT NULL' && this.condition != 'IS TRUE' && this.condition != 'IS FALSE' && this.condition != 'CONTAINED IN TABLE' && this.condition != 'NOT CONTAINED IN TABLE' && this.condition != 'IN REGEX TABLE' && this.condition != 'NOT IN REGEX TABLE'",
                "val": "",
                "required": true
              },
              "case_sensitive": {
                "type": "select",
                "label": "选择区分大小写",
                "required": true,
                "show_if": "this.condition == 'CONTAINS' || this.condition == 'CONTAINED IN STRING' || this.condition == '=' || this.condition == 'STARTS WITH' || this.condition == 'ENDS WITH' || this.condition == 'REGEX MATCH' || this.condition == 'NOT REGEX MATCH' || this.condition == 'IN REGEX TABLE' || this.condition == 'NOT IN REGEX TABLE'",
                "options": {
                  "yes": {
                    "label": "区分大小写"
                  },
                  "no": {
                    "label": "不区分大小写"
                  }
                },
                "val": "yes"
              },
              "compare_table": {
                "type": "select",
                "label": "选择表格",
                "bind": "TABLES",
                "empty": "",
                "show_if": "this.condition == 'CONTAINED IN TABLE' || this.condition == 'NOT CONTAINED IN TABLE' || this.condition == 'IN REGEX TABLE' || this.condition == 'NOT IN REGEX TABLE'",
                "required": true
              },
              "compare_column": {
                "type": "select",
                "label": "选择列",
                "table": "this.compare_table",
                "bind": "COLUMNS",
                "show_if": "this.condition == 'CONTAINED IN TABLE' || this.condition == 'NOT CONTAINED IN TABLE' || this.condition == 'IN REGEX TABLE' || this.condition == 'NOT IN REGEX TABLE'",
                "required": true
              },
              "delete": {
                "type": "button",
                "label": "删除",
                "function": "DELETE_ROW"
              }
            }
          },
          "options.apply_to_record": {
            "type": "checkbox",
            "label": "应用至记录",
            "val": "true",
            "isOn": "true",
            "isOff": "false",
            "style": "margin-top: 10px",
            "breakLine": true
          },
          "apply_to_group_box": {
            "type": "fieldset",
            "label": "应用至组",
            "show_if": "options.apply_to_record=='false'",
            "repeat": false,
            "fields": {
              "options.apply_to_group_columns": {
                "type": "select",
                "label": "选择组列",
                "required": true,
                "width": "100%",
                "table": "table_id",
                "multiple": true,
                "bind": "COLUMNS",
                "breakLine": true
              }
            }
          }
        }
      },
      "options.add_rows": {
        "type": "fieldset",
        "label": "新行选项",
        "repeat": true,
        "fields": {
          "new_row_name": {
            "type": "text",
            "label": "新行名称",
            "show_if": "$table_id.numOfLabel >= 1",
            "val": ""
          },
          "function_type": {
            "type": "select",
            "label": "任务类型",
            "show_if": "false",
            "required": true,
            "options": {
              "multi_column_functions": {
                "label": "多列"
              },
              "multi_row_functions": {
                "label": "多行"
              }
            },
            "val": "multi_row_functions"
          },
          "multi_row_functions__function": {
            "type": "select",
            "label": "选择功能",
            "show_if": "$table_id.numOfLabel >= 1",
            "required": true,
            "options": {
              "sum": {
                "label": "总和"
              },
              "average": {
                "label": "平均"
              },
              "median": {
                "label": "中位数"
              },
              "standard_deviation": {
                "label": "标准差"
              },
              "variance": {
                "label": "方差"
              },
              "product": {
                "label": "产品"
              },
              "quota": {
                "label": "比例"
              }
            },
            "val": "sum"
          },
          "multi_row_functions__function_use_filter": {
            "type": "select",
            "label": "选择功能",
            "show_if": "$table_id.numOfLabel < 1",
            "required": true,
            "options": {
              "sum": {
                "label": "总和"
              },
              "average": {
                "label": "平均"
              },
              "median": {
                "label": "中位数"
              },
              "standard_deviation": {
                "label": "标准差"
              },
              "variance": {
                "label": "方差"
              },
              "product": {
                "label": "产品"
              }
            },
            "val": "sum"
          },
          "multi_row_functions__sel_labels": {
            "type": "select",
            "label": "选择行(s)",
            "show_if": "$table_id.numOfLabel >= 1 && this.multi_row_functions__function!='quota'",
            "required": true,
            "table": "table_id",
            "multiple": true,
            "bind": "LABELS"
          },
          "multi_row_functions__sel_rows": {
            "type": "select",
            "label": "Select Rows",
            "show_if": "false",
            "required": true,
            "options": {
              "row-1": {
                "label": "行-1"
              },
              "row-2": {
                "label": "第2行"
              },
              "row-3": {
                "label": "第3行"
              },
              "row-4": {
                "label": "第4行"
              },
              "row-5": {
                "label": "第5行"
              },
              "row-6": {
                "label": "第6行"
              },
              "row-7": {
                "label": "第7行"
              },
              "row-8": {
                "label": "第8行"
              },
              "row-9": {
                "label": "第9行"
              },
              "row-10": {
                "label": "第10行"
              },
              "row+1": {
                "label": "Row+1"
              },
              "row+2": {
                "label": "Row+2"
              },
              "row+3": {
                "label": "Row+3"
              },
              "row+4": {
                "label": "Row+4"
              },
              "row+5": {
                "label": "Row+5"
              },
              "row+6": {
                "label": "Row+6"
              },
              "row+7": {
                "label": "Row+7"
              },
              "row+8": {
                "label": "Row+8"
              },
              "row+9": {
                "label": "Row+9"
              },
              "row+10": {
                "label": "Row+10"
              }
            },
            "multiple": true,
            "val": "row-1"
          },
          "multi_row_functions__sel_label_a": {
            "type": "select",
            "label": "选择标签 A",
            "show_if": "$table_id.numOfLabel >= 1 && this.multi_row_functions__function=='quota'",
            "required": true,
            "table": "table_id",
            "bind": "LABELS"
          },
          "multi_row_functions__sel_label_b": {
            "type": "select",
            "label": "选择标签 B",
            "show_if": "$table_id.numOfLabel >= 1 && this.multi_row_functions__function=='quota'",
            "required": true,
            "table": "table_id",
            "bind": "LABELS",
            "breakLine": true
          },
          "position": {
            "type": "select",
            "label": "位置",
            "show_if": "$table_id.numOfLabel >= 1",
            "required": true,
            "options": {
              "bottom": {
                "label": "底部"
              },
              "top": {
                "label": "顶部"
              },
              "above": {
                "label": "之上"
              },
              "below": {
                "label": "之下"
              }
            },
            "val": "bottom"
          },
          "positioning_row": {
            "type": "select",
            "label": "定位行",
            "show_if": "$table_id.numOfLabel >= 1 && (this.position=='above' || this.position=='below')",
            "required": true,
            "table": "table_id",
            "bind": "LABELS"
          },
          "format__category": {
            "type": "select",
            "label": "显示格式",
            "show_if": "$table_id.numOfLabel >= 1",
            "required": true,
            "options": {
              "standard": {
                "label": "标准"
              },
              "number": {
                "label": "数字"
              },
              "percentage": {
                "label": "百分比"
              },
              "text": {
                "label": "文本"
              },
              "date": {
                "label": "日期"
              },
              "currency": {
                "label": "货币"
              }
            },
            "val": "standard"
          },
          "format__rounding": {
            "type": "select",
            "label": "显示的小数位数",
            "show_if": "this.format__category=='number' || this.format__category=='percentage' || this.format__category=='currency'",
            "required": true,
            "options": {
              "no_rounding": {
                "label": "不取整"
              },
              "0": {
                "label": "0 位数"
              },
              "1": {
                "label": "1 位数"
              },
              "2": {
                "label": "2 位数"
              },
              "3": {
                "label": "3 位数"
              },
              "4": {
                "label": "4 位数"
              },
              "5": {
                "label": "5 位数"
              },
              "6": {
                "label": "6 位数"
              },
              "7": {
                "label": "7 位数"
              },
              "8": {
                "label": "8 位数"
              },
              "9": {
                "label": "9 位数"
              }
            },
            "val": "no_rounding"
          },
          "format__use_1000_separator": {
            "type": "checkbox",
            "label": "Use 1000 Separator(.)",
            "show_if": "this.format__category=='number' || this.format__category=='percentage' || this.format__category=='currency'",
            "val": "false",
            "isOn": "true",
            "isOff": "false"
          },
          "format__currency_symbol": {
            "type": "select",
            "label": "符号",
            "show_if": "this.format__category=='currency'",
            "required": true,
            "options": {
              "dollar": {
                "label": "$"
              },
              "euro": {
                "label": "€"
              },
              "pound": {
                "label": "£"
              },
              "yen": {
                "label": "¥ (Yen, currency of Japan)"
              },
              "colon": {
                "label": "₡ (Colón, currency of Costa Rica)"
              },
              "naira": {
                "label": "₦ (Naira, currency of Nigeria)"
              },
              "rupee": {
                "label": "₨ (Rupee, currency of India)"
              },
              "rupie": {
                "label": "₹ (Rupee, currency of India)"
              },
              "won": {
                "label": "₩ (Won, currency in South Korea and North Korea)"
              },
              "hebrew": {
                "label": "₪ (Currency of Israel)"
              },
              "dong": {
                "label": "₫ (Currency of Vietnam)"
              },
              "kip": {
                "label": "₭ (Kip, currency of Laos)"
              },
              "mongolia": {
                "label": "₮ (Currency of Mongolia)"
              },
              "peso": {
                "label": "₱ (Peso, currency of the Philippines)"
              },
              "guarani": {
                "label": "₲ (Guaraní, currency of Paraguay)"
              },
              "hryvnia": {
                "label": "₴ (Hryvnia, currency of Ukraine)"
              },
              "tenge": {
                "label": "₸ (Tenge, currency of Kazakhstan)"
              },
              "lira": {
                "label": "₺ (Lira, currency of Turkey)"
              },
              "manat": {
                "label": "₼ (Manat, currency of Azerbaijan)"
              },
              "rubles": {
                "label": "₽ (Rubles, currency of Russia)"
              },
              "lari": {
                "label": "₾ (Lari, currency of Georgia)"
              },
              "bitcoin": {
                "label": "₿ (Bitcoin, crypto currency)"
              }
            },
            "val": "dollar"
          },
          "format__date_format": {
            "type": "select",
            "label": "类型",
            "show_if": "this.format__category=='date'",
            "required": true,
            "options": {
              "mm/dd/yyyy": {
                "label": "3/14/2012"
              },
              "D, M dd, Y": {
                "label": "2012年3月14日，星期三"
              },
              "yyyy-mm-dd": {
                "label": "2012-03-14"
              },
              "m.dd.yyyy": {
                "label": "3.14.2012"
              },
              "m.dd.yy": {
                "label": "3.14.12"
              },
              "dd.m.yyyy": {
                "label": "14.3.2012"
              },
              "dd.m.yy": {
                "label": "14.3.12"
              },
              "M dd, Y": {
                "label": "March 14, 2012"
              },
              "dd-mm-yyyy": {
                "label": "14-03-2012"
              },
              "mm-dd-yyyy": {
                "label": "03-14-2012"
              },
              "dd-mm-yy": {
                "label": "14-03-12"
              },
              "mm-dd-yy": {
                "label": "03-14-12"
              }
            },
            "val": "mm/dd/yyyy"
          },
          "delete": {
            "type": "button",
            "label": "删除",
            "function": "DELETE_ROW"
          }
        }
      },
      "options.add_new_row": {
        "type": "checkbox",
        "label": "添加标签",
        "show_if": "$table_id.numOfLabel < 1",
        "val": "false",
        "isOn": "true",
        "isOff": "false",
        "style": "margin-top: 10px",
        "breakLine": true
      },
      "options.new_row_label": {
        "type": "text",
        "label": "Label Name",
        "show_if": "$table_id.numOfLabel < 1 && options.add_new_row == 'true'",
        "val": ""
      },
      "options.new_sel_column": {
        "type": "select",
        "label": "选择列",
        "show_if": "$table_id.numOfLabel < 1 && options.add_new_row == 'true'",
        "required": true,
        "table": "table_id",
        "multiple": false,
        "bind": "COLUMNS",
        "breakLine": true
      },
      "options.insert_on_top": {
        "type": "checkbox",
        "label": "Insert on Top",
        "show_if": "$table_id.numOfLabel < 1",
        "val": "false",
        "isOn": "true",
        "isOff": "false",
        "style": "margin-top: 10px",
        "breakLine": true
      }
    }
  }
}