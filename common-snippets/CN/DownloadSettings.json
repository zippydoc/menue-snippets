{
  "download_setting_frame": {
    "type": "fieldset",
    "label": "下载设置",
    "show_if": "output.custom_options=='true'",
    "repeat": false,
    "fields": {
      "output.options.location": {
        "type": "text",
        "label": "指定位置",
        "val": "A1",
        "breakLine": true
      },
      "output.options.formatted_as_table": {
        "type": "checkbox",
        "label": "格式化为表格",
        "val": "true",
        "isOn": "true",
        "isOff": "false",
        "breakLine": true
      },
      "output.options.visibility": {
        "type": "select",
        "label": "可见性",
        "val": "current_flow",
        "options": {
          "current_flow": {
            "label": "当前工作流"
          },
          "all_flow": {
            "label": "所有工作流"
          }
        },
        "breakLine": true
      },
      "output.options.table_mode": {
        "type": "select",
        "label": "粘贴",
        "val": "content_only",
        "options": {
          "content_only": {
            "label": "仅内容"
          },
          "fully_formatted": {
            "label": "完全格式化"
          }
        }
      }
    },
    "breakLine": true,
    "style": "margin-bottom: 20px"
  }
}