{
  "format__category": {
    "type": "select",
    "label": "格式",
    "required": true,
    "options": {
      "standard": {
        "label": "标准"
      },
      "number": {
        "label": "数字"
      },
      "percentage": {
        "label": "百分比"
      },
      "text": {
        "label": "文本"
      },
      "date": {
        "label": "日期"
      },
      "currency": {
        "label": "货币"
      }
    },
    "val": "standard"
  },
  "format__rounding": {
    "type": "select",
    "label": "取整",
    "show_if": "this.format__category=='number' || this.format__category=='percentage' || this.format__category=='currency'",
    "required": true,
    "options": {
      "no_rounding": {
        "label": "不取整"
      },
      "0": {
        "label": "0 位数"
      },
      "1": {
        "label": "1 位数"
      },
      "2": {
        "label": "2 位数"
      },
      "3": {
        "label": "3 位数"
      },
      "4": {
        "label": "4 位数"
      },
      "5": {
        "label": "5 位数"
      },
      "6": {
        "label": "6 位数"
      },
      "7": {
        "label": "7 位数"
      },
      "8": {
        "label": "8 位数"
      },
      "9": {
        "label": "9 位数"
      }
    },
    "val": "2"
  },
  "format__use_1000_separator": {
    "type": "checkbox",
    "label": "使用 1000 分隔符（.）",
    "show_if": "this.format__category=='number' || this.format__category=='percentage' || this.format__category=='currency'",
    "val": "true",
    "isOn": "true",
    "isOff": "false"
  },
  "format__currency_symbol": {
    "type": "select",
    "label": "符号",
    "show_if": "this.format__category=='currency'",
    "required": true,
    "options": {
      "dollar": {
        "label": "$"
      },
      "euro": {
        "label": "€"
      },
      "pound": {
        "label": "£"
      }
    },
    "val": "dollar"
  },
  "format__date_format": {
    "type": "select",
    "label": "类型",
    "show_if": "this.format__category=='date'",
    "required": true,
    "options": {
      "mm/dd/yyyy": {
        "label": "3/14/2012"
      },
      "D, M dd, Y": {
        "label": "2012年3月14日，星期三"
      },
      "yyyy-mm-dd": {
        "label": "2012-03-14"
      }
    },
    "val": "mm/dd/yyyy",
    "breakLine": true
  }
}