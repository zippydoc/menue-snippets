{
  "placeholder": {
    "breakLine": true
  },
  "apply_to_all_columns": {
    "type": "checkbox",
    "label": "所有列",
    "show_if": "false",
    "val": "false",
    "isOn": "true",
    "isOff": "false",
    "breakLine": true	
  },
  "apply_columns": {
    "type": "select",
    "label": "选择要应用的列",
    "show_if": "$input_table.numOfHeader >= 1",
    "required": true,
    "table": "input_table",
    "multiple": true,
    "bind": "COLUMNS",
    "breakLine": true
  },
  "apply_to_all_rows": {
    "type": "checkbox",
    "label": "所有行",
    "show_if": "false",
    "val": "false",
    "isOn": "true",
    "isOff": "false",
    "breakLine": true
  },
  "apply_rows": {
    "type": "select",
    "label": "选择要应用的行",
    "show_if": "$input_table.numOfLabel >= 1",
    "required": true,
    "table": "input_table",
    "multiple": true,
    "bind": "LABELS",
    "breakLine": true
  },
  "intersection": {
    "type": "select",
    "label": "选择",
    "show_if": "false",
    "options": {
      "all": {
        "label": "所有已选行列"
      },
      "intersection": {
        "label": "所选行列的交集"
      }
    },
    "val": "intersection"
  }
}