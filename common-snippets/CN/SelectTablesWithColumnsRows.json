{
  "input": {
    "type": "fieldset",
    "label": "选择输入表格",
    "repeat": false,
    "fields": {
      "input_table1": {
        "type": "select",
        "label": "选择表格 1",
        "bind": "TABLES",
        "empty": "",
        "required": true,
        "breakLine": true
      },
      "apply_to_all_columns1": {
        "type": "checkbox",
        "label": "所有列",
        "val": "true",
        "isOn": "true",
        "isOff": "false"
      },
      "apply_columns1": {
        "type": "select",
        "label": "选择要应用的列",
        "show_if": "apply_to_all_columns1==''false''",
        "required": true,
        "table": "input_table1",
        "multiple": true,
        "bind": "COLUMNS",
        "breakLine": true
      },
      "apply_to_all_rows1": {
        "type": "checkbox",
        "label": "所有行",
        "val": "true",
        "isOn": "true",
        "isOff": "false"
      },
      "apply_rows1": {
        "type": "select",
        "label": "选择要应用的行",
        "show_if": "apply_to_all_rows1==''false''",
        "required": true,
        "table": "input_table1",
        "multiple": true,
        "bind": "LABELS",
        "breakLine": true
      },
      "input_table2": {
        "type": "select",
        "label": "选择表格 2",
        "bind": "TABLES",
        "empty": "",
        "required": true,
        "breakLine": true
      },
      "apply_to_all_columns2": {
        "type": "checkbox",
        "label": "所有列",
        "val": "true",
        "isOn": "true",
        "isOff": "false"
      },
      "apply_columns2": {
        "type": "select",
        "label": "选择要应用的列",
        "show_if": "apply_to_all_columns2==''false''",
        "required": true,
        "table": "input_table2",
        "multiple": true,
        "bind": "COLUMNS",
        "breakLine": true
      },
      "apply_to_all_rows2": {
        "type": "checkbox",
        "label": "所有行",
        "val": "true",
        "isOn": "true",
        "isOff": "false"
      },
      "apply_rows2": {
        "type": "select",
        "label": "选择要应用的行",
        "show_if": "apply_to_all_rows2==''false''",
        "required": true,
        "table": "input_table2",
        "multiple": true,
        "bind": "LABELS",
        "breakLine": true
      }
    }
  }
}