      "options.countries__select_type": {
        "type": "select",
        "label": "Method",
        "required": true,
        "options": {
          "all": {
            "label": "Select in all"
          },
          "region": {
            "label": "Select by region"
          },
          "income_group": {
            "label": "Select by income group"
          }
        },
        "val": "region"
      },
      "options.countries__select_region": {
        "type": "select",
        "label": "Region",
        "show_if" : "options.countries__select_type=='region'",
