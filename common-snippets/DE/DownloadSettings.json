{
  "download_setting_frame": {
    "type": "fieldset",
    "Beschriftung": "Einstellungen herunterladen",
    "show_if": "output.custom_options=='true'",
    "repeat": false,
    "fields": {
      "output.options.location": {
        "type": "text",
        "Beschriftung": "Standort angeben",
        "val": "A1",
        "breakLine": true
      },
      "output.options.formatted_as_table": {
        "type": "checkbox",
        "Beschriftung": "Als Tabelle formatiert",
        "val": "true",
        "isOn": "true",
        "isOff": "false",
        "breakLine": true
      },
      "output.options.visibility": {
        "type": "select",
        "Beschriftung": "Sichtbarkeit",
        "val": "current_flow",
        "options": {
          "current_flow": {
            "Beschriftung": "Stromfluss"
          },
          "all_flow": {
            "Beschriftung": "Alle Flows"
          }
        },
        "breakLine": true
      },
      "output.options.table_mode": {
        "type": "select",
        "Beschriftung": "Einfügen",
        "val": "content_only",
        "options": {
          "content_only": {
            "Beschriftung": "Nur Inhalt"
          },
          "fully_formatted": {
            "Beschriftung": "Vollständig formatiert"
          }
        }
      }
    },
    "breakLine": true,
    "style": "margin-bottom: 20px"
  }
}