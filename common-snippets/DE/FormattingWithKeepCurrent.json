{
  "options.format__category": {
    "type": "select",
    "Beschriftung": "Format",
    "required": true,
    "options": {
      "keep_current": {
        "Beschriftung": "Strom halten"
      },
      "standard": {
        "Beschriftung": "Standard"
      },
      "number": {
        "Beschriftung": "Nummer"
      },
      "percentage": {
        "Beschriftung": "Prozentsatz"
      },
      "text": {
        "Beschriftung": "Text"
      },
      "date": {
        "Beschriftung": "Datum"
      },
      "currency": {
        "Beschriftung": "Währung"
      }
    },
    "val": "keep_current"
  },
  "options.format__rounding": {
    "type": "select",
    "Beschriftung": "Abrunden",
    "show_if": "options.format__category=='number' || options.format__category=='percentage' || options.format__category=='currency'",
    "required": true,
    "options": {
      "no_rounding": {
        "Beschriftung": "Keine Rundung"
      },
      "0": {
        "Beschriftung": "0 Ziffern"
      },
      "1": {
        "Beschriftung": "1 Ziffern"
      },
      "2": {
        "Beschriftung": "2 Ziffern"
      },
      "3": {
        "Beschriftung": "3 Ziffern"
      },
      "4": {
        "Beschriftung": "4 Ziffern"
      },
      "5": {
        "Beschriftung": "5 Ziffern"
      },
      "6": {
        "Beschriftung": "6 Ziffern"
      },
      "7": {
        "Beschriftung": "7 Ziffern"
      },
      "8": {
        "Beschriftung": "8 Ziffern"
      },
      "9": {
        "Beschriftung": "9 Ziffern"
      }
    },
    "val": "2"
  },
  "options.format__use_1000_separator": {
    "type": "checkbox",
    "Beschriftung": "1000 Trennzeichen(.) verwenden",
    "show_if": "options.format__category=='number' || options.format__category=='percentage' || options.format__category=='currency'",
    "val": "true",
    "isOn": "true",
    "isOff": "false"
  },
  "options.format__currency_symbol": {
    "type": "select",
    "Beschriftung": "Symbol",
    "show_if": "options.format__category=='currency'",
    "required": true,
    "options": {
      "dollar": {
        "Beschriftung": "$"
      },
      "euro": {
        "Beschriftung": "€"
      },
      "pound": {
        "Beschriftung": "£"
      }
    },
    "val": "dollar"
  },
  "options.format__date_format": {
    "type": "select",
    "Beschriftung": "Typ",
    "show_if": "options.format__category=='date'",
    "required": true,
    "options": {
      "mm/dd/yyyy": {
        "Beschriftung": "3/14/2012"
      },
      "D, M dd, Y": {
        "Beschriftung": "Mittwoch, 14. März 2012"
      },
      "yyyy-mm-dd": {
        "Beschriftung": "2012-03-14"
      }
    },
    "val": "mm/dd/yyyy",
    "breakLine": true
  }
}