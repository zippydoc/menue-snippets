{
  "options.format__category": {
    "type": "select",
    "label": "Format",
    "required": true,
    "options": {
      "keep_current": {
        "label": "Bisherigen beibehalten"
      },
      "standard": {
        "label": "Standard"
      },
      "number": {
        "label": "Nummer"
      },
      "percentage": {
        "label": "Prozentsatz"
      },
      "text": {
        "label": "Text"
      },
      "date": {
        "label": "Datum"
      },
      "currency": {
        "label": "Währung"
      }
    },
    "val": "keep_current"
  },
  "options.format__rounding": {
    "type": "select",
    "label": "Abrunden",
    "show_if": "options.format__category=='number' || options.format__category=='percentage' || options.format__category=='currency'",
    "required": true,
    "options": {
      "no_rounding": {
        "label": "Keine Rundung"
      },
      "0": {
        "label": "0 Ziffern"
      },
      "1": {
        "label": "1 Ziffern"
      },
      "2": {
        "label": "2 Ziffern"
      },
      "3": {
        "label": "3 Ziffern"
      },
      "4": {
        "label": "4 Ziffern"
      },
      "5": {
        "label": "5 Ziffern"
      },
      "6": {
        "label": "6 Ziffern"
      },
      "7": {
        "label": "7 Ziffern"
      },
      "8": {
        "label": "8 Ziffern"
      },
      "9": {
        "label": "9 Ziffern"
      }
    },
    "val": "2"
  },
  "options.format__use_1000_separator": {
    "type": "checkbox",
    "label": "1000 Trennzeichen(.) verwenden",
    "show_if": "options.format__category=='number' || options.format__category=='percentage' || options.format__category=='currency'",
    "val": "true",
    "isOn": "true",
    "isOff": "false"
  },
  "options.format__currency_symbol": {
    "type": "select",
    "label": "Symbol",
    "show_if": "options.format__category=='currency'",
    "required": true,
    "options": {
      "dollar": {
        "label": "$"
      },
      "euro": {
        "label": "€"
      },
      "pound": {
        "label": "£"
      }
    },
    "val": "dollar"
  },
  "options.format__date_format": {
    "type": "select",
    "label": "Typ",
    "show_if": "options.format__category=='date'",
    "required": true,
    "options": {
      "mm/dd/yyyy": {
        "label": "3/14/2012"
      },
      "D, M dd, Y": {
        "label": "Mittwoch, 14. März 2012"
      },
      "yyyy-mm-dd": {
        "label": "2012-03-14"
      }
    },
    "val": "mm/dd/yyyy",
    "breakLine": true
  }
}