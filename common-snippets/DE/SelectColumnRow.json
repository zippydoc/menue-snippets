{  "placeholder": {    "breakLine": true  },   "apply_to_all_columns": {    "type": "checkbox",     "label": "Alle Spalten",     "val": "true",     "isOn": "true",     "isOff": "false",     "breakLine": true	  },   "apply_columns": {    "type": "select",     "label": "Spalten zum Anwenden auswählen",     "show_if": "apply_to_all_columns=='false'",     "required": true,     "table": "input_table",     "multiple": true,     "bind": "COLUMNS",     "breakLine": true  },   "apply_to_all_rows": {    "type": "checkbox",     "label": "Alle Zeilen",     "val": "true",     "isOn": "true",     "isOff": "false",     "breakLine": true  },   "apply_rows": {    "type": "select",     "label": "Zu übertragende Zeilen auswählen",     "show_if": "apply_to_all_rows=='false'",     "required": true,     "table": "input_table",     "multiple": true,     "bind": "LABELS",     "breakLine": true  },   "intersection": {    "type": "select",     "label": "Auswahl",     "show_if": "false",     "options": {      "all": {        "label": "Alle ausgewählten Zeilen & Spalten"      },       "intersection": {        "label": "Schnittpunkt ausgewählter Zeilen & Spalten"      }    },     "val": "intersection"  }}