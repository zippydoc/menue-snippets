{
  "options.cut_off_run": {
    "type": "checkbox",
    "Etiqueta": "Corte de la tabla de entrada",
    "val": "false",
    "isOn": "true",
    "isOff": "false"
  },
  "options.cut_off.type": {
    "type": "select",
    "Etiqueta": "Tipo de corte",
    "show_if": "options.cut_off_run=='true'",
    "required": true,
    "options": {
      "from_first_row": {
        "Etiqueta": "Desde la primera fila"
      },
      "range": {
        "Etiqueta": "Intervalo"
      }
    },
    "val": "from_first_row"
  },
  "options.cut_off.start_row_num": {
    "type": "text",
    "Etiqueta": "Inicio de fila",
    "show_if": "options.cut_off_run=='true' && options.cut_off.type=='range'",
    "required": true,
    "minLength": 1,
    "maxLength": 7,
    "val": "1"
  },
  "options.cut_off.row_count": {
    "type": "text",
    "Etiqueta": "Recuento de filas",
    "show_if": "options.cut_off_run=='true'",
    "required": true,
    "minLength": 1,
    "maxLength": 7,
    "val": "100"
  }
}