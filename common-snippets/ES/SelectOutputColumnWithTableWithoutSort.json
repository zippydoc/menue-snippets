{
  "output.select_output_columns": {
    "type": "checkbox",
    "Etiqueta": "Seleccionar Columnas de Salida",
    "val": "false",
    "isOn": "true",
    "isOff": "false"
  },
  "output_columns": {
    "type": "fieldset",
    "Etiqueta": "Seleccionar columnas de salida",
    "show_if": "output.select_output_columns=='true'",
    "repeat": true,
    "fields": {
      "table_id": {
        "type": "select",
        "Etiqueta": "Seleccionar Tabla",
        "bind": "TABLES",
        "required": true
      },
      "field_name": {
        "type": "select",
        "Etiqueta": "Seleccionar Columna",
        "table": "this.table_id",
        "bind": "COLUMNS",
        "required": true
      },
      "is_rename": {
        "type": "checkbox",
        "Etiqueta": "Renombrar",
        "val": "false",
        "isOn": "true",
        "isOff": "false"
      },
      "rename": {
        "show_if": "this.is_rename == 'true'",
        "type": "text",
        "required": true,
        "minLength": 1,
        "maxLength": 255,
        "Etiqueta": "Nuevo Nombre de Columna",
        "style": "margin-left: 10px"
      },
      "format__category": {
        "type": "select",
        "Etiqueta": "Formato",
        "required": true,
        "options": {
          "standard": {
            "Etiqueta": "Estándar"
          },
          "number": {
            "Etiqueta": "Número"
          },
          "percentage": {
            "Etiqueta": "Porcentaje"
          },
          "text": {
            "Etiqueta": "Texto"
          },
          "date": {
            "Etiqueta": "Fecha"
          },
          "currency": {
            "Etiqueta": "Moneda"
          }
        },
        "val": "standard"
      },
      "format__rounding": {
        "type": "select",
        "Etiqueta": "Redondeo",
        "show_if": "this.format__category=='number' || this.format__category=='percentage' || this.format__category=='currency'",
        "required": true,
        "options": {
          "no_rounding": {
            "Etiqueta": "Redondeo de números"
          },
          "0": {
            "Etiqueta": "0 dígitos"
          },
          "1": {
            "Etiqueta": "1 dígitos"
          },
          "2": {
            "Etiqueta": "2 dígitos"
          },
          "3": {
            "Etiqueta": "3 dígitos"
          },
          "4": {
            "Etiqueta": "4 dígitos"
          },
          "5": {
            "Etiqueta": "5 dígitos"
          },
          "6": {
            "Etiqueta": "6 dígitos"
          },
          "7": {
            "Etiqueta": "7 dígitos"
          },
          "8": {
            "Etiqueta": "8 dígitos"
          },
          "9": {
            "Etiqueta": "9 dígitos"
          }
        },
        "val": "2"
      },
      "format__use_1000_separator": {
        "type": "checkbox",
        "Etiqueta": "Usar el Separador de 1000(.)",
        "show_if": "this.format__category=='number' || this.format__category=='percentage' || this.format__category=='currency'",
        "val": "true",
        "isOn": "true",
        "isOff": "false"
      },
      "format__currency_symbol": {
        "type": "select",
        "Etiqueta": "Símbolo",
        "show_if": "this.format__category=='currency'",
        "required": true,
        "options": {
          "dollar": {
            "Etiqueta": "$"
          },
          "euro": {
            "Etiqueta": "€"
          },
          "pound": {
            "Etiqueta": "£"
          }
        },
        "val": "dollar"
      },
      "format__date_format": {
        "type": "select",
        "Etiqueta": "Tipo",
        "show_if": "this.format__category=='date'",
        "required": true,
        "options": {
          "mm/dd/yyyy": {
            "Etiqueta": "3/14/2012"
          },
          "D, M dd, Y": {
            "Etiqueta": "Miércoles, 14 de marzo de 2012"
          },
          "yyyy-mm-dd": {
            "Etiqueta": "40982"
          }
        },
        "val": "mm/dd/yyyy",
        "breakLine": true
      },
      "delete": {
        "type": "button",
        "Etiqueta": "Borrar",
        "function": "DELETE_ROW"
      }
    },
    "breakLine": true,
    "style": "margin-bottom: 20px"
  }
}