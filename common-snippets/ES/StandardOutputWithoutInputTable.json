{
  "output": {
    "type": "fieldset",
    "label": "Definir Tabla de Salida",
    "repeat": false,
    "fields": {
      "output.custom_text": {
        "type": "text",
        "label": "Imprimir Nombre de Tabla",
        "show_if": "output.custom_options=='false' || (output.custom_options=='true' && output.compose_table_name=='false')",
        "minLength": 1,
        "val": "",
        "width": "100%",
        "breakLine": true
      },
      "output.custom_options": {
        "type": "checkbox",
        "label": "Imprimir Opciones",
        "val": "false",
        "isOn": "true",
        "isOff": "false",
        "breakLine": true
      },
      "output.compose_table_name": {
        "type": "checkbox",
        "label": "Componer Nombre de la Tabla",
        "show_if": "output.custom_options=='true'",
        "val": "false",
        "isOn": "true",
        "isOff": "false",
        "breakLine": true
      },
      "compose_table_name_frame": {
        "type": "fieldset",
        "label": "Componer Nombre de la Tabla de Salida",
        "show_if": "output.custom_options=='true' && output.compose_table_name=='true'",
        "repeat": false,
        "fields": {
          "output.prefix_rule": {
            "type": "select",
            "label": "Prefijo",
            "show_if": "output.compose_table_name=='true'",
            "val": "empty",
            "options": {
              "empty": {
                "label": "-"
              },
              "script_name": {
                "label": "Nombre de la Tarea"
              },
              "date_string_type1": {
                "label": "Fecha: dd_mm_aaaa"
              },
              "date_string_type2": {
                "label": "Fecha: aaaa_mm_dd"
              },
              "time_string": {
                "label": "Tempo: h_min_seg"
              },
              "datetime_string": {
                "label": "Fecha_Hora"
              },
              "custom": {
                "label": "Personalizado"
              }
            }
          },
          "output.prefix_text": {
            "type": "text",
            "label": "Texto del Prefijo",
            "show_if": "output.compose_table_name=='true' && output.prefix_rule=='custom'",
            "minLength": 1,
            "maxLength": 31,
            "val": ""
          },
          "output.core_delimiter": {
            "type": "text",
            "label": "Delimitador",
            "show_if": "output.compose_table_name=='true' && output.prefix_rule!='empty'",
            "val": "_",
            "width": "50px"
          },
          "output.core_rule": {
            "type": "select",
            "label": "Nombre de la Tabla",
            "show_if": "output.compose_table_name=='true'",
            "val": "script_name",
            "options": {
              "script_name": {
                "label": "Nombre de la Tarea"
              },
              "custom": {
                "label": "Personalizado"
              }
            }
          },
          "output.core_text": {
            "type": "text",
            "label": "Imprimir Nombre de Tabla",
            "show_if": "output.compose_table_name=='true' && output.core_rule=='custom'",
            "required": true,
            "minLength": 1,
            "maxLength": 31,
            "val": ""
          },
          "output.suffix_delimiter": {
            "type": "text",
            "label": "Delimitador",
            "show_if": "output.compose_table_name=='true'",
            "val": "_",
            "width": "50px"
