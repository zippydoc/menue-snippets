      "error_if_exist": {
        "Étiquette": "Créer (erreur si déjà existant)"
      },
      "error_if_not_exist": {
        "Étiquette": "Ignorer (erreur si non-existant)"
      },
      "append_all": {
        "Étiquette": "Ajouter tout"
      },
      "append_not_existing": {
        "Étiquette": "Ajouter non existant"
      },
      "append_changes": {
        "Étiquette": "Ajouter les modifications
      }
