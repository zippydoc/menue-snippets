{
  "input": {
    "type": "fieldset", 
    "label": "Sélectionner un tableau d'entrée", 
    "repeat": false, 
    "fields": {
      "input_table1": {
        "type": "select", 
        "label": "Sélectionner Tableau 1", 
        "bind": "TABLES", 
        "empty": "", 
        "required": true, 
        "breakLine": true
      }, 
      "apply_to_all_columns1": {
        "type": "checkbox", 
        "label": "Toutes les colonnes", 
        "val": "true", 
        "isOn": "true", 
        "isOff": "false"
      }, 
      "apply_columns1": {
        "type": "select", 
        "label": "Sélectionner les colonnes à appliquer", 
        "show_if": "apply_to_all_columns1=='false'", 
        "required": true, 
        "table": "input_table1", 
        "multiple": true, 
        "bind": "COLUMNS", 
        "breakLine": true
      }, 
      "apply_to_all_rows1": {
        "type": "checkbox", 
        "label": "Toutes les lignes", 
        "val": "true", 
        "isOn": "true", 
        "isOff": "false"
      }, 
      "apply_rows1": {
        "type": "select", 
        "label": "Sélectionnez les lignes à appliquer", 
        "show_if": "apply_to_all_rows1=='false'", 
        "required": true, 
        "table": "input_table1", 
        "multiple": true, 
        "bind": "LABELS", 
        "breakLine": true
      }, 
      "input_table2": {
        "type": "select", 
        "label": "Sélectionner Tableau 2", 
        "bind": "TABLES", 
        "empty": "", 
        "required": true, 
        "breakLine": true
      }, 
      "apply_to_all_columns2": {
        "type": "checkbox", 
        "label": "Toutes les colonnes", 
        "val": "true", 
        "isOn": "true", 
        "isOff": "false"
      }, 
      "apply_columns2": {
        "type": "select", 
        "label": "Sélectionner les colonnes à appliquer", 
        "show_if": "apply_to_all_columns2=='false'", 
        "required": true, 
        "table": "input_table2", 
        "multiple": true, 
        "bind": "COLUMNS", 
        "breakLine": true
      }, 
      "apply_to_all_rows2": {
        "type": "checkbox", 
        "label": "Toutes les lignes", 
        "val": "true", 
        "isOn": "true", 
        "isOff": "false"
      }, 
      "apply_rows2": {
        "type": "select", 
        "label": "Sélectionnez les lignes à appliquer", 
        "show_if": "apply_to_all_rows2=='false'", 
        "required": true, 
        "table": "input_table2", 
        "multiple": true, 
        "bind": "LABELS", 
        "breakLine": true
      }
    }
  }
}