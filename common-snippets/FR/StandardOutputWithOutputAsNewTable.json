{
  "output": {
    "type": "fieldset",
    "label": "Définir le tableau des résultats",
    "repeat": false,
    "fields": {
      "output.output_as_new_table": {
        "type": "checkbox",
        "label": "Output as New table",
        "val": "true",
        "isOn": "true",
        "isOff": "false"
      },
      "output.custom_text": {
        "type": "text",
        "label": "Sortir le nom du tableau",
        "show_if": "((output.set_options=='true' && output.compose_table_name=='false') || output.set_options=='false') && output.output_as_new_table=='true'",
        "minLength": 1,
        "val": "",
        "width": "100%",
        "breakLine": true
      },
      "output.set_options": {
        "type": "checkbox",
        "label": "Options de sortie",
        "val": "false",
        "isOn": "true",
        "isOff": "false"
      },
      "output_options_box": {
        "type": "fieldset",
        "label": "Options de sortie",
        "show_if": "output.set_options=='true'",
        "repeat": false,
        "fields": {
          "output.compose_table_name": {
            "type": "checkbox",
            "label": "Saisissez le nom du tableau",
            "show_if": "output.output_as_new_table=='true'",
            "val": "false",
            "isOn": "true",
            "isOff": "false",
            "breakLine": true
          },
          "compose_table_name_frame": {
            "type": "fieldset",
            "label": "Saisissez le nom du tableau de sortie",
            "show_if": "output.output_as_new_table=='true' && output.compose_table_name=='true'",
            "repeat": false,
            "fields": {
              "output.prefix_rule": {
                "type": "select",
                "label": "Préfixe",
                "show_if": "output.compose_table_name=='true'",
                "val": "empty",
                "options": {
                  "empty": {
                    "label": "-"
                  },
                  "script_name": {
                    "label": "Nom de la tâche"
                  },
                  "date_string_type1": {
                    "label": "Date : jj_mm_aaaa"
                  },
                  "date_string_type2": {
                    "label": "Date : aaaa_mm_jj"
                  },
                  "time_string": {
                    "label": "Heure : h_min_sec"
                  },
                  "datetime_string": {
                    "label": "Date_heure"
                  },
                  "custom": {
                    "label": "Personnaliser"
                  }
                }
              },
              "output.prefix_text": {
                "type": "text",
                "label": "Texte du préfixe",
                "show_if": "output.compose_table_name=='true' && output.prefix_rule=='custom'",
                "minLength": 1,
                "maxLength": 31,
                "val": ""
              },
              "output.core_delimiter": {
                "type": "text",
                "label": "Délimiteur",
                "show_if": "output.compose_table_name=='true' && output.prefix_rule!='empty'",
                "val": "_",
                "width": "50px"
              },
              "output.core_rule": {
                "type": "select",
                "label": "Nom du tableau",
                "show_if": "output.compose_table_name=='true'",
                "val": "input_table_name",
                "options": {
                  "input_table_name": {
                    "label": "Nom du tableau d'entrée"
                  },
                  "script_name": {
                    "label": "Nom de la tâche"
                  },
                  "custom": {
                    "label": "Personnaliser"
                  }
                }
              },
              "output.core_text": {
                "type": "text",
                "label": "Nom du tableau principal",
                "show_if": "output.compose_table_name=='true' && output.core_rule=='custom'",
                "required": true,
                "minLength": 1,
                "maxLength": 31,
                "val": ""
