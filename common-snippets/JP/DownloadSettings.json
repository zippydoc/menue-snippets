{
  "download_setting_frame": {
    "type": "fieldset",
    "label": "ダウンロード設定",
    "show_if": "output.custom_options=='true'",
    "repeat": false,
    "fields": {
      "output.options.location": {
        "type": "text",
        "label": "場所の指定",
        "val": "A1",
        "breakLine": true
      },
      "output.options.formatted_as_table": {
        "type": "checkbox",
        "label": "テーブルとしてフォーマット済み",
        "val": "true",
        "isOn": "true",
        "isOff": "false",
        "breakLine": true
      },
      "output.options.visibility": {
        "type": "select",
        "label": "可視性",
        "val": "current_flow",
        "options": {
          "current_flow": {
            "label": "現在のフロー"
          },
          "all_flow": {
            "label": "全てのフロー"
          }
        },
        "breakLine": true
      },
      "output.options.table_mode": {
        "type": "select",
        "label": "貼り付け",
        "val": "content_only",
        "options": {
          "content_only": {
            "label": "コンテンツのみ"
          },
          "fully_formatted": {
            "label": "完全フォーマット済み"
          }
        }
      }
    },
    "breakLine": true,
    "style": "margin-bottom: 20px"
  }
}