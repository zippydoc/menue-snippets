{
  "options.format__category": {
    "type": "select",
    "ラベル": "フォーマット",
    "required": true,
    "options": {
      "keep_current": {
        "ラベル": "現状維持"
      },
      "standard": {
        "ラベル": "標準"
      },
      "number": {
        "ラベル": "数"
      },
      "percentage": {
        "ラベル": "割合"
      },
      "text": {
        "ラベル": "テキスト"
      },
      "date": {
        "ラベル": "日付"
      },
      "currency": {
        "ラベル": "通貨"
      }
    },
    "val": "keep_current"
  },
  "options.format__rounding": {
    "type": "select",
    "ラベル": "四捨五入",
    "show_if": "options.format__category=='number' || options.format__category=='percentage' || options.format__category=='currency'",
    "required": true,
    "options": {
      "no_rounding": {
        "ラベル": "四捨五入無し"
      },
      "0": {
        "ラベル": "0桁"
      },
      "1": {
        "ラベル": "1桁"
      },
      "2": {
        "ラベル": "2桁"
      },
      "3": {
        "ラベル": "3桁"
      },
      "4": {
        "ラベル": "4桁"
      },
      "5": {
        "ラベル": "5桁"
      },
      "6": {
        "ラベル": "6桁"
      },
      "7": {
        "ラベル": "7桁"
      },
      "8": {
        "ラベル": "8桁"
      },
      "9": {
        "ラベル": "9桁"
      }
    },
    "val": "2"
  },
  "options.format__use_1000_separator": {
    "type": "checkbox",
    "ラベル": "1000セパレータ(.)を使用",
    "show_if": "options.format__category=='number' || options.format__category=='percentage' || options.format__category=='currency'",
    "val": "true",
    "isOn": "true",
    "isOff": "false"
  },
  "options.format__currency_symbol": {
    "type": "select",
    "ラベル": "シンボル",
    "show_if": "options.format__category=='currency'",
    "required": true,
    "options": {
      "dollar": {
        "ラベル": "$"
      },
      "euro": {
        "ラベル": "€"
      },
      "pound": {
        "ラベル": "£"
      }
    },
    "val": "dollar"
  },
  "options.format__date_format": {
    "type": "select",
    "ラベル": "タイプ",
    "show_if": "options.format__category=='date'",
    "required": true,
    "options": {
      "mm/dd/yyyy": {
        "ラベル": "3/14/2012"
      },
      "D, M dd, Y": {
        "ラベル": "水曜日、3月14日、2012"
      },
      "yyyy-mm-dd": {
        "ラベル": "2012-03-14"
      }
    },
    "val": "mm/dd/yyyy",
    "breakLine": true
  }
}