{
  "options.format__category": {
    "type": "select",
    "label": "フォーマット",
    "required": true,
    "options": {
      "keep_current": {
        "label": "現状維持"
      },
      "standard": {
        "label": "標準"
      },
      "number": {
        "label": "数"
      },
      "percentage": {
        "label": "割合"
      },
      "text": {
        "label": "テキスト"
      },
      "date": {
        "label": "日付"
      },
      "currency": {
        "label": "通貨"
      }
    },
    "val": "keep_current"
  },
  "options.format__rounding": {
    "type": "select",
    "label": "四捨五入",
    "show_if": "options.format__category=='number' || options.format__category=='percentage' || options.format__category=='currency'",
    "required": true,
    "options": {
      "no_rounding": {
        "label": "四捨五入無し"
      },
      "0": {
        "label": "0桁"
      },
      "1": {
        "label": "1桁"
      },
      "2": {
        "label": "2桁"
      },
      "3": {
        "label": "3桁"
      },
      "4": {
        "label": "4桁"
      },
      "5": {
        "label": "5桁"
      },
      "6": {
        "label": "6桁"
      },
      "7": {
        "label": "7桁"
      },
      "8": {
        "label": "8桁"
      },
      "9": {
        "label": "9桁"
      }
    },
    "val": "2"
  },
  "options.format__use_1000_separator": {
    "type": "checkbox",
    "label": "1000セパレータ(.)を使用",
    "show_if": "options.format__category=='number' || options.format__category=='percentage' || options.format__category=='currency'",
    "val": "true",
    "isOn": "true",
    "isOff": "false"
  },
  "options.format__currency_symbol": {
    "type": "select",
    "label": "シンボル",
    "show_if": "options.format__category=='currency'",
    "required": true,
    "options": {
      "dollar": {
        "label": "$"
      },
      "euro": {
        "label": "€"
      },
      "pound": {
        "label": "£"
      }
    },
    "val": "dollar"
  },
  "options.format__date_format": {
    "type": "select",
    "label": "タイプ",
    "show_if": "options.format__category=='date'",
    "required": true,
    "options": {
      "mm/dd/yyyy": {
        "label": "3/14/2012"
      },
      "D, M dd, Y": {
        "label": "水曜日、3月14日、2012"
      },
      "yyyy-mm-dd": {
        "label": "2012-03-14"
      }
    },
    "val": "mm/dd/yyyy",
    "breakLine": true
  }
}