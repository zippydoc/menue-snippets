{
  "placeholder": {
    "breakLine": true
  },
  "apply_to_all_columns": {
    "type": "checkbox",
    "label": "全ての列",
    "show_if": "false",
    "val": "false",
    "isOn": "true",
    "isOff": "false",
    "breakLine": true	
  },
  "apply_columns": {
    "type": "select",
    "label": "適用する列の選択",
    "show_if": "$input_table.numOfHeader >= 1",
    "required": true,
    "table": "input_table",
    "multiple": true,
    "bind": "COLUMNS",
    "breakLine": true
  },
  "apply_to_all_rows": {
    "type": "checkbox",
    "label": "全ての行",
    "show_if": "false",
    "val": "false",
    "isOn": "true",
    "isOff": "false",
    "breakLine": true
  },
  "apply_rows": {
    "type": "select",
    "label": "適用する行の選択",
    "show_if": "$input_table.numOfLabel >= 1",
    "required": true,
    "table": "input_table",
    "multiple": true,
    "bind": "LABELS",
    "breakLine": true
  },
  "intersection": {
    "type": "select",
    "label": "選択",
    "show_if": "false",
    "options": {
      "all": {
        "label": "全ての選択された行と列"
      },
      "intersection": {
        "label": "選択された行と列の交差点"
      }
    },
    "val": "intersection"
  }
}