{
  "options.cut_off_run": {
    "type": "checkbox",
    "label": "カットオフ入力テーブル",
    "val": "false",
    "isOn": "true",
    "isOff": "false"
  },
  "options.cut_off.type": {
    "type": "select",
    "label": "カットオフの種類",
    "show_if": "options.cut_off_run=='true'",
    "required": true,
    "options": {
      "from_first_row": {
        "label": "最初の行から"
      },
      "range": {
        "label": "範囲"
      }
    },
    "val": "from_first_row"
  },
  "options.cut_off.start_row_num": {
    "type": "text",
    "label": "開始行番号",
    "show_if": "options.cut_off_run=='true' && options.cut_off.type=='range'",
    "required": true,
    "minLength": 1,
    "maxLength": 7,
    "val": "1"
  },
  "options.cut_off.row_count": {
    "type": "text",
    "label": "行数",
    "show_if": "options.cut_off_run=='true'",
    "required": true,
    "minLength": 1,
    "maxLength": 7,
    "val": "100"
  }
}