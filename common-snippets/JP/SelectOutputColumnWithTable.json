{
  "output.select_output_columns": {
    "type": "checkbox",
    "ラベル": "出力列の選択",
    "val": "false",
    "isOn": "true",
    "isOff": "false"
  },
  "output_columns": {
    "type": "fieldset",
    "ラベル": "出力列の選択",
    "show_if": "output.select_output_columns=='true'",
    "repeat": true,
    "fields": {
      "table_id": {
        "type": "select",
        "ラベル": "テーブルの選択",
        "bind": "TABLES",
        "required": true
      },
      "field_name": {
        "type": "select",
        "ラベル": "列の選択",
        "table": "this.table_id",
        "bind": "COLUMNS",
        "required": true
      },
      "order": {
        "type": "select",
        "ラベル": "注文",
        "options": {
          "-": {
            "ラベル": "-"
          },
          "asc": {
            "ラベル": "昇順"
          },
          "desc": {
            "ラベル": "降順"
          }
        },
        "val": "-"
      },
      "is_rename": {
        "type": "checkbox",
        "ラベル": "名前の変更",
        "val": "false",
        "isOn": "true",
        "isOff": "false"
      },
      "rename": {
        "show_if": "this.is_rename == 'true'",
        "type": "text",
        "required": true,
        "minLength": 1,
        "maxLength": 255,
        "ラベル": "新しい列名",
        "style": "margin-left: 10px"
      },
      "format__category": {
        "type": "select",
        "ラベル": "フォーマット",
        "required": true,
        "options": {
          "standard": {
            "ラベル": "標準"
          },
          "number": {
            "ラベル": "数"
          },
          "percentage": {
            "ラベル": "割合"
          },
          "text": {
            "ラベル": "テキスト"
          },
          "date": {
            "ラベル": "日付"
          },
          "currency": {
            "ラベル": "通貨"
          }
        },
        "val": "standard"
      },
      "format__rounding": {
        "type": "select",
        "ラベル": "四捨五入",
        "show_if": "this.format__category=='number' || this.format__category=='percentage' || this.format__category=='currency'",
        "required": true,
        "options": {
          "no_rounding": {
            "ラベル": "四捨五入無し"
          },
          "0": {
            "ラベル": "0桁"
          },
          "1": {
            "ラベル": "1桁"
          },
          "2": {
            "ラベル": "2桁"
          },
          "3": {
            "ラベル": "3桁"
          },
          "4": {
            "ラベル": "4桁"
          },
          "5": {
            "ラベル": "5桁"
          },
          "6": {
            "ラベル": "6桁"
          },
          "7": {
            "ラベル": "7桁"
          },
          "8": {
            "ラベル": "8桁"
          },
          "9": {
            "ラベル": "9桁"
          }
        },
        "val": "2"
      },
      "format__use_1000_separator": {
        "type": "checkbox",
        "ラベル": "1000セパレータ(.)を使用",
        "show_if": "this.format__category=='number' || this.format__category=='percentage' || this.format__category=='currency'",
        "val": "true",
        "isOn": "true",
        "isOff": "false"
      },
      "format__currency_symbol": {
        "type": "select",
        "ラベル": "シンボル",
        "show_if": "this.format__category=='currency'",
        "required": true,
        "options": {
          "dollar": {
            "ラベル": "$"
          },
          "euro": {
            "ラベル": "€"
          },
          "pound": {
            "ラベル": "£"
          }
        },
        "val": "dollar"
      },
      "format__date_format": {
        "type": "select",
        "ラベル": "タイプ",
        "show_if": "this.format__category=='date'",
        "required": true,
        "options": {
          "mm/dd/yyyy": {
            "ラベル": "3/14/2012"
          },
          "D, M dd, Y": {
            "ラベル": "水曜日、3月14日、2012"
          },
          "yyyy-mm-dd": {
            "ラベル": "2012-03-14"
          }
        },
        "val": "mm/dd/yyyy",
        "breakLine": true
      },
      "delete": {
        "type": "button",
        "ラベル": "削除",
        "function": "DELETE_ROW"
      }
    },
    "breakLine": true,
    "style": "margin-bottom: 20px"
  }
}