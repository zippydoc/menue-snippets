{
  "input": {
    "type": "fieldset", 
    "label": "入力テーブルの選択", 
    "repeat": false, 
    "fields": {
      "input_table1": {
        "type": "select", 
        "label": "テーブル1の選択", 
        "bind": "TABLES", 
        "empty": "", 
        "required": true, 
        "breakLine": true
      }, 
      "apply_to_all_columns1": {
        "type": "checkbox", 
        "label": "全ての列", 
        "val": "true", 
        "isOn": "true", 
        "isOff": "false"
      }, 
      "apply_columns1": {
        "type": "select", 
        "label": "適用する列の選択", 
        "show_if": "apply_to_all_columns1=='false'", 
        "required": true, 
        "table": "input_table1", 
        "multiple": true, 
        "bind": "COLUMNS", 
        "breakLine": true
      }, 
      "apply_to_all_rows1": {
        "type": "checkbox", 
        "label": "全ての行", 
        "val": "true", 
        "isOn": "true", 
        "isOff": "false"
      }, 
      "apply_rows1": {
        "type": "select", 
        "label": "適用する行の選択", 
        "show_if": "apply_to_all_rows1=='false'", 
        "required": true, 
        "table": "input_table1", 
        "multiple": true, 
        "bind": "LABELS", 
        "breakLine": true
      }, 
      "input_table2": {
        "type": "select", 
        "label": "テーブル2の選択", 
        "bind": "TABLES", 
        "empty": "", 
        "required": true, 
        "breakLine": true
      }, 
      "apply_to_all_columns2": {
        "type": "checkbox", 
        "label": "全ての列", 
        "val": "true", 
        "isOn": "true", 
        "isOff": "false"
      }, 
      "apply_columns2": {
        "type": "select", 
        "label": "適用する列の選択", 
        "show_if": "apply_to_all_columns2=='false'", 
        "required": true, 
        "table": "input_table2", 
        "multiple": true, 
        "bind": "COLUMNS", 
        "breakLine": true
      }, 
      "apply_to_all_rows2": {
        "type": "checkbox", 
        "label": "全ての行", 
        "val": "true", 
        "isOn": "true", 
        "isOff": "false"
      }, 
      "apply_rows2": {
        "type": "select", 
        "label": "適用する行の選択", 
        "show_if": "apply_to_all_rows2=='false'", 
        "required": true, 
        "table": "input_table2", 
        "multiple": true, 
        "bind": "LABELS", 
        "breakLine": true
      }
    }
  }
}