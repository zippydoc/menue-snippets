{
  "output_config_frame": {
    "type": "fieldset",
    "label": "Output Configuration",
    "show_if": "output.custom_options=='true'",
    "repeat": false,
    "fields": {
      "output.add_tag": {
        "type": "checkbox",
        "label": "Add Tag",
        "val": "false",
        "isOn": "true",
        "isOff": "false"
      },
      "output.tag_name": {
        "type": "text",
        "label": "Tag Name",
        "show_if": "output.add_tag=='true'",
        "required": true,
        "minLength": 1,
        "maxLength": 255,
        "val": "",
        "breakLine": true,
        "style": "margin-left: 15px"
      },
      "table_type_frame": {
        "type": "fieldset",
        "label": "Table-Type",
        "repeat": false,
        "fields": {
          "output.column_headers": {
            "type": "select",
            "label": "Column-Headers",
            "val": "keep_current",
            "options": {
              "keep_current": {
                "label": "Keep Current"
              },
              "0_heading_top_row": {
                "label": "0 heading top rows"
              },
              "1_heading_top_row": {
                "label": "1 heading top row"
              },
              "2_heading_top_row": {
                "label": "2 heading top row"
              },
              "3_heading_top_row": {
                "label": "3 heading top row"
              }
            }
          },
          "output.row_lables": {
            "type": "select",
            "label": "Row-Lables",
            "val": "keep_current",
            "options": {
              "keep_current": {
                "label": "Keep Current"
              },
              "0_heading_left_columns": {
                "label": "0 heading left columns"
              },
              "1_heading_left_columns": {
                "label": "1 heading left columns"
              },
              "2_heading_left_columns": {
                "label": "2 heading left columns"
              },
              "3_heading_left_columns": {
                "label": "3 heading left columns"
              }
            }
          }
        }
      }
    }
  }
}
