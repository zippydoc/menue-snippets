{
  "output.download_setting": {
    "type": "checkbox",
    "etiqueta": "Configurações de descarga",
    "val": "false",
    "isOn": "true",
    "isOff": "false",
    "style": "margin-top: 10px"
  },
  "download_setting_frame": {
    "type": "fieldset",
    "etiqueta": "Configurações de descarga",
    "show_if": "output.download_setting=='true'",
    "repeat": false,
    "fields": {
      "output.options.location": {
        "type": "text",
        "etiqueta": "Especificar Localização",
        "val": "A1",
        "breakLine": true
      },
      "output.options.formatted_as_table": {
        "type": "checkbox",
        "etiqueta": "Formatado como Tabela",
        "val": "true",
        "isOn": "true",
        "isOff": "false",
        "breakLine": true
      },
      "output.add_tag": {
        "type": "checkbox",
        "etiqueta": "Adicionar Marcador",
        "val": "false",
        "isOn": "true",
        "isOff": "false"
      },
      "output.tag_name": {
        "type": "text",
        "etiqueta": "Nome do Rótulo",
        "show_if": "output.add_tag=='true'",
        "required": true,
        "minLength": 1,
        "maxLength": 255,
        "val": "",
        "breakLine": true,
        "style": "margin-left: 15px"
      },
      "output.options.visibility": {
        "type": "select",
        "etiqueta": "Visibilidade",
        "val": "current_flow",
        "options": {
          "current_flow": {
            "etiqueta": "Fluxo Atual"
          },
          "all_flow": {
            "etiqueta": "Todos os Fluxos"
          }
        },
        "breakLine": true
      },
      "output.options.table_mode": {
        "type": "select",
        "etiqueta": "Colar",
        "val": "content_only",
        "options": {
          "content_only": {
            "etiqueta": "Apenas Conteúdo"
          },
          "fully_formatted": {
            "etiqueta": "Totalmente Formatado"
          }
        }
      }
    },
    "breakLine": true
  }
}