{
  "download_setting_frame": {
    "type": "fieldset",
    "label": "Configurações de descarga",
    "show_if": "output.custom_options=='true'",
    "repeat": false,
    "fields": {
      "output.options.location": {
        "type": "text",
        "label": "Especificar Localização",
        "val": "A1",
        "breakLine": true
      },
      "output.options.formatted_as_table": {
        "type": "checkbox",
        "label": "Formatado como Tabela",
        "val": "true",
        "isOn": "true",
        "isOff": "false",
        "breakLine": true
      },
      "output.options.visibility": {
        "type": "select",
        "label": "Visibilidade",
        "val": "current_flow",
        "options": {
          "current_flow": {
            "label": "Fluxo Atual"
          },
          "all_flow": {
            "label": "Todos os Fluxos"
          }
        },
        "breakLine": true
      },
      "output.options.table_mode": {
        "type": "select",
        "label": "Colar",
        "val": "content_only",
        "options": {
          "content_only": {
            "label": "Apenas Conteúdo"
          },
          "fully_formatted": {
            "label": "Totalmente Formatado"
          }
        }
      }
    },
    "breakLine": true,
    "style": "margin-bottom: 20px"
  }
}