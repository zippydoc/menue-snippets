{
  "options.format__category": {
    "type": "select",
    "label": "Formato",
    "required": true,
    "options": {
      "keep_current": {
        "label": "Manter Atual"
      },
      "standard": {
        "label": "Padrão"
      },
      "number": {
        "label": "Número"
      },
      "percentage": {
        "label": "Percentagem"
      },
      "text": {
        "label": "Texto"
      },
      "date": {
        "label": "Data"
      },
      "currency": {
        "label": "Moeda"
      }
    },
    "val": "keep_current"
  },
  "options.format__rounding": {
    "type": "select",
    "label": "Arredondamento",
    "show_if": "options.format__category=='number' || options.format__category=='percentage' || options.format__category=='currency'",
    "required": true,
    "options": {
      "no_rounding": {
        "label": "Arredondamento de números"
      },
      "0": {
        "label": "0 dígitos"
      },
      "1": {
        "label": "1 dígito"
      },
      "2": {
        "label": "2 dígitos"
      },
      "3": {
        "label": "3 dígitos"
      },
      "4": {
        "label": "4 dígitos"
      },
      "5": {
        "label": "5 dígitos"
      },
      "6": {
        "label": "6 dígitos"
      },
      "7": {
        "label": "7 dígitos"
      },
      "8": {
        "label": "8 dígitos"
      },
      "9": {
        "label": "9 dígitos"
      }
    },
    "val": "2"
  },
  "options.format__use_1000_separator": {
    "type": "checkbox",
    "label": "Usar Separador(.) 1000",
    "show_if": "options.format__category=='number' || options.format__category=='percentage' || options.format__category=='currency'",
    "val": "true",
    "isOn": "true",
    "isOff": "false"
  },
  "options.format__currency_symbol": {
    "type": "select",
    "label": "Símbolo",
    "show_if": "options.format__category=='currency'",
    "required": true,
    "options": {
      "dollar": {
        "label": "$"
      },
      "euro": {
        "label": "€"
      },
      "pound": {
        "label": "£"
      }
    },
    "val": "dollar"
  },
  "options.format__date_format": {
    "type": "select",
    "label": "Tipo",
    "show_if": "options.format__category=='date'",
    "required": true,
    "options": {
      "mm/dd/yyyy": {
        "label": "3/14/2012"
      },
      "D, M dd, Y": {
        "label": "Quarta-feira, 14 de Março de 2012"
      },
      "yyyy-mm-dd": {
        "label": "2012-03-14"
      }
    },
    "val": "mm/dd/yyyy",
    "breakLine": true
  }
}