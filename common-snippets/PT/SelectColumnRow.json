{  "placeholder": {    "breakLine": true  },   "apply_to_all_columns": {    "type": "checkbox",     "label": "Todas as Colunas",     "val": "true",     "isOn": "true",     "isOff": "false",     "breakLine": true	  },   "apply_columns": {    "type": "select",     "label": "Selecionar Colunas a Aplicar",     "show_if": "apply_to_all_columns=='false'",     "required": true,     "table": "input_table",     "multiple": true,     "bind": "COLUMNS",     "breakLine": true  },   "apply_to_all_rows": {    "type": "checkbox",     "label": "Todas as linhas",     "val": "true",     "isOn": "true",     "isOff": "false",     "breakLine": true  },   "apply_rows": {    "type": "select",     "label": "Selecionar Linhas a Aplicar",     "show_if": "apply_to_all_rows=='false'",     "required": true,     "table": "input_table",     "multiple": true,     "bind": "LABELS",     "breakLine": true  },   "intersection": {    "type": "select",     "label": "Seleção",     "show_if": "false",     "options": {      "all": {        "label": "Todas as Linhas e Colunas selecionadas"      },       "intersection": {        "label": "Interseção de Linhas e Colunas selecionadas"      }    },     "val": "intersection"  }}