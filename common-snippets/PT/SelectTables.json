{
  "input": {
    "type": "fieldset", 
    "label": "Selecionar Tabela de Entrada", 
    "repeat": false, 
    "fields": {
      "tables": {
        "type": "fieldset", 
        "label": "Selecionar Tabelas", 
        "repeat": true, 
        "fields": {
          "table_id": {
            "type": "select", 
            "label": "Selecionar Tabela", 
            "bind": "TABLES"
          }, 
          "delete": {
            "type": "button", 
            "label": "Eliminar", 
            "function": "DELETE_ROW"
          }
        }
      }
    }
  }
}