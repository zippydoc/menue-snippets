{
  "output": {
    "type": "fieldset",
    "label": "Definir Tabela de Saída",
    "repeat": false,
    "fields": {
      "output.custom_text": {
        "type": "text",
        "label": "Mostrar Nome da Tabela",
        "show_if": "(output.set_options=='true' && output.compose_table_name=='false') || output.set_options=='false'",
        "minLength": 1,
        "val": "",
        "width": "100%",
        "breakLine": true
      },
      "output.set_options": {
        "type": "checkbox",
        "label": "Mostrar Opções",
        "val": "false",
        "isOn": "true",
        "isOff": "false"
      },
      "output_options_box": {
        "type": "fieldset",
        "label": "Mostrar Opções",
        "show_if": "output.set_options=='true'",
        "repeat": false,
        "fields": {
          "output.compose_table_name": {
            "type": "checkbox",
            "label": "Compôr Nome de Tabela",
            "val": "false",
            "isOn": "true",
            "isOff": "false",
            "breakLine": true
          },
          "compose_table_name_frame": {
            "type": "fieldset",
            "label": "Compôr Nome de Tabela de Saída",
            "show_if": "output.compose_table_name=='true'",
            "repeat": false,
            "fields": {
              "output.prefix_rule": {
                "type": "select",
                "label": "Prefixo",
                "show_if": "output.compose_table_name=='true'",
                "val": "empty",
                "options": {
                  "empty": {
                    "label": "-"
                  },
                  "script_name": {
                    "label": "Nome da Tarefa"
                  },
                  "date_string_type1": {
                    "label": "Data: dd_mm_aaaa"
                  },
                  "date_string_type2": {
                    "label": "Data: aaaa_mm_dd"
                  },
                  "time_string": {
                    "label": "Hora: h_min_seg"
                  },
                  "datetime_string": {
                    "label": "Data_Hora"
                  },
                  "custom": {
                    "label": "Personalizar"
                  }
                }
              },
              "output.prefix_text": {
                "type": "text",
                "label": "Adicionar Prefixo ao Texto",
                "show_if": "output.compose_table_name=='true' && output.prefix_rule=='custom'",
                "minLength": 1,
                "maxLength": 31,
                "val": ""
              },
              "output.core_delimiter": {
                "type": "text",
                "label": "Delimitador",
                "show_if": "output.compose_table_name=='true' && output.prefix_rule!='empty'",
                "val": "_",
                "width": "50px"
              },
              "output.core_rule": {
                "type": "select",
                "label": "Nome da Tabela",
                "show_if": "output.compose_table_name=='true'",
                "val": "input_table_name",
                "options": {
                  "input_table_name": {
                    "label": "Introduzir Nome da Tabela"
                  },
                  "script_name": {
                    "label": "Nome da Tarefa"
                  },
                  "custom": {
                    "label": "Personalizar"
                  }
                }
              },
              "output.core_text": {
                "type": "text",
                "label": "Nome da Tabela Principal",
                "show_if": "output.compose_table_name=='true' && output.core_rule=='custom'",
                "required": true,
                "minLength": 1,
                "maxLength": 31,
                "val": ""
