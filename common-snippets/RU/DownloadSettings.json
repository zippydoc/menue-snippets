{
  "download_setting_frame": {
    "type": "fieldset",
    "label": "Настройки загрузки",
    "show_if": "output.custom_options=='true'",
    "repeat": false,
    "fields": {
      "output.options.location": {
        "type": "text",
        "label": "Указать местоположение",
        "val": "A1",
        "breakLine": true
      },
      "output.options.formatted_as_table": {
        "type": "checkbox",
        "label": "Отформатировано как таблица",
        "val": "true",
        "isOn": "true",
        "isOff": "false",
        "breakLine": true
      },
      "output.options.visibility": {
        "type": "select",
        "label": "Видимость",
        "val": "current_flow",
        "options": {
          "current_flow": {
            "label": "Текущий процесс"
          },
          "all_flow": {
            "label": "Все процессы"
          }
        },
        "breakLine": true
      },
      "output.options.table_mode": {
        "type": "select",
        "label": "Вставить",
        "val": "content_only",
        "options": {
          "content_only": {
            "label": "Только содержание"
          },
          "fully_formatted": {
            "label": "Полностью отформатированно"
          }
        }
      }
    },
    "breakLine": true,
    "style": "margin-bottom: 20px"
  }
}