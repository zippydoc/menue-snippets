{
  "output.if_table_exist.type": {
    "type": "select",
    "label": "Если существует",
    "show_if": "output.custom_options=='true'",
    "val": "drop_if_exist",
    "options": {
      "drop_if_exist": {
        "label": "Создать или сбросить (если уже существует)"
      },
      "error_if_exist": {
        "label": "Создать новое (убрать ошибку, если есть)"
      },
      "error_if_not_exist": {
        "label": "Отменить (убрать ошибку, если ее нет)"
      },
      "append_all": {
        "label": "Добавить все"
      },
      "append_changes": {
        "label": "Применить изменения"
      }
    },
    "breakLine": true
  },
  "output.if_table_exist.append_changes__add_timestamp": {
    "type": "checkbox",
    "label": "Добавить метку времени",
    "show_if": "output.if_table_exist.type=='append_changes'",
    "val": "false",
    "isOn": "true",
    "isOff": "false"
  },
  "output.if_table_exist.append_changes__timestamp_colname": {
    "type": "text",
    "label": "Имя столбца временной метки",
    "show_if": "output.if_table_exist.type=='append_changes' && output.if_table_exist.append_changes__add_timestamp=='true'",
    "val": "Timestamp",
    "required": true,
    "breakLine": true
  },
  "output.if_table_exist.append_changes__add_row": {
    "type": "checkbox",
    "label": "Добавить Ряд",
    "show_if": "output.if_table_exist.type=='append_changes'",
    "val": "false",
    "isOn": "true",
    "isOff": "false"
  },
  "output.if_table_exist.append_changes__row_colname": {
    "type": "text",
    "label": "Имя столбца строки",
    "show_if": "output.if_table_exist.type=='append_changes' && output.if_table_exist.append_changes__add_row=='true'",
    "val": "Row",
    "required": true,
    "breakLine": true
  }
}