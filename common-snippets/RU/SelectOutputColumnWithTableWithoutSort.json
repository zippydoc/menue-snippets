{
  "output.select_output_columns": {
    "type": "checkbox",
    "label": "Выбрать выходные столбцы",
    "val": "false",
    "isOn": "true",
    "isOff": "false"
  },
  "output_columns": {
    "type": "fieldset",
    "label": "Select OutputColumns",
    "show_if": "output.select_output_columns==''true''",
    "repeat": true,
    "fields": {
      "table_id": {
        "type": "select",
        "label": "Выбрать таблицу",
        "bind": "TABLES",
        "required": true
      },
      "field_name": {
        "type": "select",
        "label": "Выбрать столбец",
        "table": "this.table_id",
        "bind": "COLUMNS",
        "required": true
      },
      "is_rename": {
        "type": "checkbox",
        "label": ,
        "val": "false",
        "isOn": "true",
        "isOff": "false"
      },
      "rename": {
        "show_if": "this.is_rename == ''true''",
        "type": "text",
        "required": true,
        "minLength": 1,
        "maxLength": 255,
        "label": "Название нового столбца",
        "style": "margin-left: 10px"
      },
      "format__category": {
        "type": "select",
        "label": "Формат",
        "required": true,
        "options": {
          "standard": {
            "label": "Стандартный"
          },
          "number": {
            "label": "Число"
          },
          "percentage": {
            "label": "Процент"
          },
          "text": {
            "label": "Текст"
          },
          "date": {
            "label": "Дата"
          },
          "currency": {
            "label": "Валюта"
          }
        },
        "val": "standard"
      },
      "format__rounding": {
        "type": "select",
        "label": "Округление",
        "show_if": "this.format__category==''number'' || this.format__category==''percentage'' || this.format__category==''currency''",
        "required": true,
        "options": {
          "no_rounding": {
            "label": "Округление чисел"
          },
          "0": {
            "label": "0 цифр"
          },
          "1": {
            "label": "1 цифра"
          },
          "2": {
            "label": "2 цифры"
          },
          "3": {
            "label": "3 цифры"
          },
          "4": {
            "label": "4 цифры"
          },
          "5": {
            "label": "5 цифр"
          },
          "6": {
            "label": "6 цифр"
          },
          "7": {
            "label": "7 цифр"
          },
          "8": {
            "label": "8 цифр"
          },
          "9": {
            "label": "9 цифр"
          }
        },
        "val": "2"
      },
      "format__use_1000_separator": {
        "type": "checkbox",
        "label": "Используйте сепаратор 1000(.)",
        "show_if": "this.format__category==''number'' || this.format__category==''percentage'' || this.format__category==''currency''",
        "val": "true",
        "isOn": "true",
        "isOff": "false"
      },
      "format__currency_symbol": {
        "type": "select",
        "label": "Символ",
        "show_if": "this.format__category==''currency''",
        "required": true,
        "options": {
          "dollar": {
            "label": "$"
          },
          "euro": {
            "label": "€"
          },
          "pound": {
            "label": "£"
          }
        },
        "val": "dollar"
      },
      "format__date_format": {
        "type": "select",
        "label": "Тип",
        "show_if": "this.format__category==''date''",
        "required": true,
        "options": {
          "mm/dd/yyyy": {
            "label": "3/14/2012"
          },
          "D, M dd, Y": {
            "label": "Wednesday, March 14, 2012"
          },
          "yyyy-mm-dd": {
            "label": "2012-03-14"
          }
        },
        "val": "mm/dd/yyyy",
        "breakLine": true
      },
      "delete": {
        "type": "button",
        "label": "Удалить",
        "function": "DELETE_ROW"
      }
    },
    "breakLine": true,
    "style": "margin-bottom: 20px"
  }
}