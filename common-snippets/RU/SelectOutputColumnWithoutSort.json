{
  "output.select_output_columns": {
    "type": "checkbox",
    "Метка": "Выбрать выходные таблицы",
    "val": "false",
    "isOn": "true",
    "isOff": "false"
  },
  "output_columns": {
    "type": "fieldset",
    "Метка": "Select OutputColumns",
    "show_if": "output.select_output_columns=='true'",
    "repeat": true,
    "fields": {
      "field_name": {
        "type": "select",
        "Метка": "Выбрать столбец",
        "table": "input_table",
        "bind": "COLUMNS",
        "required": true
      },
      "is_rename": {
        "type": "checkbox",
        "Метка": "Переименовать",
        "val": "false",
        "isOn": "true",
        "isOff": "false"
      },
      "rename": {
        "show_if": "this.is_rename == 'true'",
        "type": "text",
        "required": true,
        "minLength": 1,
        "maxLength": 255,
        "Метка": "Название нового столбца",
        "style": "margin-left: 10px"
      },
      "format__category": {
        "type": "select",
        "Метка": "Формат",
        "required": true,
        "options": {
          "standard": {
            "Метка": "Стандартный"
          },
          "number": {
            "Метка": "Число"
          },
          "percentage": {
            "Метка": "Процент"
          },
          "text": {
            "Метка": "Текст"
          },
          "date": {
            "Метка": "Дата"
          },
          "currency": {
            "Метка": "Валюта"
          }
        },
        "val": "standard"
      },
      "format__rounding": {
        "type": "select",
        "Метка": "Округление",
        "show_if": "this.format__category=='number' || this.format__category=='percentage' || this.format__category=='currency'",
        "required": true,
        "options": {
          "no_rounding": {
            "Метка": "Округление чисел"
          },
          "0": {
            "Метка": "0 цифр"
          },
          "1": {
            "Метка": "1 цифра"
          },
          "2": {
            "Метка": "2 цифры"
          },
          "3": {
            "Метка": "3 цифры"
          },
          "4": {
            "Метка": "4 цифры"
          },
          "5": {
            "Метка": "5 цифр"
          },
          "6": {
            "Метка": "6 цифр"
          },
          "7": {
            "Метка": "7 цифр"
          },
          "8": {
            "Метка": "8 цифр"
          },
          "9": {
            "Метка": "9 цифр"
          }
        },
        "val": "2"
      },
      "format__use_1000_separator": {
        "type": "checkbox",
        "Метка": "Используйте сепаратор 1000(.)",
        "show_if": "this.format__category=='number' || this.format__category=='percentage' || this.format__category=='currency'",
        "val": "true",
        "isOn": "true",
        "isOff": "false"
      },
      "format__currency_symbol": {
        "type": "select",
        "Метка": "Символ",
        "show_if": "this.format__category=='currency'",
        "required": true,
        "options": {
          "dollar": {
            "Метка": "$"
          },
          "euro": {
            "Метка": "€"
          },
          "pound": {
            "Метка": "£"
          }
        },
        "val": "dollar"
      },
      "format__date_format": {
        "type": "select",
        "Метка": "Тип",
        "show_if": "this.format__category=='date'",
        "required": true,
        "options": {
          "mm/dd/yyyy": {
            "Метка": "3/14/2012"
          },
          "D, M dd, Y": {
            "Метка": "Среда, 14 марта 2012 года"
          },
          "yyyy-mm-dd": {
            "Метка": "2012-03-14"
          }
        },
        "val": "mm/dd/yyyy",
        "breakLine": true
      },
      "delete": {
        "type": "button",
        "Метка": "Удалить",
        "function": "DELETE_ROW"
      }
    },
    "breakLine": true,
    "style": "margin-bottom: 20px"
  }
}