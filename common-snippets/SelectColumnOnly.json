        "isOff": "false",
        "breakLine": true
      },
      "apply_columns__all_but_selected": {
        "type": "checkbox",
        "label": "All but selected",
        "val": "false",
        "isOn": "true",
        "isOff": "false",
        "breakLine": true
      },
      "apply_columns": {
        "type": "select",
        "label": "Select Columns to Apply",
        "width": "100%",
        "table": "table_id",
        "multiple": true,
        "bind": "COLUMNS",
        "breakLine": true
      }
    }
  }
}
