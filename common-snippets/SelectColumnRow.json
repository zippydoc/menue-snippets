{
  "placeholder": {
    "breakLine": true
  },
  "apply_to_all_columns": {
    "type": "checkbox",
    "label": "All Columns",
    "show_if": "(output.configure_input_tables=='manual_selection_of_input_table' || output.configure_input_tables=='autoconfigure_input_table') && false",
    "val": "false",
    "isOn": "true",
    "isOff": "false",
    "breakLine": true	
  },
  "apply_columns": {
    "type": "select",
    "label": "Select Columns to Apply",
    "show_if": "(output.configure_input_tables=='manual_selection_of_input_table' || output.configure_input_tables=='autoconfigure_input_table') && $table_id.numOfHeader >= 1",
    "required": true,
    "table": "table_id",
    "multiple": true,
    "bind": "COLUMNS",
    "breakLine": true
  },
  "apply_to_all_rows": {
    "type": "checkbox",
    "label": "All Rows",
    "show_if": "(output.configure_input_tables=='manual_selection_of_input_table' || output.configure_input_tables=='autoconfigure_input_table') && false",
    "val": "false",
    "isOn": "true",
    "isOff": "false",
    "breakLine": true
  },
  "apply_rows": {
    "type": "select",
    "label": "Select Rows to Apply",
    "show_if": "(output.configure_input_tables=='manual_selection_of_input_table' || output.configure_input_tables=='autoconfigure_input_table') && $table_id.numOfLabel >= 1",
    "required": true,
    "table": "table_id",
    "multiple": true,
    "bind": "LABELS",
    "breakLine": true
  },
  "intersection": {
    "type": "select",
    "label": "Selection",
    "show_if": "(output.configure_input_tables=='manual_selection_of_input_table' || output.configure_input_tables=='autoconfigure_input_table') && false",
    "options": {
      "all": {
        "label": "All selected Rows & Columns"
      },
      "intersection": {
        "label": "Intersection of selected Rows & Columns"
      }
    },
    "val": "intersection"
  },
  "intersection": {
    "type": "select",
    "label": "Selection",
    "show_if": "(output.configure_input_tables=='manual_selection_of_input_table' || output.configure_input_tables=='autoconfigure_input_table') && false",
    "options": {
      "all": {
        "label": "All selected Rows & Columns"
      },
      "intersection": {
        "label": "Intersection of selected Rows & Columns"
      }
    },
    "val": "intersection"
  },
