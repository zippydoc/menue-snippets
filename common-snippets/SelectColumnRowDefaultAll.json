{
  "placeholder": {
    "breakLine": true
  },
  "apply_to_all_columns": {
    "type": "checkbox",
    "label": "All Columns",
    "show_if": "$table_id.numOfHeader >= 1",
    "val": "true",
    "isOn": "true",
    "isOff": "false",
    "breakLine": true	
  },
  "apply_columns": {
    "type": "select",
    "label": "Select Columns to Apply",
    "show_if": "$table_id.numOfHeader >= 1 && apply_to_all_columns=='false'",
    "required": true,
    "table": "table_id",
    "multiple": true,
    "bind": "COLUMNS",
    "breakLine": true
  },
  "apply_to_all_rows": {
    "type": "checkbox",
    "label": "All Rows",
    "show_if": "$table_id.numOfLabel >= 1",
    "val": "true",
    "isOn": "true",
    "isOff": "false",
    "breakLine": true
  },
  "apply_rows": {
    "type": "select",
    "label": "Select Rows to Apply",
    "show_if": "$table_id.numOfLabel >= 1 && apply_to_all_rows=='false'",
    "required": true,
    "table": "table_id",
    "multiple": true,
    "bind": "LABELS",
    "breakLine": true
  }
}
