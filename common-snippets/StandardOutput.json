{
  "output": {
    "type": "fieldset",
    "label": "Define Output Table",
    "repeat": false,
    "fields": {
      "output.prefix_rule": {
        "type": "select",
        "label": "Prefix",
        "show_if": "output.core_rule!='custom' && output.core_rule!='auto'",
        "val": "empty",
        "options": {
          "empty": {
            "label": "-"
          },
          "script_name": {
            "label": "Script Name"
          },
          "date_string_type1": {
            "label": "Date: dd_mm_yyyy"
          },
          "date_string_type2": {
            "label": "Date: yyyy_mm_dd"
          },
          "time_string": {
            "label": "Time: h_min_sec"
          },
          "datetime_string": {
            "label": "Date_Time"
          },
          "custom": {
            "label": "Custom"
          }
        }
      },
      "output.prefix_text": {
        "type": "text",
        "label": "Prefix Text",
        "show_if": "output.core_rule!='custom' && output.prefix_rule=='custom' && output.core_rule!='auto'",
        "minLength": 1,
        "maxLength": 31,
        "val": ""
      },
      "output.core_delimiter": {
        "type": "text",
        "label": "Delimiter",
        "show_if": "output.core_rule!='custom' && output.core_rule!='auto' && output.prefix_rule!='empty'",
        "val": "_",
        "width": "50px"
      },
      "output.core_rule": {
        "type": "select",
        "label": "Table Name",
        "val": "auto",
        "options": {
          "auto": {
            "label": "Auto"
          },
          "custom": {
            "label": "Custom"
          },
          "input_table_name": {
            "label": "Input Table Name"
          },
          "script_name": {
            "label": "Script Name"
          }
        }
      },
      "output.core_text": {
        "type": "text",
        "label": "Output Table Name",
        "show_if": "output.core_rule=='custom'",
        "required": true,
        "minLength": 1,
        "maxLength": 31,
        "val": ""
      },
      "output.suffix_delimiter": {
        "type": "text",
        "label": "Delimiter",
        "show_if": "output.core_rule!='custom' && output.core_rule!='auto'",
        "val": "_",
        "width": "50px"
      },
      "output.suffix_rule": {
        "type": "select",
        "label": "Suffix",
        "show_if": "output.core_rule!='custom' && output.core_rule!='auto'",
        "val": "script_name",
        "options": {
          "empty": {
            "label": "-"
          },
          "script_name": {
            "label": "Script Name"
          },
          "date_string_type1": {
            "label": "Date: dd_mm_yyyy"
          },
          "date_string_type2": {
            "label": "Date: yyyy_mm_dd"
          },
          "time_string": {
            "label": "Time: h_min_sec"
          },
          "datetime_string": {
            "label": "Date_Time"
          },
          "custom": {
            "label": "Custom"
          }
        }
      },
      "output.suffix_text": {
        "type": "text",
        "label": "Suffix Text",
        "show_if": "output.core_rule!='custom' && output.suffix_rule=='custom' && output.core_rule!='auto'",
        "minLength": 1,
        "maxLength": 31,
        "val": "",
        "breakLine": true
      },
      "output.timezone": {
        "type": "select",
        "label": "Timezone",
        "show_if": "output.core_rule!='custom' && output.core_rule!='auto' && (output.prefix_rule=='date_string_type1' || output.prefix_rule=='date_string_type2' || output.prefix_rule=='time_string' || output.prefix_rule=='datetime_string' || output.core_rule=='date_string_type1' || output.core_rule=='date_string_type2' || output.core_rule=='time_string' || output.core_rule=='datetime_string' || output.suffix_rule=='date_string_type1' || output.suffix_rule=='date_string_type2' || output.suffix_rule=='time_string' || output.suffix_rule=='datetime_string')",
        "val": "Europe/Berlin",
        "required": true,
        "options": {
          "Europe/Amsterdam": {
            "label": "Europe/Amsterdam"
          },
          "Europe/Berlin": {
            "label": "Europe/Berlin"
          },
          "Europe/Helsinki": {
            "label": "Europe/Helsinki"
          },
          "Europe/London": {
            "label": "Europe/London"
          },
          "Europe/Luxembourg": {
            "label": "Europe/Luxembourg"
          },
          "Europe/Oslo": {
            "label": "Europe/Oslo"
          },
          "Europe/Paris": {
            "label": "Europe/Paris"
          },
          "Europe/Rome": {
            "label": "Europe/Rome"
          },
          "US/Alaska": {
            "label": "US/Alaska"
          },
          "US/Central": {
            "label": "US/Central"
          },
          "US/Eastern": {
            "label": "US/Eastern"
          },
          "US/Pacific": {
            "label": "US/Pacific"
          }
        },
        "breakLine": true
      },
      "output.set_options": {
        "type": "checkbox",
        "label": "Output Options",
        "val": "false",
        "isOn": "true",
        "isOff": "false"
      },
      "output_options_box": {
        "type": "fieldset",
        "label": "Output Options",
        "show_if": "output.set_options=='true'",
        "repeat": false,
        "fields": {
          "output.options.location": {
            "type": "text",
            "label": "Specify Location",
            "val": "A1",
            "breakLine": true
          },
          "output.options.table_mode": {
            "type": "select",
            "val": "content_only_table",
            "options": {
              "content_only_table": {
                "label": "Content Only Table"
              },
              "fully_formatted_table": {
                "label": "Fully Formatted Table"
              }
            },
            "breakLine": true
          },
          "output.options.formatted_as_table": {
            "type": "checkbox",
            "label": "Formatted as Table",
            "val": "true",
            "isOn": "true",
            "isOff": "false",
            "breakLine": true
          },
          "output.add_tag": {
            "type": "checkbox",
            "label": "Add Tag",
            "val": "false",
            "isOn": "true",
            "isOff": "false"
          },
          "output.tag_name": {
            "type": "text",
            "label": "Tag Name",
            "show_if": "output.add_tag=='true'",
            "required": true,
            "minLength": 1,
            "maxLength": 255,
            "val": "",
            "breakLine": true,
            "style": "margin-left: 15px"
          },
          "output.options.set_visibility": {
            "type": "checkbox",
            "label": "Visibility",
            "val": "false",
            "isOn": "true",
            "isOff": "false"
          },
          "output.options.visibility": {
            "type": "select",
            "show_if": "output.options.set_visibility=='true'",
            "val": "current_flow",
            "options": {
              "current_flow": {
                "label": "Current Flow"
              },
              "all_flow": {
                "label": "All Flows"
              }
            }
          }
        }
      }
    }
  }
}