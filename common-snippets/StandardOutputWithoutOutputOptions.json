{
  "output": {
    "type": "fieldset",
    "label": "Define Output Table",
    "repeat": false,
    "fields": {
      "output.type": {
        "type": "select",
        "label": "Table Name",
        "val": "auto",
        "options": {
          "auto": {
            "label": "Auto"
          },
          "custom": {
            "label": "Custom"
          },
          "compose": {
            "label": "Compose"
          }
        }
      },
      "output.custom_text": {
        "type": "text",
        "label": "Output Table Name",
        "show_if": "output.type=='custom'",
        "required": true,
        "minLength": 1,
        "maxLength": 31,
        "val": "",
        "breakLine": true
      },
      "output.prefix_rule": {
        "type": "select",
        "label": "Prefix",
        "show_if": "output.type=='compose'",
        "val": "empty",
        "options": {
          "empty": {
            "label": "-"
          },
          "script_name": {
            "label": "Task Name"
          },
          "date_string_type1": {
            "label": "Date: dd_mm_yyyy"
          },
          "date_string_type2": {
            "label": "Date: yyyy_mm_dd"
          },
          "time_string": {
            "label": "Time: h_min_sec"
          },
          "datetime_string": {
            "label": "Date_Time"
          },
          "custom": {
            "label": "Custom"
          }
        }
      },
      "output.prefix_text": {
        "type": "text",
        "label": "Prefix Text",
        "show_if": "output.type=='compose' && output.prefix_rule=='custom'",
        "minLength": 1,
        "maxLength": 31,
        "val": ""
      },
      "output.core_delimiter": {
        "type": "text",
        "label": "Delimiter",
        "show_if": "output.type=='compose' && output.prefix_rule!='empty'",
        "val": "_",
        "width": "50px"
      },
      "output.core_rule": {
        "type": "select",
        "label": "Table Name",
        "show_if": "output.type=='compose'",
        "val": "input_table_name",
        "options": {
          "input_table_name": {
            "label": "Input Table Name"
          },
          "script_name": {
            "label": "Task Name"
          },
          "custom": {
            "label": "Custom"
          }
        }
      },
      "output.core_text": {
        "type": "text",
        "label": "Main Table Name",
        "show_if": "output.type=='compose' && output.core_rule=='custom'",
        "required": true,
        "minLength": 1,
        "maxLength": 31,
        "val": ""
      },
      "output.suffix_delimiter": {
        "type": "text",
        "label": "Delimiter",
        "show_if": "output.type=='compose'",
        "val": "_",
        "width": "50px"
      },
      "output.suffix_rule": {
        "type": "select",
        "label": "Suffix",
        "show_if": "output.type=='compose'",
        "val": "script_name",
        "options": {
          "empty": {
            "label": "-"
          },
          "script_name": {
            "label": "Task Name"
          },
          "date_string_type1": {
            "label": "Date: dd_mm_yyyy"
          },
          "date_string_type2": {
            "label": "Date: yyyy_mm_dd"
          },
          "time_string": {
            "label": "Time: h_min_sec"
          },
          "datetime_string": {
            "label": "Date_Time"
          },
          "custom": {
            "label": "Custom"
          }
        }
      },
      "output.suffix_text": {
        "type": "text",
        "label": "Suffix Text",
        "show_if": "output.type=='compose' && output.suffix_rule=='custom'",
        "minLength": 1,
        "maxLength": 31,
        "val": "",
        "breakLine": true
      },
      "output.timezone": {
        "type": "select",
        "label": "Timezone",
        "show_if": "output.type=='compose' && (output.prefix_rule=='date_string_type1' || output.prefix_rule=='date_string_type2' || output.prefix_rule=='time_string' || output.prefix_rule=='datetime_string' || output.suffix_rule=='date_string_type1' || output.suffix_rule=='date_string_type2' || output.suffix_rule=='time_string' || output.suffix_rule=='datetime_string')",
        "val": "Europe/Berlin",
        "required": true,
        "options": {
          "Europe/Amsterdam": {
            "label": "Europe/Amsterdam"
          },
          "Europe/Berlin": {
            "label": "Europe/Berlin"
          },
          "Europe/Helsinki": {
            "label": "Europe/Helsinki"
          },
          "Europe/London": {
            "label": "Europe/London"
          },
          "Europe/Luxembourg": {
            "label": "Europe/Luxembourg"
          },
          "Europe/Oslo": {
            "label": "Europe/Oslo"
          },
          "Europe/Paris": {
            "label": "Europe/Paris"
          },
          "Europe/Rome": {
            "label": "Europe/Rome"
          },
          "US/Alaska": {
            "label": "US/Alaska"
          },
          "US/Central": {
            "label": "US/Central"
          },
          "US/Eastern": {
            "label": "US/Eastern"
          },
          "US/Pacific": {
            "label": "US/Pacific"
          }
        },
        "breakLine": true
      }
    }
  }
}