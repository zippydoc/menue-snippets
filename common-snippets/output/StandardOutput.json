{
  "output": {
    "type": "fieldset",
    "label": "Output and Settings",
    "show_if": "output.output_and_setting=='true'",
    "repeat": false,
    "fields": {
      "output.output_as_new_table": {
        "type": "checkbox",
        "label": "Output as New Table",
        "val": "true",
        "isOn": "true",
        "isOff": "false",
        "breakLine": true
      },
      "output.reverse_order": {
        "type": "checkbox",
        "label": "Reverse order",
        "show_if": "output.select_output_columns_type=='all' || output.select_output_columns_type=='standard'",
        "val": "false",
        "isOn": "true",
        "isOff": "false",
        "breakLine": true
      },
      "output.add_tag": {
        "type": "checkbox",
        "label": "Add Tag",
        "val": "false",
        "isOn": "true",
        "isOff": "false"
      },
      "output.tag_name": {
        "type": "text",
        "label": "Tag Name",
        "show_if": "output.add_tag=='true'",
        "required": true,
        "minLength": 1,
        "maxLength": 255,
        "val": "",
        "breakLine": true,
        "style": "margin-left: 15px"
      }
    }
  }
}