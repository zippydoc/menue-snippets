{
  "input": {
    "type": "fieldset",
    "label": "Select Input Table",
    "style": "border-radius: 3px; border: 1px solid #ccc",
    "show_if": "output.input_table_auto_config=='false'",
    "repeat": false,
    "fields": {
      "table_id": {
        "type": "select",
        "label": "Select Table",
        "bind": "TABLES",
        "empty": "",
        "required": true,
        "breakLine": true
      },
      "remove_input_table_after_processing": {
        "type": "hidden",
        "val": "false"
      },
      "is_word": {
        "type": "hidden",
        "val": "false"
      }
    }
  }
}
