        "label": "Table has header",
        "show_if": "$rootScope.hostType=='Excel' && (configure_input_tables=='excel_sheet' || configure_input_tables=='excel_column_and_row_range' || configure_input_tables=='excel_range_name')",
        "val": "true",
        "isOn": "true",
        "isOff": "false",
        "breakLine": true
      },
      "excel_sheet__active_sheet": {
        "type": "checkbox",
        "label": "Active Sheet",
